<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="JobTest">

	<!-- 메세지맵 리스트 가져오기 -->
	<select id="JobTestDAO.getMessageList" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
		<![CDATA[
			SELECT
			    MSSAGE_CODE, MSSAGE_TY, MSSAGE_NM, MSSAGE_DC
			FROM TBCOM_MSSAGE
			WHERE 1=1 
			  AND #name# = #name#
		]]>
	</select> 
	
	
	
</sqlMap>