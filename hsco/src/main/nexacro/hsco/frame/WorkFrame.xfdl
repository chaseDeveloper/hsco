this.gv_nTimerCount = 0;
this.WorkFrame_ontimer = function(obj:Form, e:nexacro.TimerEventInfo)
{
	this.gv_nTimerCount++;
	this.killTimer(1);
	
	if(this.gv_nTimerCount <= 50)
