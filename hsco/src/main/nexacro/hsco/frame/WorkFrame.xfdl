   if(fRow > -1){ 
        // 도움말 존재하는것만 버튼 활성화
        hpcm_at = application.gds_menu.getColumn(fRow, "HPCM_AT");
       // this.alert("hpcm_at:"+hpcm_at);
     //  trace("hpcm_at:"+hpcm_at);
		if( hpcm_at == "1" ) this.btn_help.set_enable(true);
