/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	var decsnYear = this.getOwnerFrame().decsnYear;		// 결의년도
	var decsnNo	  = this.getOwnerFrame().decsnNo;		// 결의번호
	var hscoIncAcnutAt = this.getOwnerFrame().trgFormId == 'ACC070101' ? "1" : "0";
	
	if(!this.comUtils.isNull(decsnNo)) {
		this.strRtnGbn = "U";
		
		// 수입결의서 조회
		this.ds_cond.clearData();
		this.ds_cond.addRow();
		this.ds_cond.setColumn(0, "DECSN_YEAR"	, decsnYear);
		this.ds_cond.setColumn(0, "DECSN_NO"	, decsnNo);
		
		this.fn_transaction("selectIncDsn");
	} else {
		this.strRtnGbn = "I";
		var today = this.dateUtils.today();	
		
		if(this.comUtils.isNull(this.getOwnerFrame().dsIncDsnMastr)) {
			this.ds_incDsnMastr.addRow();
			this.ds_incDsnMastr.setColumn(0, "DECSN_YEAR"				, decsnYear);	// 결의년도
			this.ds_incDsnMastr.setColumn(0, "BUDGET_YEAR"				, decsnYear);	// 예산년도
			this.ds_incDsnMastr.setColumn(0, "ANACT_KND"				, "21");		// 결의서종류
			this.ds_incDsnMastr.setColumn(0, "ITNC_DE"					, today);		// 발의일자
			this.ds_incDsnMastr.setColumn(0, "INC_BUDGET_ARNG_RGIST"	, today);		// 수입예산정리부등기
			this.ds_incDsnMastr.setColumn(0, "DECSN_AMOUNT"				, "0");			// 결의금액
			this.ds_incDsnMastr.setColumn(0, "DEPT_CODE"				, this.gfn_getDeptId());	// 부서코드
			this.ds_incDsnMastr.setColumn(0, "DEPT_NM"					, this.gfn_getDeptName());	// 부서명
			this.ds_incDsnMastr.setColumn(0, "BUDGET_DEPT_CODE"			, this.gfn_getDeptId());	// 부서코드
			this.ds_incDsnMastr.setColumn(0, "BUDGET_DEPT_NM"			, this.gfn_getDeptName());	// 부서명
			this.ds_incDsnMastr.setColumn(0, "HSCO_INC_ACNUT_AT"		, hscoIncAcnutAt);
		} else {
			this.fn_loadXml(this.getOwnerFrame().dsIncDsnMastr, this.ds_incDsnMastr);
			this.fn_setDatasetType(this.ds_incDsnMastr, "I");
		}
		
		if(this.comUtils.isNull(this.getOwnerFrame().dsIncDsnDetail)) {
			this.ds_incDsnDetail.addRow();
			this.ds_incDsnDetail.setColumn(0, "DECSN_AMOUNT", "0");
			this.ds_incDsnDetail.setColumn(0, "SUPLY_AMOUNT", "0");
			this.ds_incDsnDetail.setColumn(0, "VAT", "0");
		} else {
			this.fn_loadXml(this.getOwnerFrame().dsIncDsnDetail, this.ds_incDsnDetail);
			this.tab.tabpage1.div_detail00.msk_DECSN_AMOUNT.set_cssclass("msk_WF_Readonly");
			this.tab.tabpage1.div_detail00.msk_SUPLY_AMOUNT.set_cssclass("msk_WF_Readonly");
			this.tab.tabpage1.div_detail00.msk_VAT.set_cssclass("msk_WF_Readonly");
			
			this.tab.tabpage1.div_detail00.msk_DECSN_AMOUNT.set_readonly(true);
			this.tab.tabpage1.div_detail00.msk_SUPLY_AMOUNT.set_readonly(true);
			this.tab.tabpage1.div_detail00.msk_VAT.set_readonly(true);
			
			this.fn_setDatasetType(this.ds_incDsnDetail, "I");
		}
		
		// 결의번호가 등록되어야하는 테이블의 Dataset
		if(!this.comUtils.isNull(this.getOwnerFrame().dsDecsnNo)) {
			this.ds_dscsnNo.loadXML(this.getOwnerFrame().dsDecsnNo);
		}
	}
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_incDsnMastr) 	|| 
	   Ex.util.isUpdated(this.ds_incDsnDetail)) {
	   
	   	if(!this.gfn_checkValidation_NoUpdChk(this.ds_incDsnMastr, this.ds_validation)) {
			return;
		}
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_incDsnDetail, this.ds_validation)) {
			return;
		}
		
		if(this.ds_incDsnDetail.getRowCount() == 0) {
			this.gfn_message("info.처리불가","세부내용이 입력되지 않았습니다.");
			return;
		}

		this.fn_transaction("saveIncDsn");
	} 
	else {
		this.gfn_message("comm.내역.변경.없음");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var sArgs = "";
	
	switch (strSvcId) {
		case "selectIncDsn":
			var sController   	= "hsco/mis/acc/ACC070101/selectIncDsn.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_incDsnMastr=output1 ds_incDsnDetail=output2";
			break;
		case "saveIncDsn":
			var sController   	= "hsco/mis/acc/ACC070101/saveIncDsn.do";
			var sInDatasets   	= "input1=ds_incDsnMastr:A input2=ds_incDsnDetail:U";
			var sOutDatasets  	= "ds_incDsnMastr=output1";
			
			// 결의서 팝업을 호출한 화면ID를 넘긴다.
			if(!this.comUtils.isNull(this.getOwnerFrame().trgFormId)) {
				sArgs = "TRG_FORM_ID=" + this.getOwnerFrame().trgFormId;
			}
			break;
		case "selectAtchFile":		// 첨부파일 조회
			var sController   	= "hsco/mis/acc/ACC070101/selectAtchFile.do";
			var sInDatasets   	= "input1=ds_cond_file";
			var sOutDatasets  	= "ds_fileList=output1";
			break;    
		default:
		break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if (nErrorCode == 0) {
		switch(strSvcId) {
			case "selectIncDsn":
				this.fn_authCheck();
				var fileSn = this.ds_incDsnMastr.getColumn(0, "FILE_SN");
				if(!this.comUtils.isNull(fileSn)){
					this.ds_cond_file.clearData();
					this.ds_cond_file.addRow();
					this.ds_cond_file.setColumn(0, "FILE_SN", fileSn);
					this.fn_transaction("selectAtchFile");
				}
				break;
			case "selectAtchFile":
				break;
			case "saveIncDsn":
				this.gfn_message("success.처리.성공", null, null, null, this.fn_saveCallBack);
				break;
			default:
				break;
		}
	}
	
	// 오류 및 성공 메시지를 화면에 보여준다.
	this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
}


/*
 * 함수명  	: fn_saveCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_saveCallBack = function()
{
	if(this.strRtnGbn == "I") {
		this.close("C");
	} else {
		this.fn_search();
	}
}


