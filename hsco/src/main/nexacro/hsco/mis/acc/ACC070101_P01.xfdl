	this.File_UpDownManager.download(fileSn, fileOrder, fileName);
}

this.ds_incDsnDetail_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	switch(e.columnid) {
		case "DECSN_AMOUNT":
			obj.setColumn(e.row, "SUPLY_AMOUNT", e.newvalue - obj.getColumn(e.row, "VAT"));
			this.ds_incDsnMastr.setColumn(0, "DECSN_AMOUNT", obj.getSum("DECSN_AMOUNT"));
			break;
		case "SUPLY_AMOUNT":
		case "VAT":
			obj.setColumn(e.row, "DECSN_AMOUNT", obj.getColumn(e.row, "SUPLY_AMOUNT")+obj.getColumn(e.row, "VAT"));
			this.ds_incDsnMastr.setColumn(0, "DECSN_AMOUNT", obj.getSum("DECSN_AMOUNT"));
			break;
		default:
			break;
	}
}

this.ds_incDsnDetail_onrowsetchanged = function(obj:Dataset, e:nexacro.DSRowsetChangeEventInfo)
{
	this.ds_incDsnMastr.setColumn(0, "DECSN_AMOUNT", obj.getSum("DECSN_AMOUNT"));			// 결의금액 합계
}

this.fn_sanction = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 결재 전 유효성 체크
	if(this.comUtils.isDatasetUpdated(this.ds_incDsnMastr) || this.comUtils.isDatasetUpdated(this.ds_incDsnDetail)) {
		this.gfn_message("comm.정보.저장.요청", "수입결의서");
		return;
	}
	if(this.ds_incDsnMastr.getRowCount() == 0) {
		this.gfn_message("info.처리불가","계정정보/결의정보를 찾을 수 없습니다.");
		return;
	}
	if(this.ds_incDsnDetail.getRowCount() == 0) {
		this.gfn_message("info.처리불가","세부내용을 찾을 수 없습니다.");
		return;
	}
	
	// 결재상신
	var bcncNms = new Array();
	for(var i = 0; i < this.ds_incDsnDetail.getRowCount(); i++) {
		bcncNms.push(this.ds_incDsnDetail.getColumn(i,"NM"));
	}
	
	var sumry = typeof this.ds_incDsnMastr.getColumn(0,"SUMRY1") == "undefined" ? "" : this.ds_incDsnMastr.getColumn(0,"SUMRY1");
	sumry += String.fromCharCode(13);
	sumry += typeof this.ds_incDsnMastr.getColumn(0,"SUMRY2") == "undefined" ? "" : this.ds_incDsnMastr.getColumn(0,"SUMRY2");
	
	var arrSancData = [
		 "[수입]"+this.ds_incDsnMastr.getColumn(0,"SUMRY1")				//결재제목
		,this.fn_dateStr(this.ds_incDsnMastr.getColumn(0,"ITNC_DE"),".")					//발의일자
		,this.fn_dateStr(this.ds_incDsnMastr.getColumn(0,"NHT_PBLICTE_DE"),".") 			//고지서발행
		,this.ds_incDsnMastr.getColumn(0,"PAY_TMLMT") 										//납입기한
		,this.fn_dateStr(this.ds_incDsnMastr.getColumn(0,"INC_BUDGET_ARNG_RGIST"),".") 		//수입예산정리부등기
		,this.ds_incDsnMastr.getColumn(0,"NHT_NO")											//고지서번호
		,this.ds_incDsnMastr.getColumn(0,"BUDGET_YEAR") 									//년도
		,this.ds_incDsnMastr.getColumn(0,"GWAN_NM") 										//관
		,this.ds_incDsnMastr.getColumn(0,"HANG_NM") 										//항
		,this.ds_incDsnMastr.getColumn(0,"SEHANG_NM") 										//세항
		,this.ds_incDsnMastr.getColumn(0,"MOK_NM") 											//목
		,"일금 "+this.fn_numToHangul(this.ds_incDsnMastr.getColumn(0,"DECSN_AMOUNT"))+"원정"	//총금액_한글
		,"\\"+this.comUtils.formatComma(this.ds_incDsnMastr.getColumn(0,"DECSN_AMOUNT"))		//총금액
		,bcncNms.join(", ")							 										//납입자성명
		,sumry															//적요
	];

	this.fn_callHandySanc2(
		 this						// context
		,'0000000064'				// formId
		,this.ds_incDsnMastr		// misDataset
		,"TBACC_INC_DSN_MASTR"		// tableName
		,["DECSN_YEAR","DECSN_NO"]	// tableKey
		,arrSancData				// arrSancData
		,null
		,null
		,null
		,this.ds_fileList
	);
}
