 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	
	var today = this.dateUtils.today();
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "YEAR", today.substr(0,4));
	this.ds_cond.setColumn(0, "SRCH_ST_DT", today.substr(0,4)+"0101");
	this.ds_cond.setColumn(0, "SRCH_ED_DT", today);
	this.ds_cond.setColumn(0, "PRINT_SE", "A4");
	
	this.ds_cond.setColumn(0, "STACNT_YEAR", today.substr(0,4));
	this.ds_cond.setColumn(0, "STACNT_MONTH_ST", "01");
	this.ds_cond.setColumn(0, "STACNT_MONTH_ED", today.substr(4,2));
}

/*
 * 함수명  	: initDataset
