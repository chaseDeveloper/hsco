/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}

	switch(this.tab.tabindex) {
		case 0:
			this.fn_transaction("smmprdyTableList");
			break;
		case 1:
			if(Number(this.ds_cond.getColumn(0, "STACNT_MONTH_ST"))>Number(this.ds_cond.getColumn(0, "STACNT_MONTH_ED"))){
				this.gfn_message("errors.항목값.범위.이하","월계표의 시작월|종료월");
				return;
			}
			this.fn_transaction("wolgTableList");
			break;
		case 2:
			this.fn_transaction("smTableList");
			break;
	}	
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "smmprdyTableList": 
			var sController   	= "hsco/mis/acc/ACC020406/smmprdyTableList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_smmprdyTable=output1";
			break;
		case "wolgTableList": 
			var sController   	= "hsco/mis/acc/ACC020406/wolgTableList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_wolgTable=output1";
			break;
		case "smTableList":
			var sController   	= "hsco/mis/acc/ACC020406/smTableList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_smTable=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if (nErrorCode != 0) {
		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "smmprdyTableList":
				break;
			case "wolgTableList": 
				break;
			case "smTableList":
				break;
			default:
				break;
		}

		// 오류 및 성공 메시지를 화면에 보여준다.
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "smmprdyTableList":
				break;
			case "wolgTableList": 
				break;
			case "smTableList":
				break;
			default:
				break;
		}
	}
}

