 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	switch(this.tab.tabindex) {
		case 0:
			this.fn_transaction("seAccnutAcntCodeList");
			break;
		case 1:
			this.fn_transaction("seAccnutAcntTyManageList");
			break;
	}
}

