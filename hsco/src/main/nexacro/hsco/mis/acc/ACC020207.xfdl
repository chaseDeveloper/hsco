 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACC020207_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_misFormInit(this);
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();

	// 공통코드
	var comboParams = [
	    ["ds_bdg001", "BDG001", "Y", "D", "B"]	// 사업구분(계정구분)
	   ,["ds_bdg002", "BDG002", "Y", "D", "B"]	// 수지구분
	   ,["ds_acc002", "ACC002", "Y", "S", "B"]	// 결의서구분
	   ,["ds_com022", "COM022", "Y", "S", "B"]	// 결재상태
	   ,["ds_com021", "COM021", "Y", "D", "B"]	// 은행코드
	   ,["ds_ctr049", "CTR049", "Y", "D", "B"]	// 계약방식(계약방법)
	   ,["ds_law017", "LAW017", "Y", "D", "B"]	// 지급구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_search();	//  최초조회
	};
	this.gfn_comboLoad(comboParams, callback);
}

/***********************************************************************
