 ***********************************************************************/
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	
	// 데이터셋 동적 바인딩
	this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	if(this.gfn_authGrpId("ACC_ADMIN") == -1 && this.gfn_authGrpId("ACC_USER") == -1) {
         this.div_search.div_srchDept.fn_setReadonly(true);
    }
	
	var today = this.dateUtils.today();
	
	this.ds_cond.set_enableevent(false);
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	//var todayDe = this.gfn_today();
	
	this.ds_cond.setColumn(0, "SRCH_ST_DT", this.dateUtils.addDate(today, -14, 's'));
	this.ds_cond.setColumn(0, "SRCH_ED_DT", this.dateUtils.addDate(today, 14, 's'));
	this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId()  );	// 부서코드
	this.ds_cond.setColumn(0, "DEPT_NM",   this.gfn_getDeptName());	// 부서명		
	this.ds_cond.setColumn(0, "STDR_DE", "3");
	this.ds_cond.setColumn(0, "SEARCH_GUBUN", 2);
	this.ds_cond.set_enableevent(true);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
}

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 지출결의 목록 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	this.ds_wikCptalPlan.clearData();
	this.ds_expDsnMastrHist.clearData();
	this.ds_expDsnMtrDetHist.clearData();
	this.fn_transaction("SRCH");
}

/*
 * 함수명  	: fn_searchSub
 * 설명 		: 지출결의 이력 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_searchSub = function()
{	
	this.ds_expDsnMastrHist.clearData();
	this.ds_expDsnMtrDetHist.clearData();
	this.ds_condSub.clearData();
	this.ds_condSub.addRow();
	var mRow = this.ds_wikCptalPlan.rowposition;
	var decsnYear = this.ds_wikCptalPlan.getColumn(mRow, "DECSN_YEAR");
	var decsnNo = this.ds_wikCptalPlan.getColumn(mRow, "DECSN_NO");
	
	this.ds_condSub.setColumn(0, "DECSN_YEAR", decsnYear);
	this.ds_condSub.setColumn(0, "DECSN_NO", decsnNo);
	this.fn_transaction("SRCH_SUB");
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var argument     = "";
	var callbackFunc = "fn_callBack";
	var showProgress = true;
	var bAsync       = true;
	
	switch (strSvcId) {		
		case "SRCH": // 지출결의 목록 조회
			var sController   	= "hsco/mis/acc/ACC020207/wikCptalPlanList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_wikCptalPlan=output1";
			break;
		case "SRCH_SUB": // 지출결의이력 조회
			var sController   	= "hsco/mis/acc/ACC020207/selectExpDsnMastrHist.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_expDsnMastrHist=output1 ds_expDsnMtrDetHist=output2";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, argument, callbackFunc, showProgress, bAsync);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0') {
		switch(strSvcId) {
			case "SRCH": // 지출결의 목록 조회
				break;
			case "SRCH_SUB": // 지출결의이력 조회
				break;
			default:
				break;
		}
	}
	
}


