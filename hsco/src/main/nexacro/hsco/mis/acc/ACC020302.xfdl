 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */


/***********************************************************************
 * Script Include 
 ************************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

// 업무에 필요한 공통 라이브러리 파일을 추가할 수 있다.
// TODO


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
var errorDs = [this.ds_lqttTransfrError, this.ds_bmnyTransfrDetail, this.ds_salaryTransfrError];
 
 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACC020302_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();
	this.fn_misFormInit(this);



}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	
	// 화면 로드 시 조회를 해야 하는 경우
	//========================================
	// 검색조건 컴포넌트 초기값 부여
	var today = this.dateUtils.today();
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "SRCH_DT", today);
	this.ds_cond.setColumn(0, "BPLC_SE", "01");
}

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	switch(this.tab.tabindex) {
		case 0:	// 대량이체
			this.fn_transaction("lqttTransfrList");
			break;
		case 1:	// 급여이체
			this.fn_transaction("salaryTransfrList");
			break;
	}
	
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	switch(this.tab.tabindex) {
		case 0:
			this.fn_transaction("lqttTransfrD");
			break;
		case 1:
			this.fn_transaction("salaryTransfrD");
			break;
	}
	
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "lqttTransfrList": 		// 대량이체
			var sController   	= "hsco/mis/acc/ACC020302/lqttTransfrList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_lqttTransfr=output1";
			this.ds_lqttTransfr.clearData();
			this.ds_lqttTransfrDetail.clearData();
			this.ds_lqttTransfrError.clearData();
			break;
		case "lqttTransfrDetailList": 	// 대량이체상세
			var sController   	= "hsco/mis/acc/ACC020302/lqttTransfrDetailList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_lqttTransfrDetail=output1 ds_lqttTransfrError=output2";
			this.ds_lqttTransfrDetail.clearData();
			this.ds_lqttTransfrError.clearData();
			break;
		case "salaryTransfrList": 		// 급여이체
			var sController   	= "hsco/mis/acc/ACC020302/salaryTransfrList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_salaryTransfr=output1";
			this.ds_salaryTransfr.clearData();
			this.ds_salaryTransfrDetail.clearData();
			this.ds_salaryTransfrError.clearData();
			break;
		case "salaryTransfrDetailList": // 급여이체상세
			var sController   	= "hsco/mis/acc/ACC020302/salaryTransfrDetailList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_salaryTransfrDetail=output1 ds_salaryTransfrError=output2";
			this.ds_salaryTransfrDetail.clearData();
			this.ds_salaryTransfrError.clearData();
			break;
		case "errorPorcessU":			// 오류처리
			var sController   	= "hsco/mis/acc/ACC020302/errorPorcessU.do";
			var sInDatasets   	= "input1="+errorDs[this.tab.tabindex].name+":A";
			var sOutDatasets  	= "";
			break;
		case "lqttTransfrD": 			// 대량이체삭제
			var sController   	= "hsco/mis/acc/ACC020302/lqttTransfrD.do";
			var sInDatasets   	= "input1=ds_lqttTransfr:U";
			var sOutDatasets  	= "";
			break;
		case "salaryTransfrD": 			// 급여이체삭제
			var sController   	= "hsco/mis/acc/ACC020302/salaryTransfrD.do";
			var sInDatasets   	= "input1=ds_salaryTransfr:U";
			var sOutDatasets  	= "";
			break;
		
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if (nErrorCode != 0) {
		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "lqttTransfrList": 		// 대량이체
				break;
			case "lqttTransfrDetailList": 	// 대량이체상세
				break;
			case "salaryTransfrList": 		// 급여이체
				break;
			case "salaryTransfrDetailList": // 급여이체상세
				break;
			case "errorPorcessU":			// 오류처리
				this.gfn_message("success.처리.실패");
				break;
			case "lqttTransfrD": 			// 대량이체삭제
				break;
			case "salaryTransfrD": 			// 급여이체삭제
				break;
			default:
				break;
		}

		// 오류 및 성공 메시지를 화면에 보여준다.
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "lqttTransfrList": 		// 대량이체
				break;
			case "lqttTransfrDetailList": 	// 대량이체상세
				trace("#################");
				trace(this.comUtils.isNull(this.ds_lqttTransfrDetail.getColumn(0, "ERR_CD")));
				break;
			case "salaryTransfrList": 		// 급여이체
				break;
			case "salaryTransfrDetailList": // 급여이체상세
				break;
			case "errorPorcessU":			// 오류처리
				this.gfn_message("success.처리.성공");
				break;
			case "lqttTransfrD": 			// 대량이체삭제
				this.fn_transaction("lqttTransfrList");
				break;
			case "salaryTransfrD": 			// 급여이체삭제
				this.fn_transaction("salaryTransfrList");
				break;
			default:
				break;
		}
	}
}

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: fn_print
 * 설명 		: 출력
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
 */
this.fn_print = function()
{		

}


/*
 * 함수명  	: dataset_onrowposchanged
 * 설명 		: 마스터데이터셋 행 변경 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.dataset_onrowposchanged();
 */
this.dataset_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_cond.setColumn(0, "SITE_NO", obj.getColumn(e.newrow, "SITE_NO"));
	this.ds_cond.setColumn(0, "FL_DATE", obj.getColumn(e.newrow, "FL_DATE"));
	this.ds_cond.setColumn(0, "FL_TIME", obj.getColumn(e.newrow, "FL_TIME"));
	
	switch(obj.name) {
		case "ds_lqttTransfr":
			this.fn_transaction("lqttTransfrDetailList");
			break;
		case "ds_salaryTransfr":
			this.fn_transaction("salaryTransfrDetailList");
			break;
	}
	
}

/*
 * 함수명  	: btn_errorProcess_onclick
 * 설명 		: 오류처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_errorProcess_onclick();
 */
this.btn_errorProcess_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(errorDs[this.tab.tabindex].getRowCount() > 0) {
		if(this.gfn_message("comm.행위여부", "오류처리를 진행") ){
			this.fn_transaction("errorPorcessU");
		}
	} else {
		this.gfn_message("comm.데이터.선택.없음");
	}
}

/*
 * 함수명  	: btn_del_onclick
 * 설명 		: 삭제 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_del_onclick();
 */
this.btn_del_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	switch(this.tab.tabindex) {
		case 0:
			if(this.ds_lqttTransfr.getColumn(this.ds_lqttTransfr.rowposition, "FLAG") == "EE") {
				this.gfn_message("fail.삭제.불가","전송내역을");
				return;
			}
			this.ds_lqttTransfr.deleteRow(this.ds_lqttTransfr.rowposition);
			break;
		case 1:
			if(this.ds_salaryTransfr.getColumn(this.ds_lqttTransfr.rowposition, "FLAG") == "EE") {
				this.gfn_message("fail.삭제.불가","전송내역을");
				return;
			}
			this.ds_salaryTransfr.deleteRow(this.ds_salaryTransfr.rowposition);
			break;
	}
}

/*
 * 함수명  	: btn_cancel_onclick
 * 설명 		: 삭제 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_cancel_onclick();
 */
this.btn_cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	switch(this.tab.tabindex) {
		case 0:
			this.ds_lqttTransfr.reset();
			this.ds_lqttTransfr.applyChange();
			break;
		case 1:
			this.ds_salaryTransfr.reset();
			this.ds_salaryTransfr.applyChange();
			break;
	}
}

