 * params  		: obj, e
 * return Type 	: None
 * 사용법		: this.ds_dsnList_onrowposchanged(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
 */
this.ds_dsnList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1) {
		
		this.ds_chit.clearData();
		this.ds_chitDetail.clearData();
		this.ds_chit2.clearData();
		this.ds_chitDetail2.clearData();	
	
		var decsnYear = obj.getColumn(obj.rowposition, "DECSN_YEAR");
		var decsnNo = obj.getColumn(obj.rowposition, "DECSN_NO");
	
		this.ds_cond2.clearData();
		this.ds_cond2.addRow();
		this.ds_cond2.setColumn(0, "DECSN_YEAR", decsnYear);
		this.ds_cond2.setColumn(0, "DECSN_NO"  , decsnNo);
		
		this.fn_transaction("selectChitList");
	}
}

