/*
 * 함수명  	: btn_onclick
 * 설명 		: 버튼 클릭 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_onclick();
 */
this.btn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	switch(obj.name) {
		
		case "btn_addDtls":		// 디테일 그리드: 행추가
			if(this.ds_chit.getRowCount() > 0) {		
				var nRow = this.ds_chitDetail.addRow();
				// 초기값
				this.ds_chitDetail.setColumn(nRow, "ACCNUT_YEAR", 		 this.ds_chit.getColumn(0,"ACCNUT_YEAR"));
				this.ds_chitDetail.setColumn(nRow, "CHIT_NO"    , 		 this.ds_chit.getColumn(0,"CHIT_NO"));
				this.ds_chitDetail.setColumn(nRow, "DEBTOR_AMOUNT", "0");
				this.ds_chitDetail.setColumn(nRow, "CRDIT_AMOUNT", "0");
				
			}else {
				this.gfn_message("info.처리불가","전표생성을 먼저 진행해주십시오.");
				return false;
			}
			break;
			
		case "btn_delDtls":		// 디테일 그리드: 행삭제
			if(this.ds_chitDetail.getRowCount() > 0) {
				if (this.gfn_message("confirm.삭제여부") ) {
					this.ds_chitDetail.deleteRow(this.ds_chitDetail.rowposition);
				}
			}else {
				this.gfn_message("info.처리불가","전표생성을 먼저 진행해주십시오.");
				return false;
			}
			break;
			
		case "btn_addDtls2":		// 디테일 그리드: 행추가
			if(this.ds_chit2.getRowCount() > 0) {		
				var nRow = this.ds_chitDetail2.addRow();
				// 초기값
				this.ds_chitDetail2.setColumn(nRow, "ACCNUT_YEAR", 		 this.ds_chit2.getColumn(0,"ACCNUT_YEAR"));
				this.ds_chitDetail2.setColumn(nRow, "CHIT_NO"    , 		 this.ds_chit2.getColumn(0,"CHIT_NO"));
				this.ds_chitDetail2.setColumn(nRow, "DEBTOR_AMOUNT", "0");
				this.ds_chitDetail2.setColumn(nRow, "CRDIT_AMOUNT", "0");
			}else {
				this.gfn_message("info.처리불가","전표생성을 먼저 진행해주십시오.");
				return false;
			}
			break;
			
		case "btn_delDtls2":		// 디테일 그리드: 행삭제
			if(this.ds_chitDetail2.getRowCount() > 0) {
				if (this.gfn_message("confirm.삭제여부") ) {
					this.ds_chitDetail2.deleteRow(this.ds_chitDetail2.rowposition);
				}
			}else {
				this.gfn_message("info.처리불가","전표생성을 먼저 진행해주십시오.");
				return false;
			}
			break;
	}
}

/*
 * 함수명  	: btn_delete_onclick
 * 설명 		: 삭제버튼 제어
 * params  		: obj, e
 * return Type 	: None
 * 사용법		: this.btn_delete_onclick(obj:Button,  e:nexacro.ClickEventInfo)
 */
this.btn_delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var tabIdx = this.tab_chit.tabindex;
	
	if (this.gfn_message("confirm.삭제여부") ) {
		if(tabIdx == "0") {
			this.ds_chitDetail.deleteAll();
			this.ds_chit.deleteAll();
		}else if(tabIdx == "1") {
			this.ds_chitDetail2.deleteAll();
			this.ds_chit2.deleteAll();
		}
	}
	
	 this.fn_addBtnCnrl();
}



this.btn_incmeSlip_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_dsnList.rowposition;
	if(nRow < 0) {
		this.gfn_message("info.처리불가","결의내역이 선택되지 않았습니다.");
		return;
	}
	
	if(this.ds_chit.getRowCount() > 0 || this.ds_chit2.getRowCount() > 0) {	
		if(!this.gfn_message("comm.행위여부","전표내역이 존재합니다. 재생성")) {
			return;
		}
	}
	
	this.ds_slip.clearData();
	this.ds_slip.addRow();
	this.ds_slip.setColumn(0, "DECSN_YEAR", this.ds_dsnList.getColumn(nRow, "DECSN_YEAR"));
	this.ds_slip.setColumn(0, "DECSN_NO"  , this.ds_dsnList.getColumn(nRow, "DECSN_NO"));	
	this.fn_transaction("incmeSlipI");
}


this.ds_cond_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "MIN_AM") {
		var nRow = obj.rowposition;
		var minAm = e.newvalue;
		var maxAm = (!this.comUtils.isNull(obj.getColumn(nRow, "MAX_AM")) ? nexacro.toNumber(obj.getColumn(nRow, "MAX_AM")) : "");
		if(!this.comUtils.isNull(minAm) && !this.comUtils.isNull(maxAm)) {
			if(minAm > maxAm) {
				this.gfn_message("info.처리불가","검색조건 금액의 최솟값은 최댓값보다 작아야 합니다.");
			}
		}
	}
	
	
	if(e.columnid == "MAX_AM") {
		var nRow = obj.rowposition;
		var minAm = (!this.comUtils.isNull(obj.getColumn(nRow, "MIN_AM")) ? nexacro.toNumber(obj.getColumn(nRow, "MIN_AM")) : "");
		var maxAm = e.newvalue;
		if(!this.comUtils.isNull(minAm) && !this.comUtils.isNull(maxAm)) {
			if(minAm > maxAm) {
				this.gfn_message("info.처리불가","검색조건 금액의 최솟값은 최댓값보다 작아야 합니다.");
				return false;
			}
		}
	}
}

this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "SEARCH_GUBUN") {
		var value = e.newvalue;
		if(value == "4") {	// 금액
			obj.setColumn(0, "SEARCH_TEXT", "");
			this.div_SEARCH.edt_searchText.set_visible(false);
			this.div_SEARCH.msk_minAm.set_visible(true);
			this.div_SEARCH.msk_maxAm.set_visible(true);
			this.div_SEARCH.sta_csv.set_visible(true);
		}else {
			obj.setColumn(0, "MIN_AM", "");
			obj.setColumn(0, "MAX_AM", "");
			this.div_SEARCH.msk_minAm.set_visible(false);
			this.div_SEARCH.msk_maxAm.set_visible(false);	
			this.div_SEARCH.sta_csv.set_visible(false);
			this.div_SEARCH.edt_searchText.set_visible(true);
		}
	}
}
