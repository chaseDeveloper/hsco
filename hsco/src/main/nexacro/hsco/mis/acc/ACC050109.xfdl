 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
 	if((this.comUtils.isDatasetUpdated(this.ds_chit) || this.comUtils.isDatasetUpdated(this.ds_chit2) 
 			|| this.comUtils.isDatasetUpdated(this.ds_chitDetail) || this.comUtils.isDatasetUpdated(this.ds_chitDetail2))) {
         if(!this.gfn_message("confirm.변경.취소.여부")) {
             return false;    // 아니오
         }
     }

	this.ds_dsnList.clearData();
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.fn_transaction("selectDsnList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	// ========== 데이터셋 미변경 체크
	if (!(this.comUtils.isDatasetUpdated(this.ds_chit) || this.comUtils.isDatasetUpdated(this.ds_chit2) 
			|| this.comUtils.isDatasetUpdated(this.ds_chitDetail) || this.comUtils.isDatasetUpdated(this.ds_chitDetail2))) {
		this.gfn_message("comm.데이터.변경.없음")
		return false;
	}
	
	// ==========  전표마감 체크
	if(this.ds_chit.getRowCount() > 0) {
		if(this.fn_isAccClosAt(this.ds_chit.getColumn(0,"CHIT_DE"),"C")) {
			this.gfn_message("info.처리불가","전표마감된 자료입니다.");
			return;
		}
	}

	if(this.ds_chit2.getRowCount() > 0) {
		if(this.fn_isAccClosAt(this.ds_chit2.getColumn(0,"CHIT_DE"),"C")) {
			this.gfn_message("info.처리불가","전표마감된 자료입니다.");
			return;
		}
	}

	// ========== validation
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_chit, this.ds_validation) || !this.gfn_checkValidation_NoUpdChk(this.ds_chitDetail, this.ds_validation)) {
		return;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_chit2, this.ds_validation) || !this.gfn_checkValidation_NoUpdChk(this.ds_chitDetail2, this.ds_validation)) {
		return;
	}


	// ========== 차변,대변 합계가 일치하는지 체크
	if(Number(this.ds_chitDetail.getSum("DEBTOR_AMOUNT")) != Number(this.ds_chitDetail.getSum("CRDIT_AMOUNT"))) {
		this.gfn_message("errors.항목값.범위.동일", "차변금액의 합계|대변금액의 합계");
		return;
	}
	if(Number(this.ds_chitDetail2.getSum("DEBTOR_AMOUNT")) != Number(this.ds_chitDetail2.getSum("CRDIT_AMOUNT"))) {
		this.gfn_message("errors.항목값.범위.동일", "차변금액의 합계|대변금액의 합계");
		return;
	}

	if(this.gfn_message("confirm.저장.여부"))    {
		this.saveRow = this.ds_dsnList.rowposition;
		this.fn_transaction("saveChit");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectDsnList": 		// 결의내역 목록 조회
			var sController   	= "hsco/mis/acc/ACC050109/selectDsnList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_dsnList=output1";
			break;
		case "selectChitList": 	// 전표조회
			var sController   	= "hsco/mis/acc/ACC050109/selectChitList.do";
			var sInDatasets   	= "input1=ds_cond2";
			var sOutDatasets  	= "ds_chitDetail=output1 ds_chit=output2 ds_chitDetail2=output3 ds_chit2=output4";
			break;
		case "saveChit":		// 전표저장
			var sController   	= "hsco/mis/acc/ACC050109/saveChitList.do";
			var sInDatasets   	= "input1=ds_chit:A input2=ds_chitDetail:U input3=ds_chit2:A input4=ds_chitDetail2:U";
			var sOutDatasets  	= "";
			break;
		case "incmeSlipI":		// 전표생성
 			var sController   	= "hsco/mis/acc/ACC050109/incmeSlipI.do";
 			var sInDatasets   	= "input1=ds_slip";
 			var sOutDatasets  	= "";
 			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if (nErrorCode != 0) {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "selectDsnList": 	// 결의내역 목록 조회
				if(this.saveRow > -1) {
					this.ds_dsnList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "selectChitList": 	// 전표조회
				// 신규버튼제어
				this.fn_addBtnCnrl();
				break;
			case "saveChit":		// 전표저장
				this.fn_search();
				break;
			case "incmeSlipI":		// 전표생성
				this.fn_transaction("selectChitList");
				break;
		}
	}
	
	
}

