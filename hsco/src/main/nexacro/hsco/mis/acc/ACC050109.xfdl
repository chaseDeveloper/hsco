this.ds_dsnList_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
 	 // 데이터셋 변경 체크
     if(Ex.util.isUpdated(this.ds_chit) || Ex.util.isUpdated(this.ds_chit2) || Ex.util.isUpdated(this.ds_chitDetail) || Ex.util.isUpdated(this.ds_chitDetail2)) {
         if(!this.gfn_message("confirm.변경.취소.여부")) {
             return false;
         }else {
			this.ds_chit.reset();
			this.ds_chit2.reset();
			this.ds_chitDetail.reset();
			this.ds_chitDetail2.reset();
			this.ds_chit.applyChange();
			this.ds_chit2.applyChange();
			this.ds_chitDetail.applyChange();
			this.ds_chitDetail2.applyChange();
         }
     }
}


/*
 * 함수명  	: btn_add_onclick
 * 설명 		: 분개전표/분개확정전표 신규
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_add_onclick();
 */
this.btn_add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var tabIdx = this.tab_chit.tabindex;
	
	if(tabIdx == 0) {			// 분개전표
		var nRow = this.ds_chit.addRow();
		var mRow = this.ds_dsnList.rowposition;
		var decsnNo = this.ds_dsnList.getColumn(mRow, "DECSN_NO");
		var accnutYear = this.ds_dsnList.getColumn(mRow, "DECSN_YEAR");
		var anactKnd = this.ds_dsnList.getColumn(mRow, "ANACT_KND");
		
		var dRow = this.ds_chit.rowposition;
		this.ds_chit.setColumn(dRow , "DECSN_NO"   , decsnNo);			//결의번호
		this.ds_chit.setColumn(dRow , "DECSN_YEAR"  , accnutYear);
		this.ds_chit.setColumn(dRow , "ACCNUT_YEAR", accnutYear);		//회계년도
		this.ds_chit.setColumn(dRow , "CHIT_KND"   , anactKnd);			//전표종류
		this.ds_chit.setColumn(dRow , "CHIT_SE"    , "1");	  			//전표구분
		this.ds_chit.setColumn(dRow , "CHIT_TY"    , "1");				//전표유형
	}else if(tabIdx == 1) {		// 분개확정전표
		this.ds_chit2.addRow();
		this.btn_add.set_enable(false);
		this.btn_delete.set_enable(true);
		
		var mRow = this.ds_dsnList.rowposition;
		var decsnNo = this.ds_dsnList.getColumn(mRow, "DECSN_NO");
		var accnutYear = this.ds_dsnList.getColumn(mRow, "DECSN_YEAR");
		var anactKnd = this.ds_dsnList.getColumn(mRow, "ANACT_KND");
		
		var dRow = this.ds_chit2.rowposition;
		this.ds_chit2.setColumn(dRow , "DECSN_NO", decsnNo);
		this.ds_chit2.setColumn(dRow , "DECSN_YEAR"  , accnutYear);
		this.ds_chit2.setColumn(dRow , "ACCNUT_YEAR", accnutYear);
		this.ds_chit2.setColumn(dRow , "CHIT_KND"   , anactKnd);		//전표종류
		this.ds_chit2.setColumn(dRow , "CHIT_SE"    , "1");	  			//전표구분
		this.ds_chit2.setColumn(dRow , "CHIT_TY"    , "2");				//전표유형
	}
	
	this.fn_addBtnCnrl();
}




