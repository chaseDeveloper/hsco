this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid=="DEPT_CODE" && this.comUtils.isNotNull(e.newvalue)){
		this.fn_transaction("CARD");
	}
	else if(e.columnid == "OL_CARD_YN") {
		this.fn_transaction("CARD");
	}
	else if(e.columnid == "APPROVAL_BGN_TIME") {
		var newValue = e.newvalue.replace(/\s/g, '0');
		this.div_search.msk_bgnTime.set_value(newValue);
	}
	else if(e.columnid == "APPROVAL_END_TIME") {
		var newValue = e.newvalue.replace(/\s/g, '0');
		this.div_search.msk_endTime.set_value(newValue);
	}
	else{
		if(e.columnid != "DEPT_CODE" && e.columnid != "DEPT_NAME"){
			this.fn_search();
		}else{
			this.ds_cprCard.clearData();
		}
	}
}


/*
 * 함수명  	: grd_cprCard_oncelldblclick
 * 설명 		: 그리드 더블 클릭 이벤트 처리
 * params  		: None
 * return Type 	: None
 */
this.grd_cprCard_oncelldblclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var cRow = this.ds_confmDtls.rowposition;
	
	if(this.ds_confmDtls.getRowType(e.row) == Dataset.ROWTYPE_GROUP) {
		return;
	}
	
	if(!this.comUtils.isNull(this.ds_confmDtls.getColumn(cRow , "DECSN_NO"))) {
	
		var args = {
			  arg_0     : this
			, trgFormId	: "ACC040202"												// 팝업을 호출하는 화면의 ID
			, anactKnd	: "03"
			, decsnYear	: this.ds_confmDtls.getColumn(cRow , "DECSN_YEAR")
			, decsnNo	: this.ds_confmDtls.getColumn(cRow , "DECSN_NO")
		};
		this.gfn_popup("expDsnPopup", 903, 842, "결의서", args, "mis_acc::ACC020203_P10.xfdl", "fn_popupCallback", true);
		
	} else {	
	
		if(this.comUtils.isNullEmpty(this.ds_confmDtls.getColumn(cRow , "REAL_USER"))){
			this.gfn_message("info.항목.체크.값없음","실사용자");
			return;
		}
			
		if(this.ds_confmDtls.getRowType(cRow) != Dataset.ROWTYPE_NORMAL){
			this.gfn_message("info.진행가능","변경된 정보를 저장");
			return;
		}
			
		if(this.ds_confmDtls.getColumn(cRow, "CANCEL_YN") == "1") {
			this.gfn_message("info.처리불가","취소된 내역입니다.");
			return;
		}
		
		if(this.comUtils.isNull(this.ds_confmDtls.getColumn(cRow , "BCNC_CODE"))) {
			this.gfn_message("info.처리불가","거래처 등록되지 않은 가맹점입니다.");
			return;	
		}
		
		var apprDate = this.ds_confmDtls.getColumn(cRow, "APPR_DATE");
		if(apprDate > "20200831") {
			if(this.comUtils.isNull(this.ds_confmDtls.getColumn(cRow, "FNNC_INSTT_CODE"))) {
				this.gfn_message("info.처리불가","결제은행이 등록되지 않은 카드입니다.");
				return;	
			}
			
			if(this.comUtils.isNull(this.ds_confmDtls.getColumn(cRow, "SETLE_ACNUTNO"))) {
				this.gfn_message("info.처리불가","결제계좌번호가 등록되지 않은 카드입니다.");
				return;	
			}
		}
		
		this.fn_popup_expDsn(1);
	}
}

/*
 * 함수명  	: btn_Regist_onclick
 * 설명 		: 지출일괄등록 버튼 처리
 * params  		: None
