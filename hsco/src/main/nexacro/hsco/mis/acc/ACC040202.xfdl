			}
				
			if(this.ds_confmDtls.getRowType(i) != Dataset.ROWTYPE_NORMAL){
				this.gfn_message("info.진행가능","변경된 정보를 저장");
				return;
			}
				
			if(this.ds_confmDtls.getColumn(i, "CANCEL_YN") == "1") {
				this.gfn_message("info.처리불가","취소된 내역입니다.");
				return;
			}
			
			if(this.comUtils.isNull(this.ds_confmDtls.getColumn(i , "BCNC_CODE"))) {
				this.gfn_message("info.처리불가","거래처 등록되지 않은 가맹점입니다.");
				return;	
			}
			
			var apprDate = this.ds_confmDtls.getColumn(i, "APPR_DATE");
			if(apprDate > "20200831") {
				if(this.comUtils.isNull(this.ds_confmDtls.getColumn(i, "FNNC_INSTT_CODE"))) {
					this.gfn_message("info.처리불가","결제은행이 등록되지 않은 카드입니다.");
					return;	
				}
				
				if(this.comUtils.isNull(this.ds_confmDtls.getColumn(i, "SETLE_ACNUTNO"))) {
					this.gfn_message("info.처리불가","결제계좌번호가 등록되지 않은 카드입니다.");
					return;	
				}
			}
		}
	}

	this.fn_popup_expDsn(2);
}

/*
 * 함수명  	: fn_popup_expDsn
 * 설명 		:결의서 팝업
 * params  		: None
 * return Type 	: None
 */
this.fn_popup_expDsn = function(opt)
{
	var today = this.dateUtils.today();
	var currow = this.ds_confmDtls.rowposition;
	//this.scrollpos	= this.grd_cprCard.vscrollbar.pos;
	
	this.ds_expDsnMastr.clearData();
	this.ds_expDsnMtrDet.clearData();
	this.ds_expDsnDetail.clearData();
	this.ds_expDsnDtls.clearData();
	this.ds_ccmAppr.clearData();

	if(opt == 1) {
		this.addExpDsnDetail(currow);
	} else if(opt == 2) {
		for(i = 0; i < this.ds_confmDtls.getRowCount(); i++) {		
			if(this.ds_confmDtls.getColumn(i, "CHK") == "1") {
				currow = i;
				this.addExpDsnDetail(currow);
			}
		}
	}
		
	if(this.ds_expDsnDetail.getRowCount()>0){
		var apprDate = this.ds_confmDtls.getColumn(currow, "APPR_DATE");
		
		this.ds_expDsnMastr.addRow();
		this.ds_expDsnMastr.setColumn(0, "DECSN_YEAR"				, today.substr(0,4));								// 회계년월
		this.ds_expDsnMastr.setColumn(0, "ANACT_KND"				, "03");											// 결의서구분
		this.ds_expDsnMastr.setColumn(0, "DEPT_CODE"				, this.gfn_getDeptId());	// 부서코드
		this.ds_expDsnMastr.setColumn(0, "DEPT_NM"					, this.gfn_getDeptName());	// 부서명
		this.ds_expDsnMastr.setColumn(0, "BUDGET_YEAR"				, apprDate.substr(0,4));							// 예산년도
		this.ds_expDsnMastr.setColumn(0, "ITNC_DE"					, today);											// 발의일자
		this.ds_expDsnMastr.setColumn(0, "DET_DCSN_DE"				, apprDate);										// 채무확정일자
		this.ds_expDsnMastr.setColumn(0, "PYMNT_ITNC_DE"			, apprDate);										// 지급발의일자
		this.ds_expDsnMastr.setColumn(0, "PRMPC_JRNLZPR_RGIST_DE"	, apprDate);										// 원장분개장등기
		this.ds_expDsnMastr.setColumn(0, "EXPNDTR_BUDGET_CNRL_DE"	, apprDate);										// 지출예산통제
		this.ds_expDsnMastr.setColumn(0, "CPTAL_EXPNDTR_ACNTBK_DE"	, apprDate);										// 자금지출부등기
		this.ds_expDsnMastr.setColumn(0, "DECSN_AMOUNT"				, this.ds_expDsnDetail.getSum("DECSN_AMOUNT"));		// 결의금액(승인금액)
		this.ds_expDsnMastr.setColumn(0, "PRUF_SE_CODE"				, "07");											// 증빙구분
		
		if(!this.comUtils.isNull(apprDate)) {
			this.ds_expDsnMastr.setColumn(0, "SUMRY", "카드대금("+apprDate.substr(0,4)+"년"+apprDate.substr(4,2)+"월)");	// 적요 
		}
		
		this.ds_expDsnMtrDet.addRow();
		this.ds_expDsnMtrDet.setColumn(0, "TAXXPT_AMOUNT"	, "0");													// 면세금액
		this.ds_expDsnMtrDet.setColumn(0, "VAT_DECSN_AMOUNT", "0");													// 부가세결의금액
		this.ds_expDsnMtrDet.setColumn(0, "SUPLY_AMOUNT"	, this.ds_expDsnDetail.getSum("SUPLY_AMOUNT"));			// 공급가액
		this.ds_expDsnMtrDet.setColumn(0, "VAT_AMOUNT"		, this.ds_expDsnDetail.getSum("VAT"));					// 부가세금액
		
		var args = {
			 arg_0      			: this
			,trgFormId				: "ACC040202"												// 팝업을 호출하는 화면의 ID
			,anactKnd				: "03"														// 결의서구분
			,decsnYear				: this.ds_expDsnMastr.getColumn(0, "DECSN_YEAR")			// 결의년도
			,dsExpDsnMastr			: this.ds_expDsnMastr.saveXML()								// 결의마스터 Dataset
			,dsExpDsnMtrDet 		: this.ds_expDsnMtrDet.saveXML()							// 결의마스터상세 Dataset
			,dsExpDsnDetail 		: this.ds_expDsnDetail.saveXML()							// 세부내용 Dataset
			,dsExpDsnDtls			: this.ds_expDsnDtls.saveXML()								// 결의내역 Dataset
		};
		
		this.gfn_popup("expDsnPopup", 902, 745, "결의서", args, "mis_acc::ACC020203_P10.xfdl", "fn_popupCallback", true);
	}

}

