    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACC040202
 * 화면명   : 승인내역조회
 * 화면설명 : 승인내역조회을 관리한다.
 * 작성일   : 2017.10.02
 * 작성자   : 이수지
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ************************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

// 업무에 필요한 공통 라이브러리 파일을 추가할 수 있다.
// TODO


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 this.currow 	= 0;
 this.scrollpos = 0;
 this.isAuth = false;
 
 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACC040202_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();
	this.fn_misFormInit(this);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	
	// 데이터셋 동적 바인딩
	this.div_search.div_monCal.fn_setBind("ds_cond","SRCH_YM");
	this.div_search.div_dept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NAME");
	this.div_search.div_dept.div_Search.edt_deptCode.set_cssclass("edt_WF_Essential");
	this.div_search.div_dept.div_Search.edt_deptNm.set_cssclass("edt_WF_Essential");
	
	this.div_search.div_monCal.msk_mon.set_cssclass("msk_WF_Essential");
	
	// 검색조건 컴포넌트 초기값 부여
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "SRCH_YM", this.dateUtils.today().substr(0,6));
	this.ds_cond.setColumn(0, "CARD_NO", "");
	this.ds_cond.setColumn(0, "USE_AT" , "1");
	this.ds_cond.setColumn(0, "APPROVAL_BGN_TIME", "0000");
	this.ds_cond.setColumn(0, "APPROVAL_END_TIME", "2400");
	
	
	/* 회계, 결산담당자, 비서 */
	if(this.gfn_authGrpId("ACC_ADMIN") != -1 || this.gfn_authGrpId("ACC_USER") != -1
	   || this.gfn_authGrpId("IFRS_ADMIN") != -1 || this.gfn_authGrpId("IFRS_USER") != -1 || this.gfn_authGrpId("SECRTRY") != -1) {
		this.isAuth = true;
	}else {
		this.isAuth = false;
	}
	
	// 2020.04.22. 개선요청(김홍범)
	// 사회적가치TF팀일 경우 경영기획처 법인카드 모두 보이도록 수정
	// 감사팀은 원래 경영기획처 법인카드가 보이게 되어 있었음
	if(this.gfn_getDeptId() == "010000" || this.gfn_getDeptId() == "070400") {
		this.ds_cond.setColumn(0, "EXCEPTION_AT", "1");
	} else {
		this.ds_cond.setColumn(0, "EXCEPTION_AT", "0");
	}
	
	// 겸직자 판단을 위해 추가
	this.ds_cond.setColumn(0, "EMPNO", this.gfn_getEmpNo());
	
	this.ds_cond.setColumn(0, "DEPT_CODE"		, this.gfn_getDeptId());	// 부서코드
	this.ds_cond.setColumn(0, "DEPT_NAME"		, this.gfn_getDeptName());	// 부서명
	if(!this.isAuth){
		// 2018-03-23 황제혁 주임 요청으로 3월만 한시적 해제, 4월 1일 이후 다시 제한
		this.div_search.div_dept.set_enable(false);
	}
	
	// 결의서코드 정보
	this.fn_transaction("selectExpCode");
	
	//공통코드
	var comboParams = [
		["ds_com022", "COM022", "Y", "전체", "B"]	// 결재상태
	];
	var callback = function (svcId, errorCode, errorMsg) {
		this.ds_cond.set_enableevent(true);
		this.fn_transaction("CARD");
	};
	this.gfn_comboLoad (comboParams, callback);
}

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}

	this.fn_transaction("SRCH");
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if (this.comUtils.isDatasetUpdated(this.ds_confmDtls)) {
		if(this.gfn_message("confirm.변경.취소.여부")){
			this.ds_confmDtls.reset();
			this.ds_confmDtls.applyChange();
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
	}
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 버튼 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_save= function()
{
	if (this.comUtils.isDatasetUpdated(this.ds_confmDtls)) {
		this.fn_transaction("SAVE");
	}else{
		this.gfn_message("comm.데이터.변경.없음");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectExpCode":	// 지출결의서 코드정보
			var sController   	= "hsco/mis/acc/ACC020102/fnncInsttCodeList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fnncInstt=output1";
			break;
		case "SRCH": // 지출내역 조회
			var sController   	= "hsco/mis/acc/ACC040202/confmDtlsList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_confmDtls=output1";
			break;
		case "SAVE": // 지출내역 저장
			var sController   	= "hsco/mis/acc/ACC040202/confmDtlsU.do";
			var sInDatasets   	= "input1=ds_confmDtls:U";
			var sOutDatasets  	= "";
			break;
		case "CARD": // 카드 조회
			var sController   	= "hsco/mis/acc/ACC040202/cardList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cprCard=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0) {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {		
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "SRCH":
				break;
			case "SAVE": // 지출내역 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "CARD":
				this.ds_cprCard.insertRow(0);
				this.ds_cprCard.setColumn(0, "CARD_NO", "");
				this.ds_cprCard.setColumn(0, "CARD_NM", "전체");
				/*
				var fRow = this.ds_cprCard.findRow("CARD_NO", "9430160177422774");
				if(fRow > -1 && this.comUtils.isNotNull(this.ds_cond.getColumn(0, "DEPT_CODE"))){
					this.div_search.cmb_card.set_index(fRow);
				}else{
					this.ds_cond.setColumn(0, "CARD_NO", "");
				}
				*/
				this.fn_search();
				break;
			default:
				break;
		}
	}
}



