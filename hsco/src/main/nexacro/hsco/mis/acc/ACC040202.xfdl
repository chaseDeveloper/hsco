/*
 * 함수명  	: fn_popupCallback
 * 설명 		: 팝업창 콜백 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_popupCallBack();
 */
this.fn_popupCallback = function(strID, variant)
{

	if(strID == "expDsnPopup") {
		if(variant == "I") {
			this.fn_search();
		}
	}

	if(strID == "btn_empPopup") {
