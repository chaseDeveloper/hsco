/*
 * 함수명  	: ds_cprCardManage_onrowposchanged
 * 설명 		: 변경이력 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cprCardManage_onrowposchanged();
 */
this.ds_cprCardManage_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(obj.getRowType(e.newrow) != Dataset.ROWTYPE_INSERT) {
		this.div_detail1.MaskEdit00.set_cssclass("msk_WF_Readonly");
		this.div_detail1.MaskEdit00.set_readonly(true);
		
		this.ds_cond2.clearData();
		this.ds_cond2.copyData(this.ds_cond);
		this.ds_cond2.setColumn(0, "CARD_NO", obj.getColumn(e.newrow, "CARD_NO"));
		var sSvcID        	= "SRCH_HIST";                    
		var sController   	= "hsco/mis/acc/ACC040101/cprCardChangeHistList.do";
		var sInDatasets   	= "input1=ds_cond";
		var sOutDatasets  	= "ds_cprCardChangeHist=output1";
		var sArgs = "";	
		Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs); 
	}
	else {
		this.div_detail1.MaskEdit00.set_cssclass("msk_WF_Essential");
		this.div_detail1.MaskEdit00.set_readonly(false);
	}
}

