 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가한다.
	// TODO
}


/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 마감일관리 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// 변경여부 체크
	if(Ex.util.isUpdated(this.ds_cprCardManage)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	
	this.fn_transaction("SRCH");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 법인카드 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	// 필수칼럼: 카드번호, 부서코드, 사용여부
	if(!this.gfn_checkValidation(this.ds_cprCardManage, this.ds_validation)) {
		return;
	}
	
	if(!this.gfn_message("confirm.저장.여부")){
		return;
	}
	
	var sSvcID        	= "SAVE";                    
	var sController   	= "hsco/mis/acc/ACC040101/cprCardManageCUD.do";	
	var sInDatasets   	= "input1=ds_cprCardManage:U";
	var sOutDatasets  	= "";
	var sArgs = "";	    
	Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs);
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "SRCH_FNNC_INSTT":
			var sController   	= "hsco/mis/acc/ACC020102/fnncInsttCodeList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fnncInstt=output1";
		break;
		case "SRCH": // 지출내역 조회
			var sSvcID        	= "SRCH";                    
			var sController   	= "hsco/mis/acc/ACC040101/cprCardManageList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cprCardManage=output1";
			var sArgs = "";
		break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if (nErrorCode < 0) 
	{
		//return ExtNx.core.comMsg("alert",strErrorMsg);		
	}
	
	switch(strSvcId) {
		case "SRCH_FNNC_INSTT":
			this.ds_fnncInsttCard.copyData(this.ds_fnncInstt);
			this.ds_fnncInsttCard.filter("FNNC_INSTT_CODE > 300 && FNNC_INSTT_CODE < 400");
			this.fn_transaction("SRCH");
		case "SRCH":
			if(this.saveRow != -1) {
				this.ds_cprCardManage.set_rowposition(this.saveRow);
				this.saveRow = -1;
			}
			this.div_detail1.MaskEdit00.set_cssclass("msk_WF_Readonly");
			this.div_detail1.MaskEdit00.set_readonly(true);
			break;
		case "SRCH_HIST":
			break;
		case "SAVE":
			this.saveRow = this.ds_cprCardManage.rowposition;
			this.gfn_message("success.처리.성공");
			this.fn_search();
			break;
		default:
			break;
	}
	
}


/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: fn_insert
 * 설명 		: 신규
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var nRow = this.ds_cprCardManage.addRow();
	this.ds_cprCardChangeHist.clearData();
	this.ds_cprCardManage.setColumn(nRow, "USE_AT", "1");						// 사용여부
	this.ds_cprCardManage.setColumn(nRow, "DEPT_CMNUSE_AT", "0");				// 부서공용여부
	this.ds_cprCardManage.setColumn(nRow, "CHANGE_SE", "1");					// 변경구분	
	this.ds_cprCardManage.setColumn(nRow, "CHANGE_DE", this.dateUtils.today());	// 변경일자
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete= function()
{	
	if(this.ds_cprCardManage.getRowCount() > 0) {
		if(this.ds_cprCardManage.getRowType(this.ds_cprCardManage.rowposition) != Dataset.ROWTYPE_INSERT) {
			this.gfn_message("comm.기등록값.삭제불가")
			return;
		}
	
		if (this.gfn_message("confirm.삭제여부") ) {
			this.ds_cprCardManage.deleteRow(this.ds_cprCardManage.rowposition);
		}
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if(this.gfn_message("confirm.취소여부") ){
		this.ds_cprCardManage.reset();
	}	

}

/*
