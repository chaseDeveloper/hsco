}


/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	
	// 공통코드
	var comboParams = [
		 ["ds_prj012", "PRJ012", "Y", "D", "B"]	// 사업분류
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 조회 이벤트 / 함수 호출
		this.fn_transaction("SRCH_FNNC_INSTT");
	};
	// 데이터셋 동적 바인딩
	this.div_detail1.div_budgetDept.fn_setBind("ds_cprCardManage","BUDGET_DEPT","BUDGET_DEPT_NM");
	this.div_detail1.div_monCal.fn_setBind("ds_cprCardManage","END_YM");
	this.div_detail1.div_monCal.msk_mon.set_cssclass("msk_WF_Essential");
	this.div_detail2.div_nowEmp.fn_setBind("ds_cprCardManage","NOW_EMPNO","NOW_EMPNM");
	this.div_detail2.div_dept.fn_setBind("ds_cprCardManage","DEPT_CODE","DEPT_NM");
	this.div_SEARCH.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	this.div_detail2.div_dept.div_Search.edt_deptCode.set_cssclass("edt_WF_Essential");
	this.div_detail2.div_dept.div_Search.edt_deptNm.set_cssclass("edt_WF_Essential");
	
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "USE_AT", "1");
	this.ds_cond.setColumn(0, "CARD_SE", "1");
	
	this.gfn_comboLoad (comboParams, callback);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
