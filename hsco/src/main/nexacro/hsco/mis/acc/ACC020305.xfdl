this.fn_getGrpCount = function(currow)
{
	var chkColumn = this.ds_chkList.getColumn(currow, "CHK_COLUMN");
	var chkGroup  = this.ds_chkList.getColumn(currow, "CHK_GROUP");
	var grpAt     = this.ds_chkList.getRowType(currow) == Dataset.ROWTYPE_GROUP ? '1' : '0';
	var rtnData   = 0;
	if (chkGroup == 'A') {
		if(grpAt == '1') {
			/*
			rtnData += this.ds_expDsn.getCaseCount("SMLPZ == '1'",           "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("FEMALE_ENTRPRS == '1'",  "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("DSPSN == '1'",           "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("DSPSN_STD_BPLC == '1'",  "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("SRSILL_DSPSN == '1'",    "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("SOCTY_ENTRPRS == '1'",   "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("SOCTY_CPER_MXTR == '1'", "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("TCNDVLP == '1'",         "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("GREEN_PRDUCT == '1'",    "DECSN_AMOUNT");
			*/
			rtnData = '-';
		} else {
			rtnData = this.ds_expDsn.getCaseCount(chkColumn + " == '1'", "DECSN_AMOUNT")
		}
	} else if(chkGroup == 'B') {
		if(grpAt == '1') {
			rtnData += this.ds_expDsn.getCaseCount("CNTRCT_SE == '1'", "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("CNTRCT_SE == '2'", "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("CNTRCT_SE == '3'", "DECSN_AMOUNT");
			rtnData += this.ds_expDsn.getCaseCount("CNTRCT_SE == '4'", "DECSN_AMOUNT");
		} else {
			rtnData = this.ds_expDsn.getCaseCount("CNTRCT_SE == '" + chkColumn + "'", "DECSN_AMOUNT");
		}
	} else {
		rtnData = 0;
	}
	return rtnData;
}


