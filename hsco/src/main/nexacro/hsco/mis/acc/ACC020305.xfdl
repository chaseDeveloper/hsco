};

 
 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACC020305_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_misFormInit(this);
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();

	// 공통코드
	var comboParams = [
		 ["ds_ctr019", "CTR019", "Y", "A", "B"]	// 기업구분
		,["ds_ctr022", "CTR022", "Y", "A", "B"]	// 계약구분
		,["ds_ctr030", "CTR030", "Y", "A", "B"]	// 조달여부
		,["ds_ctr049", "CTR049", "Y", "A", "B"]	// 계약방법
		,["ds_acc030", "ACC030", "Y", "A", "B"]	// 세금계산서구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	
	// 데이터셋 동적 바인딩
	this.div_SEARCH.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	var today = this.dateUtils.today();
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "SRCH_ST_DT", today.substr(0,4) + "0101");
	this.ds_cond.setColumn(0, "SRCH_ED_DT", today);
	this.ds_cond.setColumn(0, "NTS_CONFM_AT", "1");
	
	if(this.gfn_authGrpId("CTR_ADMIN") != -1) {
		this.ds_cond.setColumn(0, "DEPT_CODE", "000000");
		this.ds_cond.setColumn(0, "DEPT_NM",   "화성도시공사");
	} else {
		this.div_SEARCH.div_srchDept.set_enable(true);
		this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId()  );
		this.ds_cond.setColumn(0, "DEPT_NM",   this.gfn_getDeptName());
	}
}

