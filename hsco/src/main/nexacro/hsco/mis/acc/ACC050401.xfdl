 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	//alert(this.ds_cond.getColumn(0,"CHK_NON_PRMPC"));
	this.fn_transaction("SRCH");
}


/*
