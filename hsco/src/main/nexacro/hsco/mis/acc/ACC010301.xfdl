 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0) {
		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "SRCH": //계정과목 리스트 목록
				break;
			case "iemSRCH": // 상세계정과목 조회
				break;			
			case "SAVE": // 저장
				break;
			case "COPY":
				break;
			default:
				break;
		}
		// 오류 및 성공 메시지를 화면에 보여준다.
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {		
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "SRCH": //계정과목 리스트 목록
				break;
			case "iemSRCH": // 상세계정과목 조회
				break;			
			case "SAVE": // 저장
				this.fn_transaction("SRCH");
				break;
			case "COPY":
				this.fn_transaction("SRCH");
				break;
			default:
				break;
		}
	}	
}

/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: 검색조건(회계코드) 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.fn_popupCallBack = function(strID, variant)
{

	var dsTemp = new Dataset();
	dsTemp.loadXML(variant);
	
	//선택한것이 없을때 2016.07.08
	if(dsTemp.rowcount == 0){
		return;
	}
	
	switch(strID){
		case "btn_acntPopup":
			this.ds_cond.setColumn(0, "ACCNUT_ACNT_CODE", dsTemp.getColumn(0, "ACCNUT_ACNT_CODE"));
			this.ds_cond.setColumn(0, "ACCNUT_ACNT_NM", dsTemp.getColumn(0, "ACCNUT_ACNT_NM"));
			break;
		case "btn_divacntPopup":
			var nRow = this.ds_fnnrTblatManage.rowposition;
			if(dsTemp.getColumn(0,"ACNT_SE") !=  this.ds_fnnrTblatManage.getColumn(nRow, "FNNR_TBLAT_SE_CODE").substr(1,1)){
			 	this.gfn_message("errors.값확인.요청", this.cmb_FNNR_TBLAT_SE_CODE.text+" 계정코드가|계정과목구분을");
			}else{
				this.ds_fnnrTblatManage.setColumn(nRow, "ACCNUT_ACNT_CODE", dsTemp.getColumn(0, "ACCNUT_ACNT_CODE"));
				this.ds_fnnrTblatManage.setColumn(nRow, "ACCNUT_ACNT_NM", dsTemp.getColumn(0, "ACCNUT_ACNT_NM"));
				this.ds_fnnrTblatManage.setColumn(nRow, "ACNT_LEVEL", dsTemp.getColumn(0, "ACNT_LEVEL"));
			}
			break;
		case "iemGrid":
			var nRow = this.ds_fnnrTblatIEMManage.rowposition;
			this.ds_fnnrTblatIEMManage.setColumn(nRow, "ACCNUT_ACNT_CODE", dsTemp.getColumn(0, "ACCNUT_ACNT_CODE"));
			this.ds_fnnrTblatIEMManage.setColumn(nRow, "ACCNUT_ACNT_NM", dsTemp.getColumn(0, "ACCNUT_ACNT_NM"));
			break;
	}
}

