 * 설명 		: 계약의뢰관리 내역 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	if(Ex.util.isUpdated(this.ds_fnnrTblatManage)) {
		if(!this.gfn_message("comm.행위여부","[계정과목Grid]변경된 데이터를 취소하고 계속 진행") ){
			return false;	// 아니오
		}
	}
	
	this.ds_fnnrTblatManage.clearData();
	this.ds_fnnrTblatIEMManage.clearData();
	this.fn_transaction("SRCH");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 계약의뢰관리 내역 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// update check
	if(!this.comUtils.isDatasetUpdated(this.ds_fnnrTblatManage) && !this.comUtils.isDatasetUpdated(this.ds_fnnrTblatIEMManage)) {
		this.gfn_message("comm.내역.변경.없음");
		return false;
	}
	
	// validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_fnnrTblatManage, this.ds_validation) ||
	   !this.gfn_checkValidation_NoUpdChk(this.ds_fnnrTblatIEMManage, this.ds_validation)) {
		return;
	}
	
	// save
	if(this.gfn_message("confirm.저장.여부") ){
		this.fn_transaction("SAVE");
	}
}


/**********************************************************************
 * CALL TRANSACTION 영역 
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "SRCH": //계정과목 리스트 목록
			var sController   	= "hsco/mis/acc/ACC010301/fnnrTblatManagelist.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_fnnrTblatManage=output1";
			break;
		case "iemSRCH": // 상세계정과목 조회
			this.ds_iemcond.clearData();
			this.ds_iemcond.addRow();
			this.ds_iemcond.copyRow(0, this.ds_fnnrTblatManage, this.ds_fnnrTblatManage.rowposition);
			var sController   	= "hsco/mis/acc/ACC010301/fnnrTblatIEMManagelist.do";
			var sInDatasets   	= "input1=ds_iemcond";
			var sOutDatasets  	= "ds_fnnrTblatIEMManage=output1";
			break;			
		case "SAVE": // 저장
			var sController   	= "hsco/mis/acc/ACC010301/fnnrTblatManageCUD.do";
			var sInDatasets   	= "input1=ds_fnnrTblatManage:U input2=ds_fnnrTblatIEMManage:U";
			var sOutDatasets  	= "";
			break;
		case "COPY": //전년도복사
			var sController   	= "hsco/mis/acc/ACC010301/saveCopyPrvYear.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

