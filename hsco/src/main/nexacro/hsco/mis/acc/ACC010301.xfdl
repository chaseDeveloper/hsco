 * 사용법		: n/a
 */
this.grd_manage_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	this.fn_transaction("iemSRCH");
}
/*
 * 함수명  	: chk_DDCT_SE_onchanged
 * 설명 		: 차감여부에 따른 차감구분 readonly 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.chk_DDCT_SE_onchanged = function(obj:CheckBox, e:nexacro.CheckBoxChangeEventInfo)
{
	this.cmb_DDCT_SE.set_readonly(obj.value == '0' || obj.value == null? 'true' : 'false');
}

/*
 * 함수명  	: grd_IEMmanage_onexpandup
 * 설명 		: IEM그리드 회계과정 선택팝업
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.grd_IEMmanage_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	var parm = {
	     arg_0    : this
	    ,year     : this.ds_cond.getColumn(0, "ACCNUT_YEAR")
	};
	this.gfn_popup("iemGrid", 600, 500, "회계계정", parm, "mis_acc::ACC010103_P01.xfdl", "fn_popupCallBack", true);
}

/*
 * 함수명  	: ds_fnnrTblatManage_canrowposchange
 * 설명 		: Dataset 행 변경 전 이벤트 처리(계정과목 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_acntCode_canrowposchange(obj,e);
 */

this.ds_fnnrTblatManage_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(Ex.util.isUpdated(this.ds_fnnrTblatIEMManage)) {
		if(!this.gfn_message("comm.행위여부","[집계계정과목Grid]변경된 데이터를 취소하고 계속 진행") ){
			return false;	// 아니오
		}
	}
}

/**********************************************************************
* 조회조건 회계코드 backspace, delete 키보드 입력시 값 지움
***********************************************************************/ 
this.div_search_edt_accntNm_onkeyup = function(obj:Edit, e:nexacro.KeyEventInfo)
{
	if(e.keycode == '8'){   // backspace
		this.div_search.edt_acntCode.set_value("");
		this.div_search.edt_accntNm.set_value("");
	}else if(e.keycode == '46'){  // delete
		this.div_search.edt_acntCode.set_value("");
		this.div_search.edt_accntNm.set_value("");
	}
}
