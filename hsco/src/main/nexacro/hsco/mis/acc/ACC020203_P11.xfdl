//checkBox(제품구분) 클릭시 이벤트
this.fn_chkBoxOnClick = function(obj:CheckBox,  e:nexacro.ClickEventInfo)
{
	var targetDiv = this.tab.tabpage1.div_detail00.div_PrductSe;
	if(obj.name == "chk_crrspndAbsence") { //해당없음 
		if(obj.value == 1){ //선택시
			for(var i = 0; i < targetDiv.components.length; i++){
				var item = targetDiv.components.get_item(i)
				switch(item.toString()){
					case "[object CheckBox]":
						if(item.name != "chk_crrspndAbsence"){
							item.set_value(false);
						}
					break;
				}
			}
		}
	}else{
		if(obj.value == 1){ //선택시
			for(var i = 0; i < targetDiv.components.length; i++){
				var item = targetDiv.components.get_item(i)
				switch(item.toString()){
					case "[object CheckBox]":
						if(item.name == "chk_crrspndAbsence"){
							item.set_value(false);
						}
					break;
				}
			}
		}
	}
