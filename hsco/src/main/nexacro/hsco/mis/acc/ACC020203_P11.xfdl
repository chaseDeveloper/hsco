this.fn_errorCheck = function(){
	
	//전송오류시 은행 계좌번호 수정 가능하게
	var CMPNSP_NO  = this.ds_expDsnDetail.getColumn(this.ds_expDsnDetail.rowposition, "CMPNSP_NO");
	var ERROR_AT  = this.ds_expDsnDetail.getColumn(this.ds_expDsnDetail.rowposition, "ERROR_AT");
	var LAST_SANCTN_STTUS  = this.ds_expDsnMastr.getColumn(this.ds_expDsnMastr.rowposition, "LAST_SANCTN_STTUS");
	//alert("CMPNSP_NO = " +  CMPNSP_NO + "   ERROR_AT =" + ERROR_AT);
	
	if(this.comUtils.isNullEmpty(CMPNSP_NO) && ERROR_AT == "1"){

		this.btn_save.set_enable(true);
		this.btn_close2.set_enable(true);
		this.btn_actPopup.set_enable(false);
		
		this.tab.tabpage1.div_detail00.cmd_fnncInstt.set_readonly(false);
		this.tab.tabpage1.div_detail00.edt_acnutno.set_readonly(false);
		this.tab.tabpage1.div_detail00.edt_chghy.set_readonly(false);
				
		this.tab.tabpage1.div_detail00.cmd_fnncInstt.set_cssclass();
		this.tab.tabpage1.div_detail00.edt_acnutno.set_cssclass();
		this.tab.tabpage1.div_detail00.edt_chghy.set_cssclass();
		
	}
	else if(LAST_SANCTN_STTUS == "S00" || LAST_SANCTN_STTUS == "S01"){
	
		this.tab.tabpage1.div_detail00.cmd_fnncInstt.set_readonly(true);
		this.tab.tabpage1.div_detail00.edt_acnutno.set_readonly(true);
		this.tab.tabpage1.div_detail00.edt_chghy.set_readonly(true);
				
		this.tab.tabpage1.div_detail00.cmd_fnncInstt.set_cssclass("cmb_WF_Readonly");
		this.tab.tabpage1.div_detail00.edt_acnutno.set_cssclass("edt_WF_Readonly");
		this.tab.tabpage1.div_detail00.edt_chghy.set_cssclass("edt_WF_Readonly");
	
	}
}


