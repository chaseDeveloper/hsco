/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	this.fn_transaction("chitList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(this.ds_chit.getRowCount() == 1 && this.ds_chitDetail.getRowCount() <= 0) {
		this.gfn_message("info.항목.건수.이상", "전표상세내역을 1건");
		return
	}

	// 차변,대변 합계가 일치하는지 체크
	trace(Number(this.ds_chitDetail.getSum("DEBTOR_AMOUNT")));
	trace(Number(this.ds_chitDetail.getSum("CRDIT_AMOUNT")));
	if(Number(this.ds_chitDetail.getSum("DEBTOR_AMOUNT")) != Number(this.ds_chitDetail.getSum("CRDIT_AMOUNT"))) {
		this.gfn_message("errors.항목값.범위.동일", "차변금액의 합계|대변금액의 합계");
		return;
	}
	if(Ex.util.isUpdated(this.ds_chit) || Ex.util.isUpdated(this.ds_chitDetail)) {	
	// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_chitDetail, this.ds_validation)) {
			return;
		}
		else if(!this.gfn_checkValidation_NoUpdChk(this.ds_chit, this.ds_validation)) {
			return;
		}
		this.fn_transaction("saveChit");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "chitList": 		// 전표마스터
			this.ds_chit.clearData();
			this.ds_chitDetail.clearData();
			var sController   	= "hsco/mis/acc/ACC060204/chitList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_chit=output1";
			break;
		case "chitDetailList": 			// 전표상세
			this.ds_chitDetail.clearData();
			var sController   	= "hsco/mis/acc/ACC060204/chitDetailList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_chitDetail=output1";
			break;
		case "saveChit":		// 전표저장
			var sController   	= "hsco/mis/acc/ACC060204/saveChit.do";
			var sInDatasets   	= "input1=ds_chit:U input2=ds_chitDetail:U";
			var sOutDatasets  	= "";
			break;
		case "spaccSlipToSeSlipAi":	// 구분회계전표생성
			var sController   	= "hsco/mis/acc/ACC060204/spaccSlipToSeSlipAi.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cond=output1";
			break;
		case "spaccReSlipToSeSlipAi":	// 구분회계전표생성
			var sController   	= "hsco/mis/acc/ACC060204/spaccReSlipToSeSlipAi.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cond=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	if (nErrorCode == 0) {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "chitList":
				break;
			case "chitDetailList":
				break;
			case "saveChit":
				this.fn_transaction("chitList");
				break;
			
			case "spaccSlipToSeSlipAi":
				var cnt = this.ds_cond.getColumn(0,"OUT_SLIP_CNT");
				if(cnt > 0) {
					this.gfn_message("info.진행가능","구분회계전표가 [" + cnt + "]건 생성되었습니다.");
					
					this.fn_transaction("chitList");
				} 
				else {
					this.gfn_message("info.처리불가","구분회계전표가 생성되지 않았습니다.");
				}
				break;
			case "spaccReSlipToSeSlipAi":
				this.gfn_message("info.진행가능","역분개 전표가 생성되었습니다.");
				this.fn_transaction("chitList");
				
				break;
			default:
				break;
				
		}
	}
	
	
}

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var today = this.dateUtils.today();
	this.ds_chitDetail.clearData();
	var nRow = this.ds_chit.addRow();
	// 기본값 설정
	
	this.ds_chit.setColumn(nRow, "ACCNUT_YEAR", today.substr(0,4));	// 회계년도
	this.ds_chit.setColumn(nRow, "CHIT_DE", today);					// 전표일자
	this.ds_chit.setColumn(nRow, "CHIT_SE", "3");					// 전표구분(대체전표)
	this.ds_chit.setColumn(nRow, "CPTAL_EXPNDTR_DE", today);		// 자금지출일자
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete= function()
{	
	if(this.ds_chit.getRowCount() > 0) {i
		if (this.gfn_message("confirm.삭제여부") ) {
			this.ds_chit.deleteRow(this.ds_chit.rowposition);
