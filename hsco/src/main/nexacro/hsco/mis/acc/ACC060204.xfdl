				  arg_0      : this
				 ,searchDs   : this.ds_cond.saveXML()       // 검색조건(YEAR, BSNS_SE, BALC_SE, DEPT_CODE 컬럼만 이용)
				 ,strObj     : "" // 수정불가능 처리할 오브젝트 명(ex : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE,div_srchDept")
				 ,year_S     : year            // 예산년도(srchDs보다 우선순위를 가짐)
				 ,bsnsSe_S   : null            // 사업구분(srchDs보다 우선순위를 가짐)
				 ,balcSe_S   : null            // 수지구분(srchDs보다 우선순위를 가짐)
				 ,deptCode_S : null            // 부서코드(srchDs보다 우선순위를 가짐)
				 ,acntPopup  : true            // (true : 예산계정코드선택팝업, false : 지출단위선택팝업)
			};
			this.gfn_popup("btn_srchBdgPopup", 900, 635, "예산선택팝업", parm, "com::bdgAcntPop.xfdl", "fn_popupCallBack", true);
			break;
		case "btn_bsnsPopup":	// 검색조건: 사업코드
			this.gfn_popup("btn_bsnsPopup", "", "", "사업코드", "", "pms_prj::prjList.xfdl", "fn_popupCallBack", true);
			break;
		case "btn_prmpcPopup":	// 검색조건: 원가항목
			this.gfn_popup("btn_prmpcPopup", "", "", "원가항목", {year:year}, "mis_acc::ACC000000_P02.xfdl", "fn_popupCallBack", true);
			break;
		case "btn_seAccnutPopup": // 검색조건 : 구분회계
			this.gfn_popup("btn_seAccnutPopup", "", "", "구분회계", {year:year}, "mis_acc::ACC000000_P03.xfdl", "fn_popupCallBack", true);
			break;
		case "btn_excelDn":		// 마스터: 그리드 엑셀 다운로드
			this.gfn_exportExcel(this.grd_chit, "exportExl");
			break;
		case "btn_bdgPopup":	// 마스터: 예산과목, 지출단위			
			var parm = {
				  arg_0      : this
				 ,searchDs   : this.ds_cond.saveXML()       // 검색조건(YEAR, BSNS_SE, BALC_SE, DEPT_CODE 컬럼만 이용)
				 ,strObj     : "" // 수정불가능 처리할 오브젝트 명(ex : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE,div_srchDept")
				 ,year_S     : year            // 예산년도(srchDs보다 우선순위를 가짐)
				 ,bsnsSe_S   : null            // 사업구분(srchDs보다 우선순위를 가짐)
				 ,balcSe_S   : null            // 수지구분(srchDs보다 우선순위를 가짐)
				 ,deptCode_S : null            // 부서코드(srchDs보다 우선순위를 가짐)
				 ,acntPopup  : false           // (true : 예산계정코드선택팝업, false : 지출단위선택팝업)
			};
			this.gfn_popup("btn_bdgPopup", 900, 635, "예산선택팝업", parm, "com::bdgAcntPop.xfdl", "fn_popupCallBack", true);
			break;
			
		case "btn_addDtls":		// 디테일 그리드: 행추가
			if(this.ds_chit.getRowCount() > 0) {		
				var nRow = this.ds_chitDetail.addRow();
				// 초기값
				this.ds_chitDetail.setColumn(nRow, "ACCNUT_YEAR", 		this.ds_chit.getColumn(0,"ACCNUT_YEAR"));
				this.ds_chitDetail.setColumn(nRow, "BUDGET_YEAR", 		this.ds_chit.getColumn(0,"BUDGET_YEAR"));
				this.ds_chitDetail.setColumn(nRow, "BSNS_SE", 			this.ds_chit.getColumn(0,"BSNS_SE"));
				this.ds_chitDetail.setColumn(nRow, "BALC_SE", 			this.ds_chit.getColumn(0,"BALC_SE"));
				this.ds_chitDetail.setColumn(nRow, "BUDGET_ACNT_CODE", 	this.ds_chit.getColumn(0,"BUDGET_ACNT_CODE"));
				this.ds_chitDetail.setColumn(nRow, "DEPT_CODE", 		this.ds_chit.getColumn(0,"DEPT_CODE"));
				this.ds_chitDetail.setColumn(nRow, "EXPNDTR_UNIT_CODE", this.ds_chit.getColumn(0,"EXPNDTR_UNIT_CODE"));
				if(this.ds_chit.getRowType(this.ds_chit.rowposition)!=2){
					this.ds_chitDetail.setColumn(nRow, "CHIT_NO", 		this.ds_chit.getColumn(this.ds_chit.rowposition,"CHIT_NO"));
				}
				this.ds_chitDetail.setColumn(nRow, "DEBTOR_AMOUNT",		0);
				this.ds_chitDetail.setColumn(nRow, "CRDIT_AMOUNT",		0);
			}
			break;
		case "btn_delDtls":		// 디테일 그리드: 행삭제
			if(this.ds_chitDetail.getRowCount() > 0) {
				if (this.gfn_message("confirm.삭제여부") ) {
					this.ds_chitDetail.deleteRow(this.ds_chitDetail.rowposition);
				}
			}
			break;
		case "btn_excelDnDtls":	// 디테일 그리드: 엑셀 다운로드
			this.gfn_exportExcel(this.grd_chitDetail, "exportExl");
			break;
		case "btn_create":	// 구분회계 전표생성
			var fRow = this.ds_acc023.findRow("CODE", this.ds_cond.getColumn(0, "CHIT_SE"));
			var msg = new Array();
			msg.push("[ ");
			msg.push(this.comUtils.formatMask(this.ds_cond.getColumn(0, "CHIT_FROM_DE"),"####-##-##"));
			msg.push(" ~ ");
			msg.push(this.comUtils.formatMask(this.ds_cond.getColumn(0, "CHIT_TO_DE"),"####-##-##"));
			msg.push(" ] ");
			msg.push("구분회계 전표를 생성");
			
			if(this.gfn_message("comm.행위여부",msg.join("")) ){
				this.fn_transaction("spaccSlipToSeSlipAi");
			}
			
			break;
		case "btn_create00":	// 구분회계 전표생성
			if( this.ds_chit.getColumn(this.ds_chit.rowposition, "CHIT_DE").substring(4,6) == 12){
				this.gfn_message("info.처리불가","12월 대상 역분개 전표는 생성할 수 없습니다.");
				return false;
			}
			var fRow = this.ds_acc023.findRow("CODE", this.ds_cond.getColumn(0, "CHIT_SE"));
			var msg = new Array();
			msg.push("[ 전표번호 : ");
			msg.push(this.ds_chit.getColumn(this.ds_chit.rowposition, "CHIT_NO"));
			msg.push(" ] ");
			msg.push("역분개 전표를 생성");
			
			if(this.gfn_message("comm.행위여부",msg.join("")) ){
				this.ds_cond.setColumn(0, "ACCNUT_YEAR", this.ds_chit.getColumn(this.ds_chit.rowposition, "ACCNUT_YEAR"));
				this.ds_cond.setColumn(0, "CHIT_NO", 	 this.ds_chit.getColumn(this.ds_chit.rowposition, "CHIT_NO"));
				this.fn_transaction("spaccReSlipToSeSlipAi");
			}
			
			break;
	}
}

