/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if(this.gfn_message("confirm.취소여부") ){
		this.ds_chit.reset();
		this.ds_chit.applyChange();
		this.ds_chitDetail.reset();
		this.ds_chitDetail.applyChange();
	}	
}

