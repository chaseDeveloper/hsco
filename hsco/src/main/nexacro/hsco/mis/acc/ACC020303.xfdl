		 ["ds_acc001", "ACC001", "Y", "A", "B"]	// 사업장구분
		,["ds_acc003", "ACC003", "Y", "D", "B"]	// 지출은행
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 조회 이벤트 / 함수 호출
		//this.fn_search();
