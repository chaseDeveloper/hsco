***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}

	this.ds_dlryStdrRate.clearData();
	this.ds_dlryStdrAccount.clearData();
	this.ds_dlryStdrResult.clearData();
	
	// transaction
	this.ds_cond.setColumn(0, "YYYY", String(this.ds_cond.getColumn(0, "YYYYMM")).substr(0,4));
	trace(this.ds_cond.getColumn(0, "YYYY"));
	
	this.fn_transaction("RATE");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 배부항목관리 내역 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if(this.fn_isAccClosAt(this.ds_cond.getOrgColumn(0,"YYYYMM"),"S2")) {
		this.gfn_message("info.처리불가","결산마감된 자료입니다.");
		return;
	}

	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_dlryStdrRate, this.ds_validation)) {
		return false;
	}
	this.fn_transaction("SAVE");
}


/*
 * 함수명  	: fn_print
 * 설명 		: 인쇄
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
 */
this.fn_print = function()
{
	var titles = ["배부비율", "배부결과"];
	var vOpts = new this.cf_Opts;
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setArgs("title", titles[this.tab_dlry.tabindex]);
	vOpts.setSaveFilename(titles[this.tab_dlry.tabindex]);
	vOpts.clearLocalDs();
	
	if(this.tab_dlry.tabindex==0){
	  vOpts.setGrid(this.tab_dlry.tabpage1.grd);
	}
	else if(this.tab_dlry.tabindex==1){
	  vOpts.setGrid(this.tab_dlry.tabpage2.grd_result);
	}
	vOpts.setReportPath("/common/hscoLandscapeA3.jrf");

	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		opts : vOpts
	};
	this.gfn_popup("Popup_Report", 884, 735, titles[this.tab_dlry.tabindex], parm, "common::frmReportPopup.xfdl", "");
}
/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "RATE":		// 배부비율
			var sController   	= "hsco/mis/acc/ACC060205/dlryStdrRate.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_dlryStdrIem=output1 ds_dlryStdrRate=output2";
			break;
		
		case "ACCOUNT":		// 배부결과 탭 : 배부계정
			var sController   	= "hsco/mis/acc/ACC060205/dlryStdrAccount.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_dlryStdrAccount=output1";
			break;
		case "RESULT":		// 배부결과 탭 : 배부결과
			var sController   	= "hsco/mis/acc/ACC060205/dlryStdrResult.do";
			var sInDatasets   	= "input1=ds_cond2";
			var sOutDatasets  	= "ds_dlryStdrResult=output1";
			break;
		
		case "SAVE":		// 배부저장
			var sController   	= "hsco/mis/acc/ACC060205/dlryStdrSave.do";
			var sInDatasets   	= "input1=ds_dlryStdrRate:U";
			var sOutDatasets  	= "";
			break;
		case "PROCEDURE":	//배부실행
			var sController   	= "hsco/mis/acc/ACC060205/dlryStdrProcedure.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "PROCEDURECHIT":	//배부 전표 생성
			var sController   	= "hsco/mis/acc/ACC060205/dlryStdrChitProcedure.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "spaccStacntProcSe":		// 구분회계결산 실행
			var sController   	= "hsco/mis/acc/ACC060206/spaccStacntProcSe.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "stacntYn":		// 구분회계 결산처리 유무
			var sController   	= "hsco/mis/acc/ACC060205/stacntYn.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_stacntYn=output1";
			break;
		case "spaccSlipToSeSlipAi2":	// 조정구분회계전표생성
			var sController   	= "hsco/mis/acc/ACC060206/spaccSlipToSeSlipAi2.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cond=output1";
			break;
	}
	if(arguments.length > 1) {
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", "", false, arguments[1]); 
	} else {
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
	}
}

/*
* 함수명  		: div_search_btn_Procedure_onclick
