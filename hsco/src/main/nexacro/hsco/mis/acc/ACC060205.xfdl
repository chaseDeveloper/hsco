}

/*
 * 함수명  	: ds_dlryStdrAccount_onrowsetchanged
 * 설명 		: 구분회계DS 행 (도시공사전체) 삽입후 이벤트 처리. 재무상태표를 조회한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_seAccnutAcntCode_onrowposchanged();
 */
this.ds_dlryStdrAccount_onrowsetchanged = function(obj:Dataset, e:nexacro.DSRowsetChangeEventInfo)
{
	this.ds_cond2.clearData();
	this.ds_cond2.addRow();
	this.ds_cond2.setColumn(0, "YYYYMM", this.ds_cond.getOrgColumn(0, "YYYYMM"));
	this.ds_cond2.setColumn(0, "ACCNUT_ACNT_CODE", obj.getColumn(0, "ACCNUT_ACNT_CODE"));
	
	this.fn_transaction("RESULT");
}
/*
 * 함수명  	: ds_dlryStdrAccount_onrowposchanged
 * 설명 		: 구분회계DS 행변경 이벤트 처리. 결산처리를 조회한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_seAccnutAcntCode_onrowposchanged();
 */
this.ds_dlryStdrAccount_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_cond2.clearData();
	this.ds_cond2.addRow();
	this.ds_cond2.setColumn(0, "YYYYMM", this.ds_cond.getOrgColumn(0, "YYYYMM"));
	this.ds_cond2.setColumn(0, "ACCNUT_ACNT_CODE", obj.getColumn(obj.rowposition, "ACCNUT_ACNT_CODE"));
	
	this.fn_transaction("RESULT");
}
