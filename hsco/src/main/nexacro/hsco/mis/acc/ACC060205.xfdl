/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

/*
* 함수명  	: fn_callBack
* 설명 		: Transaction Callback 처리
* params  		: strSvcId    : 서비스요청 ID
*                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
*                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
* return Type 	: None
* 사용법		: 
*/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode == 0){
		switch (strSvcId) {
			case "RATE":		// 배부비율
				trace(this.ds_dlryStdrRate.saveXML());
 				if(this.ds_dlryStdrIem.rowcount > 0){
 					
 					for(var i=0; i<this.ds_dlryStdrIem.rowcount; i++){
 						if(this.ds_dlryStdrIem.getColumn(i, "DLRY_RATE_CALC_AT") == "0"){
 							this.tab_dlry.tabpage1.grd.setCellProperty("Body", (i*2) + 4, "edittype", "none");
 							this.tab_dlry.tabpage1.grd.setCellProperty("Body", (i*2) + 4, "editdisplay", "none");
 						} 
 						else {
 							this.tab_dlry.tabpage1.grd.setCellProperty("Body", (i*2) + 4, "edittype", "none");
 							this.tab_dlry.tabpage1.grd.setCellProperty("Body", (i*2) + 4, "editdisplay", "none");
 						}
 					}
 					
 				}
				this.fn_transaction("ACCOUNT");
				break;
			case "ACCOUNT":		// 배부계정
				break;
			case "RESULT":		// 배부결과
				break;
			case "PROCEDURE":	// 배부실행
				this.gfn_message("success.처리.성공");
				this.div_search.btn_ProcedureChit.set_enable(true);
				this.fn_transaction("RATE");
				break;
			case "PROCEDURECHIT":	// 배부전표
				this.gfn_message("success.처리.성공");
				this.div_search.btn_process.set_enable(true);
				this.fn_transaction("RATE");
				break;
			case "spaccStacntProcSe":	// 배부전표집계
				this.gfn_message("success.처리.성공");
				this.div_search.btn_create.set_enable(true);
				this.fn_transaction("RATE");
				break;	
				
			case "SAVE":
				this.fn_transaction("RATE");
			break;
			
			case "stacntYn":
				if(this.ds_stacntYn.getColumn(0,"STACNT_YN") == 'Y'){
					this.fn_PROCEDURE();
				}else{
					var year =this.ds_stacntYn.getColumn(0,"YYYY");
					var month =this.ds_stacntYn.getColumn(0,"MM");
					this.gfn_message("info.처리불가",year + "년"+ month +"월 구분회계 결산내역이 없습니다.");
				}
			break;
			
			case "spaccSlipToSeSlipAi2":
				this.gfn_message("success.처리.성공");
				this.div_search.btn_process00.set_enable(true);
// 				var cnt = this.ds_cond.getColumn(0,"OUT_SLIP_CNT");
// 				if(cnt > 0) {
// 					this.gfn_message("info.진행가능","조정구분회계전표가 [" + cnt + "]건 생성되었습니다.");
// 					this.div_search.btn_process00.set_enable(true);
// 				} 
// 				else {
