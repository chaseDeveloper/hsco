//그리드 css 변경(이전순번과 데이터비교후 마킹)
this.fn_setGridCss = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var curRow = e.newrow;
	var targetGrd = "";
	var sn = obj.getColumn(curRow, "SN");
	var prevRow = "";
	switch (obj.name) {		
		case "ds_incDsnMastrHist": 
			targetGrd = this.grd_hist1;
			prevRow = obj.findRow("SN", sn-1);
			break;
		case "ds_incdsnDetailHist": 
			targetGrd = this.grd_hist2;
			var seq = obj.getColumn(curRow, "SEQ");
			prevRow = obj.findRowExpr("SN ==" + (sn-1) + "&& SEQ ==" + seq);
			break;
	}
	
	if(sn > 0){
		for(var iCol = 0; iCol < obj.getColCount(); iCol++){
			var colId = obj.getColID(iCol);
			var curVal = this.comUtils.trimString(obj.getColumn(curRow, iCol));
			var prevVal = this.comUtils.trimString(obj.getColumn(prevRow, iCol));
			if(colId != "SN" && colId != "OCCRRNC_DT"){
				var grdIdx = targetGrd.getBindCellIndex("body", colId);
				if(curVal != prevVal){
					targetGrd.setCellProperty("body", grdIdx, "selectcolor", "crimson");
					targetGrd.setCellProperty("body", grdIdx, "selectbackground", "blanchedalmond");
				}else{
					targetGrd.setCellProperty("body", grdIdx, "selectcolor", "");
					targetGrd.setCellProperty("body", grdIdx, "selectbackground", "");
				}
			}
		}
	}else{
		for(var grdIdx = 0; grdIdx < targetGrd.getCellCount("head"); grdIdx++){
			targetGrd.setCellProperty("body", grdIdx, "selectcolor", "");
			targetGrd.setCellProperty("body", grdIdx, "selectbackground", "");
		}
	}
}


/*
 * 함수명  	: ds_cond_oncolumnchanged
