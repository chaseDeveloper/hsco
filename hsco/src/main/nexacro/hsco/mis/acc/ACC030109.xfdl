	var today = this.dateUtils.today();
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "PBLICTE_FROM_DE", today.substr(0,4)+"0101");
	this.ds_cond.setColumn(0, "PBLICTE_TO_DE", today);
}

