*/
this.div_work_grd_propseWnpzList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
	}
	this.gfn_gridSort(obj, e);
}

 /*
 * 함수명  	: ds_propseWnpzList_oncolumnchanged
 * 설명 		: 등급컬럼 변경시 평점 입력
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_propseWnpzList_oncolumnchanged();
 */
this.ds_propseWnpzList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var nRow = this.ds_propseWnpzList.rowposition;
	if(e.columnid == "WNPZ_GRAD_SE") {
		var wnpgGradeSe = this.ds_propseWnpzList.getColumn(nRow, "WNPZ_GRAD_SE");
		var findRow = this.ds_wnpzGradSe.findRow("CODE", wnpgGradeSe, -1);
		this.ds_propseWnpzList.setColumn(nRow, "EVL_GRADE", this.ds_wnpzGradSe.getColumn(findRow, "MSTK_VAL"));
	}
	if(e.columnid == "WNPZ_GRADE") {
		if(e.newvalue < 0) {
			this.gfn_message("errors.항목값.범위.이상", "입상평점|0");
			this.ds_propseWnpzList.setColumn(nRow, "WNPZ_GRADE", e.oldvalue);
			return false;
		}
		if(e.newvalue > 100) {
			this.gfn_message("errors.항목값.범위.이하", "입상평점|100");
			this.ds_propseWnpzList.setColumn(nRow, "WNPZ_GRADE", e.oldvalue);
			return false;
		}
	}
}

