/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.checkDs = [
  this.ds_propseWnpzList
];
this.saveRow = -1;
/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.HRM040201_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();
	this.fn_init_dataset();
}

this.HRM040201_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	this.fn_loadCombo();
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.init_Form();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	var comboParams = [
			["ds_ofcpsSe"    , "HRM003", "1", "전체"],		// 직위구분
			["ds_rspofcSe"   , "_HRM214", "1", "전체"],		// 직책구분
			["ds_wnpzSe"     , "SUP004", "1", "전체"],		// 제안(입상)구분
			["ds_wnpzGradSe" , "SUP007", "1", "전체"],		// 등급구분
			["ds_wnpzSeG"    , "SUP004", "1", ""],			// 제안(입상)구분(Grid)
			["ds_wnpzGradSeG", "SUP007", "1", ""]			// 등급구분(Grid)
		];
	
	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: 공통 callback
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callbackAfter();
 */
this.fn_callbackAfter = function ()
{
	this.div_search.div_deptCode.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	var today = this.gfn_today();
	var startDe = this.dateUtils.addMonth(today, -12, 's');
	this.ds_cond.setColumn(0, "END_DT", today);
	this.ds_cond.setColumn(0, "BGN_DT", startDe);
	this.fn_search();
}

/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/


/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(제안입상자)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(this.tab_pbncd.tabindex == 0 ){
		
		// validation 체크(제안입상자)
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
			return;
		}
		
		this.ds_propseWnpzList.clearData();
		this.fn_transaction("tbhrmPropseWnpzList");
	} else {
		// validation 체크(사외수상자)
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
			return;
		}
		this.ds_outCompanyWnpzList.clearData();
		this.fn_transaction("tbhrmOutCompanyWnpzList");
	}
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if(this.tab_pbncd.tabindex == 0 ){
		// 데이터셋 수정 여부체크
		if (!this.comUtils.isDatasetUpdated(this.ds_propseWnpzList)) {
			this.gfn_message("comm.데이터.변경.없음");
			return false;
		}
		
		// validation 체크(제안입상자)
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_propseWnpzList, this.ds_validation)) {
			return;
		}
		
		if(this.gfn_message("confirm.저장.여부")) {
			this.saveRow = this.ds_propseWnpzList.rowposition;
			this.fn_transaction("tbhrmPropseWnpzSave");
		}
	} else {
		// 데이터셋 수정 여부체크
		if (!this.comUtils.isDatasetUpdated(this.ds_outCompanyWnpzList)) {
			this.gfn_message("comm.데이터.변경.없음");
			return false;
		}
		if(this.gfn_message("confirm.저장.여부")) {
			this.saveRow = this.ds_propseWnpzList.rowposition;
			this.fn_transaction("tbhrmOutCompanyWnpzSave");
		}
	
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "tbhrmPropseWnpzList":	// 제안입상자 조회
			var sController   	= "/hsco/mis/hrm/HRM040201/tbhrmPropseWnpzList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_propseWnpzList=output1";
			break;
		case "tbhrmOutCompanyWnpzList":	// 사외수상자 조회
			var sController   	= "/hsco/mis/hrm/HRM040201/tbhrmOutCompanyWnpzList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_outCompanyWnpzList=output1";
			break;
		case "tbhrmPropseWnpzSave":	 // 저장
			var sController   	= "/hsco/mis/hrm/HRM040201/tbhrmPropseWnpzSave.do";
			var sInDatasets   	= "input1=ds_propseWnpzList:U";
			var sOutDatasets  	= "";
			break;
		case "tbhrmOutCompanyWnpzSave":	 // 저장
			var sController   	= "/hsco/mis/hrm/HRM040201/tbhrmOutCompanyWnpzSave.do";
			var sInDatasets   	= "input1=ds_outCompanyWnpzList:U";
			var sOutDatasets  	= "";
			break;
			
		case "saveSanc":					// 전자결재
			var misKey    = "HRM"
			var iDiv      = "";
			var title     = "";
			var dataTitle = "";
			var dataValue = "";
			// 001 : 기본기안
			iDiv      = "001"
			title     = "제안입상";
			dataTitle = "제안입상";
			
			this.ds_sanc.clearData();
			var nRow = this.ds_sanc.addRow();
			this.ds_sanc.setColumn(nRow, "MIS_KEY",    misKey     );
			this.ds_sanc.setColumn(nRow, "I_DIV",      iDiv       );
			this.ds_sanc.setColumn(nRow, "TITLE",      title      );
			this.ds_sanc.setColumn(nRow, "DATA_TITLE", dataTitle  );
			this.ds_sanc.setColumn(nRow, "DATA_VALUE", null  );
			this.ds_sanc.setColumn(nRow, "DEPT_CD",    this.gfn_getDeptId() );
			this.ds_sanc.setColumn(nRow, "EMP_CD",     this.fn_getEmpno() );
			
			var sController   	= "hsco/mis/hrm/HRM040201/saveSanc.do";
			var sInDatasets   	= "input1=ds_sanc";
			var sOutDatasets  	= "ds_sanc=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "tbhrmPropseWnpzList":	// 조회(제안입상자)
				if(this.saveRow > -1){
					this.ds_propseWnpzList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "tbhrmOutCompanyWnpzList":	// 조회(제안입상자)
				if(this.saveRow > -1){
					this.ds_outCompanyWnpzList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;	
			case "tbhrmPropseWnpzSave": // 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "tbhrmOutCompanyWnpzSave": // 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "saveSanc":
				var oUrl = this.ds_sanc.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
				break;
		}
	}
}


 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "empnoPop") {
		var dsEmpNo = new Dataset();
		var loadCnt = dsEmpNo.loadXML(sReturn);
		if(loadCnt) {
			
			var deptNm   = dsEmpNo.getColumn(0, "DEPT_NM");
			var empNo    = dsEmpNo.getColumn(0, "EMPNO");
			var empNm    = dsEmpNo.getColumn(0, "EMPNM");
			var ofcpsSe  = dsEmpNo.getColumn(0, "OFCPS");
			var rspofcSe = dsEmpNo.getColumn(0, "RSPOFC_SE");
			if(this.tab_pbncd.tabindex == 0){
				var pwlRow   = this.ds_propseWnpzList.rowposition;
				this.ds_propseWnpzList.setColumn(pwlRow, "DEPT_NM", deptNm);
				this.ds_propseWnpzList.setColumn(pwlRow, "EMPNO", empNo);
				this.ds_propseWnpzList.setColumn(pwlRow, "EMPNM", empNm);
				this.ds_propseWnpzList.setColumn(pwlRow, "OFCPS_SE", ofcpsSe);
				this.ds_propseWnpzList.setColumn(pwlRow, "RSPOFC_SE", rspofcSe);
			}
		}
	}
}

