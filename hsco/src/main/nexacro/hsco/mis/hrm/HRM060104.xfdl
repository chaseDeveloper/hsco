 * 설명 		: 평정자선택(검색조건) 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_search_btn_evl_man_onclick(obj,e);
 */
this.div_search_btn_evl_man_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var parm = {
		 arg_0      : this
		,evlYear    : this.ds_cond.getColumn(0, "EVL_YEAR")
		,htSe       : this.ds_cond.getColumn(0, "HT_SE")
		,evlManSe   : '02'
	};
	this.gfn_popup("HRM060103_P01", 400, 400, "평정자선택", parm, "mis_hrm::HRM060103_P01.xfdl", "fn_popupCallBack", true);
}


/*
 * 함수명  	: btn_print_onclick
 * 설명 		: 등급별 분포인원 출력 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_print_onclick(obj,e);
 */
this.btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var sRow     = this.ds_cond_sub.rowposition;
	var evlYear  = this.ds_cond_sub.getColumn(sRow, "EVL_YEAR");
	var htSe     = this.ds_cond_sub.getColumn(sRow, "HT_SE");
	
	this.ds_print.clearData();
	var nRow = this.ds_print.addRow();
	this.ds_print.setColumn(nRow, "EVL_YEAR", evlYear);
	this.ds_print.setColumn(nRow, "HT_SE",    htSe);
	
	this.gfn_ReportPopup(this, "MIS/HRM/HRM060124_R01.crf", this.ds_cond);
}


/*
 * 함수명  	: fn_setEnableObj
 * 설명 		: 평정가능 기간에 따른 오브젝트 동적제어
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_setEnableObj(enableAt);
 */
this.fn_setEnableObj = function(enableAt)
{
	if(this.comUtils.isNull(enableAt)) {
		enableAt = '0';
	}
	
	if(enableAt == '1') {
		this.grd_work_scre_evl.set_readonly(false);
		this.tab_sub.tp1.div_dtl.txt_EVL_2_OPINION.set_readonly(false);
		this.tab_sub.tp1.div_dtl.txt_EVL_2_OPINION.set_cssclass("");
		this.sta_enableAt.set_text("[평정 가능]");
		this.sta_enableAt.style.set_color("blue");
	} else {
		this.grd_work_scre_evl.set_readonly(true);
		this.tab_sub.tp1.div_dtl.txt_EVL_2_OPINION.set_readonly(true);
		this.tab_sub.tp1.div_dtl.txt_EVL_2_OPINION.set_cssclass("txt_WF_Readonly");
		this.sta_enableAt.set_text("[평정 불가]");
		this.sta_enableAt.style.set_color("red");
	}
}


this.grd_work_evl_trgter_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.gfn_gridSort(obj, e);			// Grid 데이터 정렬
}
