		this.fn_search();
	}
}


/*
 * 함수명  	: ds_work_scre_evl_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_work_scre_evl_oncolumnchanged(obj,e);
 */
this.ds_work_scre_evl_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var idxPrefix = (e.columnid+"").indexOf("EVL_GRAD");
	var idxSuffix = (e.columnid+"").indexOf("_AT");
	if(idxPrefix > -1 && idxSuffix > -1) {
		obj.set_enableevent(false);
		if(e.columnid != "EVL_GRAD1_AT") {
			obj.setColumn(e.row, "EVL_GRAD1_AT", "");
		}
		if(e.columnid != "EVL_GRAD2_AT") {
			obj.setColumn(e.row, "EVL_GRAD2_AT", "");
		}
		if(e.columnid != "EVL_GRAD3_AT") {
			obj.setColumn(e.row, "EVL_GRAD3_AT", "");
		}
		if(e.columnid != "EVL_GRAD4_AT") {
			obj.setColumn(e.row, "EVL_GRAD4_AT", "");
		}
		if(e.columnid != "EVL_GRAD5_AT") {
			obj.setColumn(e.row, "EVL_GRAD5_AT", "");
		}
		obj.set_enableevent(true);
		
		var evlGrad  = null;
		var evlScore = null;
		switch(e.columnid) {
			case "EVL_GRAD1_AT":
				evlGrad = '1';
				evlScore = obj.getColumn(e.row, "IEM_GRAD1_SCORE");
				break;
			case "EVL_GRAD2_AT":
				evlGrad = '2';
				evlScore = obj.getColumn(e.row, "IEM_GRAD2_SCORE");
				break;
			case "EVL_GRAD3_AT":
				evlGrad = '3';
				evlScore = obj.getColumn(e.row, "IEM_GRAD3_SCORE");
				break;
			case "EVL_GRAD4_AT":
				evlGrad = '4';
				evlScore = obj.getColumn(e.row, "IEM_GRAD4_SCORE");
				break;
			case "EVL_GRAD5_AT":
				evlGrad = '5';
				evlScore = obj.getColumn(e.row, "IEM_GRAD5_SCORE");
				break;
		}
		if(e.newvalue != '1') {
			evlGrad  = null;
			evlScore = null;
		}
		obj.setColumn(e.row, "EVL_GRAD",  evlGrad);
		obj.setColumn(e.row, "EVL_SCORE", evlScore);
	}
}



