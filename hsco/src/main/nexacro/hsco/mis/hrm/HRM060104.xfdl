/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectWorkEvlTrgter":
			var sController   	= "/hsco/mis/hrm/HRM060104/selectWorkEvlTrgter.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_work_evl_trgter=output1 ds_work_evl_pd=output2 ds_gradDistrbTable=output3 ds_evlManInfo=output4";
			break;
		case "selectEvlScreEvl":
			var sController   	= "/hsco/mis/hrm/HRM060104/selectEvlScreEvl.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_work_scre_evl=output1 ds_evl_opinion=output2 ds_evl_nmpr=output3 ds_crevalNmprDistrb=output4";
			break;
		case "saveEvlScreEvl":
			var sController   	= "/hsco/mis/hrm/HRM060104/saveEvlScreEvl.do";
			var sInDatasets   	= "input1=ds_work_scre_evl:U input2=ds_evl_opinion:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
 * Transaction Callback 영역
***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectWorkEvlTrgter":
				// 평정기간의 해당유무를 체크하여 오브젝트 동적제어
				var enableAt = this.ds_work_evl_pd.getColumn(0, "ENABLE_AT");
				this.fn_setEnableObj(enableAt);
				
				this.sta_errorMsg.set_visible(false);
				if(this.ds_work_evl_trgter.getRowCount() > 0 && nRowPos > -1){
					this.ds_work_evl_trgter.set_rowposition(nRowPos);
					nRowPos = -1;
				}
				
				this.ds_work_evl_trgter.set_enableevent(false);
				this.ds_work_evl_trgter.set_updatecontrol(false);
				for(var fRow = 0; fRow < this.ds_work_evl_trgter.getRowCount(); fRow++) {
					var evl1Score = this.ds_work_evl_trgter.getColumn(fRow, "EVL_1_SCORE");
					var evl2Score = this.ds_work_evl_trgter.getColumn(fRow, "EVL_2_SCORE");
					var totScore  = this.ds_work_evl_trgter.getColumn(fRow, "TOT_EVL_SCORE");
					
					evl1Score = evl1Score ? evl1Score.replace(" 점","") : evl1Score;
					evl2Score = evl2Score ? evl2Score.replace(" 점","") : evl2Score;
					totScore  = totScore  ? totScore.replace(" 점","")  : totScore;
					
					this.ds_work_evl_trgter.setColumn(fRow, "EVL_1_SCORE", evl1Score);
					this.ds_work_evl_trgter.setColumn(fRow, "EVL_2_SCORE", evl2Score);
					this.ds_work_evl_trgter.setColumn(fRow, "TOT_EVL_SCORE", totScore);
				}
				this.ds_work_evl_trgter.set_updatecontrol(true);
				this.ds_work_evl_trgter.set_enableevent(true);
				
				var suprrAt = this.ds_evlManInfo.getColumn(0, "SUPRR_AT");
// 				if(suprrAt == "Y") {
// 					this.grd_gradDistrbTable.setFormatColProperty(1, "size", 70);
// 					this.tab_sub.tp2.grd_crevalNmprDistrb.setFormatColProperty(2, "size", 100);
// 				} else {
// 					this.grd_gradDistrbTable.setFormatColProperty(1, "size", 0);
// 					this.tab_sub.tp2.grd_crevalNmprDistrb.setFormatColProperty(2, "size", 0);
// 				}
				
				break;
			case "selectEvlScreEvl":
				this.sta_errorMsg.set_visible(false);
				this.sta_errorMsg00.set_visible(false);
				var isError = false;
				var year = this.ds_cond.getColumn(0, "EVL_YEAR");
				
				if(year >= 2020) {
					for(var fRow = 0; fRow < this.ds_gradDistrbTable.getRowCount(); fRow++) {
						var color = this.ds_gradDistrbTable.getColumn(fRow, "COLOR");
						if(color == "red") {
							isError = true;
							break;
						}
					}
					if(isError) {
						this.tab_sub.set_tabindex(1);
						this.sta_errorMsg00.set_visible(true);
					}
				} else {
					for(var fRow = 0; fRow < this.ds_evl_nmpr.getRowCount(); fRow++) {
						// 등급별 인원기준
						var gradNmpr = this.ds_evl_nmpr.getColumn(fRow, "GRAD_NMPR");
						// 실제 저장된 등급별인원
						var gradNm = this.ds_evl_nmpr.getColumn(fRow, "GRAD_NM");
						var caseCount = this.ds_evl_nmpr.getCaseCount("GRAD_NM=='"+ gradNm +"'");
						// 등수가 있는 사람이 있는지 체크
						var scoreRnk = this.ds_evl_nmpr.getColumn(fRow, "SCORE_RNK");
						if(!this.comUtils.isNull(scoreRnk)) {
							gradNmpr  = nexacro.toNumber(gradNmpr,0,0,0);
							caseCount = nexacro.toNumber(caseCount,0,0,0);
							if(caseCount > gradNmpr) {
								isError = true;
								break;
							}
						}
					}
					if(isError) {
						var errMsg = "평정인원분포를 초과하는 점수결과가 존재합니다. 수정바랍니다.";
						this.tab_sub.set_tabindex(1);
						this.sta_errorMsg.set_visible(true);
					}
				}
				break;
			case "saveEvlScreEvl":
				this.fn_search();
				break;
		}
	}
}


