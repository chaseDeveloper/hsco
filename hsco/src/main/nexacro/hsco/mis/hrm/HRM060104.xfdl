/*
 * 함수명  	: grd_work_evl_trgter_oncellclick
 * 설명 		: Dataset 행 변경 전 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_work_evl_trgter_oncellclick(obj,e);
 */
this.grd_work_evl_trgter_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var sRow     = this.ds_cond_sub.rowposition;
	var evlYear  = this.ds_cond_sub.getColumn(sRow, "EVL_YEAR");
	var htSe     = this.ds_cond_sub.getColumn(sRow, "HT_SE");
	var empno    = this.ds_cond_sub.getColumn(sRow, "EMPNO");
	
	var tRow     = this.ds_work_evl_trgter.rowposition;
	var deptCode = this.ds_work_evl_trgter.getColumn(tRow, "DEPT_CODE");
	
	this.ds_print.clearData();
	var nRow = this.ds_print.addRow();
	this.ds_print.setColumn(nRow, "EVL_YEAR", evlYear);
	this.ds_print.setColumn(nRow, "YEAR",     evlYear);
	this.ds_print.setColumn(nRow, "HT_SE",    htSe);
	this.ds_print.setColumn(nRow, "EMPNO",    empno);
	
	if(e.col == 14) {
		this.ds_print.setColumn(nRow, "DEPT_CODE", deptCode);
		this.gfn_ReportPopup(this, "MIS/HRM/HRM060125_R01.crf", this.ds_print);
	} else if(e.col == 15) {
		this.gfn_ReportPopup(this, "MIS/HRM/HRM060126_R01.crf", this.ds_print);
	} else if(e.col == 16) {
		this.gfn_ReportPopup(this, "MIS/HRM/HRM060127_R01.crf", this.ds_print);
	}
}



