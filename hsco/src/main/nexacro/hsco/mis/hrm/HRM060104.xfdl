/*
 * 함수명  	: fn_search_dtl
 * 설명 		: 상세조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_dtl();
 */
this.fn_search_dtl = function()
{
	var evlYear   = this.ds_cond.getColumn(0, "EVL_YEAR");
	var htSe      = this.ds_cond.getColumn(0, "HT_SE");
	var evlMan    = this.ds_cond.getColumn(0, "EVL_MAN_2_EMPNO");
	
	var suprrAt   = this.ds_evlManInfo.getColumn(0, "SUPRR_AT");
	
	var etRow     = this.ds_work_evl_trgter.rowposition;
	var empno     = this.ds_work_evl_trgter.getColumn(etRow, "EMPNO");
	var totNmpr   = this.ds_work_evl_trgter.getColumn(etRow, "CLSF_CNT");
	var jssfcSe   = this.ds_work_evl_trgter.getColumn(etRow, "JSSFC_SE");
	var clsfSe    = this.ds_work_evl_trgter.getColumn(etRow, "CLSF_SE");
	var evlStepSe = this.ds_work_evl_trgter.getColumn(etRow, "EVL_STEP_SE");
	
	this.ds_cond_sub.clearData();
	var nRow = this.ds_cond_sub.addRow();
	this.ds_cond_sub.setColumn(nRow, "EVL_YEAR",       evlYear);
	this.ds_cond_sub.setColumn(nRow, "HT_SE",          htSe   );
	this.ds_cond_sub.setColumn(nRow, "EVL_MAN_EMPNO",  evlMan );
	this.ds_cond_sub.setColumn(nRow, "EMPNO",          empno  );
	this.ds_cond_sub.setColumn(nRow, "TOT_NMPR",       totNmpr);
	this.ds_cond_sub.setColumn(nRow, "JSSFC_SE",       jssfcSe);
	this.ds_cond_sub.setColumn(nRow, "CLSF_SE",        clsfSe );
	this.ds_cond_sub.setColumn(nRow, "EVL_STEP_SE",    evlStepSe);
	this.ds_cond_sub.setColumn(nRow, "SUPRR_AT",       suprrAt);
	
	if(evlYear >= 2020) {
		this.ds_cond_sub.setColumn(nRow, "YEAR_SE", "02");
	} else {
		this.ds_cond_sub.setColumn(nRow, "YEAR_SE", "01");
	}
	
	this.ds_work_scre_evl.clearData();
	this.ds_evl_opinion.clearData();
	this.ds_evl_nmpr.clearData();
	this.ds_crevalNmprDistrb.clearData();
	
	this.fn_transaction("selectEvlScreEvl");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (!this.comUtils.isDatasetUpdated(this.ds_work_scre_evl) && !this.comUtils.isDatasetUpdated(this.ds_evl_opinion)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if(this.comUtils.isDatasetUpdated(this.ds_work_scre_evl)) {
		for(var fRow = 0; fRow < this.ds_work_scre_evl.getRowCount(); fRow++) {
			var evlGrad = this.ds_work_scre_evl.getColumn(fRow, "EVL_GRAD");
			if(this.comUtils.isNull(evlGrad)) {
				var evlSeNm  = this.ds_work_scre_evl.getColumn(fRow, "EVL_SE_NM");
				var evlIemNm = this.ds_work_scre_evl.getColumn(fRow, "EVL_IEM_NM");
				
				var errMsg = "[" + (fRow+1) + "행] " + evlSeNm + " / " + evlIemNm + "\n";
				errMsg    += "해당항목에 대한 평가를 진행하여 주시기 바랍니다";
				alert(errMsg);
				return false;
			}
		}
	}
	
	// save
	if(this.gfn_message("confirm.저장.여부"))	{
		nRowPos = this.ds_work_evl_trgter.rowposition;
		this.fn_transaction("saveEvlScreEvl");
	}
}



