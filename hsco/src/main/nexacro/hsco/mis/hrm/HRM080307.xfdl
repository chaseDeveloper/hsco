	var parm = {
	   arg_0      : this
	  , year      : year
	  , empno     : empno
	  , sn        : sn
	};
	this.gfn_popup("popEdcTy", 500, 465, "", parm, "mis_hrm::HRM080307_P01.xfdl", "fn_popEdcCallBack", true);
}
