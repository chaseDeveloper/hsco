			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_odrSeList=output1";
			break;	
		// =============================
		case "selectTbhrmEcnySportGnrlR": // 입사지원관리 기본사항 불러오기
			var sController   	= "hsco/mis/hrm/HRM020201/tbhrmEcnySportGnrlR.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_ecnySportGnrlList=output1";
			break;
		case "selectTbhrmEcnySportGnrlTab": // 입사지원관리 기본사항 불러오기(탭)
			var sController   	= "hsco/mis/hrm/HRM020201/selectTbhrmEcnySportGnrlTab.do";
			var sInDatasets   	= "input1=ds_cond2";
			var sOutDatasets  	= "ds_edcList=output1 ds_qualfList=output2 ds_exprncCareerList=output3 ds_fileList=output4";
			break;
		case "saveTbhrmEcnySportGnrl":	// 입사지원관리 저장하기
			var sController   	= "hsco/mis/hrm/HRM020201/tbhrmEcnySportGnrlCUD.do";
			var sInDatasets   	= "input1=ds_ecnySportGnrlList:U input2=ds_edcList:U input3=ds_qualfList:U input4=ds_exprncCareerList:U";
			var sOutDatasets  	= "";
			break;	
		case "selectAtchFile":			// 파일조회
			var sController 	= "hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets		= "input1=ds_cond_file";
			var sOutDatasets	= "ds_photo=output1";
			break;
		case "saveExcelUpload":		// 엑셀 업로드
			var sController   	= "hsco/mis/hrm/HRM020201/saveExcelUpload.do";
			var sInDatasets   	= "input1=ds_excelSave:U";
			var sOutDatasets  	= "";
			break;
 		case "savePwd":				// 패스워드 저장
 			var sController   	= "hsco/mis/hrm/HRM020201/savePwd.do";
 			var sInDatasets   	= "input1=ds_pwd:U";
			var sOutDatasets  	= "";
 			break;
