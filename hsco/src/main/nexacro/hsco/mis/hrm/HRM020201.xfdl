	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "HPM", "recruit");
	this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/

 /*
 * 함수명  	: fn_search
 * 설명 		: 입사지원관리 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(검색조건)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.ds_ecnySportGnrlList.clearData();
	this.fn_transaction("selectTbhrmEcnySportGnrlR")
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_ecnySportGnrlList) && !this.comUtils.isDatasetUpdated(this.ds_edcList) && !this.comUtils.isDatasetUpdated(this.ds_qualfList)&& !this.comUtils.isDatasetUpdated(this.ds_exprncCareerList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	// validation 체크(기본사항)
 	if(!this.gfn_checkValidation_NoUpdChk(this.ds_ecnySportGnrlList, this.ds_validation)) {
 		return;
 	}
	// validation 체크(교육사항탭)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_edcList, this.ds_validation)) {
		return;
	}
	// validation 체크(자격사항탭)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_qualfList, this.ds_validation)) {
		return;
	}
	// validation 체크(경험경력사항탭)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_exprncCareerList, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")){
		this.saveRow  = this.ds_ecnySportGnrlList.rowposition;
		this.saveRow1 = this.ds_edcList.rowposition;
		this.saveRow2 = this.ds_qualfList.rowposition;
		this.saveRow3 = this.ds_exprncCareerList.rowposition;
		this.fn_transaction("saveTbhrmEcnySportGnrl");
	}
}

/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(상세조회)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function(){
	
	// clearData()

	this.ds_exprncCareerList.clearData();
	this.ds_qualfList.clearData();
	this.ds_edcList.clearData();
	
	if(this.ds_ecnySportGnrlList.rowcount > 0) {
		
		// 응시분야
 		var cRow = this.ds_ecnySportGnrlList.rowposition;	
	
		var cRow = this.ds_ecnySportGnrlList.rowposition;
		var empmnYear = this.ds_ecnySportGnrlList.getColumn(cRow, "EMPMN_YEAR");
		var odrSe = this.ds_ecnySportGnrlList.getColumn(cRow, "ODR_SE");
		var apyexmNo = this.ds_ecnySportGnrlList.getColumn(cRow, "APYEXM_NO");
		var fileSn = this.ds_ecnySportGnrlList.getColumn(cRow, "FILE_SN");

		//탭
		this.ds_cond2.clearData();
		this.ds_cond2.addRow();
		this.ds_cond2.setColumn(0, "EMPMN_YEAR", empmnYear);
		this.ds_cond2.setColumn(0, "ODR_SE"    , odrSe);
		this.ds_cond2.setColumn(0, "APYEXM_NO" , apyexmNo);
		this.ds_cond2.setColumn(0, "FILE_SN"   , fileSn);

		//  5개 조회
		this.fn_transaction("selectTbhrmEcnySportGnrlTab");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// ========= 검색조건 =========
		case "selectEmpmnYearCombo": 		// 채용년도 조회(검색조건)
