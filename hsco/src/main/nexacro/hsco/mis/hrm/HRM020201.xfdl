	var chk1 = this.div_print.chk_print1.value;	// 응시원서
	var chk2 = this.div_print.chk_print2.value;	// 입사지원서
	var chk3 = this.div_print.chk_print3.value;	// 자기소개서

	this.ds_cond_print.setColumn(0, "CHK1", chk1);
	this.ds_cond_print.setColumn(0, "CHK2", chk2);
