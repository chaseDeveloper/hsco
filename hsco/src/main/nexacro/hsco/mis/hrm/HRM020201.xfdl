 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectEmpmnYearCombo": 		// 채용년도 조회(검색조건)
				this.div_search.cmb_empmnYear.set_index(0);
				break;
			case "selectOdrSeCombo": 			// 차수, 채용공고 조회(검색조건)
				this.div_search.cmb_odrSe.set_index(0);
				break;	
			case "selectTbhrmEcnySportGnrlR": // 조회
				break;				
			case "selectTbhrmEcnySportGnrlTab":
				var photoFileId = this.ds_ecnySportGnrlList.getColumn(this.ds_ecnySportGnrlList.rowposition, "PHOTO_FILE_ID");
				if(this.comUtils.isNotNull(photoFileId)) {
					this.ds_cond_file.clearData();
					this.ds_cond_file.addRow();
					this.ds_cond_file.setColumn(0, "FILE_SN", photoFileId);
					
					this.fn_transaction("selectAtchFile");
				}else{
					this.ds_photo.clearData();
				}
				break;
			case "selectAtchFile": // 첨부파일조회
				if(this.ds_photo.rowcount>0){
					this.fn_fileView();
				}else{
					this.div_detail.iv_files.set_image("");
				}
				break;
			case "saveTbhrmEcnySportGnrl": // 입사지원관리 저장하기
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "deleteFile": // 사진삭제
			 this.fn_search();
				break;	
			case "saveExcelUpload":
				this.gfn_message("info.정상처리");
				this.fn_search();
				break;
 			case "savePwd":
 				this.gfn_message("success.처리.성공");
 				this.ds_pwd.clearData();
 				this.div_detail.edt_pwd.set_value("");
 		 	case "saveProperAt":
 				this.gfn_message("success.처리.성공");
 				this.ds_properAt.clearData();
 			break;
 			case "setRlApyexmNo":		// 응시번호 부여
 				this.gfn_message("success.처리.성공");
 				this.fn_search();
 			break;
 		       // 첨부파일 삭제
			case "deleteAtchFile":
	
            break;
		}
	}
}

