/*
 * 함수명  	: btn_print_onclick
 * 설명 		: 일괄/개별출력
 * params  		: obj, e
 * return Type 	: None
 * 사용법		: this.btn_print_onclick(obj:Button,  e:nexacro.ClickEventInfo)
 */
this.btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//validation 체크
    if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
        return;
    }
	
	this.ds_cond_print.clearData();
	this.ds_cond_print.addRow();
	
	var empmnYear = this.ds_cond.getColumn(0, "EMPMN_YEAR");					// 채용년도
	var odrSe 	  = this.ds_cond.getColumn(0, "ODR_SE");						// 차수
	
	var cmpetSe 	    = this.ds_cond.getColumn(0, "CMPET_SE");
	var empmnSe         = this.ds_cond.getColumn(0, "EMPMN_SE");
	var apyexmClsfSe    = this.ds_cond.getColumn(0, "APYEXM_CLSF_SE");
	var apyexmRealmSe   = this.ds_cond.getColumn(0, "APYEXM_REALM_SE");	
	var nm        		= this.ds_cond.getColumn(0, "NM_KOREAN");	
	
	var cRow	  = this.ds_ecnySportGnrlList.rowposition;
	var printSe = "";
	var apyexmNo = "";

	// 일괄
	if(obj.name == "btn_bundle") {
		this.div_print.sta_text.set_text("일괄출력");
		if(this.ds_ecnySportGnrlList.rowcount < 1) {
			this.gfn_message("info.처리불가","인쇄할 지원자가 없습니다.");
			return;
		}
		printSe = "1";
	
	// 개별
	}else if(obj.name == "btn_individual") {
		this.div_print.sta_text.set_text("개별출력");
		if(this.ds_ecnySportGnrlList.rowposition == -1) {
			this.gfn_message("info.처리불가","인쇄할 지원자를 선택해주세요.");
			return;
		}
		apyexmNo = this.ds_ecnySportGnrlList.getColumn(cRow, "APYEXM_NO")
		printSe  = "2";
	}

	this.ds_cond_print.setColumn(0, "EMPMN_YEAR", empmnYear);
	this.ds_cond_print.setColumn(0, "ODR_SE"    , odrSe);
	this.ds_cond_print.setColumn(0, "PRINT_SE"  , printSe);
	
	this.ds_cond_print.setColumn(0, "APYEXM_NO"   , apyexmNo);
	this.ds_cond_print.setColumn(0, "CMPET_SE"       , cmpetSe);
	this.ds_cond_print.setColumn(0, "EMPMN_SE"       , empmnSe);
	this.ds_cond_print.setColumn(0, "APYEXM_CLSF_SE" , apyexmClsfSe);
	this.ds_cond_print.setColumn(0, "APYEXM_REALM_SE", apyexmRealmSe);
	this.ds_cond_print.setColumn(0, "NM_KOREAN"      , encodeURIComponent(nm));
	
	// 체크박스 초기화
	this.div_print.chk_print1.set_value("1");	// 응시원서
	this.div_print.chk_print2.set_value("1");	// 입사지원서
	this.div_print.chk_print3.set_value("1");	// 자기소개서
	this.div_print.set_visible(true);
}


/*
 * 함수명  	: div_print_btn_print_onclick
 * 설명 		: 인쇄기능
 * params  		: obj, e
 * return Type 	: None
