/*
 * 함수명  	: fn_popCallBack
 * 설명 		: 팝업 콜백 처리
 * params  		: sPopupId, sReturn
 * return Type 	: None
 * 사용법		: this.fn_popCallBack(sPopupId, sReturn)
 */
this.fn_popCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "popRealm") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
        if(loadCnt) {
			var sn			  = dsTemp.getColumn(0, "SN");  			// 분야순번
			var cmpetSe       = dsTemp.getColumn(0, "CMPET_SE");		// 경쟁구분
			var empmnSe       = dsTemp.getColumn(0, "EMPMN_SE");		// 채용구분
			var apyexmClsfSe  = dsTemp.getColumn(0, "APYEXM_CLSF_SE");	// 응시직급
			var apyexmRealmSe = dsTemp.getColumn(0, "APYEXM_REALM_SE");	// 응시분야
			
			var cRow = this.ds_ecnySportGnrlList.rowposition;
			this.ds_ecnySportGnrlList.setColumn(cRow, "RCRIT_REALM_SN" , sn);
			this.ds_ecnySportGnrlList.setColumn(cRow, "CMPET_SE" , cmpetSe);
			this.ds_ecnySportGnrlList.setColumn(cRow, "EMPMN_SE" , empmnSe);
			this.ds_ecnySportGnrlList.setColumn(cRow, "APYEXM_CLSF_SE"  , apyexmClsfSe);
			this.ds_ecnySportGnrlList.setColumn(cRow, "APYEXM_REALM_SE" , apyexmRealmSe);
        }
	}
}


