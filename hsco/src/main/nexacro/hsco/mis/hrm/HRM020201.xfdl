			var nmEng          = this.ds_excelUpload.getColumn(fRow, "Column7");	// 성명영문
			var sexdstnSe      = this.ds_excelUpload.getColumn(fRow, "Column8");	// 성별구분
			var brthdy         = this.ds_excelUpload.getColumn(fRow, "Column9");	// 생년월일
			var nowZip         = this.ds_excelUpload.getColumn(fRow, "Column10");	// 현재우편번호
