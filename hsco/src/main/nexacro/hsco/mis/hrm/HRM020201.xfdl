	this.gfn_ReportPopup(this, "MIS/HRM/HRM020201_R04.crf", this.ds_cond_print);
	this.div_print.set_visible(false);
}



/*
 * 함수명  	: div_detail_btn_pwdChange_onclick
 * 설명 		: 비번 변경
 * params  		: obj, e
 * return Type 	: None
 * 사용법		: this.div_detail_btn_pwdChange_onclick(obj:Button,  e:nexacro.ClickEventInfo)
 */
this.div_detail_btn_pwdChange_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow      = this.ds_ecnySportGnrlList.rowposition;
	var empmnYear = this.ds_ecnySportGnrlList.getColumn(nRow, "EMPMN_YEAR");
	var odrSe     = this.ds_ecnySportGnrlList.getColumn(nRow, "ODR_SE");
	var apyexmNo  = this.ds_ecnySportGnrlList.getColumn(nRow, "APYEXM_NO");
	var pwd       = this.ds_ecnySportGnrlList.getColumn(nRow, "PWD");
	var email     = this.ds_ecnySportGnrlList.getColumn(nRow, "CTTPC_EMAIL");

	var pwdReg = new RegExp("^((?=.*?[A-Z])|(?=.*?[a-z]))(?=.*?[#?!@$%^&*)~(+-]).{8,}$", "g"); 
	var pwdMatch = pwdReg.test(pwd);

	if(!pwdMatch) {
		this.gfn_message("info.처리불가","비밀번호는 영문자, 특수문자 포함 8자리로 입력해주십시오.");
		return false;
	}

	this.ds_pwd.clearData();
	this.ds_pwd.addRow();
	this.ds_pwd.setColumn(0, "EMPMN_YEAR", empmnYear);
	this.ds_pwd.setColumn(0, "ODR_SE", odrSe);
	this.ds_pwd.setColumn(0, "APYEXM_NO", apyexmNo);
	this.ds_pwd.setColumn(0, "PWD", pwd);
	this.ds_pwd.setColumn(0, "CTTPC_EMAIL", email);

	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_pwd, this.ds_validation)) {
		return;
	}
		
	if(application.confirm("해당 비밀번호로 변경 하시겠습니까?")) {
		this.fn_transaction("savePwd");
	}
}

/*
 * 함수명  	: div_detail_chk_properAt_onchanged
 * 설명 		: 적격여부 변경 시 발생
 * params  		: obj, e
 * return Type 	: None
 * 사용법		: this.div_detail_chk_properAt_onchanged(obj:CheckBox, e:nexacro.CheckBoxChangeEventInfo)
 */
this.div_detail_chk_properAt_onchanged = function(obj:CheckBox, e:nexacro.CheckBoxChangeEventInfo)
{
	var nRow = this.ds_ecnySportGnrlList.rowposition;
	var empmnYear = this.ds_ecnySportGnrlList.getColumn(nRow, "EMPMN_YEAR");
	var odrSe = this.ds_ecnySportGnrlList.getColumn(nRow, "ODR_SE");
	var apyexmNo = this.ds_ecnySportGnrlList.getColumn(nRow, "APYEXM_NO");
	var properAt = e.postvalue;

	this.ds_properAt.clearData();
	this.ds_properAt.addRow();
	this.ds_properAt.setColumn(0, "EMPMN_YEAR"   , empmnYear);
	this.ds_properAt.setColumn(0, "ODR_SE"       , odrSe);
	this.ds_properAt.setColumn(0, "APYEXM_NO" , apyexmNo);
	this.ds_properAt.setColumn(0, "PROPER_AT"    , properAt);

	var properStr = "";
	
	if(properAt == "0") {
		properStr = "부적격";
	}else {
		properStr = "적격";
	}
		
	if(!application.confirm("적격여부를 "+properStr+"으로 변경하시겠습니까?")) {
		return false;
	}

	// 적격여부 변경
	this.fn_transaction("saveProperAt");
}


