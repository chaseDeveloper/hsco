    if(nRowCnt){
		if(sPopupId == "gnrlPopup"){
			var nRow = this.ds_wrtngScrpro.rowposition;
            this.ds_wrtngScrpro.setColumn(nRow, "APYEXM_NO", 		 dsRtn.getColumn(0, "APYEXM_NO"));
            this.ds_wrtngScrpro.setColumn(nRow, "NM_KOREAN",  		 dsRtn.getColumn(0, "NM_KOREAN"));
            this.ds_wrtngScrpro.setColumn(nRow, "BRTHDY",			 dsRtn.getColumn(0, "BRTHDY"));
            this.ds_wrtngScrpro.setColumn(nRow, "CMPET_SE", 		 dsRtn.getColumn(0, "CMPET_SE"));
            this.ds_wrtngScrpro.setColumn(nRow, "EMPMN_SE",  		 dsRtn.getColumn(0, "EMPMN_SE"));
            this.ds_wrtngScrpro.setColumn(nRow, "APYEXM_CLSF_SE",	 dsRtn.getColumn(0, "APYEXM_CLSF_SE"));
            this.ds_wrtngScrpro.setColumn(nRow, "APYEXM_REALM_SE",	 dsRtn.getColumn(0, "APYEXM_REALM_SE"));
            this.ds_wrtngScrpro.setColumn(nRow, "RCRIT_REALM_SN" ,	 dsRtn.getColumn(0, "SN"));
		}
	}	
}

