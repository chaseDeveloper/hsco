 /*
 * 함수명  	: btn_excelUpload_onclick
 * 설명 		: 엑셀파일 업로드
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_excelUpload_onclick();
 */
this.btn_excelUpload_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var year = this.ds_cond.getColumn(0, "EMPMN_YEAR");
	var odr  = this.ds_cond.getColumn(0, "ODR_SE");
		
	if(this.ds_wrtngScrpro.rowcount > 0) {
		if(!this.gfn_message("comm.행위여부", year+"년도 "+odr+"차수의 총점과 결과 데이터를 모두 초기화 후 엑셀 업로드를 진행합니다.\n계속")) {
			return false;
		}
	}
	
	var arg = {
		targetDs : this.ds_import,
		headSet : false,
		callBack : function () {
									
			if(this.ds_import.getRowCount() > 1){
				this.ds_wrtngScrproExcel.clearData();
				for(var fRow = 5; fRow < this.ds_import.getRowCount(); fRow++) {
					
					var apyexmNo = this.ds_import.getColumn(fRow, "Column1");
					// 중복 응시번호 걸러내기
					if(this.ds_wrtngScrproExcel.findRowExpr("APYEXM_NO = '"+apyexmNo+"'") == -1) {
				
						var nRow = this.ds_wrtngScrproExcel.addRow();
						
						// 가산
						var addi = this.ds_import.getColumn(fRow, "Column17");
						if(!this.comUtils.isNull(addi)) {
							 addi = nexacro.toNumber(addi) * 100;
