 * 설명 		: 필기전형관리 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_wrtngScrpro.clearData();
	
	// validation 체크(검색조건)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.fn_transaction("selectWrtngScrproList");
}


/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 검색조건 데이터셋 컬럼 변경시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged();
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "EMPMN_YEAR") {
		this.fn_transaction("selectOdrSeCombo");
	}else if(e.columnid == "ODR_SE") {
		this.fn_search();
	}
}

 /*
 * 함수명  	: fn_insert
 * 설명 		: 필기전형관리 신규이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_insert = function()
{
	var year = this.ds_cond.getColumn(0, "EMPMN_YEAR");
	var odr  = this.ds_cond.getColumn(0, "ODR_SE");
	var nRow = this.ds_wrtngScrpro.addRow();
	
	this.ds_wrtngScrpro.setColumn(nRow, "EMPMN_YEAR", year);
	this.ds_wrtngScrpro.setColumn(nRow, "ODR_SE",  odr);      
}

 /*
 * 함수명  	: fn_delete
 * 설명 		: 필기전형관리 삭제이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_delete = function()
{
	if(this.ds_wrtngScrpro.rowposition>-1){
		this.ds_wrtngScrpro.deleteRow(this.ds_wrtngScrpro.rowposition);
	}
}

 /*
 * 함수명  	: fn_save
 * 설명 		: 필기전형관리 저장 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	 // 데이터셋 미변경 체크
    if (!this.comUtils.isDatasetUpdated(this.ds_wrtngScrpro)) {
        this.gfn_message("comm.데이터.변경.없음")
        return false;
    }

	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_wrtngScrpro, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_wrtngScrpro.rowposition;
		this.fn_transaction("saveWrtngScrproCU");
	}
}

/*
 * 함수명        : fn_cancel
 * 설명          : 취소 이벤트 처리
 * params        : None
 * return Type    : None
 * 사용법        : this.fn_cancel ();
 */
this.fn_cancel= function()
{
     // 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_wrtngScrpro)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }

    if (this.gfn_message("confirm.취소여부")) {
        this.ds_wrtngScrpro.reset();
        this.ds_wrtngScrpro.applyChange();
    }
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// ========= 검색조건 =========
		case "selectEmpmnYearCombo": 		// 채용년도 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/empmnYearComboList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_empmnYearList=output1";
			break;
		case "selectOdrSeCombo": 			// 차수, 채용공고 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/odrSeComboList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_odrSeList=output1";
			break;	
		// =============================
		case "selectWrtngScrproList": 		// 대상자목록 불러오기
			var sController   	= "hsco/mis/hrm/HRM020311/wrtngScrproList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_wrtngScrpro=output1";
			break;
		case "saveWrtngScrproCU": 			// 대상자 저장
			var sController   	= "hsco/mis/hrm/HRM020311/wrtngScrproCUD.do";
			var sInDatasets   	= "input1=ds_wrtngScrpro:U";
			var sOutDatasets  	= "";
			break;
		case "excelUpload":
			var sController   	= "hsco/mis/hrm/HRM020311/excelUpload.do";
			var sInDatasets   	= "input1=ds_cond input2=ds_wrtngScrproExcel:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else if (nErrorCode == 0){
		switch (strSvcId) {
				case "selectEmpmnYearCombo": 	// 채용년도 조회(검색조건)
				this.div_search.cmb_empmnYear.set_index(0);
				this.fn_transaction("selectOdrSeCombo");
				break;
			case "selectOdrSeCombo": 			// 차수, 채용공고 조회(검색조건)
				this.div_search.cmb_odrSe.set_index(0);
				this.fn_search();
				break;	
			case "selectTbhrmEtcScrproList": 	// 대상자목록
				break;
			case "saveWrtngScrproCU": 			// 인적성검사내역 저장하기
				this.fn_search();
				break;	
			case "excelUpload":
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}

/*
 * 함수명  	: fn_popupCallback
 * 설명 		: 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback5();
 */
