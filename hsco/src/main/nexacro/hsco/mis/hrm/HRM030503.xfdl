 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_crtfList.reset();		//DATASET ROLLBACK
		this.ds_crtfList.applyChange();	//DATASET COMMIT
	}
}

