			var fileName  = this.ds_fileList2.getColumn(this.ds_fileList2.rowposition, "ORGINL_FILE_NM");
			
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
	// 통지서 삭제
	if(e.cell == 16) {
		if(this.ds_fileList2.rowcount <= 0){
			this.gfn_message("info.파일정보.없음"); //삭제할 파일정보가 없습니다.
			return false;
		}
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		
		this.ds_fileList2.deleteAll();
		
		var strSvcId   	 = "deleteFile";
		var sController  = "hsco/cmm/file/deleteFile.do";
		var sInDatasets  = "input1=ds_fileList2:U";
		var sOutDatasets = "ds_fileList2=output1";
		
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
	}
}

/*
* 함수명		: div_work_grd_infoList_onheadclick
* 설명			: 헤드클릭 정렬, 체크박스 전체전택
* params		: None
* return Type	: None
* 사용법		: this.div_work_grd_infoList_onheadclick();
*/
this.div_work_grd_infoList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
	}
	this.gfn_gridSort(obj, e);
}

 /*
 * 함수명  	: ds_infoList_onrowposchanged
 * 설명 		: 행 변경시 첨부파일 조회 
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_infoList_onrowposchanged();
 */
this.ds_infoList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_fileList1.clearData();
	this.ds_fileList2.clearData();
	this.ds_cond_file1.clearData();
	this.ds_cond_file2.clearData();
	this.ds_cond_file1.addRow();
	this.ds_cond_file2.addRow();
	var nRow = this.ds_infoList.rowposition;
	if(this.ds_infoList.getColumn(nRow, "FILE_SN")){
		this.ds_cond_file1.setColumn(0, "FILE_SN", this.ds_infoList.getColumn(nRow, "FILE_SN"));
		this.ds_cond_file2.setColumn(0, "FILE_SN", this.ds_infoList.getColumn(nRow, "FILE_SN"));
		//cnt = cnt+1;
	}
	else if(!this.ds_infoList.getColumn(nRow, "FILE_SN")){
		this.ds_cond_file1.setColumn(0, "FILE_SN","");
		this.ds_cond_file2.setColumn(0, "FILE_SN","");
	}
	this.fn_transaction("selectFileList");  // 첨부파일 조회
}

