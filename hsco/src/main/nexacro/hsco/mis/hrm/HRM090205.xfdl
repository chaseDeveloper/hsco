			//this.gfn_message("err.파일.다운로드.정보없음");
			return false;
		}
		if(this.ds_infoList.getColumn(nRow, "FILE_SN")){
			this.ds_cond_file1.setColumn(0, "FILE_SN", this.ds_infoList.getColumn(nRow, "FILE_SN"));
