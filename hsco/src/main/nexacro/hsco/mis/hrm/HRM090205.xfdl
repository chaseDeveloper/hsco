this.div_work_grd_infoList_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.cell == 0) {
		obj.setCellProperty("Head", e.cell, "text", "0");
	}
	// 청구서 업로드
	if(e.cell == 8) {
		var nRow = this.ds_infoList.rowposition;
		var fileSN   = this.ds_infoList.getColumn(nRow,"FILE_SN");
		var reqstDe  = this.ds_infoList.getColumn(nRow,"REQST_DE");
		var rceptDe  = this.ds_infoList.getColumn(nRow,"RCEPT_DE");
		var deptCode = this.ds_infoList.getColumn(nRow,"CHRG_DEPT_CODE");
		this.ds_selectFileSn1.clearData();
		this.ds_selectFileSn1.addRow();
		this.ds_selectFileSn1.setColumn(0,"FILE_SN", fileSN);
		this.ds_selectFileSn1.setColumn(0,"REQST_DE", reqstDe);
		this.ds_selectFileSn1.setColumn(0,"RCEPT_DE", rceptDe);
		this.ds_selectFileSn1.setColumn(0,"CHRG_DEPT_CODE", deptCode);

		if(this.ds_infoList.getRowType(nRow)=='2'){
			this.gfn_message("info.처리불가","신규등록의 경우 저장 후 업로드가 가능합니다.");
			return false;
		}
		if(this.ds_fileList1.rowcount>0){
			this.gfn_message("info.파일.업로드.허용건수초과", "1");
			return false;
		}
		this.saveRow = nRow;
		// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
		this.fn_autoFileSn(
			 "MIS"											// 대분류   (SYS_SE)
			,"HRM"											// 업무구분 (JOB_SE)
			, this.File_UpDownManager	            		// fileManager 오브젝트
