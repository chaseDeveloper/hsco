			, ["RCEPT_NO", "RCEPT_DE", "CHRG_DEPT_CODE"]	// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
		);
	}
	// 청구서 다운로드
	if(e.cell == 9) {
		var nRow = this.ds_infoList.rowposition;
		var fileCnt = this.ds_infoList.getColumn(nRow, "FILE_CNT_1");
