this.fn_insert = function()
{
// 	this.ds_temp.clearData();
// 	this.fn_transaction("getRceptNo");  // 접수번호 채번

	var rceptNo  = this.ds_infoList.getMax("RCEPT_NO");
	var nRceptNo = nexacro.toNumber(rceptNo) + 1;
	
	var nRow = this.ds_infoList.addRow();
	//this.ds_infoList.setColumn(nRow, "RCEPT_NO", nRceptNo);
	
	if(!this.isAuth) {
		this.ds_infoList.setColumn(nRow, "CHRG_DEPT_CODE", this.gfn_getDeptId());
		this.ds_infoList.setColumn(nRow, "DEPT_NM", this.gfn_getDeptName());
	}
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	this.gfn_delMultiRow(this.ds_infoList);
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if(this.gfn_message("confirm.취소여부")) {
		this.ds_infoList.reset();		//DATASET ROLLBACK
		this.ds_infoList.applyChange();	//DATASET COMMIT
		return;
	}
}

