/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/


/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(정보공개처리내역)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(정보공개처리내역)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_infoList.clearData();
	this.fn_transaction("tbhrmInfoList");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_infoList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(정보공개처리)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_infoList, this.ds_validation)) {
		return;
	}
	
	for(var nRow = 0; nRow < this.ds_infoList.getRowCount(); nRow++) {
		var decsnSe = this.ds_infoList.getColumn(nRow, "DECSN_SE");
		if(decsnSe == '003') {
			if(!this.ds_infoList.getColumn(nRow, "CN_ND_PRVONSH")) {
				this.gfn_message("comm.항목.입력.을", "비공개(내용및사유)");
				return false;
			}
		}
		/*
		var rceptNo = this.ds_infoList.getColumn(nRow, "RCEPT_NO");
		if(7 != (rceptNo+"").replace(" ", "").length) {
			this.gfn_message("errors.항목값.길이.일치", "접수번호|7");
			return false;
		}
		*/
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_infoList.rowposition;
		this.fn_transaction("tbhrmInfoSave");
	}
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "tbhrmInfoList":	// 정보공개처리내역 조회
			var sController   	= "/hsco/mis/hrm/HRM090205/tbhrmInfoList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_infoList=output1";
			break;
		case "tbhrmInfoSave":	// 저장
			var sController   	= "/hsco/mis/hrm/HRM090205/tbhrmInfoSave.do";
			var sInDatasets   	= "input1=ds_infoList:U";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":	// 첨부파일 조회
			this.ds_fileList1.clearData();
			this.ds_fileList2.clearData();
			var sController   	= "hsco/mis/hrm/HRM020102/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond_file1 input2=ds_cond_file2";
			var sOutDatasets  	= "ds_fileList1=output1 ds_fileList2=output2";
			break;
		case "getRceptNo":		// 접수번호 채번
			var sController   	= "/hsco/mis/hrm/HRM090205/getRceptNo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_temp=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "tbhrmInfoList":	// 조회(정보공개처리내역)
				this.ds_infoList.set_rowposition(this.saveRow);
				
				this.ds_fileList1.clearData();
				this.ds_fileList2.clearData();
				var nRow = this.saveRow;
				//if(this.ds_infoList.getColumn(nRow, "FILE_SN")){
					this.ds_cond_file1.setColumn(nRow, "FILE_SN", this.ds_infoList.getColumn(nRow, "FILE_SN"));
					this.ds_cond_file2.setColumn(nRow, "FILE_SN", this.ds_infoList.getColumn(nRow, "FILE_SN"));
					this.fn_transaction("selectFileList");  // 첨부파일 조회
				//}
				break;
			case "selectFileList":
			
			
				var nRow = this.ds_infoList.rowposition;
				this.ds_fileList1.filter("FILE_ORDR%2 != 0");
				this.ds_fileList2.filter("FILE_ORDR%2 != 1");

				var fileCnt1 = this.ds_fileList1.getRowCount();
				var fileCnt2 = this.ds_fileList2.getRowCount();
				
				this.ds_infoList.set_enableevent(false);
				this.ds_infoList.set_updatecontrol(false);
				this.ds_infoList.setColumn(nRow, "FILE_CNT_1", fileCnt1);
				this.ds_infoList.setColumn(nRow, "FILE_CNT_2", fileCnt2);
				this.ds_infoList.set_updatecontrol(true);
				this.ds_infoList.set_enableevent(true);
				break;
			case "deleteFile":
				this.fn_transaction("selectFileList");
				break;
			case "tbhrmInfoSave":	// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "getRceptNo":		// 접수번호 채번
				var rceptNo = this.ds_temp.getColumn(0, "RCEPT_NO");
				var nRow = this.ds_infoList.addRow();
				this.ds_infoList.setColumn(nRow, "RCEPT_NO", rceptNo);
				break;
		}
	}
}

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{

	if(sPopupId == "deptPop") {
		var dsDeptCode = new Dataset();
		loadCnt = dsDeptCode.loadXML(sReturn);
		if(loadCnt) {
			var ilRow = this.ds_infoList.rowposition;
			var deptCode = dsDeptCode.getColumn(0, "DEPT_CODE");
			var deptNm   = dsDeptCode.getColumn(0, "DEPT_NM");
			
			this.ds_infoList.setColumn(ilRow, "CHRG_DEPT_CODE", deptCode);
			this.ds_infoList.setColumn(ilRow, "DEPT_NM"       , deptNm);
		}
	}
}

