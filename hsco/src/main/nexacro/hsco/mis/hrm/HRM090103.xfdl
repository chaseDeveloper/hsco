/*
 * 함수명  	: grd_biList_onexpandup
 * 설명 		: 그리드 버튼 클릭 후 이벤트 처리(Popup)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_biList_onexpandup(obj,e);
 */
this.grd_biList_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(e.cell == '9') {
		// 제안부서
		var parm = {
			 arg_0      : this
			,deptCode   : ''
			,deptNm     : ''
			,onlySelectLast : true
		};
		this.gfn_popup("deptPop", 400, 400, "부서선택", parm, "com::deptCode_P01.xfdl", "fn_popupCallBack", true);
	}
}


