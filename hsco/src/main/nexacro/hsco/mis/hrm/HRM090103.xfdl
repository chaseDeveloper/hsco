***********************************************************************
*
***********************************************************************
*/



/**********************************************************************
* Script Include 
***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";


/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.saveRow = -1;
this.File_UpDownManager = null;

/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.HRM090103_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// form 초기화
	this.fn_init_form();

	// dataset 초기화
	this.fn_init_dataset();
}

this.HRM090103_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// ..
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 초기 검색조건
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
 	this.ds_cond.setColumn(nRow, "MTG_YEAR", this.comUtils.getClientDate("YYYY"));	// 당해년도
	
	// 공통코드
	var comboParams = [
	    ["ds_hrm075" , "HRM075", "Y", "A", "B"]	// ds_hrm075  : 안건구분(검색조건)
	   ,["ds_hrm075G", "HRM075", "Y", "D", "B"]	// ds_hrm075G : 안건구분(상세정보)
	   ,["ds_hrm076" , "HRM076", "Y", "A", "B"]	// ds_hrm076  : 보고안건(검색조건)
	   ,["ds_hrm076G", "HRM076", "Y", "D", "B"]	// ds_hrm076G : 보고안건(상세정보)
	];
	var callback = function (svcId, errorCode, errorMsg) {		
		this.grd_biList.setFormatColProperty(1, "size",0);
		
		// 최초조회
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "HRM");
	this.File_UpDownManager.init(fileConfig);
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(의안목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// clear Datasets
	this.ds_biList.clearData();
	this.ds_mtgAtndncNmst.clearData();
	this.ds_fileList.clearData();
	
	// transaction
	this.fn_transaction("selectBiList");
}


/*
 * 함수명  	: fn_searchSub
 * 설명 		: 조회 이벤트 처리(회의참석명부, 첨부파일)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_searchSub();
 */
this.fn_searchSub = function()
{
	// clear Datasets
	this.ds_mtgAtndncNmst.clearData();
	this.ds_fileList.clearData();
	
	// transaction
	this.fn_transaction("selectMtgAtndncNmst");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(의안, 회의참석명부)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_biList) && !this.comUtils.isDatasetUpdated(this.ds_mtgAtndncNmst)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(의안)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_biList, this.ds_validation)) {
		return;
	}
	// validation 체크(회의참석명부)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_mtgAtndncNmst, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		this.saveRow = this.ds_biList.rowposition;
		this.fn_transaction("save");
	}
}


/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBiList":			// 의안목록 조회
			var sController   	= "/hsco/mis/hrm/HRM090103/selectBiList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_biList=output1";
			break;
		case "selectMtgAtndncNmst":		// 회의참석명부, 첨부파일 조회
			var sController   	= "/hsco/mis/hrm/HRM090103/selectMtgAtndncNmst.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_mtgAtndncNmst=output1 ds_fileList=output2";
			break;
		case "save":					// 저장
			var sController   	= "/hsco/mis/hrm/HRM090103/save.do";
			var sInDatasets   	= "input1=ds_biList:U input2=ds_mtgAtndncNmst:U";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":			// 첨부파일 조회
			var sController   	= "hsco/mis/hrm/HRM090103/selectFileList.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_fileList=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0'){
		switch (strSvcId) {
			case "selectBiList":			// 의안목록 조회
				if(this.saveRow > -1){
					this.ds_biList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "selectMtgAtndncNmst":		// 회의참석명부, 첨부파일 조회
				this.ds_fileList.filter("USE_AT == '1'");
				break;
			case "save":					// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectFileList":			// 첨부파일 조회
				this.ds_fileList.filter("USE_AT == '1'");
				break;
		}
	}
}



/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 제안부서
	if(sPopupId == "deptPop") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
 		if(loadCnt) {
			var deptCode = dsTemp.getColumn(0, "DEPT_CODE");
 			var deptNm   = dsTemp.getColumn(0, "DEPT_NM");
 			var nRow     = this.ds_biList.rowposition;
 			
			this.ds_biList.setColumn(nRow, "PROPSE_DEPT_CODE", deptCode);
			this.ds_biList.setColumn(nRow, "PROPSE_DEPT_NM"  , deptNm);
 		}
	}
}



/**********************************************************************
* Transaction 영역 끝
***********************************************************************/



/**********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
***********************************************************************
*
* 1. Dataset 관련 이벤트 처리
* 2. Button 관련 이벤트 처리
* 3. Grid 관련 이벤트 처리
* 4. 공통함수 처리
* 5. Object 관련 이벤트 처리
* 6. 첨부파일버튼 관련 이벤트 처리
* 
***********************************************************************/



/**********************************************************************
* Dataset 관련 이벤트 처리
***********************************************************************/


/*
 * 함수명  	: ds_biList_onrowposchanged
 * 설명 		: Dataset 행 변경 후 이벤트 처리(의안목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_biList_onrowposchanged(obj,e);
 */
this.ds_biList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{	
	// 서브 조회(회의참석명부, 첨부파일)
	if(e.newrow > -1) {
		this.ds_condSub.clearData();
		var nRow = this.ds_condSub.addRow();
		var mtgYear = obj.getColumn(e.newrow, "MTG_YEAR");
		var sn      = obj.getColumn(e.newrow, "SN");
		var fileSn  = obj.getColumn(e.newrow, "FILE_SN");
		
		this.ds_condSub.setColumn(nRow, "MTG_YEAR", mtgYear);
		this.ds_condSub.setColumn(nRow, "SN"      , sn);
		this.ds_condSub.setColumn(nRow, "FILE_SN" , fileSn);
		
		this.fn_searchSub();
	}
}


/**********************************************************************
* Button 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: btn_AddBi_onclick
 * 설명 		: '행추가' 버튼 클릭 후 이벤트 처리(의안목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_AddBi_onclick(obj,e);
 */
this.btn_AddBi_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow    = this.ds_biList.addRow();
	var mtrSe   = this.ds_hrm075G.getColumn(0, "CODE");
	var mtgYear    = this.ds_cond.getColumn(0, "MTG_YEAR");
	
	this.ds_biList.setColumn(nRow, "MTG_YEAR", mtgYear);
	this.ds_biList.setColumn(nRow, "MTR_SE"  , mtrSe);
	this.ds_biList.setColumn(nRow, "CONFM_SE", '0');
	this.ds_biList.setColumn(nRow, "CMNL_DLBRT_CMIT_AT", '0');
}


/*
 * 함수명  	: btn_DeleteBi_onclick
 * 설명 		: '행삭제' 버튼 클릭 후 이벤트 처리(의안목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_DeleteBi_onclick(obj,e);
 */
this.btn_DeleteBi_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow    = this.ds_biList.rowposition;
	var confmSe = this.ds_biList.getColumn(nRow, "CONFM_SE");
	
	if(confmSe != '0') {
		this.gfn_message("info.처리불가", "승인구분이 대기인 건만 수정할 수 있습니다.");
		return false;
	}
	
	if(!this.gfn_message("confirm.삭제여부")) {
		return false;	// 아니오
	}
	this.ds_biList.deleteRow(nRow);
}


/*
 * 함수명  	: btn_CancelBi_onclick
 * 설명 		: '행취소' 버튼 클릭 후 이벤트 처리(의안목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_CancelBi_onclick(obj,e);
 */
this.btn_CancelBi_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_biList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	this.ds_biList.reset();
	this.ds_biList.applyChange();
}




/*
 * 함수명  	: btn_AddNmst_onclick
 * 설명 		: '행추가' 버튼 클릭 후 이벤트 처리(회의참석명부)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_AddNmst_onclick(obj,e);
 */
this.btn_AddNmst_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow        = this.ds_biList.rowposition;
	var confmSe     = this.ds_biList.getColumn(pRow, "CONFM_SE");
	var nowAsembyCo = this.ds_biList.getColumn(pRow, "NOW_ASEMBY_CO");
	var nmstCnt     = this.ds_mtgAtndncNmst.rowcount;
	
	if(pRow < 0) {
		this.gfn_message("info.처리불가", "의안이 존재하지 않습니다.");
		return false;
	}
	if(confmSe != '0') {
		this.gfn_message("info.처리불가", "승인구분이 대기인 건만 수정할 수 있습니다.");
		return false;
	}
	if(this.comUtils.isNull(nowAsembyCo)) {
		this.gfn_message("info.처리불가", "의안의 의원현원을 입력해 주십시오.");
		return false;
	}
	if(nowAsembyCo == nmstCnt) {
		this.gfn_message("info.처리불가", "회의인원 수는 의원현원 수보다 작거나 같아야 합니다.");
		return false;
	}
	
	var nRow    = this.ds_mtgAtndncNmst.addRow();
	var mtgYear = this.ds_biList.getColumn(pRow, "MTG_YEAR");
	var sn      = this.ds_biList.getColumn(pRow, "SN");
	
	this.ds_mtgAtndncNmst.setColumn(nRow, "MTG_YEAR", mtgYear);
	this.ds_mtgAtndncNmst.setColumn(nRow, "SN"      , sn);
	this.ds_mtgAtndncNmst.setColumn(nRow, "CONFM_SE", '0');
}


/*
 * 함수명  	: btn_DeleteNmst_onclick
 * 설명 		: '행삭제' 버튼 클릭 후 이벤트 처리(회의참석명부)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_DeleteNmst_onclick(obj,e);
 */
this.btn_DeleteNmst_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow    = this.ds_biList.rowposition;
	var confmSe = this.ds_biList.getColumn(pRow, "CONFM_SE");
	
	if(confmSe != '0') {
		this.gfn_message("info.처리불가", "승인구분이 대기인 건만 수정할 수 있습니다.");
		return false;
	}
	if(!this.gfn_message("confirm.삭제여부")) {
		return false;	// 아니오
	}
	
	this.ds_mtgAtndncNmst.deleteRow(this.ds_mtgAtndncNmst.rowposition);
}


/*
 * 함수명  	: btn_CancelNmst_onclick
 * 설명 		: '행취소' 버튼 클릭 후 이벤트 처리(회의참석명부)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_CancelNmst_onclick(obj,e);
 */
this.btn_CancelNmst_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_mtgAtndncNmst)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	this.ds_mtgAtndncNmst.reset();
	this.ds_mtgAtndncNmst.applyChange();
}



