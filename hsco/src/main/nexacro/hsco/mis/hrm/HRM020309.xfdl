this.fn_cancel = function()
{
    // 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_papersScrpro)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }

    if (this.gfn_message("confirm.취소여부")) {
        this.ds_papersScrpro.reset();
        this.ds_papersScrpro.applyChange();
    }
}


 /*
 * 함수명  	: fn_search
 * 설명 		: 서류전형관리 조회 이벤트 처리            
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_papersScrpro.clearData();

	// validation 체크(검색조건)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.fn_transaction("selectEmpmnRcritRealmList");

}


 /*
 * 함수명  	: fn_save
 * 설명 		: 서류전형관리 저장 이벤트 처리             
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 미변경 체크
    if (!this.comUtils.isDatasetUpdated(this.ds_papersScrpro)) {
        this.gfn_message("comm.데이터.변경.없음")
        return false;
    }

    // validation 체크
    if(!this.gfn_checkValidation_NoUpdChk(this.ds_papersScrpro, this.ds_validation)) {
        return;
    }

    if(this.gfn_message("confirm.저장.여부")) {
        this.saveRow = this.ds_empmnRcritRealmList.rowposition;
        this.fn_transaction("save");
    }
}


/***********************************************************************
* 데이터셋
************************************************************************/


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// ========= 검색조건 =========
		case "selectEmpmnYearCombo": 		// 채용년도 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/empmnYearComboList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_empmnYearList=output1";
			break;
		case "selectOdrSeCombo":			// 차수, 채용공고 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/odrSeComboList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_odrSeList=output1";
			break;	
		// =============================
		case "selectEmpmnRcritRealmList": 	// 모집분야 목록 조회
			var sController   	= "hsco/mis/hrm/HRM020309/selectEmpmnRcritRealmList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_empmnRcritRealmList=output1";
			break;		
		case "select":						// 지원자현황 불러오기
			var sController   	= "hsco/mis/hrm/HRM020309/selectPapersScrproList.do";
			var sInDatasets   	= "input1=ds_cond2";
			var sOutDatasets  	= "ds_papersScrpro=output1";
			break;		
		case "save":						// 서류전형내역 저장하기
			var sController   	= "hsco/mis/hrm/HRM020309/papersScrproListCU.do";
			var sInDatasets   	= "input1=ds_papersScrpro:U";
			var sOutDatasets  	= "";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectEmpmnYearCombo": 		// 채용년도 조회(검색조건)
				this.div_search.cmb_empmnYear.set_index(0);
				break;
			case "selectOdrSeCombo": 			// 차수, 채용공고 조회(검색조건)
				this.div_search.cmb_odrSe.set_index(0);
				break;		
			case "selectEmpmnRcritRealmList": 	// 모집분야 목록 조회
				break;
			case "select": 						// 대상자목록 불러오기			
				if(this.saveRow > -1) {
					this.ds_empmnRcritRealmList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "save":						//서류전형내역 저장하기
				this.fn_search();
				break;	
		}
	}
}

