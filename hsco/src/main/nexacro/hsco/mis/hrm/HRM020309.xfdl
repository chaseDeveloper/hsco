/***********************************************************************
 * dataset 관련 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: ds_empmnRcritRealmList_onrowposchanged
 * 설명 		: ds_empmnRcritRealmList 행 변경 후 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_empmnRcritRealmList_onrowposchanged();
 */
this.ds_empmnRcritRealmList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1){		
		// 모집분야
		var nRow 		  = this.ds_empmnRcritRealmList.rowposition;
		var empmnYear 	  = this.ds_empmnRcritRealmList.getColumn(nRow, "EMPMN_YEAR");
		var odrSe 		  = this.ds_empmnRcritRealmList.getColumn(nRow, "ODR_SE");
		var rcritRealmSn  = this.ds_empmnRcritRealmList.getColumn(nRow, "SN");
		
		var post_apyexmClsfSe  = this.ds_empmnRcritRealmList.getColumn(e.oldrow, "APYEXM_CLSF_SE");		// 이전행 응시직급
		var post_apyexmRealmSe = this.ds_empmnRcritRealmList.getColumn(e.oldrow, "APYEXM_REALM_SE");	// 이전행 응시분야
		
		var apyexmClsfSe  = this.ds_empmnRcritRealmList.getColumn(nRow, "APYEXM_CLSF_SE");		// 응시직급
		var apyexmRealmSe = this.ds_empmnRcritRealmList.getColumn(nRow, "APYEXM_REALM_SE");		// 응시분야
		
		// 동적 그리드
		this.fn_setGrid(apyexmClsfSe, apyexmRealmSe);

		this.ds_cond2.clearData();
		this.ds_cond2.addRow();
		this.ds_cond2.setColumn(0, "EMPMN_YEAR"	  , empmnYear);
		this.ds_cond2.setColumn(0, "ODR_SE"		  , odrSe);
		this.ds_cond2.setColumn(0, "RCRIT_REALM_SN", rcritRealmSn);
		
		this.fn_transaction("select");
	}
}

/***********************************************************************
 * 사용자 정의 함수
 ***********************************************************************/
/*
 * 함수명  	: fn_setDiv
 * 설명 		: 응시직급, 응시분야에 따라 디테일 그리드(일반직, 시설관리원(주차관리원), 전임강사, 사무보조(안내데스크) 달라짐
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_setDiv(obj,e);
 */
this.fn_setGrid = function (apyexmClsfSe, apyexmRealmSe) {
		
	// 응시직급, 응시분야에 따라 디테일 그리드(일반직, 시설관리원(주차관리원), 전임강사, 사무보조(안내데스크) 달라짐
		
	this.grd_papersScrpro.set_enableredraw(false);
	
	// 그리드 초기화(기존컬럼 남기고 삭제)
	for(var i=this.grd_papersScrpro.getCellCount("Head"); i> 9; i--) {
		this.grd_papersScrpro.deleteContentsCol(i);
	}
		
	// 전임강사 or 체육강사
	if(apyexmClsfSe == "601" || apyexmClsfSe == "602") {
		
		// 관련분야지도경력
		var nCol = this.grd_papersScrpro.appendContentsCol("body", true);
		var nCol = this.grd_papersScrpro.appendContentsCol("body", true);
		this.grd_papersScrpro.mergeContentsCell("Head", 0, nCol-1, 0, nCol, nCol-1, false);

		this.grd_papersScrpro.setCellProperty("Head", nCol-1, "text", "관련분야지도경력");
		this.grd_papersScrpro.setCellProperty("Head", nCol, "text", "공공");
		this.grd_papersScrpro.setCellProperty("Head", nCol+1, "text", "민간");
		this.grd_papersScrpro.setCellProperty("Body", nCol-1  , "text", "bind:LTRS_SCORE");
		this.grd_papersScrpro.setCellProperty("Body", nCol , "text", "bind:SMLPZ_SCORE");
		this.grd_papersScrpro.setFormatColProperty(nCol-1, "size", 100);
		this.grd_papersScrpro.setFormatColProperty(nCol, "size", 100);
		this.grd_papersScrpro.setCellProperty("Summ", nCol-1, "text" , "expr:nexacro.round(dataset.getAvg('LTRS_SCORE'), 2)");
		this.grd_papersScrpro.setCellProperty("Summ", nCol  , "text" , "expr:nexacro.round(dataset.getAvg('SMLPZ_SCORE'), 2)");

		// 관련분야 자격증(1)
		var nCol = this.grd_papersScrpro.appendContentsCol("body", true);
		this.grd_papersScrpro.mergeContentsCell("Head", 0, nCol, 1, nCol, nCol+2, false);

		this.grd_papersScrpro.setCellProperty("Head", nCol-1, "text", "관련분야\n자격증(1)");
		this.grd_papersScrpro.setCellProperty("Body", nCol, "text", "bind:TCHNLGY_CRQFC_SCORE");
		this.grd_papersScrpro.setFormatColProperty(nCol, "size", 100);
		this.grd_papersScrpro.setCellProperty("Summ", nCol, "text" , "expr:nexacro.round(dataset.getAvg('TCHNLGY_CRQFC_SCORE'), 2)");

		// 관련분야 자격증(2)
		var nCol = this.grd_papersScrpro.appendContentsCol("body", true);
		this.grd_papersScrpro.mergeContentsCell("Head", 0, nCol, 1, nCol, nCol+2, false);
		this.grd_papersScrpro.setCellProperty("Head", nCol-1, "text", "관련분야\n자격증(2)");
		this.grd_papersScrpro.setCellProperty("Body", nCol, "text", "bind:ADMINIST_CRQFC_SCORE");
		this.grd_papersScrpro.setFormatColProperty(nCol, "size", 100);
		this.grd_papersScrpro.setCellProperty("Summ", nCol, "text" , "expr:nexacro.round(dataset.getAvg('ADMINIST_CRQFC_SCORE'), 2)");
		
		// 대회수상 경력
		var nCol = this.grd_papersScrpro.appendContentsCol("body", true);
		this.grd_papersScrpro.mergeContentsCell("Head", 0, nCol, 1, nCol, nCol+2, false);
		this.grd_papersScrpro.setCellProperty("Head", nCol-1, "text", "대회수상\n경력");
		this.grd_papersScrpro.setCellProperty("Body", nCol, "text", "bind:CNFRNC_WNPZ_CAREER");
		this.grd_papersScrpro.setFormatColProperty(nCol, "size", 100);
		this.grd_papersScrpro.setCellProperty("Summ", nCol, "text" , "expr:nexacro.round(dataset.getAvg('CNFRNC_WNPZ_CAREER'), 2)");
		
		// 포상
		var nCol = this.grd_papersScrpro.appendContentsCol("body", true);
