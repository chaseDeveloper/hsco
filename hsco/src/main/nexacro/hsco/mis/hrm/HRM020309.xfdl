this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "EMPMN_YEAR") {
		this.fn_transaction("selectOdrSeCombo");
	}else if(e.columnid == "ODR_SE") {
		this.fn_search();
	}
}


this.ds_empmnRcritRealmList_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	// 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_papersScrpro)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }
    
    this.ds_papersScrpro.reset();
    this.ds_papersScrpro.applyChange();
}

