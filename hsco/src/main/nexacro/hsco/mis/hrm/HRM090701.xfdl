/*
* 함수명		: div_work_grd_srvcList_onheadclick
* 설명			: 헤드클릭 정렬, 체크박스 전체전택
* params		: None
* return Type	: None
* 사용법		: this.div_work_grd_srvcList_onheadclick();
*/
this.div_work_grd_srvcList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
		
		var _cnt = 0;
		for(var _row = 0;_row < this.ds_srvcList.rowcount;_row++) {
			var s_SELF_CNFIRM_AT = this.ds_srvcList.getColumn(_row, "CONFM_AT");
			var s_CHK = this.ds_srvcList.getColumn(_row, "CHK");
			
			if(s_SELF_CNFIRM_AT == "1" && s_CHK == "1") {
				_cnt++;
				this.ds_srvcList.setColumn(_row, "CHK", "0");
			}
		}
		
		if(_cnt>0) alert("확인완료건은 선택할 수 없습니다.");
	} else if(e.col == 2) {
		if(!this.isAuth) return;
		this.gfn_setGridCheckAll(obj, e);
		for(var _row = 0;_row < this.ds_srvcList.rowcount;_row++) {
			var deptCode = "";
			//var deptNm   = "";
			var empno    = "";
			var empnm    = "";
			var de       = "";
			
			var selfConfirmAt = this.ds_srvcList.getColumn(_row, "CONFM_AT")
			var selfConfirmAtOrg = this.ds_srvcList.getOrgColumn(_row, "CONFM_AT")
			if(selfConfirmAt == selfConfirmAtOrg) {
				//deptCode = this.ds_srvcList.getOrgColumn(_row, "CNFRMR_DEPT_CODE");
				//deptNm   = this.ds_srvcList.getOrgColumn(_row, "CNFRMR_DEPT_NM");
				//empno    = this.ds_srvcList.getOrgColumn(_row, "CNFRMR_EMPNO");
				//empnm    = this.ds_srvcList.getOrgColumn(_row, "CNFRMR_EMPNM");
				//de       = this.ds_srvcList.getOrgColumn(_row, "CNFIRM_DE");
			} else if(selfConfirmAt == '1') {
				deptCode = this.gfn_getDeptId();
				//deptNm   = this.gfn_getDeptName();
				empno    = this.fn_getEmpno();
				empnm    = this.fn_getEmpnm();
				de       = this.gfn_today();
			}
			
			//this.ds_srvcList.setColumn(_row, "CNFRMR_DEPT_CODE", deptCode);
			//this.ds_srvcList.setColumn(_row, "CNFRMR_DEPT_NM"  , deptNm  );
			//this.ds_srvcList.setColumn(_row, "CNFRMR_EMPNO"    , empno   );
			//this.ds_srvcList.setColumn(_row, "CNFRMR_EMPNM"    , empnm   );
			//this.ds_srvcList.setColumn(_row, "CNFIRM_DE"       , de      );
		}
	}
	this.gfn_gridSort(obj, e);
}

