/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(자원봉사정보)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(자원봉사정보)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_srvcList.clearData();
	this.fn_transaction("tbhrmSrvcList");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_srvcList) && !this.comUtils.isDatasetUpdated(this.ds_prtcpntList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(자원봉사)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_srvcList, this.ds_validation)) {
		return;
	}
	
	// validation 체크(참가자)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_prtcpntList, this.ds_validation)) {
		return;
	}
	
	// 시간 체크
	var nRow = this.ds_srvcList.rowposition;
	var nRowType = this.ds_srvcList.getRowType(nRow);
	var beginTM = this.ds_srvcList.getColumn(nRow, "BEGIN_TM");
	var endTM   = this.ds_srvcList.getColumn(nRow, "END_TM");
	if(nRowType == 2 || nRowType == 4) {
		if(beginTM.indexOf(" ") > -1) {
			this.gfn_message("errors.항목.입력값.범위", "실제 시작시각|00:00|23:59");
			return false;
		}
		if(endTM.indexOf(" ") > -1) {
			this.gfn_message("errors.항목.입력값.범위", "실제 종료시각|00:00|23:59");
			return false;
		}
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_srvcList.rowposition;
		this.fn_transaction("tbhrmSrvcSave");
	}
}

/*
 * 함수명  	: btn_uploadFiles_onclick
 * 설명 		: '파일업로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_uploadFiles_onclick(obj,e);
 */
this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(!this.ds_srvcList.getColumn(this.ds_srvcList.rowposition, "SN")) {
		// this.gfn_message("errors.값확인.요청", "유효한 자원봉사가|자원봉사 내역을 다시");
		this.gfn_message("info.처리불가", "사회공헌활동 내역을 먼저 저장하세요.");
		return;
	} 
	
	if(this.ds_srvcList.rowposition < 0 ) {
		this.gfn_message("comm.데이터.선택.음");
		return false;
	}
	if(this.ds_fileList.rowcount>0){
		this.gfn_message("info.파일.업로드.허용건수초과", "1");
		return false;
	}
	// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
	this.fn_autoFileSn(
		 "MIS"									// 대분류   (SYS_SE)
		,"HRM"									// 업무구분 (JOB_SE)
		, this.File_UpDownManager	            // fileManager 오브젝트
		, this.ds_fileList					    // fileObject
		, this.ds_srvcList			        	// fileSn을 불러오거나 반영해야 하는 컬럼
		, "TBHRM_RESRCE_SRVC"					// fileSn을 저장할 테이블
		, "FILE_ID"								// fileSn 컬럼명이 다를 경우를 대비
		, ["SN"]						     	// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
	);
}

/*
 * 함수명  	: btn_downFiles_onclick
 * 설명 		: '파일다운로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
