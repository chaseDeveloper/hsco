 /*
 * 함수명  	: ds_srvcList_onrowposchanged
 * 설명 		: 행 변경시 참가자 목록 조회            
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_srvcList_onrowposchanged();
 */
this.ds_srvcList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var slRow = this.ds_srvcList.rowposition;

	var sn = this.ds_srvcList.getColumn(slRow, "SN");
	
	this.ds_prtcpntCond.clearData();
	this.ds_prtcpntCond.addRow();
	this.ds_prtcpntCond.setColumn(0, "SN", sn);
	
	this.ds_prtcpntList.clearData();
	this.fn_transaction("getPrtcpntSearch"); 
	this.fn_setReadonly(e.newrow);
}

