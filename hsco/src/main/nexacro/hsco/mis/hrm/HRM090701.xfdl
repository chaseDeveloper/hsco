 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var nRow = this.ds_srvcList.addRow();
	
	if(!this.isAuth) {
		this.ds_srvcList.setColumn(nRow,"DEPT_CODE",this.gfn_getDeptId());
		this.ds_srvcList.setColumn(nRow,"DEPT_NM",this.gfn_getDeptName());
		this.ds_srvcList.setColumn(nRow, "RSPNBER_EMPNO", this.fn_getEmpno());			// 사번
		this.ds_srvcList.setColumn(nRow, "RSPNBER_EMPNM", this.fn_getEmpnm());			// 사원명
	}
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	this.gfn_delMultiRow(this.ds_srvcList);
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if(this.gfn_message("confirm.취소여부")) {
		this.ds_srvcList.reset();		//DATASET ROLLBACK
		this.ds_srvcList.applyChange();	//DATASET COMMIT
		return;
	}
}

 /*
 * 함수명  	: btn_rowInsert_onclick
 * 설명 		: 행추가 버튼 클릭
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_rowInsert_onclick();
 */
this.btn_rowInsert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var slRow = this.ds_srvcList.rowposition;

	if(!this.ds_srvcList.getColumn(slRow, "SN")) {
		// this.gfn_message("errors.값확인.요청", "유효한 자원봉사가|자원봉사 내역을 다시");
		this.gfn_message("info.처리불가", "사회공헌활동 내역을 먼저 저장하세요.");
	}
	else {
		var rowCnt = this.ds_prtcpntList.rowcount;
		var max = this.ds_srvcList.getColumn(slRow, "SRVC_MXMM_NMPR_CO");
		
		if(rowCnt < max) {
			var nRow = this.ds_prtcpntList.addRow();
			this.ds_prtcpntList.setColumn(nRow, "SN", this.ds_srvcList.getColumn(slRow, "SN"));
			
			this.ds_prtcpntList.setColumn(nRow, "PRTCPNT_RCOGN_SRVC_TIME", this.ds_srvcList.getColumn(slRow, "RCOGN_SRVC_TIME"));
		}
		else {
			this.gfn_message("info.처리불가", "참가자 목록이 가득 찼습니다. 더이상 추가할 수 없습니다.");
		}
	}
}

 /*
 * 함수명  	: btn_rowDelete_onclick
 * 설명 		: 행삭제 버튼 클릭
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_rowDelete_onclick();
 */
this.btn_rowDelete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_delMultiRow(this.ds_prtcpntList);
}

 /*
 * 함수명  	: btn_rowCancel_onclick
 * 설명 		: 행취소 버튼 클릭
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_rowCancel_onclick();
 */
this.btn_rowCancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.gfn_message("confirm.취소여부")) {
		this.ds_prtcpntList.reset();		//DATASET ROLLBACK
		this.ds_prtcpntList.applyChange();	//DATASET COMMIT
		return;
	}
}

/***********************************************************************
* 사용자 정의 함수
************************************************************************/

this.ds_srvcList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	
	if(e.columnid == "CHK") {
		var s_SELF_CNFIRM_AT = obj.getColumn(e.row, "CONFM_AT");
		var s_CHK = obj.getColumn(e.row, "CHK");
		
		if(s_SELF_CNFIRM_AT == "1" && s_CHK == "1") {
			alert("확인완료건은 선택할 수 없습니다.");
			obj.setColumn(e.row, "CHK", "0");
		}
	}
}

this.fn_setReadonly = function(_row)
{
	if(this.ds_srvcList.getColumn(_row, "CONFM_AT") == "1") {
		this.div_top.btn_uploadFiles.set_enable(false);
		this.div_top.btn_downFiles.set_enable(false);
		this.div_top.btn_downFiles00.set_enable(false);
		this.div_work.btn_rowInsert.set_enable(false);
		this.div_work.btn_rowDelete.set_enable(false);
		this.div_work.btn_rowCancel.set_enable(false);
		this.div_work.btn_demoDown.set_enable(false);
		this.div_work.Button25.set_enable(false);
	} else {
		this.div_top.btn_uploadFiles.set_enable(true);
		this.div_top.btn_downFiles.set_enable(true);
		this.div_top.btn_downFiles00.set_enable(true);
		this.div_work.btn_rowInsert.set_enable(true);
		this.div_work.btn_rowDelete.set_enable(true);
		this.div_work.btn_rowCancel.set_enable(true);
		this.div_work.btn_demoDown.set_enable(true);
		this.div_work.Button25.set_enable(true);
	}
}

this.btn_demoDown_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_exportExcel(this.div_work.grd_prtcpntListDemo, "prtcpntListDemo");	
}

this.Button25_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var slRow = this.ds_srvcList.rowposition;

	if(!this.ds_srvcList.getColumn(slRow, "SN")) {
		// this.gfn_message("errors.값확인.요청", "유효한 자원봉사가|자원봉사 내역을 다시");
		this.gfn_message("info.처리불가", "사회공헌활동 내역을 먼저 저장하세요.");
		return;
	}
	
	var arg = {
		targetDs : this.ds_import,
		headSet : false,
		callBack : function () {
			if(this.ds_import.getRowCount() > 1){
				var nRow;
				this.ds_prtcpntList.set_updatecontrol(false);
				var max = this.ds_srvcList.getColumn(slRow, "SRVC_MXMM_NMPR_CO");
				
				for(i=1; i< this.ds_import.rowcount; i++) {
				
					var rowCnt = this.ds_prtcpntList.rowcount;
					if(rowCnt >= max) {
						this.gfn_message("info.처리불가", "참가자 목록이 가득 찼습니다. 더이상 추가할 수 없습니다.");
						return;
					}
		
					nRow = this.ds_prtcpntList.addRow();
					this.ds_prtcpntList.setColumn(nRow, "SN", this.ds_srvcList.getColumn(this.ds_srvcList.rowposition, "SN"));
					
					this.ds_prtcpntList.setColumn(nRow, "DEPT_NM",      this.ds_import.getColumn(i, "Column0"));
					this.ds_prtcpntList.setColumn(nRow, "PRTCPNT_EMPNO",      this.ds_import.getColumn(i, "Column1"));
					this.ds_prtcpntList.setColumn(nRow, "PRTCPNT_EMPNM",      this.ds_import.getColumn(i, "Column2"));
					this.ds_prtcpntList.setColumn(nRow, "PRTCPNT_RCOGN_SRVC_TIME",      this.ds_import.getColumn(i, "Column3"));
					this.ds_prtcpntList.setRowType(nRow,"I");
				}
				this.ds_prtcpntList.set_updatecontrol(true);
			}
		}
	};
	this.gfn_importExcel(arg);
}
