	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		arg_1 : this.ds_cond00.getColumn(0,"BNDT_SE"),
		arg_2 : null,
		arg_3 : null
	};
	this.gfn_popup("HRM070301_p1", 500,500, "당직자 선택", parm, "mis_hrm::HRM070301_P1.xfdl", "fn_callback1");
	
}

/*
 * 함수명  	: fn_delete
