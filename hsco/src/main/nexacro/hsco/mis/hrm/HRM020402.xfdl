/***********************************************************************
 * button 관련 이벤트 처리
 ************************************************************************/
 /*
 * 함수명  	: div_work_btn_dtaCreat_onclick
 * 설명 		: 인사자료생성 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_work_btn_dtaCreat_onclick();
 */
this.div_work_btn_dtaCreat_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_empmnLastResultList)) {
		this.gfn_message("info.처리불가","변경된 데이터가 존재합니다.\n저장 또는 취소 후 다시 진행해주십시오.");
		return false;
    }
			
    this.ds_hrmastrCreat.clearData();
    var newRow = this.ds_hrmastrCreat.rowposition;
    for(var i = 0; i < this.ds_empmnLastResultList.rowcount; i++) {
		var dcsnAt = this.ds_empmnLastResultList.getColumn(i, "EMPMN_DCSN_AT");
		var creatAt = this.ds_empmnLastResultList.getColumn(i, "DTA_CREAT_AT");
				
		// 확정여부가 확정이고, 자료생성이 되지않은 건들
		if(dcsnAt == '1' && creatAt != '1') {
			newRow = this.ds_hrmastrCreat.addRow();
			this.ds_hrmastrCreat.copyRow(newRow, this.ds_empmnLastResultList, i); 
		}
    }

    if(this.ds_hrmastrCreat.rowcount > 0) {
		if(application.confirm("최종입사대상자를 인사자료생성 하시겠습니까?\n채용확정상태가 입사확정이면서 인사자료생성이 되지 않은 건만 인사자료 생성됩니다.")) {
			this.fn_transaction("hrmastrC");
		}
	}else {
		this.gfn_message("info.처리불가","인사자료 생성할 최종입사대상자가 없습니다.\n채용확정상태와 인사자료생성 여부를 다시 한 번 확인해주세요.");
	}
}

/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 검색조건 데이터셋 컬럼 변경시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged();
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "EMPMN_YEAR") {
		this.fn_transaction("selectOdrSeCombo");
	}else if(e.columnid == "ODR_SE") {
		this.fn_search();
	}
}

