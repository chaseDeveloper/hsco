 */
this.btn_rowInsert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var elRow = this.DS_PSNCPA_MST.rowposition;
	var stdrDe = this.DS_PSNCPA_MST.getColumn(elRow, "STDR_DE");
	if(this.comUtils.isNotNull(stdrDe)) {
