***********************************************************************************/
this.btn_Approval_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_flxwkInfo.rowposition;
	
	// 전자결재 상태 체크 2016.7
	// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
	// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
	// 기안전인것 : 재상신 및 수정/삭제
	// 결재상태가 상신전 외는 신규로 다시 진행.
	var sanctnKnd = this.ds_flxwkInfo.getColumn(this.ds_flxwkInfo.rowposition, "SANCTN_STS");
	var cancelSe = this.rdo_CANCEL_SE.value;
	
	switch (sanctnKnd) {
			case "1": case "2": case "4": case "8": case "1024": case "DUP":
			this.gfn_message("comm.결재.진행");
			return false;
			
			case "32": case "256": case "512": case "4096": 
			this.gfn_message("comm.결재.완료");
			return false;
			
			default:
	}
		
	if(Ex.util.isUpdated(this.ds_flxwkInfo)){
			this.gfn_message("comm.정보.저장.요청", "수정된");
			return false;
	}

	var filds = new Array(26);
	var deptNm        = this.ds_flxwkInfo.getColumn(nRow, "DEPT_NM");
	var clsfSe        = this.ds_flxwkInfo.getColumn(nRow, "CLSF_SE");
	var empNm         = this.ds_flxwkInfo.getColumn(nRow, "EMPNM");
	var attendTm      = this.ds_flxwkInfo.getColumn(nRow, "ATTEND_TM");
	var lvffcTm       = this.ds_flxwkInfo.getColumn(nRow, "LVFFC_TM");
	var cAtt1         = this.ds_flxwkInfo.getColumn(nRow, "C_ATT_1");
	var cLvff1        = this.ds_flxwkInfo.getColumn(nRow, "C_LVFF_1");
	var monWorkTm     = this.div_work.msk_monWorkTm.text.replace("__:__", "");
	var cAtt2         = this.ds_flxwkInfo.getColumn(nRow, "C_ATT_2");
	var cLvff2        = this.ds_flxwkInfo.getColumn(nRow, "C_LVFF_2");
	var tuesWorkTm    = this.div_work.msk_tuesWorkTm.text.replace("__:__", "");
	var cAtt3         = this.ds_flxwkInfo.getColumn(nRow, "C_ATT_3");
	var cLvff3        = this.ds_flxwkInfo.getColumn(nRow, "C_LVFF_3");
	var wednsWorkTm   = this.div_work.msk_wendsWorkTm.text.replace("__:__", "");
	var cAtt4         = this.ds_flxwkInfo.getColumn(nRow, "C_ATT_4");
	var cLvff4        = this.ds_flxwkInfo.getColumn(nRow, "C_LVFF_4");
	var thursWorkTm   = this.div_work.msk_thirsWorkTm.text.replace("__:__", "");
	var cAtt5         = this.ds_flxwkInfo.getColumn(nRow, "C_ATT_5");
	var cLvff5        = this.ds_flxwkInfo.getColumn(nRow, "C_LVFF_5");
	var friWorkTm     = this.div_work.msk_friWorkTm.text.replace("__:__", "");
	var bgnDe         = this.ds_flxwkInfo.getColumn(nRow, "BGNDE");
	var endDe         = this.ds_flxwkInfo.getColumn(nRow, "ENDDE");
	var vrscManClsfNm = this.ds_flxwkInfo.getColumn(nRow, "VRSC_MAN_CLSF_NM");
	var vrscManEmpNm  = this.ds_flxwkInfo.getColumn(nRow, "VRSC_MAN_EMPNM");
	var reqstPrvonsh  = this.ds_flxwkInfo.getColumn(nRow, "REQST_PRVONSH");
	
	attendTm = this.comUtils.isNull(attendTm) == 1 ? '' : attendTm.substr(0, 2) + ':' + attendTm.substr(2, 4);
	lvffcTm  = this.comUtils.isNull(lvffcTm)  == 1 ? '' : lvffcTm.substr(0, 2)  + ':' + lvffcTm.substr(2, 4);
	cAtt1    = this.comUtils.isNull(cAtt1)    == 1 ? '' : cAtt1.substr(0, 2)    + ':' + cAtt1.substr(2, 4);
	cLvff1   = this.comUtils.isNull(cLvff1)   == 1 ? '' : cLvff1.substr(0, 2)   + ':' + cLvff1.substr(2, 4);
	cAtt2    = this.comUtils.isNull(cAtt2)    == 1 ? '' : cAtt2.substr(0, 2)    + ':' + cAtt2.substr(2, 4);
	cLvff2   = this.comUtils.isNull(cLvff2)   == 1 ? '' : cLvff2.substr(0, 2)   + ':' + cLvff2.substr(2, 4);
	cAtt3    = this.comUtils.isNull(cAtt3)    == 1 ? '' : cAtt3.substr(0, 2)    + ':' + cAtt3.substr(2, 4);
	cLvff3   = this.comUtils.isNull(cLvff3)   == 1 ? '' : cLvff3.substr(0, 2)   + ':' + cLvff3.substr(2, 4);
	cAtt4    = this.comUtils.isNull(cAtt4)    == 1 ? '' : cAtt4.substr(0, 2)    + ':' + cAtt4.substr(2, 4);
	cLvff4   = this.comUtils.isNull(cLvff4)   == 1 ? '' : cLvff4.substr(0, 2)   + ':' + cLvff4.substr(2, 4);
	cAtt5    = this.comUtils.isNull(cAtt5)    == 1 ? '' : cAtt5.substr(0, 2)    + ':' + cAtt5.substr(2, 4);
	cLvff5   = this.comUtils.isNull(cLvff5)   == 1 ? '' : cLvff5.substr(0, 2)   + ':' + cLvff5.substr(2, 4);
	
	bgnDe = bgnDe.substr(0, 4) + '-' + bgnDe.substr(4, 2) + '-' + bgnDe.substr(6, 2);
	endDe = endDe.substr(0, 4) + '-' + endDe.substr(4, 2) + '-' + endDe.substr(6, 2);
	
	filds[0]  = deptNm;
	filds[1]  = clsfSe;
	filds[2]  = empNm;
	filds[3]  = attendTm;
	filds[4]  = lvffcTm;
	filds[5]  = cAtt1;
	filds[6]  = cLvff1;
	filds[7]  = monWorkTm;
	filds[8]  = cAtt2;
	filds[9]  = cLvff2;
	filds[10] = tuesWorkTm;
	filds[11] = cAtt3;
	filds[12] = cLvff3;
	filds[13] = wednsWorkTm;
	filds[14] = cAtt4;
	filds[15] = cLvff4;
	filds[16] = thursWorkTm;
	filds[17] = cAtt5;
	filds[18] = cLvff5;
	filds[19] = friWorkTm;
	filds[20] = bgnDe;
	filds[21] = endDe;
	filds[22] = vrscManClsfNm;
	filds[23] = vrscManEmpNm;
	filds[24] = reqstPrvonsh;
	
	var dataValue = filds.join("|");
	var dataTitle = "유연근무신청서";
	var title     = "유연근무신청서";
	if (cancelSe  == '1') {
		dataTitle = dataTitle + '(취소)';
		title     = title + '(취소)';
	}
	
	this.ds_approve.clearData();	
	this.ds_approve.addRow();
	
	this.ds_approve.setColumn(0, "TITLE",      title                );
	this.ds_approve.setColumn(0, "DATA_TITLE", dataTitle            );
	this.ds_approve.setColumn(0, "DATA_VALUE", dataValue            );
	this.ds_approve.setColumn(0, "DEPT_CD",    this.gfn_getDeptId() );
	this.ds_approve.setColumn(0, "EMP_CD",     this.fn_getEmpno() );
	
	this.ds_approve.setColumn(0, "EMPNO", 	   this.ds_flxwkInfo.getColumn(0,"EMPNO"));
	this.ds_approve.setColumn(0, "SEQ",        this.ds_flxwkInfo.getColumn(0,"SEQ"));
	this.ds_approve.setColumn(0, "SANCTN_NO",  this.ds_flxwkInfo.getColumn(0,"SANCTN_NO"));
	this.ds_approve.setColumn(0, "CANCEL_SE",  cancelSe); //@@@@
	//alert("dataValue = " + dataValue);
	if(this.gfn_message("comm.행위여부", "결재를 상신"))	{
		//Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs);		
		this.fn_transaction("approve");
	}
}


this.rdo_CANCEL_SE_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	var draftKnd = this.parent.arg_4;
	var bEnable = (draftKnd == "1" || draftKnd == "9") ? false : true;

	if (obj.value == '')  {
		this.btn_Save.set_enable(bEnable);
		this.btn_Approval.set_enable(bEnable);
	} else {
		this.btn_Save.set_enable(true);
		this.btn_Approval.set_enable(true);
	}
	
}


