this.btn_Save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 수정 여부체크
// 	if (!this.comUtils.isDatasetUpdated(this.ds_flxwkInfo)){
// 		this.gfn_message("comm.데이터.변경.없음");
// 		return false;
// 	}

	// validation 체크
	if(this.ds_flxwkInfo.getColumn(0, "EMPNO") == undefined){
		this.gfn_message("comm.항목.입력.을","사번"); 
		return false;
	}
	
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_flxwkInfo, this.ds_validation)) {
		return;
	}
	
	
	//민원 등 업무대행자 필수입력사항 체크
	if(this.ds_flxwkInfo.getColumn(0,"VRSC_MAN_EMPNO") == undefined 
		|| this.ds_flxwkInfo.getColumn(0,"VRSC_MAN_EMPNM") == undefined ){
		this.gfn_message("comm.항목.입력.을","민원 등 업무대행자"); 
		return;
	}
	
	//신청사유 필수입력사항 체크
	if(this.div_work.txt_reqstPrvonsh.value == undefined){
		this.gfn_message("comm.항목.입력.을","신청사유"); 
		return;
	}

	//취소/재신청 처리 
	var cancelSe = this.rdo_CANCEL_SE.value;
	if(cancelSe == '1' || cancelSe == '2' )	{ 
		if (this.ds_flxwkInfo.getColumn(0, "CANCEL_SE") == undefined || this.ds_flxwkInfo.getColumn(0, "CANCEL_SE") == "") { 
			//신규자료로 다시 생성함  
			this.ds_flxwkInfo.set_updatecontrol(false);
			this.ds_flxwkInfo.setRowType(0, Dataset.ROWTYPE_INSERT );
			
			this.ds_flxwkInfo.setColumn (0, "SEQ", 	""); 	
			this.ds_flxwkInfo.setColumn (0, "SANCTN_NO", "");  
			this.ds_flxwkInfo.setColumn (0, "SANCTN_STS", "");  
			this.ds_flxwkInfo.setColumn (0, "SANCTN_STTS", "");  
			this.ds_flxwkInfo.setColumn (0, "DRFT_STS", "");  
			this.ds_flxwkInfo.setColumn (0, "CANCEL_SE", cancelSe); 
		}
	}
		
	var rowType = this.ds_flxwkInfo.getRowType(0);
	if(this.gfn_message("confirm.저장.여부"))	{
		if(rowType == Dataset.ROWTYPE_INSERT ||  this.ds_flxwkInfo.getColumn(0, "SEQ") == "") {
			this.fn_transaction("flxwkInfoC");	//등록
		} else  {
			this.fn_transaction("flxwkInfoU");	//수정
		}
	}
}

