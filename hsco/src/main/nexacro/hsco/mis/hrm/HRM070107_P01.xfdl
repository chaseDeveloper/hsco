			var sInDatasets   	= "input02=ds_cond2";
			var sOutDatasets  	= "ds_userInfo=output02";
		break;	
		
		//유연근무신청 목록 상세조회
		case "selectFlxwkInfo":
			var sController   	= "hsco/mis/hrm/HRM070107_P01/selectFlxwkInfo.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_flxwkInfo=output01";
		break;	
		
		//유연근무 신청 등록
		case "flxwkInfoC":
			var RADIO_SE = this.ds_flxwkInfo.getColumn(0, "RADIO_SE");
			switch(RADIO_SE) {
				// ** 탄력적
				case '0':
					var ATT_TM   = this.ds_flxwkInfo.getColumn(0, "ATTEND_TM");
					var LVFF_TM  = this.ds_flxwkInfo.getColumn(0, "LVFFC_TM");
					var attTime  = '';
					var lvffTime = '';
					
					if(ATT_TM.indexOf(' ') != -1) {
						this.gfn_message("info.처리불가", "입력한 출근시간의 형식이 부적합합니다.");
						return false;
					}
					if(LVFF_TM.indexOf(' ') != -1) {
						this.gfn_message("info.처리불가", "입력한 퇴근시간의 형식이 부적합합니다.");
						return false;
					}
					if(ATT_TM.substr(2, 4) >= 60) {
						this.gfn_message("errors.항목.입력값.범위", "출근시각|00:00|23:59");
						return false;
					}
					if(LVFF_TM.substr(2, 4) >= 60) {
						this.gfn_message("errors.항목.입력값.범위", "퇴근시각|00:00|23:59");
						return false;
					}
					
					//9999 의 형태일 경우 99:99로 변경 
					if((ATT_TM+"").replace(" ", "").length == 4){
						attTime = ATT_TM.substring(0,2) + ":" + ATT_TM.substring(2,4);
					}
					if((LVFF_TM+"").replace(" ", "").length == 4){
						lvffTime = LVFF_TM.substring(0,2) + ":" + LVFF_TM.substring(2,4);
					}
					break;
				// ** 선택적
				case '1':
					//TBHRM_FLXWK_CHOISE (TABLE)에 저장할 DATASET 생성
					this.ds_flxwkChoiseInfo.clearData();
