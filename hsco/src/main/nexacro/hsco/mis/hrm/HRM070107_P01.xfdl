/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//현재 로그인 사용자 정보 조회 
		case "selectUserInfo":
