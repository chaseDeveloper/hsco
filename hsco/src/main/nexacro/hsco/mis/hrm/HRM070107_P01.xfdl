 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//부서코드 조회 콜백 
			case "selectDeptcodeList":
				if(flag == 1)
				{
					this.fn_search();
				}	
			break;
			//조회 콜백
			case "selectFlxwkInfo":
				//탄력적 근무제 출근시간, 퇴근시간 중 하나라도 값이 있을 경우 탄력적 근무제 선택
				if(!this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "ATTEND_TM")) || !this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "LVFFC_TM")))
				{
					this.ds_flxwkInfo.set_updatecontrol(false);
					this.ds_flxwkInfo.setColumn(0, "RADIO_SE", 0);
					this.ds_flxwkInfo.set_updatecontrol(true);
					this.div_work.msk_monAttendTm.set_enable(false);
					this.div_work.msk_tuesAttendTm.set_enable(false);
					this.div_work.msk_wendsAttendTm.set_enable(false);
					this.div_work.msk_thirsAttendTm.set_enable(false);
					this.div_work.msk_friAttendTm.set_enable(false);
					this.div_work.msk_monLvffcTm.set_enable(false);
					this.div_work.msk_tuesLvffcTm.set_enable(false);
					this.div_work.msk_wendsLvffcTm.set_enable(false);
					this.div_work.msk_thirsLvffcTm.set_enable(false);
					this.div_work.msk_friLvffcTm.set_enable(false);
					this.div_work.msk_monWorkTm.set_enable(false);
					this.div_work.msk_tuesWorkTm.set_enable(false);
					this.div_work.msk_wendsWorkTm.set_enable(false);
					this.div_work.msk_thirsWorkTm.set_enable(false);
					this.div_work.msk_friWorkTm.set_enable(false);
				//선택적 근무제 월,화,수,목,금 출근시간중 하나라도 값이 있을 경우 선택적 근무제 선택
				}else if(!this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "C_ATT_1")) 
					  || !this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "C_ATT_2"))
					  || !this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "C_ATT_3"))
					  || !this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "C_ATT_4"))
					  || !this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "C_ATT_5")))
				{
					this.ds_flxwkInfo.set_updatecontrol(false);
					this.ds_flxwkInfo.setColumn(0, "RADIO_SE", 1);
					this.ds_flxwkInfo.set_updatecontrol(true);
					this.div_work.msk_attendTm.set_enable(false);
					this.div_work.msk_lvffcTm.set_enable(false);
				}
				
				//더블클릭 하여 상세조회일 경우 근무시간 계산
				for(var i = 1 ; i <= 5 ; i++)
				{
					var attendTm = this.ds_flxwkInfo.getColumn(0,i*2 + 11);
					var lvffcTm  = this.ds_flxwkInfo.getColumn(0,i*2 + 12);
					
					if(attendTm == undefined || attendTm == null) attendTm = "";
					if(lvffcTm == undefined || lvffcTm == null) lvffcTm = "";
					
					attendTm = (nexacro.toNumber(attendTm.substring(0,2)) * 60) + nexacro.toNumber(attendTm.substring(2,4)); //출근시간 분단위
					lvffcTm = (nexacro.toNumber(lvffcTm.substring(0,2)) * 60) + nexacro.toNumber(lvffcTm.substring(2,4));  //퇴근시간 분단위
					
					var subSum = lvffcTm - attendTm - 60;
					var workTimeH = Math.floor(subSum / 60,0);
					var workTimeM = subSum - workTimeH * 60;
					
					if(workTimeH < 10)
					{
						workTimeH = "0" + workTimeH.toString();
					}else{
						workTimeH = workTimeH.toString();
					}
					if(workTimeM < 10)
					{
						workTimeM = "0" + workTimeM.toString();
					}else{
						workTimeM = workTimeM.toString();
					}
					
					var workTime = workTimeH + workTimeM;
					switch(i)
					{
						case 1 :
							this.div_work.msk_monWorkTm.set_value(workTime);
						break;
						case 2 :
							this.div_work.msk_tuesWorkTm.set_value(workTime);
						break;
						case 3 :
							this.div_work.msk_wendsWorkTm.set_value(workTime);
						break;
						case 4 :
							this.div_work.msk_thirsWorkTm.set_value(workTime);
						break;
						case 5 :
							this.div_work.msk_friWorkTm.set_value(workTime);
						break;
					}
				}
					
				if (this.ds_flxwkInfo.getColumn(0, "CANCEL_SE") == '1' ||
					this.ds_flxwkInfo.getColumn(0, "CANCEL_SE") == '2' ) {
					
					var cancelSe = this.ds_flxwkInfo.getColumn(0, "CANCEL_SE");
					
					this.rdo_CANCEL_SE.set_index(cancelSe);
					this.rdo_CANCEL_SE.set_readonly(true);
				}    				
				break;
			//수정 콜백
			case "flxwkInfoU":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			//저장 콜백
			case "flxwkInfoC":
				this.gfn_message("success.처리.성공", null, null, null, this.fn_popupCallback);				
				break;
			
			//로그인 사용자 정보 콜백
			case "selectUserInfo":
				//초기값 셋팅
				this.ds_flxwkInfo.setColumn(0, "DEPT_CODE", this.ds_userInfo.getColumn(0, "DEPT_CODE"));
				this.ds_flxwkInfo.setColumn(0, "DEPT_NM",   this.ds_userInfo.getColumn(0, "DEPT_NM"));
				this.ds_flxwkInfo.setColumn(0, "EMPNO",     this.ds_userInfo.getColumn(0, "EMPNO"));
				this.ds_flxwkInfo.setColumn(0, "EMPNM",     this.ds_userInfo.getColumn(0, "EMPNM"));
				this.ds_flxwkInfo.setColumn(0, "RSPOFC_SE", this.ds_userInfo.getColumn(0, "RSPOFC_SE"));
				this.ds_flxwkInfo.setColumn(0, "OFCPS_SE",  this.ds_userInfo.getColumn(0, "OFCPS_SE"));
				this.ds_flxwkInfo.setColumn(0, "REQST_DE",  this.gfn_today());
				this.ds_flxwkInfo.setColumn(0, "BGNDE",     this.gfn_today().substring(0,6) + '01')
				this.ds_flxwkInfo.setColumn(0, "ENDDE",     this.gfn_lastDateNum(this.gfn_today().substring(0,6)));
				this.ds_flxwkInfo.setColumn(0, "SANCTN_KND", '0');
				this.ds_flxwkInfo.setColumn(0, "RADIO_SE", 0);
				//탄력적 근무제 일 경우 선택적 근무제 enable(false) && value clear
				//                         탄력적 근무제 enable(true)
				this.div_work.msk_attendTm.set_enable(true);
				this.div_work.msk_lvffcTm.set_enable(true);
				this.div_work.msk_monAttendTm.set_value("");
				this.div_work.msk_tuesAttendTm.set_value("");
				this.div_work.msk_wendsAttendTm.set_value("");
				this.div_work.msk_thirsAttendTm.set_value("");
				this.div_work.msk_friAttendTm.set_value("");
				this.div_work.msk_monLvffcTm.set_value("");
				this.div_work.msk_tuesLvffcTm.set_value("");
				this.div_work.msk_wendsLvffcTm.set_value("");
				this.div_work.msk_thirsLvffcTm.set_value("");
				this.div_work.msk_friLvffcTm.set_value("");
				this.div_work.msk_monWorkTm.set_value("");
				this.div_work.msk_tuesWorkTm.set_value("");
				this.div_work.msk_wendsWorkTm.set_value("");
				this.div_work.msk_thirsWorkTm.set_value("");
				this.div_work.msk_friWorkTm.set_value("");
				this.div_work.msk_monAttendTm.set_enable(false);
				this.div_work.msk_tuesAttendTm.set_enable(false);
				this.div_work.msk_wendsAttendTm.set_enable(false);
				this.div_work.msk_thirsAttendTm.set_enable(false);
				this.div_work.msk_friAttendTm.set_enable(false);
				this.div_work.msk_monLvffcTm.set_enable(false);
				this.div_work.msk_tuesLvffcTm.set_enable(false);
				this.div_work.msk_wendsLvffcTm.set_enable(false);
				this.div_work.msk_thirsLvffcTm.set_enable(false);
				this.div_work.msk_friLvffcTm.set_enable(false);
				this.div_work.msk_monWorkTm.set_enable(false);
				this.div_work.msk_tuesWorkTm.set_enable(false);
				this.div_work.msk_wendsWorkTm.set_enable(false);
				this.div_work.msk_thirsWorkTm.set_enable(false);
				this.div_work.msk_friWorkTm.set_enable(false);
				this.ds_flxwkInfo.setRowType(0, Dataset.ROWTYPE_INSERT );
				this.rdo_CANCEL_SE.set_index("");

			break;
			
			//전자결재콜백
			case "approve":
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
			break;
		}
	}
