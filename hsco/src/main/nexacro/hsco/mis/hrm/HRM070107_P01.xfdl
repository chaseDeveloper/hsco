    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID    : HRM070107_P01.xfdl
 * 화면명    : 유연근무신청상세(팝업)
 * 화면설명  : 유연근무목록을 상세조회, 신규, 수정 한다.
 * 작성일    : 2015.11.20
 * 작성자    : 이지영
 * 수정이력  : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************/

/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
* 화면 변수 선언부
***********************************************************************/
var empNo = this.parent.EMPNO;
var seq   = this.parent.SEQ;
var flag  = this.parent.FLAG; //상세보기일경우 1 신규추가일경우 null
this.checkDs = [
  this.ds_flxwkInfo
];
/***********************************************************************
* onInit 영역
***********************************************************************/
this.HRM070107_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();	
	
	this.ds_flxwkInfo.clearData();
	this.ds_flxwkInfo.addRow();
	this.div_work.div_empNo1.fn_setBind("ds_flxwkInfo","EMPNO","EMPNM");
	this.div_work.div_empNo2.fn_setBind("ds_flxwkInfo","VRSC_MAN_EMPNO","VRSC_MAN_EMPNM");
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "EMPNO", empNo);
	this.ds_cond.setColumn(0, "SEQ",   seq);
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.HRM070107_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	if(typeof flag == "undefined"){ //신규일때 취소선택불가
		this.rdo_CANCEL_SE.set_readonly(true);
	}
}


/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	// 공통코드
	var comboParams = [["ds_rspofcSe", "_HRM214", "1", "전체"],//직책
			           ["ds_ofcpsSe",  "HRM003", "1", "전체"],//직위
			           ["ds_jssfcSe",  "HRM001", "1", "전체"],//직종
			           ["ds_sanctnSttusCode", "COM023", "1", ""],   // 전자결재상태
		              ];
		              
	var callback = function (svcId, errorCode, errorMsg) {
		//신규추가일경우
		if(flag != 1){
			//현재 로그인 사용자 정보 가져오기
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();
			this.ds_cond2.setColumn(0, "EMPNO", this.parent.EMPNO);
			
			if(this.parent.EMPNO == undefined){
				this.div_work.div_empNo1.set_enable(true);
			}
			
			this.fn_transaction("selectUserInfo");
		//상세조회 일 경우
		}else{
			this.fn_cudSetup();		// 전자결재 추가 (2016.7)
			this.fn_search();
		}
	};
	this.gfn_comboLoad (comboParams, callback);
}


/**********************************************************************
* dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	
}


/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	

	this.fn_transaction("selectFlxwkInfo");
}

/***********************************************************************
 * 설명: 결재에 따른 CONTROL 상태 UPDATE (2016.7)
 ************************************************************************/
 this.fn_cudSetup = function()
 {
	// 전자결재 상태 체크 2016.7
	// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
	// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
	// 기안전인것 : 재상신 및 수정/삭제
	// 결재상태가 상신전 외는 신규로 다시 진행.
	
	var draftKnd = this.parent.arg_4;
	
	var bEnable = (draftKnd == "1" || draftKnd == "9") ? false : true;
	
	this.btn_Save.set_enable(bEnable);
	this.btn_Approval.set_enable(bEnable);
	this.rdo_CANCEL_SE.set_readonly(bEnable);

 }

