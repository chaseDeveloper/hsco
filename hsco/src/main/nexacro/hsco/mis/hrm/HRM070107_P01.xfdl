/**********************************************************************
* 설명: 근무시간 계산
************************************************************************/
this.ds_flxwkInfo_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var bgnDe = e.columnid == "BGNDE" ? e.newvalue : obj.getColumn(e.row, "BGNDE");
	var endDe = e.columnid == "ENDDE" ? e.newvalue : obj.getColumn(e.row, "ENDDE");
	
	// 근무기간 설정
	if(e.columnid == "BGNDE" || e.columnid == "ENDDE") {
		if(!this.comUtils.isNull(bgnDe) && !this.comUtils.isNull(endDe)) {
			if(bgnDe > endDe){
				var errMsg = e.columnid == "BGNDE" ? "근무시작일자는 근무종료일자를" : "근무종료일자는 근무시작일자를";
				this.gfn_message("info.처리불가", errMsg + "초과할 수 없습니다.");
			} else if(bgnDe.substr(0, 6) < endDe.substr(0, 6)) {
				this.gfn_message("info.처리불가", "근무기간은 같은 달만 신청할 수 있습니다.");
			}
		}
	}
	
	if(e.columnid =="EMPNO")
	{
		this.ds_flxwkInfo.setColumn(this.ds_flxwkInfo.rowposition, "DEPT_CODE", this.div_work.div_empNo1.DS_EMPNO.getColumn(0, "DEPT_CODE"));
		this.ds_flxwkInfo.setColumn(this.ds_flxwkInfo.rowposition, "RSPOFC_SE", this.div_work.div_empNo1.DS_EMPNO.getColumn(0, "RSPOFC_SE"));
		this.ds_flxwkInfo.setColumn(this.ds_flxwkInfo.rowposition, "OFCPS_SE", this.div_work.div_empNo1.DS_EMPNO.getColumn(0, "OFCPS"));
	}
	
	if(e.columnid == "C_ATT_1" || e.columnid == "C_LVFF_1" )
	{
		if(!this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "C_ATT_1")) && !this.comUtils.isNull(this.ds_flxwkInfo.getColumn(0, "C_LVFF_1")))
		{
			var attendTm = this.ds_flxwkInfo.getColumn(0, "C_ATT_1");
			var lvffcTm  = this.ds_flxwkInfo.getColumn(0, "C_LVFF_1");
			attendTm = (nexacro.toNumber(attendTm.substring(0,2)) * 60) + nexacro.toNumber(attendTm.substring(2,4)); //출근시간 분단위
			lvffcTm = (nexacro.toNumber(lvffcTm.substring(0,2)) * 60) + nexacro.toNumber(lvffcTm.substring(2,4));   //퇴근시간 분단위
			var subSum = lvffcTm - attendTm - 60;
			var workTimeH = Math.floor(subSum / 60,0);
			var workTimeM = subSum - workTimeH * 60;
			if(workTimeH < 10)
			{
				workTimeH = "0" + workTimeH.toString();
			}else{
				workTimeH = workTimeH.toString();
			}
			if(workTimeM < 10)
			{
				workTimeM = "0" + workTimeM.toString();
			}else{
				workTimeM = workTimeM.toString();
			}
			var workTime = workTimeH + workTimeM;
			this.div_work.msk_monWorkTm.set_value(workTime);
		}
	}
	if(e.columnid == "C_ATT_2" || e.columnid == "C_LVFF_2")
