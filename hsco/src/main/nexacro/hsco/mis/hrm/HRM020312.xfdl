 * 화면설명 : 인적성 결과를 관리한다.
 * 작성일   : 2017.07.13
 * 작성자   : 이수지
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 *   
 ***********************************************************************
 */
 
 /***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
// 업무에 필요한 공통 라이브러리 파일을 추가할 수 있다.
// TODO

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/

this.saveRow = -1;
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.HRM020312_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{

}

this.HRM020312_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
	
	// 검색조건 데이터셋
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_empmnYearList.clearData();
	
 	this.ds_cond.setColumn(0, "CMPET_SE"       , "");	// 경쟁구분
 	this.ds_cond.setColumn(0, "EMPMN_SE"       , "");	// 채용구분
 	this.ds_cond.setColumn(0, "APYEXM_CLSF_SE" , "");	// 응시직급구분
 	this.ds_cond.setColumn(0, "APYEXM_REALM_SE", "");	// 응시분야구분
 	
 	// 공통코드
	var comboParams = [
		 ["ds_hrm002_s", "HRM002", "1", "전체"]	 	//응시분야(검색조건)
		,["ds_hrm082_s", "HRM082", "1", "전체"] 	//경쟁구분(검색조건)
		,["ds_hrm083_s", "HRM083", "1", "전체"] 	//채용구분(검색조건)
		,["ds_hrm084_s", "HRM084", "1", "전체"] 	//응시직급구분(검색조건)
		,["ds_hrm066"  , "HRM066", "1", ""] 		//평가결과구분
	];
	
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_transaction("selectEmpmnYearCombo");
	}
	this.gfn_comboLoad(comboParams, callback);
}

/***********************************************************************
 * Form 오브젝트 초기화
 ***********************************************************************/ 
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/***********************************************************************
 * Dataset 오브젝트 초기화
 ***********************************************************************/ 
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/

 /*
 * 함수명  	: fn_search
 * 설명 		: 인적성관리 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_ncsScrpro.clearData();
	
	// validation 체크(검색조건)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.fn_transaction("select");
}


 /*
 * 함수명  	: fn_save
 * 설명 		: 인적성관리 저장 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 미변경 체크
    if (!this.comUtils.isDatasetUpdated(this.ds_ncsScrpro)) {
        this.gfn_message("comm.데이터.변경.없음")
        return false;
    }
    
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_ncsScrpro, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_ncsScrpro.rowposition;
		this.fn_transaction("save");
	}
}


/*
 * 함수명        : fn_cancel
 * 설명          : 취소 이벤트 처리
 * params        : None
 * return Type    : None
 * 사용법        : this.fn_cancel ();
 */
this.fn_cancel= function()
{
     // 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_ncsScrpro)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }

    if (this.gfn_message("confirm.취소여부")) {
        this.ds_ncsScrpro.reset();
        this.ds_ncsScrpro.applyChange();
    }
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// ========= 검색조건 =========
		case "selectEmpmnYearCombo": 		// 채용년도 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/empmnYearComboList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_empmnYearList=output1";
			break;
		case "selectOdrSeCombo":			// 차수, 채용공고 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/odrSeComboList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_odrSeList=output1";
			break;	
		// =============================
		case "select":						// 대상자목록 불러오기
			var sController   	= "hsco/mis/hrm/HRM020312/selectNcsScrproList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_ncsScrpro=output1";
			break;
		case "save":						// 대상자 저장
			var sController   	= "hsco/mis/hrm/HRM020312/saveNcsScrproList.do";
			var sInDatasets   	= "input1=ds_ncsScrpro:U";
			var sOutDatasets  	= "";
			break;
		case "excelUpload":					// 대상자 저장(엑셀)
			var sController   	= "hsco/mis/hrm/HRM020312/excelUpload.do";
			var sInDatasets   	= "input1=ds_cond input2=ds_ncsScrproExcel:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else if (nErrorCode == 0){
		switch (strSvcId) {
			// ========= 검색조건 =========
			case "selectEmpmnYearCombo":	// 채용년도 조회(검색조건)
				this.div_search.cmb_empmnYear.set_index(0);
				break;
			case "selectOdrSeCombo": 		// 차수, 채용공고 조회(검색조건)
				this.div_search.cmb_odrSe.set_index(0);
				break;	
			// =============================
			case "select": 					// 대상자목록
				break;
			case "save": 					// 대상자저장
				this.fn_search();
				break;
			case "excelUpload":				// 엑셀 업로드
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;	
		}
	}
}


/***********************************************************************
* Transaction 영역 끝
************************************************************************/
/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/

/***********************************************************************
 * dataset 관련 이벤트
 ***********************************************************************/
/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 검색조건 데이터셋 컬럼 변경시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged();
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "EMPMN_YEAR") {
		this.fn_transaction("selectOdrSeCombo");
	}else if(e.columnid == "ODR_SE") {
		this.fn_search();
	}
}

/***********************************************************************
 * grid 관련 이벤트
 ***********************************************************************/
/*
 * 함수명  	: grd_ncsScrpro_onheadclick
 * 설명 		: 헤드클릭 정렬, 체크박스 전체전택
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_ncsScrpro_onheadclick();
 */
