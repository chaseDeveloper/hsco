{
	this.gfn_gridSort(obj, e);
}


/***********************************************************************
 * button 관련 이벤트
 ***********************************************************************/
 /*
 * 함수명  	: btn_excelUpload_onclick
 * 설명 		: 엑셀파일 업로드
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_excelUpload_onclick();
 */
this.btn_excelUpload_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var year = this.ds_cond.getColumn(0, "EMPMN_YEAR");
	var odr  = this.ds_cond.getColumn(0, "ODR_SE");
		
	if(this.ds_ncsScrpro.rowcount > 0) {
		if(!this.gfn_message("comm.행위여부", year+"년도 "+odr+"차수의 총점과 결과 데이터를 모두 초기화 후 엑셀 업로드를 진행합니다.\n계속")) {
			return false;
		}
	}
		
		var arg = {
			targetDs : this.ds_import,
			headSet : false,
			callBack : function () {

				if(this.ds_import.getRowCount() > 1){
					this.ds_ncsScrproExcel.clearData();
					for(var fRow = 7; fRow < this.ds_import.getRowCount(); fRow++) {
						
						var apyexmNo = this.ds_import.getColumn(fRow, "Column0");
						
						// 중복 응시번호 걸러내기
						if(!this.comUtils.isNull(apyexmNo) && this.ds_ncsScrproExcel.findRowExpr("APYEXM_NO = '"+apyexmNo+"'") == -1) {
					
							var nRow = this.ds_ncsScrproExcel.addRow();
 							
 							var	dstnctn = !this.comUtils.isNull(this.ds_import.getColumn(fRow, "Column13")) ? this.ds_import.getColumn(fRow, "Column13") : "";	// 판별

							this.ds_ncsScrproExcel.setColumn(nRow, "EMPMN_YEAR"			 ,year);                                              // 년도
							this.ds_ncsScrproExcel.setColumn(nRow, "ODR_SE"				 ,odr);                            					  // 차수
							this.ds_ncsScrproExcel.setColumn(nRow, "APYEXM_NO"           ,apyexmNo);          							      // 수험번호
							this.ds_ncsScrproExcel.setColumn(nRow, "CNFDNC"              ,this.ds_import.getColumn(fRow, "Column3"));          // 신뢰도
							this.ds_ncsScrproExcel.setColumn(nRow, "FLS_RESPNS"          ,this.ds_import.getColumn(fRow, "Column4"));          // 허구반응
							this.ds_ncsScrproExcel.setColumn(nRow, "CHARCT_SCORE"        ,this.ds_import.getColumn(fRow, "Column5"));          // 인성점수
							this.ds_ncsScrproExcel.setColumn(nRow, "CALM_SCORE"          ,this.ds_import.getColumn(fRow, "Column6"));          // 침착성
							this.ds_ncsScrproExcel.setColumn(nRow, "APPRSL"              ,this.ds_import.getColumn(fRow, "Column7"));          // 감정
							this.ds_ncsScrproExcel.setColumn(nRow, "EMT"                 ,this.ds_import.getColumn(fRow, "Column8"));          // 정서
							this.ds_ncsScrproExcel.setColumn(nRow, "DTY_SCORE"           ,this.ds_import.getColumn(fRow, "Column9"));          // 직무점수
