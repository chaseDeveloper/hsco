this.ds_ctsnnList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var sttus = obj.getOrgColumn(e.newrow, "PROGRS_STTUS");
	
	if(sttus == "002" || sttus == "003") {
		this.div_work.cal_ctsnnBgnDe.set_readonly(true);
		this.div_work.cal_ctsnnEndDe.set_readonly(true);
		this.div_work.div_sear.edt_applcntEmpNo.set_readonly(true);
