/*
 * 함수명  	: grd_edcctExcclc_onexpanddown
 * 설명 		: 교육비 정산 그리드 onexpanddown 이벤트 함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_edcctExcclc_onexpanddown
 */
this.grd_edcctExcclc_onexpanddown = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	var pRow  = this.ds_edcList.rowposition;
	var enableAt  = this.ds_edcList.getColumn(pRow, "EXCCLC_CONFM_AT");	// 교육담당자 정산 승인여부
	var cnfirmAt  = this.ds_edcList.getColumn(pRow, "CNFIRM_AT");		// 팀장 정산 승인여부
	var exxcclcAt = this.ds_edcList.getColumn(pRow, "EXCCLC_AT");		// 정산 여부
	
	if(pRow < 0) {
		this.gfn_message("info.처리불가", "선택된 교육이 존재하지 않습니다.");
		return false;
	}
	if(enableAt == '1' || cnfirmAt == '1' || exxcclcAt == '1') {
		this.gfn_message("info.처리불가", "승인 또는 정산이 완료된 교육은 수정할 수 없습니다.");
		return false;
	}
	
	if(e.cell == 1) {		// 예산계정선택
		var pRow = this.ds_edcctExcclc.rowposition;
		var pDeptCode = this.ds_cond.getColumn(pRow, "DEPT_CODE");
		var pDeptNm   = this.ds_cond.getColumn(pRow, "DEPT_NM");
		
		var parm = {
			 arg_0      : this
			,deptCode	: pDeptCode
			,deptNm		: pDeptNm
		};
		
		this.gfn_popup("HRM080306_P01", 1024, 500, "", parm, "mis_hrm::HRM080306_P01.xfdl", "fn_popupCallBack", true);
	}
}

/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.fn_popupCallBack = function(strID, variant)
{
	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(variant);
	
	// 값이 존재하지 않을 경우에는 아무런 값을 덮어쓰지 않음
	if(!loadCnt) {
		return false;
	}
	
	var pRow = this.ds_edcctExcclc.rowposition;
		switch(strID) {
			// 예산계정선택
		case "HRM080306_P01":
			this.ds_edcctExcclc.setColumn(pRow, "BUDGET_YEAR"      , dsTemp.getColumn(0, "YEAR"));
			this.ds_edcctExcclc.setColumn(pRow, "BSNS_SE"          , dsTemp.getColumn(0, "BSNS_SE"));
			this.ds_edcctExcclc.setColumn(pRow, "BALC_SE"          , dsTemp.getColumn(0, "BALC_SE"));
			this.ds_edcctExcclc.setColumn(pRow, "BUDGET_ACNT_CODE" , dsTemp.getColumn(0, "BUDGET_ACNT_CODE"));
			this.ds_edcctExcclc.setColumn(pRow, "BUDGET_ACNT_NM"   , dsTemp.getColumn(0, "BUDGET_ACNT_NM"));
			this.ds_edcctExcclc.setColumn(pRow, "BUDGET_DEPT_CODE" , dsTemp.getColumn(0, "DEPT_CODE"));
			this.ds_edcctExcclc.setColumn(pRow, "BUDGET_DEPT"	   , dsTemp.getColumn(0, "DEPT_NM"));
			break;
		case "popEdcTy":	// 교육유형(조회조건)
			var sn         = dsTemp.getColumn(0, "SN");
 			var edcTyLclas = dsTemp.getColumn(0, "EDC_TY_LCLAS");
 			var edcTyMlsfc = dsTemp.getColumn(0, "EDC_TY_MLSFC");
 			var edcTySclas = dsTemp.getColumn(0, "EDC_TY_SCLAS");
 			var edcTyStr = edcTyLclas + "-" + edcTySclas;
 			
 			this.ds_cond.setColumn(0, "EDC_TY_SN", sn      );
 			this.ds_cond.setColumn(0, "EDC_TY_NM", edcTyStr);
			break;
		case "popEdcCl":	// 교육분류(조회조건)
			var sn          = dsTemp.getColumn(0, "SN");
 			var abilitySeNm = dsTemp.getColumn(0, "ABILITY_SE_NM");
 			var abilityNm   = dsTemp.getColumn(0, "ABILITY_NM");
 			var edcClStr = abilitySeNm + "-" + abilityNm;
 			
 			this.ds_cond.setColumn(0, "EDC_CL_SN", sn      );
 			this.ds_cond.setColumn(0, "EDC_CL_NM", edcClStr);
			break;
		case "expDsnPopup":
			this.fn_search();	// 교육비정산 팝업에서 저장이 일어나므로, 팝업 종료시 강제 재조회
			break;
	}
}

