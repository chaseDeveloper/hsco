this.fn_cal_Amt = function(Dataset, EventSe){
	var temp1 = Dataset.getCaseSum("EDCCT_SE == '003'", "USE_AM");
	var temp2 = Dataset.getCaseSum("EDCCT_SE == '004'", "USE_AM");
	var temp3 = Dataset.getCaseSum("EDCCT_SE == '005'", "USE_AM");
	var temp4 = Dataset.getCaseSum("EDCCT_SE == '006'", "USE_AM");
	
	if(EventSe == 'on'){
		this.ds_edcctExcclc.setColumn(0, "SUM_DE", this.ds_edcctExcclc.getColumn(0, "SUM_DE") + temp1);
		this.ds_edcctExcclc.setColumn(0, "SUM_MEAL", this.ds_edcctExcclc.getColumn(0, "SUM_MEAL") + temp2);
		this.ds_edcctExcclc.setColumn(0, "SUM_TRNSPORT", this.ds_edcctExcclc.getColumn(0, "SUM_TRNSPORT") + temp3);
		this.ds_edcctExcclc.setColumn(0, "SUM_STAYNG", this.ds_edcctExcclc.getColumn(0, "SUM_STAYNG") + temp4);
		this.ds_edcctExcclc.setColumn(0, "EXCCLC_AM", this.ds_edcctExcclc.getColumn(0, "EXCCLC_AM") + temp1 + temp2 + temp3 + temp4);
	} else {
		this.ds_edcctExcclc.setColumn(0, "SUM_DE", this.ds_edcctExcclc.getColumn(0, "SUM_DE") - temp1);
		this.ds_edcctExcclc.setColumn(0, "SUM_MEAL", this.ds_edcctExcclc.getColumn(0, "SUM_MEAL") - temp2);
		this.ds_edcctExcclc.setColumn(0, "SUM_TRNSPORT", this.ds_edcctExcclc.getColumn(0, "SUM_TRNSPORT") - temp3);
		this.ds_edcctExcclc.setColumn(0, "SUM_STAYNG", this.ds_edcctExcclc.getColumn(0, "SUM_STAYNG") - temp4);
		this.ds_edcctExcclc.setColumn(0, "EXCCLC_AM", this.ds_edcctExcclc.getColumn(0, "EXCCLC_AM") - ( temp1 + temp2 + temp3 + temp4));
	}
}

