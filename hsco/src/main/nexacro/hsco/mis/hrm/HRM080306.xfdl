/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if(   !Ex.util.isUpdated(this.ds_edcList)
	   && !Ex.util.isUpdated(this.ds_edcctExcclc)
	   && !Ex.util.isUpdated(this.ds_cprCardUseDtls)
	   && !Ex.util.isUpdated(this.ds_rcipt)
	   && !Ex.util.isUpdated(this.ds_fxamtPymnt)) {
		return false;
	}
	
	if(!this.gfn_message("confirm.변경.취소.여부")) {
		return false;	// 아니오
	}
	
	this.ds_edcList.reset();
	this.ds_edcctExcclc.reset();
	this.ds_cprCardUseDtls.reset();
	this.ds_rcipt.reset();
	this.ds_fxamtPymnt.reset();
	
	this.ds_edcList.applyChange();
	this.ds_edcctExcclc.applyChange();
	this.ds_cprCardUseDtls.applyChange();
	this.ds_rcipt.applyChange();
	this.ds_fxamtPymnt.applyChange();
}


/**********************************************************************
* Dataset 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: ds_edcList_onrowposchanged
 * 설명 		: 교육목록 행 변경시 이벤트 처리
 * params  		: none
 * return Type 	: none
 * 사용법		: this.ds_edcList_onrowposchanged(obj, e)
 */
this.ds_edcList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_edcctExcclc.clearData();
	if(e.newrow > -1) {
		this.ds_condSub.clearData();
		var nRow = this.ds_condSub.addRow();
		var year  = obj.getColumn(e.newrow, "YEAR");
		var empno = obj.getColumn(e.newrow, "EMPNO");
		var sn    = obj.getColumn(e.newrow, "SN");
		var edcTy = obj.getColumn(e.newrow, "EDC_TY");
		
		this.ds_condSub.setColumn(nRow, "YEAR"  , year);
		this.ds_condSub.setColumn(nRow, "EMPNO" , empno);
		this.ds_condSub.setColumn(nRow, "SN"    , sn);
		this.ds_condSub.setColumn(nRow, "EDC_TY", edcTy);
		
		this.ds_edcctExcclc.addRow();
		this.ds_edcctExcclc.copyRow(0, this.ds_edcList, e.newrow);
		this.ds_edcctExcclc.applyChange();
		
		this.ds_condFileT4.clearData();
		this.ds_condFileT4.addRow();
		this.ds_condFileT4.setColumn(0, "FILE_SN", obj.getColumn(e.newrow, "EDCCT_FILE_SN"));
		this.fn_transaction("selectFileT4");
	} else {
		this.ds_cprCardUseDtls.clearData();
		this.ds_rcipt.clearData();
		this.ds_fxamtPymnt.clearData();
	}
	
	if(obj.getRowCount() == 0) {
		this.btn_decsn.set_visible(false);
	}
}

/*
 * 함수명  	: ds_rcipt_onrowposchanged
 * 설명 		: (현금)영수증 사용내역 행 변경시 이벤트 처리
 * params  		: none
 * return Type 	: none
 * 사용법		: this.ds_rcipt_onrowposchanged(obj, e)
 */
this.ds_rcipt_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_fileList.clearData();
	
	if(e.newrow > -1) {
		var fileSn = obj.getColumn(e.newrow, "FILE_SN");
		
		if(!this.comUtils.isNull(fileSn)) {
			this.ds_condFile.setColumn(0, "FILE_SN", fileSn);
			this.fn_transaction("selectFileList");  // 첨부파일 조회
		}
	}
}


