 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{	
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {	
 			case "selectTbhrmHrMastrList": // 인사마스터 기본정보 불러오기
				this.ds_cond02.clearData();
				var nRow = this.ds_cond02.addRow();
				this.ds_cond02.setColumn(nRow,"EMPNO",this.ds_empInfo.getColumn(this.ds_empInfo.rowposition,"EMPNO")); 
				this.ds_cond02.setColumn(nRow,"SALARY_SE",this.ds_empInfo.getColumn(this.ds_empInfo.rowposition,"SALARY_SE")); 
				this.ds_cond02.setColumn(nRow,"SRCLS",this.ds_empInfo.getColumn(this.ds_empInfo.rowposition,"SRCLS")); 
				this.ds_cond02.setColumn(nRow,"PAY_CLSF_SE",this.ds_empInfo.getColumn(this.ds_empInfo.rowposition,"PAY_CLSF_SE")); 
				this.ds_cond02.setColumn(nRow,"JSSFC_SE",this.ds_empInfo.getColumn(this.ds_empInfo.rowposition,"JSSFC_SE")); 
				
				if(this.ds_fileList.getRowCount() > 0) {
					this.div_work.img_photo.set_image("/hsco/cmm/download/viewImage.do" + "?FILE_ORDR=1&FILE_SN=" + this.ds_empInfo.getColumn(0, "FILE_SN"));
				}
				
				this.fn_transaction("selectTbhrmHrMastrSalR");
 				break;	
 			case "selectTbhrmHrMastrSalR": // 인사마스터 기본정보(급여관련) 불러오기
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"SALARY_PYMNT_BANK_SE",this.ds_sal.getColumn(0,"SALARY_PYMNT_BANK_SE"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"SALARY_PYMNT_BANK_ACOUNT",this.ds_sal.getColumn(0,"SALARY_PYMNT_BANK_ACOUNT"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"ALLWNC_PYMNT_BANK_SE",this.ds_sal.getColumn(0,"ALLWNC_PYMNT_BANK_SE"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"ALLWNC_PYMNT_BANK_ACOUNT",this.ds_sal.getColumn(0,"ALLWNC_PYMNT_BANK_ACOUNT"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"BSLRY",this.ds_sal.getColumn(0,"BSLRY"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"AMOUNT",this.ds_sal.getColumn(0,"AMOUNT"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"RETIRE_ANNTY_TY_SE",this.ds_sal.getColumn(0,"RETIRE_ANNTY_TY_SE"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"RFS_BANK_SE",this.ds_sal.getColumn(0,"RFS_BANK_SE"));
				this.ds_empInfo.setColumn(this.ds_empInfo.rowposition,"RFS_SBSCRB_DE",this.ds_sal.getColumn(0,"RFS_SBSCRB_DE"));
				
				this.ds_empInfo.set_updatecontrol(false);
				this.ds_empInfo.setRowType(this.ds_empInfo.rowposition, Dataset.ROWTYPE_NORMAL);
				this.ds_empInfo.set_updatecontrol(true);
				this.fn_tabSearch();
 				break;	
  			case "selectHrEtcList": // 인사마스터 기본사항 탭 불러오기
				if(this.ds_tbhrmHrEtc.rowcount==0){
					var nRow = this.ds_tbhrmHrEtc.addRow();
					this.ds_tbhrmHrEtc.setColumn(nRow,"EMPNO",this.ds_empInfo.getColumn(this.ds_empInfo.rowposition,"EMPNO")); 
					this.ds_tbhrmHrEtc.set_updatecontrol(false);
					this.ds_tbhrmHrEtc.setRowType(this.ds_tbhrmHrEtc.rowposition, Dataset.ROWTYPE_NORMAL);
					this.ds_tbhrmHrEtc.set_updatecontrol(true);
				}
 				break;
 			case "saveTbhrmHrMastrList": // 인사마스터 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
 				break;
		}
	}
}

