/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{

	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_empInfo) && !this.comUtils.isDatasetUpdated(this.ds_tbhrmHrEtc) && !this.comUtils.isDatasetUpdated(this.ds_tbhrmHrFamily) && !this.comUtils.isDatasetUpdated(this.ds_tbhrmHrAcdmcr)&& !this.comUtils.isDatasetUpdated(this.ds_tbhrmHrSocty)&& !this.comUtils.isDatasetUpdated(this.ds_tbhrmHrCareer)&& !this.comUtils.isDatasetUpdated(this.ds_tbhrmHrQualf)) {
		this.gfn_message("comm.데이터.변경.없음"); //저장할내역이없습니다.
		return false;
	}
	// validation 체크(기본정보)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_empInfo, this.ds_validation)) {
		return;
	}
	// validation 체크(기본사항탭)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_tbhrmHrEtc, this.ds_validation)) {
		return;
	}
	
	var email = this.div_work.edt_CTTPC_EMAIL.value;
	if(!this.comUtils.isEmail(email)){
		this.gfn_message("errors.항목.값.무효","이메일 형식");
		return false;
	}
	
	var phone = this.div_work.edt_CTTPC_MOBLPHON.value;
	if(!this.comUtils.isPhone("HP_NO", phone)){
		this.gfn_message("errors.항목.값.무효","휴대폰번호 형식");
		return false;
	}
	
	var telno = this.div_work.edt_CTTPC_TELNO.value;
	if(!this.comUtils.isPhone("TEL_NO", telno)){
		this.gfn_message("errors.항목.값.무효","전화번호 형식");
		return false;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.fn_transaction("saveTbhrmHrMastrList");
	}
}
