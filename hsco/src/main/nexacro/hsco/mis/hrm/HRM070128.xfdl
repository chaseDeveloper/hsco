	this.ds_hour.clearData();
	var nRow = 0;
	for(var i=0; i<15; i++) {
		nRow = this.ds_hour.addRow();
		this.ds_hour.setColumn(nRow, "CODE", i);
		this.ds_hour.setColumn(nRow, "CODE_NM", i+"시간");
	}

}

