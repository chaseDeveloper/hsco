/*
 * 함수명  	: btn_trgterCreat_onclick
 * 설명 		: 기초자료생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_trgterCreat_onclick();
 */
this.btn_trgterCreat_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_promtPrsList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	if(!this.ds_cond.getColumn(0, 'APPLC_DE')) {
		this.gfn_message("comm.입력.요청.를", "기준일자");
	}
	else {
		var sToday = this.ds_cond.getColumn(0, "APPLC_DE");
		var msg = ''
		msg += sToday.substr(0, 4) + '년 ';
		msg += sToday.substr(4, 2) + '월 ';
		msg += "일반승급대상자 기초자료를 생성합니다";
		if(this.gfn_message("confirm.삭제.진행", msg)) {
			this.ds_procedure.clearData();
			this.ds_procedure.addRow();
			var IN_INSERT_ID = this.fn_getEmpno();
			var sToday = this.ds_cond.getColumn(0, "APPLC_DE");
			this.ds_procedure.setColumn(0, "IN_INSERT_ID", IN_INSERT_ID);
			this.ds_procedure.setColumn(0, "ARG_TYPE", '000');
			this.ds_procedure.setColumn(0, "ARG_DATE", sToday);
			
			this.fn_transaction("tbhrmTrgterCreat");
		}
	}
}

/*
 * 함수명  	: btn_promtAt_onclick
 * 설명 		: 승급적용
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_promtAt_onclick();
 */
this.btn_promtAt_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var checkField = 0;
	var errMsg = '';
	
	for(var i = 0; i < this.ds_promtPrsList.rowcount; i++) {
		var chk      = this.ds_promtPrsList.getColumn(i, "CHK");
		var nowSrcls = this.ds_promtPrsList.getColumn(i, "NOW_SRCLS");
		
		//결재상태 체크 2016.08.29
		var SANCTN_STS      = this.ds_promtPrsList.getColumn(i, "SANCTN_STS");
		
		if(chk == '1') {
			
			//결재상태 체크 2016.08.29
			/*
			if(SANCTN_STS != "256"){ //완료
				//this.gfn_message("errors.값확인.요청", i+1 + " 행은 전자결재가 완료되지 않았습니다. 결재상태를 확인하세요");
				this.alert(i+1 + " 행은 전자결재가 완료되지 않았습니다. 결재상태를 확인하세요");
				this.ds_promtPrsList.setColumn(i, "CHK",0);
				return;
			}
			*/
			if(this.ds_promtPrsList.getColumn(i, "PROMT_PROCESS_AT") == '1' || nowSrcls == '31' || nowSrcls == '') {
				if(errMsg.length > 0) {
					errMsg += ", " + (i+1);
				}
				else {
					errMsg += (i+1);
				}
			}
			checkField += 1;
		}
	}
	
	if(errMsg.length > 0) {
		this.gfn_message("errors.값확인.요청", errMsg + " 행은 이미 승급 적용되었거나 승급 대상이|적용 항목 혹은 현재 호봉을");
		return false;
	}
	if(errMsg == 0 && checkField > 0) {
		if(this.gfn_message("comm.행위여부", "승급을 적용")) {
			this.fn_transaction("tbhrmPromtAt");
		}
	}
	else if(checkField == 0) {
		this.gfn_message("comm.항목.다중선택.요청", "승급을 적용할");
		return false;
	}
}

