/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "tbhrmTrgterCreat":		// 일반승진대상자 생성
			var sController   	= "/hsco/mis/hrm/HRM050201/tbhrmTrgterCreat.do";
			var sInDatasets   	= "input1=ds_procedure";
			var sOutDatasets  	= "";
			break;
		case "tbhrmPromtPrsList":		// 호봉승급정보 조회
			var sController   	= "/hsco/mis/hrm/HRM050201/tbhrmPromtPrsList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_promtPrsList=output1";
			break;
		case "tbhrmPromtAt":			// 승급적용
			var sController   	= "/hsco/mis/hrm/HRM050201/tbhrmPromtAt.do";
			var sInDatasets   	= "input1=ds_promtPrsList:U";
			var sOutDatasets  	= "";
			break;
		case "tbhrmPromtAtC":			// 승급취소
			var sController   	= "/hsco/mis/hrm/HRM050201/tbhrmPromtAtC.do";
			var sInDatasets   	= "input1=ds_promtPrsList:U";
			var sOutDatasets  	= "";
			break;
		case "tbhrmPromtSave":			// 저장
			var sController   	= "/hsco/mis/hrm/HRM050201/tbhrmPromtSave.do";
			var sInDatasets   	= "input1=ds_promtPrsList:U";
			var sOutDatasets  	= "";
			break;
		
		case "saveSanc":					// 전자결재
			var misKey    = "HRM"
			var iDiv      = "";
			var title     = "";
			var dataTitle = "";
			var dataValue = "";
			// 001 : 기본기안
			iDiv      = "001"
			title     = "호봉승급";
			dataTitle = "호봉승급";
			
			this.ds_sanc.clearData();
			var nRow = this.ds_sanc.addRow();
			this.ds_sanc.setColumn(nRow, "MIS_KEY",    misKey     );
			this.ds_sanc.setColumn(nRow, "I_DIV",      iDiv       );
			this.ds_sanc.setColumn(nRow, "TITLE",      title      );
			this.ds_sanc.setColumn(nRow, "DATA_TITLE", dataTitle  );
			this.ds_sanc.setColumn(nRow, "DATA_VALUE", null  );
			this.ds_sanc.setColumn(nRow, "DEPT_CD",    this.gfn_getDeptId() );
			this.ds_sanc.setColumn(nRow, "EMP_CD",     this.fn_getEmpno() );
			
			var sController   	= "hsco/mis/hrm/HRM050201/saveSanc.do";
			var sInDatasets   	= "input1=ds_sanc";
			var sOutDatasets  	= "ds_sanc=output1";
			break;
			
			
		case "approve":	// 전자결재 2016.08.29
			var sController   	= "hsco/mis/hrm/HRM050201/approve.do";	
			var sInDatasets   	= "input1=ds_approve input2=ds_promtPrsList";
			var sOutDatasets  	= "ds_approve=output1";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "tbhrmTrgterCreat":				// 일반승진대상자 생성
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "tbhrmPromtPrsList":				// 조회(호봉승급정보)
				if(this.saveRow > -1){
					this.ds_promtPrsList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "tbhrmPromtAt":					// 승급적용
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "tbhrmPromtAtC":					// 승급취소
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "tbhrmPromtSave":					// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "saveSanc":
				var oUrl = this.ds_sanc.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
				break;
				
			case "approve": // 전자결재 2016.08.29
				
				var oUrl = this.ds_approve.getColumn(0, "O_URL");				
				//system.execBrowser(oUrl);
				this.gfn_exportExcel(this.div_work.grd_promtPrsList, "promtPrsList", oUrl);
 				
 				break;
		}
	}
}

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "empnoPop") {
		var dsEmpNo = new Dataset();
		var loadCnt = dsEmpNo.loadXML(sReturn);
		if(loadCnt) {
			
			var pplRow   = this.ds_promtPrsList.rowposition;
			var empNm    = dsEmpNo.getColumn(0, "EMPNM");
			var nowSrcls = dsEmpNo.getColumn(0, "SRCLS");
			// 호봉(승급전) 값이 없으면 0으로
			nowSrcls = nexacro.toNumber(!this.comUtils.isNull(nowSrcls) ? nowSrcls : 0);
			var deptCode = dsEmpNo.getColumn(0, "DEPT_CODE");
			var deptNm   = dsEmpNo.getColumn(0, "DEPT_NM");
			var empNo    = dsEmpNo.getColumn(0, "EMPNO");
			var jssfcSe  = dsEmpNo.getColumn(0, "JSSFC_SE");
			var clsfCode = dsEmpNo.getColumn(0, "CLSF_SE");
			var ofcpsSe  = dsEmpNo.getColumn(0, "OFCPS");
			var rspofcSe = dsEmpNo.getColumn(0, "RSPOFC_SE");
			var ecnyDe   = dsEmpNo.getColumn(0, "ECNY_DE");
			var cnwkYycnt        = dsEmpNo.getColumn(0, "CNWK_YYCNT");				// 근속년수
			var cnwkyycntStdrMt  = dsEmpNo.getColumn(0, "CNWK_YYCNT_STDR_MT");		// 근속기준월
			var srclsPromtStdrMt = dsEmpNo.getColumn(0, "SRCLS_PROMT_STDR_MT");		// 호봉승급기준월
			var cnwkyycntStdrYy  = dsEmpNo.getColumn(0, "CNWK_YYCNT_STDR_YY");		// 근속기준년
			var srclsPromtStdrYy = dsEmpNo.getColumn(0, "SRCLS_PROMT_STDR_YY");		// 호봉승급기준년
			cnwkYycnt        = !this.comUtils.isNull(cnwkYycnt) ? cnwkYycnt+"년" : "";
			cnwkyycntStdrMt  = !this.comUtils.isNull(cnwkyycntStdrMt) ? cnwkyycntStdrMt+"월" : "";
			srclsPromtStdrMt = !this.comUtils.isNull(srclsPromtStdrMt) ? srclsPromtStdrMt+"월" : "";
			cnwkyycntStdrYy  = !this.comUtils.isNull(cnwkyycntStdrYy) ? cnwkyycntStdrYy+"년" : "";
			srclsPromtStdrYy = !this.comUtils.isNull(srclsPromtStdrYy) ? srclsPromtStdrYy+"년" : "";
			
			var applcDe  = this.ds_cond.getColumn(0, "APPLC_DE");
			var sToday   = this.gfn_today();
			var promtSrcls = (parseInt(nowSrcls) + 1).toString();
			//promtSrcls = promtSrcls.padLeft(2, "0");
			
			var findRow = this.ds_promtPrsList.findRow("EMPNO", empNo);
			if(findRow > -1) {
				var errMsg = "해당연월에 중복된 사원이 존재합니다";
				errMsg    += "\n";
				errMsg    += "(" + deptNm + " " + empNm + ")";
				alert(errMsg);
				return false;
			}
			
			this.ds_promtPrsList.setColumn(pplRow, "DEPT_CODE"       , deptCode);			// 소속코드
			this.ds_promtPrsList.setColumn(pplRow, "DEPT_NM"         , deptNm);				// 소속명
			this.ds_promtPrsList.setColumn(pplRow, "EMPNO"           , empNo);				// 사번
			this.ds_promtPrsList.setColumn(pplRow, "EMPNM"           , empNm);				// 사원명
			this.ds_promtPrsList.setColumn(pplRow, "JSSFC_SE"        , jssfcSe);			// 직종
			this.ds_promtPrsList.setColumn(pplRow, "CLSF_SE"         , clsfCode);			// 직급
			this.ds_promtPrsList.setColumn(pplRow, "OFCPS_SE"        , ofcpsSe);			// 직위
			this.ds_promtPrsList.setColumn(pplRow, "RSPOFC_SE"       , rspofcSe);			// 직책
			this.ds_promtPrsList.setColumn(pplRow, "NOW_SRCLS"       , nowSrcls);			// 현호봉
			this.ds_promtPrsList.setColumn(pplRow, "ECNY_DE"         , ecnyDe);				// 입사일
			this.ds_promtPrsList.setColumn(pplRow, "PROMT_PROCESS_DE", applcDe + "01"); 	// 승급처리일자
			this.ds_promtPrsList.setColumn(pplRow, "PROMT_SRCLS"     , promtSrcls); 		// 승급호봉
			this.ds_promtPrsList.setColumn(pplRow, "NEXT_STEP"       , promtSrcls); 		// 최소승급호봉
			//this.ds_promtPrsList.setColumn(pplRow, "PROMT_SE"        , "001");			// 승급구분
			var sPromtSe = this.ds_cond.getColumn(0, "PROMT_SE");
			this.ds_promtPrsList.setColumn(pplRow, "PROMT_SE"        , sPromtSe);			// 승급구분 mod.
			this.ds_promtPrsList.setColumn(pplRow, "PROMT_DE"        , sToday);				// 승급일
			this.ds_promtPrsList.setColumn(pplRow, "CNWK_YYCNT"          , cnwkYycnt); 			// 근속년수
			this.ds_promtPrsList.setColumn(pplRow, "CNWK_YYCNT_STDR_MT"  , cnwkyycntStdrMt); 	// 근속기준월
			this.ds_promtPrsList.setColumn(pplRow, "SRCLS_PROMT_STDR_MT" , srclsPromtStdrMt); 	// 호봉승급기준월
			this.ds_promtPrsList.setColumn(pplRow, "CNWK_YYCNT_STDR_YY"  , cnwkyycntStdrYy); 	// 근속기준년
			this.ds_promtPrsList.setColumn(pplRow, "SRCLS_PROMT_STDR_YY" , srclsPromtStdrYy); 	// 호봉승급기준년		
		}
	}
}

/**********************************************************************
* Transaction 영역 끝
***********************************************************************/

/**********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
***********************************************************************
*
* 1. Grid 관련 함수 처리
* 2. 공통함수 처리
* 3. 사용자 정의 함수 처리
* 
***********************************************************************/

/**********************************************************************
* Grid 관련 함수 처리
***********************************************************************/

/*
 * 함수명  	: div_work_grd_promtPrsList_onexpandup
 * 설명 		: Grid expand 이미지 클릭 후 이벤트 처리(승급대상자 정보)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_work_grd_promtPrsList_onexpandup(obj,e);
 */
this.div_work_grd_promtPrsList_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(e.cell == '3') {
		var nLeft    = system.clientToScreenX(this, 10);
		var nTop     = system.clientToScreenY(this, 10);
		var newChild = new ChildFrame;
		newChild.init("empnoPop", "absolute", nLeft, nTop, 400, 400, null, null , "com::empno_P01.xfdl");
		newChild.set_openalign("center middle");
		newChild.set_dragmovetype("all");
		newChild.showModal(this.getOwnerFrame(), {deptCode:"",deptNm:"",empNo:"",empNm:"",jssfcSe:"",clsfCode:"",ofcpsSe:"",rspofcSe:"",nowSrcls:"",ecny_de:""}, this, "fn_popupCallBack");
	}
}

 /*
 * 함수명  	: div_work_grd_rwardCnferList_oncellclick
 * 설명 		: 체크 전체해제
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_work_grd_rwardCnferList_oncellclick();
 */
this.div_work_grd_promtPrsList_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.cell == 0) {
		obj.setCellProperty("Head", e.cell, "text", "0");
	}
}

/*
* 함수명		: div_work_grd_promtPrsList_onheadclick
* 설명			: 헤드클릭 정렬, 체크박스 전체전택
* params		: None
* return Type	: None
* 사용법		: this.div_work_grd_promtPrsList_onheadclick();
*/
this.div_work_grd_promtPrsList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
	}
	this.gfn_gridSort(obj, e);
}

/**********************************************************************
* 공통함수 처리
***********************************************************************/

 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
    var sPromtSe = this.ds_cond.getColumn(0, "PROMT_SE");
	if(this.comUtils.isNull(sPromtSe)) {
		this.gfn_message("info.처리불가","승급구분을 선택해 주시기바랍니다.");
		return false;
	} else if(sPromtSe != '001') {
		this.gfn_message("errors.값확인.요청", "승급구분이 '호봉획정(수시)'가|승급구분을");
		return false;
	}
	
	var nRow = this.ds_promtPrsList.addRow();
	this.ds_promtPrsList.setColumn(nRow, "PROMT_SE", sPromtSe);	
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	var errMsg = "";
	
	var cnt = this.ds_promtPrsList.getCaseCount("CHK == '1'");
	if(cnt == 0) {
		this.gfn_message("info.처리불가","선택된 데이터가 없습니다.");
		return false;
	}
	
	for(var i = 0; i < this.ds_promtPrsList.rowcount; i++) {
		var chk   = this.ds_promtPrsList.getColumn(i, "CHK");
		var sttus = this.ds_promtPrsList.getColumn(i, "PROMT_PROCESS_AT");
		
		if(chk == '1') {
			if(sttus == 1) {
				if(errMsg.length > 0) {
					errMsg += ", " + (i+1);
				}
				else {
					errMsg += (i+1);
				}
			}
		}
	}
	
	if(errMsg.length > 0) {
		this.gfn_message("fail.삭제.불가", errMsg + "행은 이미 승급 적용되어");
		return false;
	}
	else {	
		this.gfn_delMultiRow(this.ds_promtPrsList);
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if(this.gfn_message("confirm.취소여부")) {
		this.ds_promtPrsList.reset();		//DATASET ROLLBACK
		this.ds_promtPrsList.applyChange();	//DATASET COMMIT
		return;
	}
}


/***********************************************************************
* 사용자 정의 함수
************************************************************************/

/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 승급구분에 따른 기초자료생성 버튼 조작
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged();
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(!(this.comUtils.isNull(e.newvalue) && this.comUtils.isNull(e.oldvalue))) {
		this.fn_search();
	}
}
