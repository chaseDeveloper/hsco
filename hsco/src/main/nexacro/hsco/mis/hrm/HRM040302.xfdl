 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_gnfdRegist_onclick();
 */
this.btn_gnfdRegist_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_dscplDspsList.rowposition;
	var dcsnDe1 = this.ds_dscplDspsList.getColumn(nRow, "DSCPL_DCSN_DE");
	var dcsnDe2 = this.ds_dscplDspsList.getColumn(nRow, "RETRIAL_DCSN_DE");
	
	if(dcsnDe1 || dcsnDe2) {
		if(this.gfn_message("comm.행위여부", "징계를 발령등록")) {
			var dsTemp = new Dataset();
			dsTemp.copyData(this.ds_dscplDspsList);
			dsTemp.clearData();
			var nRow = dsTemp.addRow();
			dsTemp.copyRow(nRow, this.ds_dscplDspsList, this.ds_dscplDspsList.rowposition);
			
			var pageUrl = "mis_hrm::HRM030201.xfdl";
			var param = dsTemp.saveXML();
			
			this.gfn_Link2Page(pageUrl, param);
		}
		else {
			return false;
		}
	}
	else {
		this.gfn_message("info.처리불가", "확정되지 않은 징계는 발령등록할 수 없습니다.");
