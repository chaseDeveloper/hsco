 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback2();
 */
this.fn_callback2 = function(sPopupId, sReturn)
{
	if(sPopupId == "Popup1")
	{
		this.ds_rduct.clearData();
        nRowCnt = this.ds_rduct.loadXML(sReturn);
        if(!nRowCnt){
            this.ds_rduct.addRow();
        } else {
