/*
 * 함수명  	: ds_dscplDspsList_oncolumnchanged
 * 설명 		: 확정징계 관련 정보 / 재심확정 관련 정보 자동 입력
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_dscplDspsList_oncolumnchanged();
 */
this.ds_dscplDspsList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var cRowp = this.ds_dscplDspsList.rowposition;
		
	// 확정징계구분, 재심징계구분, 금품수수  변경시 감점변경
	if(e.columnid == "DCSN_DSCPL_SE" || e.columnid == "RECV_AT" || e.columnid == "RETRIAL_DSCPL_SE"){
		if(this.ds_dscplDspsList.getColumn(cRowp, "DCSN_DSCPL_SE")){
			var dscplStdrList_rowp = this.ds_dscplStdrList.findRow("DSLRC_CODE", e.columnid == "RETRIAL_DSCPL_SE" ? this.ds_dscplDspsList.getColumn(cRowp, "RETRIAL_DSCPL_SE") : this.ds_dscplDspsList.getColumn(cRowp, "DCSN_DSCPL_SE"));
			this.ds_dscplDspsList.setColumn(cRowp, "DMRTR_SCORE"       ,this.ds_dscplStdrList.getColumn(dscplStdrList_rowp, "DMRTR_SCORE"));
			this.ds_dscplDspsList.setColumn(cRowp, "DMRTR_TMLMT_YYCNT" ,this.ds_dscplDspsList.getColumn(cRowp, "RECV_AT") == "1" ? this.ds_dscplStdrList.getColumn(dscplStdrList_rowp, "DMRTR_TMLMT_YYCNT_RECV") : this.ds_dscplStdrList.getColumn(dscplStdrList_rowp, "DMRTR_TMLMT_YYCNT_BASS"));
		}
	}
	
	// 징계확정일자 - 감점적용일자
	if(e.columnid == "DSCPL_DCSN_DE"){
		this.ds_dscplDspsList.setColumn(cRowp, "DMRTR_APPLC_DE", this.ds_dscplDspsList.getColumn(cRowp, "DSCPL_DCSN_DE"));
	}
	
	// 사원정보 셋팅
	if(e.columnid == "EMPNO"){
		var deptNm  = this.ds_empno.getColumn(this.ds_empno.findRow("EMPNO", e.newvalue), "DEPT_NM");
		var jssfcNm = this.ds_empno.getColumn(this.ds_empno.findRow("EMPNO", e.newvalue), "JSSFC_CLSF");
		var ofcpsNm = this.ds_empno.getColumn(this.ds_empno.findRow("EMPNO", e.newvalue), "OFCPS_NM");
		
		this.ds_dscplDspsList.setColumn(cRowp, "NOW_DEPT_NM", deptNm);
		this.ds_dscplDspsList.setColumn(cRowp, "DSCPL_DEPT_NM", deptNm);
		this.ds_dscplDspsList.setColumn(cRowp, "NOW_JSSFC_CLSF_NM", jssfcNm);
		this.ds_dscplDspsList.setColumn(cRowp, "DSCPL_JSSFC_CLSF_NM", jssfcNm);
		this.ds_dscplDspsList.setColumn(cRowp, "NOW_OFCPS_NM", ofcpsNm);
		this.ds_dscplDspsList.setColumn(cRowp, "DSCPL_OFCPS_NM", ofcpsNm);
	}
}

