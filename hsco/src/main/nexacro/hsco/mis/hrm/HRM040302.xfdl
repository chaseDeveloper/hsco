    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : HRM040302
 * 화면명   : 징계처분관리
 * 화면설명 : 징계처분을 관리한다.
 * 작성일   : 2015.09.09
 * 작성자   : 김형태
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.checkDs = [
  this.ds_dscplDspsList
];
this.saveRow = -1;
/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.HRM040302_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.HRM040302_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();
}

/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.init_Form();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	

}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	var comboParams = [
			["ds_dscplSttusSe"  , "_HRM219", "Y", "A", "B"],		// 징계상태
			["ds_demandDscplSe" , "_HRM220", "Y", "D", "B"]		// 요구징계
		];
	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: 공통 callback
 * params  		: None
 * return Type 	: None 
 * 사용법		: this.fn_callbackAfter();
 */
this.fn_callbackAfter = function ()
{
	this.div_search.div_EMP.fn_setBind("ds_cond", "EMPNO", "EMPNM");
	this.div_search.div_deptCode.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	this.div_work.tab_dtls.tpg_dscplDemand.div_work01.div_EMP.fn_setBind("ds_dscplDspsList", "EMPNO", "EMPNM");
	
	var today = this.gfn_today();
	var startDe = this.dateUtils.addMonth(today, -12, 's');
	this.ds_cond.setColumn(0, "END_DT", today);
	this.ds_cond.setColumn(0, "BGN_DT", startDe);
	this.ds_cond.setColumn(0, "ERSR_AT", "0");
	
	this.fn_transaction("selectDscplStdrList");
}

/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/

/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(징계처분관리)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(징계요구내역)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_dscplDspsList.clearData();
	this.fn_transaction("selectDscplDspsList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{

	for(var i = 0; i < this.ds_dscplDspsList.getRowCount(); i++) {
		var demandDe   = parseInt(this.ds_dscplDspsList.getColumn(i, "DSCPL_DEMAND_DE"));
		var dcsnDe     = parseInt(this.ds_dscplDspsList.getColumn(i, "DSCPL_DCSN_DE"));
		var reRequstDe = parseInt(this.ds_dscplDspsList.getColumn(i, "RETRIAL_REQUST_DE"));
		var reDcsnDe   = parseInt(this.ds_dscplDspsList.getColumn(i, "RETRIAL_DCSN_DE"));
		var retrialDe  = parseInt(this.ds_dscplDspsList.getColumn(i, "RETRIAL_DE"));

		// 데이터셋 수정 여부체크
		if (!this.comUtils.isDatasetUpdated(this.ds_dscplDspsList)) {
			this.gfn_message("comm.데이터.변경.없음");
			return false;
		}
		
		// 징계확정일자와 징계요구일자 비교
		if(dcsnDe) {
			if(demandDe > dcsnDe) {
				this.gfn_message("errors.항목값.범위.이상", "징계확정일자|징계요구일자");
				return false;
			}
		}
		// 재심확정일자와 재심요구일자 비교
		if(reRequstDe) {
			if(reRequstDe < dcsnDe) {
				this.gfn_message("errors.항목값.범위.이상", "재심요구일자|징계확정일자");
				return false;
			}
			if(retrialDe) {
				if(reRequstDe > retrialDe) {
					this.gfn_message("errors.항목값.범위.이상", "재심일자|재심요구일자");
					return false;
				}
			}
			if(reDcsnDe) {
				if(reRequstDe > reDcsnDe) {
					this.gfn_message("errors.항목값.범위.이상", "재심확정일자|재심요구일자");
					return false;
				}
			}
		}
		
		if(this.ds_dscplDspsList.getColumn(i, "RDUCT_AT") == 1){
			if(this.ds_dscplDspsList.getColumn(i, "DSCPL_DCSN_DE") == null || this.ds_dscplDspsList.getColumn(i, "DSCPL_DCSN_DE") == ""){
				this.gfn_message("comm.err", "감경을 입력한 경우 징계확정일자는 필수입니다.");
				return false;
			}
		}
	}

	// validation 체크(징계요구내역)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_dscplDspsList, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_dscplDspsList.rowposition;
		this.fn_transaction("saveDscplDsps");
	}
}

/*
 * 함수명  	: btn_dscplErsr_onclick
 * 설명 		: 징계말소
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_dscplErsr_onclick();
 */
this.btn_dscplErsr_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var errMsg = "";
	var checkField = 0;
	
	for(var i = 0; i < this.ds_dscplDspsList.rowcount; i++)
	{
		var chk = this.ds_dscplDspsList.getColumn(i, "CHK");
		var today = this.gfn_today();
		var dscplErsrDe = this.ds_dscplDspsList.getColumn(i, "DSCPL_ERSR_DE");		//징계말소일자
		var reDscplErsrDe = this.ds_dscplDspsList.getColumn(i, "RE_DSCPL_ERSR_DE");	//재심_징계말소일자
		var date = !this.comUtils.isNull(reDscplErsrDe) ? reDscplErsrDe : dscplErsrDe;
						
		// 정합성 체크(체크된것 중에서, 날짜 갱신할 필요 없는 것들은 제외)
		if(chk == '1') {
			if(!this.comUtils.isNull(date)) {
				// 제약조건 1 : 말소일은 오늘보다 이전이거나 같아야 함
				// 제약조건 2 : 징계일자와 재심일자는 존재해야 함 ==> ??
				var frDate = this.comUtils.stringToDate(today);		// 오늘
				var toDate = this.comUtils.stringToDate(date);		// 말소일
				frDate = (frDate instanceof Date) ? frDate : utils.getDate(frDate);
				toDate = (toDate instanceof Date) ? toDate : utils.getDate(toDate);
								
				if(!(frDate >= toDate)) {
					if(errMsg.length > 0) {
						errMsg += ", " + (i+1);
					}
					else {
						errMsg += (i+1);
					}
				}
			} else {
				if(errMsg.length > 0) {
					errMsg += ", " + (i+1);
				}
				else {
					errMsg += (i+1);
				}
			}
			checkField += 1;
		}
	}
	
	if(errMsg.length > 0) {
		this.gfn_message("errors.값확인.요청", errMsg + " 행은 징계 말소 대상이|다시 한번");
		return false;
	}
	
	// 징계말소
	if(errMsg.length == 0 && checkField > 0) {
		if(this.gfn_message("comm.행위여부", "징계를 말소")) {
		
			this.saveRow = this.ds_dscplDspsList.rowposition;
			this.fn_transaction("ersrU");
		}
		
	}else if(checkField == 0) {
		this.gfn_message("comm.항목.다중선택.요청", "징계를 말소할");
		return false;
	}
}

