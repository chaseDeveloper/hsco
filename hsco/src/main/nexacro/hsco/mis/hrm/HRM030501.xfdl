/*
 * 함수명  	: ds_crtfList_oncolumnchanged
 * 설명 		: 데이터셋 데이터 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_crtfList_oncolumnchanged();
 */
this.ds_crtfList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "OUTPT_MAN_IHIDNUM") {
		var ihidnum = e.newvalue;
		if(ihidnum.indexOf(' ') > -1) {
			alert("입력하신 주민등록번호에 공백이 존재합니다.");
			obj.setColumn(e.row, e.columnid, e.oldvalue);
		}
	}
	
	if(e.columnid == "CRTF_SE") {
		var crtfSe = e.newvalue;
		var crtfNm = this.ds_hrm021.getColumn(this.ds_hrm021.findRow("CODE", crtfSe), "CODE_NM");
		obj.setColumn(e.row, "CRTF_NM", crtfNm);
		
		if(crtfSe == '001'){
			obj.setColumn(e.row, "HFFC_BGNDE", this.ds_empInfo.getColumn(0, "ECNY_DE"));
			obj.setColumn(e.row, "HFFC_ENDDE", this.gfn_today());
			this.div_work.cal_bgnDe.set_cssclass("cal_WF_Essential");
			this.div_work.cal_endDe.set_cssclass("cal_WF_Essential");
		} else if (crtfSe == '002'){
			obj.setColumn(e.row, "HFFC_BGNDE", this.ds_empInfo.getColumn(0, "ECNY_DE"));
			obj.setColumn(e.row, "HFFC_ENDDE", this.ds_empInfo.getColumn(0, "RETIRE_DE"));
			this.div_work.cal_bgnDe.set_cssclass("cal_WF_Essential");
			this.div_work.cal_endDe.set_cssclass("cal_WF_Essential");
		} else {
			obj.setColumn(e.row, "HFFC_BGNDE", this.ds_empInfo.getColumn(0, "ECNY_DE"));
			obj.setColumn(e.row, "HFFC_ENDDE", this.ds_empInfo.getColumn(0, "RETIRE_DE"));
			this.div_work.cal_bgnDe.set_cssclass("");
			this.div_work.cal_endDe.set_cssclass("");
		}
	}
}


 /*
 * 함수명  	: ds_rwardCnferList_onrowposchanged
 * 설명 		: 행 변경시 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_rwardCnferList_onrowposchanged();
 */
this.ds_rwardCnferList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var nRow = this.ds_crtfList.rowposition;
	var nRowType = this.ds_crtfList.getRowType(nRow);
	var confmAt = this.ds_crtfList.getColumn(nRow, "CONFM_AT");
	var crtfSe = this.ds_crtfList.getColumn(nRow, "CRTF_SE");
	
	if(nRowType == 2) {
		this.div_work.cmb_crtfSe.set_readonly(false);
		this.div_work.spn_reqstQy.set_readonly(false);
		this.div_work.edt_presentnOfficNm.set_readonly(false);
		this.div_work.txt_rm.set_readonly(false);
		this.div_work.txt_issuprvonsh.set_readonly(false);
		this.div_work.edt_deptNm.set_readonly(false);
		this.div_work.edt_clsfNm.set_readonly(false);
		this.div_work.edt_ofcpsNm.set_readonly(false);
		this.div_work.cal_bgnDe.set_readonly(false);
		this.div_work.cal_endDe.set_readonly(false);
	} else {
		if(confmAt == '0') {
			this.div_work.cmb_crtfSe.set_readonly(false);
			this.div_work.spn_reqstQy.set_readonly(false);
			this.div_work.edt_presentnOfficNm.set_readonly(false);
			this.div_work.txt_rm.set_readonly(false);
			this.div_work.txt_issuprvonsh.set_readonly(false);
			this.div_work.edt_deptNm.set_readonly(false);
			this.div_work.edt_clsfNm.set_readonly(false);
			this.div_work.edt_ofcpsNm.set_readonly(false);
			this.div_work.cal_bgnDe.set_readonly(false);
			this.div_work.cal_endDe.set_readonly(false);
		} else {
			this.div_work.cmb_crtfSe.set_readonly(true);
			this.div_work.spn_reqstQy.set_readonly(true);
			this.div_work.edt_presentnOfficNm.set_readonly(true);
			this.div_work.txt_rm.set_readonly(true);
			this.div_work.txt_issuprvonsh.set_readonly(true);
			this.div_work.edt_deptNm.set_readonly(true);
			this.div_work.edt_clsfNm.set_readonly(true);
			this.div_work.edt_ofcpsNm.set_readonly(true);
			this.div_work.cal_bgnDe.set_readonly(true);
			this.div_work.cal_endDe.set_readonly(true);
		}
	}
	
	if(crtfSe == '001' || crtfSe == '002'){
		this.div_work.cal_bgnDe.set_cssclass("cal_WF_Essential");
		this.div_work.cal_endDe.set_cssclass("cal_WF_Essential");
	} else {
		this.div_work.cal_bgnDe.set_cssclass("");
		this.div_work.cal_endDe.set_cssclass("");
	}
}

/**********************************************************************
* 공통함수 처리
***********************************************************************/

 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var empno = this.ds_cond.getColumn(0, "APPLCNT_EMPNO");
	
	if(this.comUtils.isNull(empno)) {
		this.gfn_message("comm.항목.입력.을", "사번")
	} else {
		var nRow = this.ds_crtfList.addRow();
		this.ds_crtfList.setColumn(nRow, "APPLCNT_EMPNO", empno);
		this.ds_crtfList.setColumn(nRow, "APPLCNT_EMPNM", this.ds_cond.getColumn(0, "APPLCNT_EMPNM"));
		this.ds_crtfList.setColumn(nRow, "DEPT_NM", this.ds_cond.getColumn(0, "DEPT_NM"));
		this.ds_crtfList.setColumn(nRow, "REQST_YEAR", this.gfn_today().substr(0, 4));
		this.ds_crtfList.setColumn(nRow, "REQST_DE", this.gfn_today());
		this.ds_crtfList.setColumn(nRow, "CRTF_SE", '001');
		this.ds_crtfList.setColumn(nRow, "REQST_QY", 1);
		this.ds_crtfList.setColumn(nRow, "PRPOS_SE", '001');
		this.ds_crtfList.setColumn(nRow, "JSSFC_CLSF"   , this.ds_empInfo.getColumn(0, "JSSFC_CLSF"));
		
		var adres       = this.ds_empInfo.getColumn(0, "ADRES");
		var adresDetail = this.ds_empInfo.getColumn(0, "ADRES_DETAIL");
		
		adres       = this.comUtils.isNull(adres)       ? '' : adres;
		adresDetail = this.comUtils.isNull(adresDetail) ? '' : adresDetail;
		
		this.ds_crtfList.setColumn(nRow, "APPLCNT_ADRES", adres + ' ' + adresDetail);
		this.ds_crtfList.setColumn(nRow, "OUTPT_MAN_NM", this.ds_empInfo.getColumn(0, "EMPNM"));
		this.ds_crtfList.setColumn(nRow, "OUTPT_MAN_NM_CHCRT", this.ds_empInfo.getColumn(0, "NM_CHCRT"));
		this.ds_crtfList.setColumn(nRow, "OUTPT_MAN_IHIDNUM", this.ds_empInfo.getColumn(0, "IHIDNUM_ENCPT"));
		
		this.ds_crtfList.setColumn(nRow, "DEPT_NM", this.ds_empInfo.getColumn(0, "DEPT_NM"));
		this.ds_crtfList.setColumn(nRow, "CLSF_NM", this.ds_empInfo.getColumn(0, "CLSF_NM"));
		this.ds_crtfList.setColumn(nRow, "OFCPS_NM", this.ds_empInfo.getColumn(0, "OFCPS_NM"));
		this.ds_crtfList.setColumn(nRow, "HFFC_BGNDE", this.ds_empInfo.getColumn(0, "ECNY_DE"));
		this.ds_crtfList.setColumn(nRow, "HFFC_ENDDE", this.gfn_today());
	}
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	//this.gfn_delMultiRow(this.ds_crtfList);
	var nRow = this.ds_crtfList.rowposition;
	var sConfmAt = this.ds_crtfList.getColumn(nRow, "CONFM_AT"); //승인여부
	
	if (sConfmAt == "1"){
		this.gfn_message("info.처리불가","승인된 자료입니다.삭제할 수 없습니다.");
		return;
	}
	
	if(this.gfn_message("confirm.삭제여부")){
		this.ds_crtfList.deleteRow(nRow);
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_crtfList.reset();		//DATASET ROLLBACK
		this.ds_crtfList.applyChange();	//DATASET COMMIT
		return;
	}
}


/***********************************************************************
* 사용자 정의 함수
************************************************************************/

 /*
 * 함수명  	: fn_print
 * 설명 		: 출력 버튼 클릭시
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
 */
this.fn_print = function()
{
	var nRow = this.ds_crtfList.rowposition;
	if(this.ds_crtfList.getColumn(nRow, "CONFM_AT") != "1") {
		this.gfn_message("info.처리불가","[인사담당자] 승인되지 않았습니다.\n출력할 수 없습니다.");
		return false;
	}
	
	if(this.ds_crtfList.getColumn(nRow, "CNFIRM_AT") != "1") {
		this.gfn_message("info.처리불가","[확인자] 승인되지 않았습니다.\n출력할 수 없습니다.");
		return false;
	}
	
	if(this.dateUtils.format(this.gfn_today(), 'yyyymmdd') > this.ds_crtfList.getColumn(nRow, "OUTPT_TMLMT_DE").substr(0,8)) {
		this.gfn_message("info.처리불가","출력기한이 지났습니다. 출력할 수 없습니다.");
		return false;
	}
	if(this.ds_crtfList.getColumn(nRow, "OUTPT_AT") == '1' ) {
		this.gfn_message("info.처리불가","이미 출력되었습니다.");
		return false;
	}
	
	if(this.div_work.cmb_crtfSe.value!= '001' && this.div_work.cmb_crtfSe.value != '002'){
		this.gfn_message("info.처리불가","재직증명서와 경력증명서만 출력가능합니다.");
		return false;
	}
	
// 	if(this.div_work.cmb_crtfSe.value != '001' && this.ds_crtfList.getColumn(nRow, "APPLCNT_EMPNM") != this.ds_crtfList.getColumn(nRow, "OUTPT_MAN_NM")){
// 		this.gfn_message("info.처리불가","대리신청건은 재직증명서만 출력할 수 있습니다.");
// 		return false;
// 	}
	
	this.ds_condPrint.clearData();
	this.ds_condPrint.addRow();
	
	this.ds_condPrint.setColumn(0, "APPLCNT_EMPNO", this.ds_crtfList.getColumn(nRow, "APPLCNT_EMPNO"));
	this.ds_condPrint.setColumn(0, "REQST_SEQ",     this.ds_crtfList.getColumn(nRow, "REQST_SEQ")    );
	this.ds_condPrint.setColumn(0, "REQST_YEAR",    this.ds_crtfList.getColumn(nRow, "REQST_YEAR")   );
	this.ds_condPrint.setColumn(0, "PRPOS_NM",      this.ds_crtfList.getColumn(nRow, "PRPOS_NM")     );
	this.ds_condPrint.setColumn(0, "CONFM_DE",      this.ds_crtfList.getColumn(nRow, "CONFM_DE")     );
	this.ds_condPrint.setColumn(0, "TODAY",         this.gfn_today()                                 );
	this.ds_condPrint.setColumn(0, "S_EMPNO",       this.fn_getEmpno()                               );
	this.ds_condPrint.setColumn(0, "EMPNM",         this.ds_crtfList.getColumn(nRow, "APPLCNT_EMPNM"));
	this.ds_condPrint.setColumn(0, "OUTPT_MAN_NM",  this.ds_crtfList.getColumn(nRow, "OUTPT_MAN_NM"));
	
	this.ds_crtfList.set_updatecontrol(false);
	this.ds_crtfList.setColumn(nRow, "OUTPT_AT", 1);
	this.ds_crtfList.set_updatecontrol(true);
	
	this.fn_transaction("printReport");
}


/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 사번 변경시 사원정보 세팅
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged();
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == 'APPLCNT_EMPNO') {
		this.fn_transaction("getEmpInfo");
	}
	if(e.columnid == 'DEPT_CODE') {
		this.div_search.div_EMP.fn_set_deptCode(e.newvalue);
		this.ds_cond.setColumn(0, "APPLCNT_EMPNM", "");
		this.ds_cond.setColumn(0, "APPLCNT_EMPNO", "");
	}
}


/*
* 함수명		: div_work_grd_crtfList_onheadclick
* 설명			: 헤드클릭 정렬, 체크박스 전체전택
* params		: None
* return Type	: None
* 사용법		: this.div_work_grd_crtfList_onheadclick();
*/
this.div_work_grd_crtfList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.gfn_gridSort(obj, e);
}

