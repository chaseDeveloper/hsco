    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : HRM040303
 * 화면명   : 재심요청관리
 * 화면설명 : 재심요청을 관리한다.
 * 작성일   : 2015.09.09
 * 작성자   : 김형태
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.checkDs = [
  this.ds_dscplDspsList
];
this.saveRow = -1;
/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.HRM040303_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.HRM040303_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();
}

/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.init_Form();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);



}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	var comboParams = [
			["ds_dscplSttusSe"  , "_HRM219", "1", "전체"],		// 징계상태
			["ds_demandDscplSe" , "_HRM220", "1", "D"]			// 요구징계
		];
	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
	


}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: 공통 callback
 * params  		: None
 * return Type 	: None 
 * 사용법		: this.fn_callbackAfter();
 */
this.fn_callbackAfter = function ()
{
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	
	this.ds_cond.setColumn(0, "EMPNO",  this.fn_getEmpno());
	this.ds_cond.setColumn(0, "EMPNM",  this.fn_getEmpnm());
	this.div_search.div_EMP.fn_setBind("ds_cond", "EMPNO", "EMPNM");
	this.div_work.tab_dtls.tpg_dscplDemand.div_work01.div_EMP.fn_setBind("ds_dscplDspsList", "EMPNO", "EMPNM");


	this.fn_transaction("selectDscplStdrList");
	
	this.fn_search();
}

/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/

/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(징계처분관리)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(징계요구내역)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_dscplDspsList.clearData();
	this.fn_transaction("selectDscplDspsList");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// validation 체크(징계요구내역)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_dscplDspsList, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_dscplDspsList.rowposition;
		this.fn_transaction("saveDscplDsps");
	}
}
/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//  징계코드 목록조회
		case "selectDscplStdrList":		
			var sController   	= "/hsco/mis/hrm/HRM040303/selectDscplStdrList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_dscplStdrList=output1";
			break;
		// 징계처분 목록 조회
		case "selectDscplDspsList":		
			var sController   	= "/hsco/mis/hrm/HRM040303/selectDscplDspsList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_dscplDspsList=output1";
			break;
		// 재심요청저장
		case "saveDscplDsps":		
			var sController   	= "/hsco/mis/hrm/HRM040303/saveDscplDsps.do";
			var sInDatasets   	= "input1=ds_dscplDspsList:U";
			var sOutDatasets  	= "";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			// 징계코드 목록조회
			case "selectDscplStdrList":				
				break;
			// 징계처분 목록 조회
			case "selectDscplDspsList":
				if(this.saveRow > -1){
					this.ds_dscplDspsList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			// 재심요청저장
			case "saveDscplDsps":
				this.fn_search();
				break;
		}
	}
}


/**********************************************************************
* Transaction 영역 끝
***********************************************************************/



/**********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
***********************************************************************
*
* 1. Gird 관련 이벤트 처리
* 2. tab 관련 이벤트 처리
* 3. 공통함수 처리
* 4. 사용자 정의 함수
* 
***********************************************************************/

/**********************************************************************
* Grid 관련 이벤트 처리
***********************************************************************/

/**********************************************************************
* 공통함수 처리
***********************************************************************/
/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_dscplDspsList.reset();			//DATASET ROLLBACK
		this.ds_dscplDspsList.applyChange();	//DATASET COMMIT
		return;
	}
}
/***********************************************************************
* 사용자 정의 함수
************************************************************************/

/*
 * 함수명  	: ds_dscplDspsList_onrowposchanged
 * 설명 		: 재심일자가 있다면 또는 징계확정일자가 없다면  재심요구일자, 재심청구사유 disabled              
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_dscplDspsList_onrowposchanged();
 */
this.ds_dscplDspsList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var cRowp = this.ds_dscplDspsList.rowposition;

	if(this.ds_dscplDspsList.getColumn(cRowp, "RETRIAL_DE") || !this.ds_dscplDspsList.getColumn(cRowp, "DSCPL_DCSN_DE")) {
		this.div_work.tab_dtls.tpg_retrialRequest.div_work01.cal_retrialRequstDe.set_enable(false);
		this.div_work.tab_dtls.tpg_retrialRequest.div_work01.txt_retrialRequstPrvonsh.set_enable(false);
	}else {
		this.div_work.tab_dtls.tpg_retrialRequest.div_work01.cal_retrialRequstDe.set_enable(true);
		this.div_work.tab_dtls.tpg_retrialRequest.div_work01.txt_retrialRequstPrvonsh.set_enable(true);
	}
}

this.div_work_tab_dtls_tpg_dscplDemand_div_work01_edt_rwardDtlsSeq_oneditclick = function(obj:Edit,  e:nexacro.EditClickEventInfo)
{
	
}
]]></Script>
