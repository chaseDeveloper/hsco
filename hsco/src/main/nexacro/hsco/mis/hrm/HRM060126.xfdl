/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	// 공통코드
	/*******************************************************************************
	 * params  		: 1. param_1    : 반환받을 DATASET 명 			(예: DS_AREA_NO_CODE)
					  2. param_2	: 그룹코드						(예 : COM007)
					  3. param_3	: 사용여부		    			(예 : A(전체), Y(사용), N(사용안함))
					  4. param_4	: 콤보박스INDEX(0) 표시		(예 : A(전체), S(선택), E(공백), D(첫번째데이터부터시작) )
					  5. param_5	: 코드+코드명					(예 : G(코드명), B(코드-코드명) )
	 ******************************************************************************/
	
	var comboParams = [
		["ds_htSe", "HRM049", "Y", "D", "B"]		// 반기구분
	];
	
	var callback = function (svcId, errorCode, errorMsg) {
		this.div_search.sta_registText.set_text("");
		
		this.ds_cond.clearData();
		this.ds_cond.addRow();
 		this.ds_cond.setColumn(0, "EVL_YEAR" , this.dateUtils.format(this.gfn_today(),"yyyy"));
 		this.ds_cond.setColumn(0, "HT_SE"    , this.comUtils.getClientDate("MM") <= '06' ? '01' : '02');
		
		// 인사담당자(HRM_USER), 인사결재자("HRM_ADMIN")
		if(this.gfn_authGrpId("HRM_ADMIN") == -1 && this.gfn_authGrpId("HRM_EVL") == -1) {
			// 이때는 담당자 플래그 true로 바꿔서 권한분리 부탁드립니다.	
			this.div_search.edt_empnm.set_readonly(true);
			this.div_search.edt_empno.set_readonly(true);
			this.div_search.edt_empnm.set_cssclass("edt_WF_Readonly");
			this.div_search.edt_empno.set_cssclass("edt_WF_Readonly");
			this.div_search.btn_search.set_visible(false);
		}
		
		this.ds_cond.setColumn(0, "EMPNO", this.gfn_getEmpNo());
		this.ds_cond.setColumn(0, "EMPNM", this.gfn_getUserName());
		
		this.fn_transaction("selectSearchList");		// (검색조건에 해당하는 사원의 가장 마지막 근무평정 기간을 초기 검색조건으로 설정)
	};
	this.gfn_comboLoad (comboParams, callback);
}


/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/


/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(Ex.util.isUpdated(this.ds_distrbOfcrkDtlsList) || Ex.util.isUpdated(this.ds_jobPrtnDtlsAcmsltList) || Ex.util.isUpdated(this.ds_coDvlpCntrbtEffectList)
	|| Ex.util.isUpdated(this.ds_dtyexcPlanList) || Ex.util.isUpdated(this.ds_etcList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}

	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.fn_transaction("selectEmpChk");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 수정가능여부 체크
	if(this.updateYn != 'Y'){
		this.gfn_message("info.처리불가", this.message);
		return;
	}
	
	// 데이터셋 수정 여부체크(업무추진실적 세부내역)
	if (!(this.comUtils.isDatasetUpdated(this.ds_distrbOfcrkDtlsList)
		|| this.comUtils.isDatasetUpdated(this.ds_jobPrtnDtlsAcmsltList)
		|| this.comUtils.isDatasetUpdated(this.ds_coDvlpCntrbtEffectList)
		|| this.comUtils.isDatasetUpdated(this.ds_dtyexcPlanList)
		|| this.comUtils.isDatasetUpdated(this.ds_etcList))) {
		this.gfn_message("comm.데이터.변경.없음")
		return false;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		this.fn_transaction("saveJobPrtnAcmsltList");
	}
}


/*
 * 함수명  	: fn_print
 * 설명 		: 인쇄
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
 */
this.fn_print = function()
{
	if(this.ds_empInfo.rowcount>0){
		this.gfn_ReportPopup(this, "MIS/HRM/HRM060126_R01.crf", this.ds_cond);
	}
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// 최초 조회(검색조건에 해당하는 사원의 가장 마지막 근무평정 기간을 초기 검색조건으로 설정)
		case "selectSearchList":	
			var sController   	= "/hsco/mis/hrm/HRM060126/selectSearchList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_condTemp=output1";
			break;
		// 제외 대상자 목록 조회
		case "selectEmpChk":	
			var sController   	= "/hsco/mis/hrm/HRM060126/selectEmpChk.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_empChk=output1";
			break;
		// 사원정보, 업무추진실적 세부내역 목록 조회
		case "selectJobPrtnAcmsltList":	
			var sController   	= "/hsco/mis/hrm/HRM060126/selectJobPrtnAcmsltList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	=  "ds_acmsltSe=output1";					// 실적구분
			    sOutDatasets  	+= " ds_acmsltPd=output2";					// 실적기간
				sOutDatasets  	+= " ds_empInfo=output3";					// 사원정보
				sOutDatasets  	+= " ds_distrbOfcrkDtlsList=output4";		// 탭1(분장사무 내역(근무부서별))
				sOutDatasets  	+= " ds_jobPrtnDtlsAcmsltList=output5";		// 탭2(업무추진 내역 및 실적)
				sOutDatasets  	+= " ds_coDvlpCntrbtEffectList=output6";	// 탭3(공사발전 기여효과)	
				sOutDatasets  	+= " ds_dtyexcPlanList=output7";			// 탭4(직무수행 계획)
				sOutDatasets  	+= " ds_etcList=output8";					// 탭5(기타)
			break;
		// 업무추진실적 세부내역 저장
		case "saveJobPrtnAcmsltList":
			var sController   	= "/hsco/mis/hrm/HRM060126/saveJobPrtnAcmsltList.do";
			var sInDatasets  	 = "input1=ds_distrbOfcrkDtlsList:U";		// 탭1(분장사무 내역(근무부서별))
				sInDatasets  	+= " input2=ds_jobPrtnDtlsAcmsltList:U";	// 탭2(업무추진 내역 및 실적)
				sInDatasets  	+= " input3=ds_coDvlpCntrbtEffectList:U";	// 탭3(공사발전 기여효과)	
				sInDatasets  	+= " input4=ds_dtyexcPlanList:U";			// 탭4(직무수행 계획)
				sInDatasets  	+= " input5=ds_etcList:U";					// 탭5(기타)
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			// 최초 조회(검색조건에 해당하는 사원의 가장 마지막 근무평정 기간을 초기 검색조건으로 설정)
			case "selectSearchList":
				
				var evlYear    = this.ds_cond.getColumn(0, "EVL_YEAR");
				var htSe = this.ds_cond.getColumn(0, "HT_SE") == "01" ? "02" : "01" ;
				    evlYear = htSe == "01" ? nexacro.toNumber(evlYear)+1 : evlYear;
				var htSe_cRowP = this.ds_htSe.findRow("CODE", htSe);
				var htSeNm       = this.ds_htSe.getColumn(htSe_cRowP, "CODE_NM");
				this.tab_jobPrtnAcmslt.tpg_04.set_text(evlYear+"년도 "+htSeNm+" 직무수행 계획");				
				
				if(this.ds_condTemp.rowcount == 0) {					
					this.updateYn = 'N';
					
					this.gfn_message("info.처리불가","해당하는 근무평가 기간이 없습니다.");
					this.message = "현재 입력된 사원은 대상자가 아닙니다.";
					
					this.tab_jobPrtnAcmslt.tpg_01.grd_distrbOfcrkDtlsList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_02.grd_jobPrtnDtlsAcmsltList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_03.grd_coDvlpCntrbtEffectList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_04.grd_dtyexcPlanList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_05.grd_etcList.set_readonly(true);
					
					return;
				}
				this.ds_cond.setColumn(0, "EVL_YEAR", this.ds_condTemp.getColumn(0, "EVL_YEAR"));
				this.ds_cond.setColumn(0, "HT_SE", this.ds_condTemp.getColumn(0, "HT_SE"));
				
				this.fn_search();
				break;
			// 대상자 목록 조회
			case "selectEmpChk":
				
				var evlYear    = this.ds_cond.getColumn(0, "EVL_YEAR");
				var htSe = this.ds_cond.getColumn(0, "HT_SE") == "01" ? "02" : "01" ;
				    evlYear = htSe == "01" ? nexacro.toNumber(evlYear)+1 : evlYear;
				var htSe_cRowP = this.ds_htSe.findRow("CODE", htSe);
				var htSeNm       = this.ds_htSe.getColumn(htSe_cRowP, "CODE_NM");
				this.tab_jobPrtnAcmslt.tpg_04.set_text(evlYear+"년도 "+htSeNm+" 직무수행 계획");
					
				if(this.ds_empChk.getColumn(0, "EMP_CNT") == 0) {
					this.ds_acmsltPd.clearData();
					this.ds_empInfo.clearData();
					this.ds_distrbOfcrkDtlsList.clearData();
					this.ds_jobPrtnDtlsAcmsltList.clearData();
					this.ds_coDvlpCntrbtEffectList.clearData();
					this.ds_dtyexcPlanList.clearData();
					this.ds_etcList.clearData();
					
					this.div_search.sta_registText.set_text("");
					
					this.updateYn = 'N';
					
					this.gfn_message("info.처리불가","현재 입력된 사원은 대상자가 아닙니다.");
					this.message = "현재 입력된 사원은 대상자가 아닙니다.";
					
					this.tab_jobPrtnAcmslt.tpg_01.grd_distrbOfcrkDtlsList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_02.grd_jobPrtnDtlsAcmsltList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_03.grd_coDvlpCntrbtEffectList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_04.grd_dtyexcPlanList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_05.grd_etcList.set_readonly(true);
					
					return;
				}
				
				this.fn_transaction("selectJobPrtnAcmsltList");
				break;
			// 사원정보, 업무추진실적 세부내역 목록 조회
			case "selectJobPrtnAcmsltList":
				// 등록기간 텍스트 설정
				var text = "";
				if(this.ds_acmsltPd.rowcount > 0){
					var bgnDe = this.dateUtils.format(this.ds_acmsltPd.getColumn(0, "INDVDL_BGNDE"),"yyyy-mm-dd");
					var endDe = this.dateUtils.format(this.ds_acmsltPd.getColumn(0, "INDVDL_ENDDE"),"yyyy-mm-dd");
					text = "(등록기간 : " + bgnDe + " ~ " + endDe + ")";
					
					this.updateYn = this.ds_acmsltPd.getColumn(0, "UPDATE_YN");
				}else {
					text = "(등록기간 지정안됨.)"
					
					this.updateYn = 'N';
				}
				
				this.div_search.sta_registText.set_text(text);
				
				// 수정 가능 여부에 따른 그리드 readonly 속성 설정
				if(this.updateYn == 'Y'){
					this.message = "";
					
					this.tab_jobPrtnAcmslt.tpg_01.grd_distrbOfcrkDtlsList.set_readonly(false);
					this.tab_jobPrtnAcmslt.tpg_02.grd_jobPrtnDtlsAcmsltList.set_readonly(false);
					this.tab_jobPrtnAcmslt.tpg_03.grd_coDvlpCntrbtEffectList.set_readonly(false);
					this.tab_jobPrtnAcmslt.tpg_04.grd_dtyexcPlanList.set_readonly(false);
					this.tab_jobPrtnAcmslt.tpg_05.grd_etcList.set_readonly(false);
				} else {
					this.gfn_message("info.처리불가","등록 기간이 아닙니다.");
					this.message = "현재 입력된 사원은 대상자가 아닙니다.";
					
					this.tab_jobPrtnAcmslt.tpg_01.grd_distrbOfcrkDtlsList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_02.grd_jobPrtnDtlsAcmsltList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_03.grd_coDvlpCntrbtEffectList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_04.grd_dtyexcPlanList.set_readonly(true);
					this.tab_jobPrtnAcmslt.tpg_05.grd_etcList.set_readonly(true);
				}
				break;
			// 업무추진실적 세부내역 저장
			case "saveJobPrtnAcmsltList":
				this.fn_search();
				break;
		}
	}
}


