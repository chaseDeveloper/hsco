/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	// 수정가능여부 체크
	if(this.updateYn != 'Y'){
		this.gfn_message("info.처리불가", this.message);
		return;
	}
	
	if (this.gfn_message("confirm.취소여부")) {
	
		this.ds_distrbOfcrkDtlsList.reset();
		this.ds_jobPrtnDtlsAcmsltList.reset();
		this.ds_coDvlpCntrbtEffectList.reset();
		this.ds_dtyexcPlanList.reset();
		this.etcList.reset();

		this.ds_distrbOfcrkDtlsList.applyChange();
		this.ds_jobPrtnDtlsAcmsltList.applyChange();
		this.ds_coDvlpCntrbtEffectList.applyChange();
		this.ds_dtyexcPlanList.applyChange();
		this.etcList.applyChange();
		
		return;
	}
}


