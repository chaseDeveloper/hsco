/*
 * 함수명  	: fn_dynamicGrid
 * 설명 		: 평가집계 공통코드 이용하여 동적그리드 생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_dynamicGrid();
 */

this.fn_dynamicGrid = function() {

	this.grd_intrvwEvlDetail.set_enableredraw(false);
	
	// 그리드 초기화(기존컬럼 남기고 삭제)
	for(var i=this.grd_intrvwEvlDetail.getCellCount("Head"); i> 3; i--) {
		this.grd_intrvwEvlDetail.deleteContentsCol(i);
	}
	
	for(var i = 0; i < this.ds_hrm074.getRowCount(); i++) {
		// column 추가
		var nCol = this.grd_intrvwEvlDetail.appendContentsCol("body", true);
		
		// Head 셋팅
		this.grd_intrvwEvlDetail.setCellProperty("Head", nCol-1 ,"text", this.ds_hrm074.getColumn(i, "CODE_NM"));
		this.grd_intrvwEvlDetail.mergeContentsCell("Head", 0, nCol, 1, nCol, nCol-1, false);

		// Body 셋팅 
		if(this.ds_hrm074.getColumn(i, "ETC1") != "2") { 	// 가점 외 콤보 바인딩
			this.grd_intrvwEvlDetail.setCellProperty("Body", nCol, "displaytype", "combo");
			this.grd_intrvwEvlDetail.setCellProperty("Body", nCol, "combodataset", "ds_evlNm");
			this.grd_intrvwEvlDetail.setCellProperty("Body", nCol, "combocodecol", "CODE");
			this.grd_intrvwEvlDetail.setCellProperty("Body", nCol, "combodatacol", "CODE_NM");
		}else {
			this.grd_intrvwEvlDetail.setCellProperty("Body", nCol, "displaytype", "normal");
		}
		// 코드 값
		this.grd_intrvwEvlDetail.setCellProperty("Body", nCol, "text", "bind:EVL_IEM_CODE_"+this.ds_hrm074.getColumn(i, "CODE"));
		
		// Summ 셋팅 (실제값)	
		this.grd_intrvwEvlDetail.setCellProperty("Summ", nCol, "text" , "expr:nexacro.round(dataset.getAvg('EVL_IEM_"+this.ds_hrm074.getColumn(i, 'CODE')+"'), 2)");

		this.grd_intrvwEvlDetail.setFormatColProperty(nCol, "size", 200);
	}
	
	this.grd_intrvwEvlDetail.set_enableredraw(true);
}


/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 검색조건 데이터셋 컬럼 변경시
