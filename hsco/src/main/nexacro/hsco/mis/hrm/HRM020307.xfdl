 /*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리            
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_intrvwEvl)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }
    
	// validation 체크(검색조건)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_intrvwEvl.clearData();
	this.ds_intrvwEvlDetail.clearData();
	
	this.fn_transaction("selectIntrvwEvl");
}


 /*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리             
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if(Ex.util.isUpdated(this.ds_intrvwEvl)){
		if(this.gfn_message("confirm.저장.여부"))	{
			this.saveRow = this.ds_intrvwEvl.rowposition;
			this.fn_transaction("selectIntrvwCU");
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
	}
}


 /*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 이벤트 처리       
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	// 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_intrvwEvl)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }

    if (this.gfn_message("confirm.취소여부")) {
        this.ds_intrvwEvl.reset();
        this.ds_intrvwEvl.applyChange();
    }
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// ========= 검색조건 =========
		case "selectEmpmnYearCombo": 	// 채용년도 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/empmnYearComboList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_empmnYearList=output1";
			break;
		case "selectOdrSeCombo":		// 차수, 채용공고 조회(검색조건)
			var sController   	= "hsco/mis/hrm/HRM020201/odrSeComboList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_odrSeList=output1";
			break;
		// =============================
		case "selectIntrvwEvl":			// 대상자 목록 조회
			var sController   	= "hsco/mis/hrm/HRM020307/selectIntrvwEvl.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_intrvwEvl=output1";
			break;
		case "selectIntrvwEvlDetail":	// 평가집계 목록 조회
			var sController   	= "hsco/mis/hrm/HRM020307/selectIntrvwEvlDetail.do";
			var sInDatasets   	= "input5=ds_cond2";
			var sOutDatasets  	= "ds_intrvwEvlDetail=output5";
			break;
		case "selectIntrvwCU":			// 대상자 목록 저장
			var sController   	= "hsco/mis/hrm/HRM020307/intrvwEvlCU.do";
			var sInDatasets   	= "input1=ds_intrvwEvl:U";
			var sOutDatasets  	= "";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			// ========= 검색조건 =========
			case "selectEmpmnYearCombo":	// 채용년도 조회(검색조건)
				this.div_search.cmb_empmnYear.set_index(0);
				this.fn_transaction("selectOdrSeCombo");
				break;
			case "selectOdrSeCombo": 		// 차수, 채용공고 조회(검색조건)
				this.div_search.cmb_odrSe.set_index(0);
				this.fn_search();
				break;
			// =============================
			case "selectIntrvwEvl":			// 대상자 목록 조회
				break;
			case "selectIntrvwEvlDetail":	// 평가집계 목록 조회
				// 면접문항에 대하여 동적그리드
				this.fn_dynamicGrid();
				break;
			case "selectIntrvwCU":			// 대상자 목록 저장
				this.fn_search();
				break;
		}
	}
}

