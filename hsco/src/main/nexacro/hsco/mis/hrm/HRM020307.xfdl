		var nRow = this.ds_cond2.addRow();
		
		this.ds_cond2.setColumn(nRow,"EMPMN_YEAR",this.ds_intrvwEvl.getColumn(this.ds_intrvwEvl.rowposition,"EMPMN_YEAR"));
		this.ds_cond2.setColumn(nRow,"ODR_SE"    ,this.ds_intrvwEvl.getColumn(this.ds_intrvwEvl.rowposition,"ODR_SE"));
		this.ds_cond2.setColumn(nRow,"APYEXM_NO" ,this.ds_intrvwEvl.getColumn(this.ds_intrvwEvl.rowposition,"RL_APYEXM_NO"));
		this.fn_transaction("selectIntrvwEvlDetail");
	}
}

