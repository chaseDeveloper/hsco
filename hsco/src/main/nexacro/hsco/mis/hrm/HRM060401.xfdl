/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectEvlGnrlzSm":
			var sController  = "hsco/mis/hrm/HRM060401/selectEvlGnrlzSm.do";
			var sInDatasets  = "input1=ds_cond";
			var sOutDatasets = "ds_pdList=output0 ds_evl_gnrlz_sm_1=output1 ds_evl_gnrlz_sm_2=output2 ds_evl_gnrlz_sm_3=output3 ds_evl_gnrlz_sm_4=output4 ds_evl_gnrlz_sm_5=output5";
     		break;
		case "calcEvlGnrlzSm":
			var sController  = "hsco/mis/hrm/HRM060401/calcEvlGnrlzSm.do";
			var sInDatasets  = "input1=ds_cond";
			var sOutDatasets = "";
     		break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}



/**********************************************************************
 * Transaction Callback 영역
***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectEvlGnrlzSm":
				
				
				if(this.ds_pdList.rowcount == 0){
					this.ds_pdList.addRow();
					this.ds_pdList.setColumn(0, "HT_ENDDE", this.dateUtils.today());
				} else {
					var endDe = this.ds_pdList.getColumn(0, "HT_ENDDE");
					
					if(this.comUtils.isNull(endDe)){
						this.ds_pdList.setColumn(0, "HT_ENDDE", this.dateUtils.today());
					}
				}
				break;
			case "calcEvlGnrlzSm":
				this.fn_search();
				break;
		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/


// 검색조건 변경 후 이벤트 처리
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	this.fn_search();
}


// (expr) 해당 차수만큼의 이전 년도를 계산
this.fn_get_year = function(minValue)
{
	var evlYear = this.ds_cond.getColumn(0, "EVL_YEAR");
	var htSe    = this.ds_cond.getColumn(0, "HT_SE");
	
	var alpha = (htSe % 2) - 1;
	
	var calcValue = evlYear - Math.floor((minValue + alpha) / 2);
	calcValue = isNaN(calcValue) ? "" : calcValue;
	return calcValue + "년";
}


// (expr) 해당 차수만큼의 이전 반기를 계산
this.fn_get_htSe = function(minValue)
{
	var evlYear = this.ds_cond.getColumn(0, "EVL_YEAR");
	var htSe    = this.ds_cond.getColumn(0, "HT_SE");
	
	var alpha = (htSe % 2);
	
	var calcValue = "0" + (((minValue + alpha) % 2) + 1);
	var findRow = this.ds_hrm049.findRow("CODE", calcValue);
	var rtnVal = this.ds_hrm049.getColumn(findRow, "CODE_NM");
	return rtnVal;
}


this.btn_evlCalc_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// update check
	if ( this.comUtils.isDatasetUpdated(this.ds_cond)) {
		this.gfn_message("info.처리불가", "검색조건이 변경되어 진행할 수 없습니다.");
		return false;
	}
	
	var endDe = this.ds_pdList.getColumn(0, "HT_ENDDE");
	
	if(this.comUtils.isNull(endDe)){
		this.gfn_message("info.처리불가", "기준일자를 입력 바랍니다.");
		return false;
	}
	
	if(!application.confirm("현재 차수의 데이터를 삭제하고 평정결과를 새롭게 집계합니다.\n계속 진행하시겠습니까?")) {
		return false;	// 아니오
	}

	this.ds_cond.setColumn(0, "HT_ENDDE", this.ds_pdList.getColumn(0, "HT_ENDDE"));
	
	this.fn_transaction("calcEvlGnrlzSm");
}
