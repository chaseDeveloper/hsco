      <BindItem id="item3" compid="div_detail1.edt_vhcleNm" propid="value" datasetid="ds_diary" columnid="VHCLE_NM"/>
      <BindItem id="item4" compid="div_detail1.msk_opratSm" propid="value" datasetid="ds_diary" columnid="OPRAT_SM"/>
      <BindItem id="item5" compid="div_detail1.msk_refuelQy" propid="value" datasetid="ds_diary" columnid="REFUEL_QY"/>
      <BindItem id="item6" compid="div_detail1.msk_totOlUsgqty" propid="value" datasetid="ds_diary" columnid="TOT_OL_USGQTY"/>
      <BindItem id="item7" compid="div_detail1.msk_bfrtBnt" propid="value" datasetid="ds_diary" columnid="BFRT_BNT"/>
      <BindItem id="item8" compid="div_detail1.msk_todayAcmtl" propid="value" datasetid="ds_diary" columnid="TODAY_ACMTL"/>
      <BindItem id="item9" compid="div_detail1.cal_opratDe" propid="value" datasetid="ds_diary" columnid="OPRAT_DE"/>
      <BindItem id="item2" compid="div_search.edt_vhcleNm" propid="value" datasetid="ds_cond" columnid="VHCLE_NM"/>
    </Bind>
    <Script type="xscript5.0"><![CDATA[
/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";


/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.isAuth = false;
this.saveRow = -1;


/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.HRM090405_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}

this.HRM090405_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
	
	// 검색조건 부서 바인딩
	 this.div_search.div_DEPT.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");

	// 검색조건 초기 데이터 설정
	var today = this.gfn_today();
	var startDe = this.dateUtils.addDate(today, -7, 's');
	
	// 서브 부서 바인딩
	this.div_detail1.div_srchDept.fn_setBind("ds_diary","DRVER_DEPT_CODE","DRVER_DEPT_NM");
	// 서브 사원 바인딩
	this.div_detail1.div_EMP.fn_setBind("ds_diary","DRVER_EMPNO","DRVER_EMPNM");
	
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	this.ds_cond.setColumn(nRow, "ENDDE",     today);
	this.ds_cond.setColumn(nRow, "BGNDE",     startDe);
	this.ds_cond.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId());
	this.ds_cond.setColumn(nRow, "DEPT_NM",   this.gfn_getDeptName());
	
 	/* 권한 해제 요청 */
 	if(this.gfn_authGrpId("HRM_ADMIN") != -1 || this.gfn_authGrpId("USER_VEHICLE") != -1) {// 이때는 담당자 플래그 true로 바꿔서 권한분리 부탁드립니다.
 		this.isAuth = true;
 	}else {
 		this.isAuth = false;
 	}

	if(!isAuth) {	
		// 운전자부서, 운전자 수정 불가
		this.div_detail1.div_EMP.div_sear.edt_empno.set_readonly(true);
		this.div_detail1.div_EMP.div_sear.edt_emplnm.set_readonly(true);
		this.div_detail1.div_srchDept.div_Search.edt_deptCode.set_readonly(true);
		this.div_detail1.div_srchDept.div_Search.edt_deptNm.set_readonly(true);
	}
	
	this.fn_search();
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.init_Form();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// ..
}

/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/


/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(Ex.util.isUpdated(this.ds_diary) || Ex.util.isUpdated(this.ds_dtls)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;    // 아니오
        }
    }
    
	// validation
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	// clear datasets
 	this.ds_dtls.clearData();
 	this.ds_diary.set_enableevent(false);
 	this.ds_diary.clearData();
 	this.ds_diary.set_enableevent(true);
	
	// search
	this.fn_transaction("select");
}


/*
 * 함수명  	: fn_search_sub
 * 설명 		: (상세)조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function()
{	
	var dRow = this.ds_diaryList.rowposition;
	var vhcleCode  = this.ds_diaryList.getColumn(dRow, "VHCLE_CODE");
	var opratSn    = this.ds_diaryList.getColumn(dRow, "OPRAT_SN");
	
	this.ds_cond_sub.clearData();
	var nRow = this.ds_cond_sub.addRow();
	this.ds_cond_sub.setColumn(nRow, "VHCLE_CODE" , vhcleCode);
	this.ds_cond_sub.setColumn(nRow, "OPRAT_SN"   , opratSn);
		
	this.fn_transaction("selectSub");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (!this.comUtils.isDatasetUpdated(this.ds_diary) && !this.comUtils.isDatasetUpdated(this.ds_dtls)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_diary, this.ds_validation)) {
		return;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_dtls, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_diary.rowposition;
		this.fn_transaction("save");
	}
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "select":
			var sController   	= "hsco/mis/hrm/HRM090405/select.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_diaryList=output1";
			break;
		case "selectSub":
			var sController   	= "/hsco/mis/hrm/HRM090405/selectSub.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_diary=output1 ds_dtls=output2";
			break;
		case "save":
			var sController   	= "/hsco/mis/hrm/HRM090405/save.do";
			var sInDatasets   	= "input1=ds_diary:A input2=ds_dtls:U";
			var sOutDatasets  	= "";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
		return false;
	}
	
	switch (strSvcId) {
		case "select":
			if(this.saveRow > -1){
				this.ds_diary.set_rowposition(this.saveRow);
				this.saveRow = -1;
			}
			break;
		case "selectSub":
			// 오브젝트 제어
			this.fn_setObjectCnrl();
			break;
		case "save":
			this.gfn_message("success.처리.성공");
			this.fn_search();
			break;
	}
}


 /*
 * 함수명  	: fn_PopBsnsCallback
 * 설명 		: 팝업 콜백 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                sReturn  : 팝업에서 넘어오는 데이터
 * return Type 	: None
 * 사용법		: this.fn_PopBsnsCallback();
 */
this.fn_PopCallback = function(sPopupId, sReturn)
{
	if(sPopupId == "caralReqst") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {	
			var caralcBgnde     = dsTemp.getColumn(0, "CARALC_BGNDE");		// 시작일자
			var caralcSeq 		= dsTemp.getColumn(0, "CARALC_SEQ");		// 배차순번
			var drverEmpno		= dsTemp.getColumn(0, "DRVER_EMPNO");		// 운전자사번
			var drverEmpnm		= dsTemp.getColumn(0, "DRVER_EMPNM");		// 운전자명
			var drverDeptCode	= dsTemp.getColumn(0, "DRVER_DEPT_CODE");	// 운전자부서코드
			var drverDeptNm		= dsTemp.getColumn(0, "DRVER_DEPT_NM");		// 운전자부서명
			var vhcleCode		= dsTemp.getColumn(0, "VHCLE_CODE");		// 차량코드
			var vhcleNo			= dsTemp.getColumn(0, "VHCLE_NO");			// 차량번호
			var vhcleNm			= dsTemp.getColumn(0, "VHCLE_NM");			// 차량명
			
			//var cond_deptCode = this.ds_cond.getColumn(0, "DEPT_CODE"); 
					
			var nRow = this.ds_diary.rowposition;
			this.ds_diary.setColumn(nRow, "OPRAT_DE"	  , caralcBgnde);
			this.ds_diary.setColumn(nRow, "CARALC_SEQ"	  , caralcSeq);
			this.ds_diary.setColumn(nRow, "DRVER_EMPNO"	  , drverEmpno);
			this.ds_diary.setColumn(nRow, "DRVER_EMPNM"	  , drverEmpnm);
			this.ds_diary.setColumn(nRow, "DRVER_DEPT_CODE", drverDeptCode);
			this.ds_diary.setColumn(nRow, "DRVER_DEPT_NM"  , drverDeptNm);
			this.ds_diary.setColumn(nRow, "VHCLE_CODE"	   , vhcleCode);
			this.ds_diary.setColumn(nRow, "VHCLE_NO"	   , vhcleNo);
			this.ds_diary.setColumn(nRow, "VHCLE_NM"	   , vhcleNm);
		}
	}else if(sPopupId == "vhcleSearch") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {	
			var vhcleCode		= dsTemp.getColumn(0, "VHCLE_CODE");		// 차량코드
			var vhcleNo			= dsTemp.getColumn(0, "VHCLE_NO");			// 차량번호
			var vhcleNm			= dsTemp.getColumn(0, "VHCLE_NM");			// 차량명
// 			var deptCode	    = dsTemp.getColumn(0, "MANAGE_DEPT_CODE");	// 관리부서코드
// 			var deptNm		    = dsTemp.getColumn(0, "DEPT_NM");			// 관리부서명
							
			var deptCode = this.ds_cond.getColumn(0, "DEPT_CODE");	// 부서코드(검색조건)
			var deptNm = this.ds_cond.getColumn(0, "DEPT_NM");	// 부서명(검색조건)
							
			var nRow = this.ds_diary.rowposition;
			this.ds_diary.setColumn(nRow, "VHCLE_CODE"	   , vhcleCode);
			this.ds_diary.setColumn(nRow, "VHCLE_NO"	   , vhcleNo);
			this.ds_diary.setColumn(nRow, "VHCLE_NM"	   , vhcleNm);
			this.ds_diary.setColumn(nRow, "DRVER_DEPT_CODE", deptCode);
			this.ds_diary.setColumn(nRow, "DRVER_DEPT_NM"  , deptNm);
		}
	}else if(sPopupId == "condVhcle"){
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {	
			this.ds_cond.setColumn(0, "VHCLE_NM",   dsTemp.getColumn(0, "VHCLE_NM"));
			this.ds_cond.setColumn(0, "VHCLE_CODE", dsTemp.getColumn(0, "VHCLE_CODE"));
		}
    }
}




/**********************************************************************
* Transaction 영역 끝
***********************************************************************/

/**********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
***********************************************************************
*
* 1. 공통함수 처리
* 2. Button 관련 이벤트 처리
* 3. Gird 관련 이벤트 처리
* 4. Dataset 관련 이벤트 처리
* 5. 사용자 정의 함수
***********************************************************************/

/**********************************************************************
* 공통함수 처리
***********************************************************************/

 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	if(Ex.util.isUpdated(this.ds_diary) || Ex.util.isUpdated(this.ds_dtls)) {
		if(!this.gfn_message("comm.행위여부","변경된 데이터가 있습니다. 저장하지 않고 진행") ){
			return false;
		}
	}

	this.ds_diary.clearData();
	this.ds_dtls.clearData();
	this.ds_diary.addRow();
	
	var today = this.gfn_today();
	this.ds_diary.setColumn(0, "OPRAT_DE"  , today);
	this.ds_diary.setColumn(0, "SANCTN_KND", "0");
	
	// 오브젝트 제어
	this.fn_setObjectCnrl();
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
	if(['1','4','5'].indexOf(this.ds_diary.getColumn(this.ds_diary.rowposition, "SANCTN_KND")) > -1) {
		this.gfn_message("fail.삭제.불가", "결재가 진행중이거나 완료된 건은")
	} else{
		if(this.ds_diary.rowcount > 0) {
			if(!this.gfn_message("confirm.삭제여부")) {
				return false;
			}
			this.ds_diary.deleteRow(this.ds_diary.rowposition);
			this.ds_dtls.deleteAll();
		}
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if (!this.gfn_message("confirm.취소여부")) {
		return false;
	}
	
	this.ds_diary.reset();
	this.ds_diary.applyChange();
}

/*
 * 함수명  	: fn_sanction
 * 설명 		: 결재 버튼 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_sanction();
 */
this.fn_sanction = function()
{
	
	var nRow = this.ds_diary.rowposition;
	
	// 결재 전 유효성 체크
	if(this.comUtils.isDatasetUpdated(this.ds_diary) || this.comUtils.isDatasetUpdated(this.ds_dtls)) {
		this.gfn_message("comm.정보.저장.요청", "차량운행일지");
		return;
	}

	// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
	if(['1','4','5'].indexOf(this.ds_diary.getColumn(nRow, "SANCTN_KND")) > -1) {
		this.gfn_message("info.처리불가","결재가 진행중이거나 완료된 데이터는 결재할 수 없습니다.");
		return;
	}
	
	var cnt 			= 0;
	var info			= new Array(7);
	var arrSancData 	= new Array(78);
		
	
	if(this.ds_diary.rowcount == 0) {
		this.gfn_message("info.처리불가","차량운행일지 내역이 선택되지 않았습니다.");
		return;
	}	
		
	for(var i = 0; i < this.ds_dtls.getRowCount(); i++) {
		info= [
			 this.ds_dtls.getColumn(i, "TKCAR_NM")			// 승차자
			,this.ds_dtls.getColumn(i, "BUSINS")			// 용무
			,this.ds_dtls.getColumn(i, "DSTN")				// 목적지
			,this.fn_time(this.ds_dtls.getColumn(i, "START_TM"))					// 출발
			,this.fn_time(this.ds_dtls.getColumn(i, "ARVL_TM"))						// 도착
			,this.comUtils.formatComma(this.ds_dtls.getColumn(i, "OPRAT_DSTNC"))	// 키로
			,this.ds_dtls.getColumn(i, "RM")										// 비고		
		];

		if(cnt < 10) {
			arrSancData[ 8 + cnt*7] = info[0];
			arrSancData[ 9 + cnt*7] = info[1];
			arrSancData[ 10 + cnt*7] = info[2];
			arrSancData[ 11 + cnt*7] = info[3];
			arrSancData[ 12 + cnt*7] = info[4];
			arrSancData[ 13 + cnt*7] = info[5];
			arrSancData[ 14 + cnt*7] = info[6];
		}
		cnt++;
	}
		
	var opratDe = this.ds_diary.getColumn(nRow, "OPRAT_DE");
	var opratDeFotmat = opratDe.substr(0, 4) + "년 " + opratDe.substr(4, 2) + "월 "+ opratDe.substr(6, 2) +"일";
	
	arrSancData[0] = "차량운행일지( 차량번호 : "+ this.ds_diary.getColumn(nRow, "VHCLE_NO") +", 운행일자 : "+opratDeFotmat+")";	// 결재제목																			// 결재제목
	arrSancData[1] = opratDeFotmat;														// 운행일자
	arrSancData[2] = this.ds_diary.getColumn(nRow, "VHCLE_NO");							// 차량번호
	arrSancData[3] = this.ds_diary.getColumn(nRow, "DRVER_EMPNM");							// 운전자
	arrSancData[4] = this.fn_pointformatComma(this.ds_diary.getColumn(nRow, "OPRAT_SM"));		// 운행계
	arrSancData[5] = this.fn_pointformatComma(this.ds_diary.getColumn(nRow, "BFRT_BNT"));		// 전일누계
	arrSancData[6] = this.fn_pointformatComma(this.ds_diary.getColumn(nRow, "TODAY_ACMTL"));	// 금일누계
	arrSancData[7] = this.fn_pointformatComma(this.ds_diary.getColumn(nRow, "REFUEL_QY"));		// 급유량
	
	this.fn_callHandySanc2(
