/*
 * 함수명  	: ds_dtls_cancolumnchange
 * 설명 		: 운행내역 데이터 변경 전 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_dtls_cancolumnchange();
 */
this.ds_dtls_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "START_TM") {
		var tm = (e.newvalue).replace(/[ ]/g, "");
		if(tm.length != 4) {
			alert("시각은 4자리여야 합니다.");
			obj.setColumn(e.row, "START_TM", e.oldvalue);
			return false;
		}else {
			var frontTm = tm.substr(0, 2);
			var backTm = tm.substr(2, 2);		
			if(frontTm > 23) {
				alert("시는 24시를 넘을 수 없습니다.");
				obj.setColumn(e.row, "START_TM", e.oldvalue);
				return false;
			}
			
			if(backTm > 59) {
				alert("분은 59분을 넘을 수 없습니다.");
				obj.setColumn(e.row, "START_TM", e.oldvalue);
				return false;
			}
		}
	}else if(e.columnid == "ARVL_TM") {
		var tm = (e.newvalue).replace(/[ ]/g, "");
		if(tm.length != 4) {
			alert("시각은 4자리여야 합니다.");
			obj.setColumn(e.row, "ARVL_TM", e.oldvalue);
			return false;
		}else {
			var frontTm = tm.substr(0, 2);
			var backTm = tm.substr(2, 2);
		
			if(frontTm > 23) {
				alert("시는 24시를 넘을 수 없습니다.");
				obj.setColumn(e.row, "ARVL_TM", e.oldvalue);
				return false;
			}
			
			if(backTm > 59) {
				alert("분은 59분을 넘을 수 없습니다.");
				obj.setColumn(e.row, "ARVL_TM", e.oldvalue);
				return false;
			}
		}	
	}
}

