* 설명	: 대행자 검색팝업 호출
***********************************************************************************/
this.btn_Assets_Popup1_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		deptCd	: this.div_search.div_deptCode.div_Search.edt_deptCode.value
	};
	
	this.gfn_popup("empPop3", 400, 400, "사원조회팝업", parm, "com::empno_P01.xfdl", "btn_Assets_Popup1Callback", true);
}

/***********************************************************************************
* 설명	: 대행자 검색팝업 콜백
***********************************************************************************/
this.btn_Assets_Popup1Callback = function(sPopupId, sReturn)
{
	if(sPopupId == "empPop3") {
		var dataset = new Dataset();
        nRowCnt = dataset.loadXML(sReturn);
        if(nRowCnt){
			this.div_search.edt_empno.set_value(dataset.getColumn(0, "EMPNO"));
			this.div_search.edt_empnm.set_value(dataset.getColumn(0, "EMPNM"));
        }
	}
	
	this.fn_search();
}]]></Script>
