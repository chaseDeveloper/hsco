/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 this.checkDs = [
  this.ds_cmgList
];
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.HRM070106_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();	
}
/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.HRM070106_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	var today = this.gfn_today();
	var startDe = today.substr(0, 6);
	this.ds_cond.setColumn(0, "STDR_YM"  , startDe);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();
	this.ds_cond.addRow();		
	this.div_search.div_deptCode.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	this.div_search.div_gnfdDe.fn_setBind("ds_cond", "STDR_YM");
	var searchYn = "N";
	/* 권한 분배 */
	if ( this.gfn_authcode("006") > -1 ) { //인사담당자, 급여담당자
		this.div_search.div_deptCode.set_enable(true);
		this.div_search.btn_Vrsc_man_search1.set_visible(true);
		this.div_search.edt_empnm.set_enable(true);
	}else{ //일반사용자
		this.ds_cond.setColumn(0, "EMPNM", this.fn_getEmpnm());
		this.ds_cond.setColumn(0, "EMPNO"    , this.fn_getEmpno());
		searchYn = "Y";
	}
	
	// 공통코드
	var comboParams = [["ds_rspofcSe", "_HRM214", "1", "전체"],//직책
						["ds_ofcpsSe",  "HRM003", "1", "전체"],//직위
						["ds_jssfcSe",  "HRM001", "1", "전체"],//직종
		              ];
		              
	var callback = function (svcId, errorCode, errorMsg) {
		if(searchYn == "Y"){
			this.fn_search();
		}
	};
	this.gfn_comboLoad (comboParams, callback);
}


/**********************************************************************
* dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	
}
/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	this.fn_transaction("selectCmgList");
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		//출퇴근내역 목록 조회
		case "selectCmgList":		
			this.ds_cmgList.clearData();
			
			var sController   	= "hsco/mis/hrm/HRM070106/selectCmgList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cmgList=output01";
		break;	
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//조회 콜백
			case "selectCmgList" :
				if(this.ds_cmgList.getRowCount() == 0) {
					this.gfn_message("info.데이터.없음");
				}else{
					for(var i =1 ; i <= 31 ; i ++){
						if(!this.comUtils.isNull(this.ds_cmgList.getColumn(0 , "DAY"+i)))
						{
							//HEAD 요일 설정
							this.div_work.grd_cmgList.setCellProperty("Head", 5+i, "text", i+ "일(" + this.ds_cmgList.getColumn(0 , "DAY"+i) +  ")" );
							
							if(i > 27){ //조회조건에 따라 row가 0일 경우 날짜 컬럼이 모두 사라지는 경우 방지 
										//모두 사라질 경우 재 조회 시 그리드 생성에 시간 소요 多
							
							//28일 부터 월말 COULMN SIZE 60 
								this.div_work.grd_cmgList.setFormatColProperty((2*i) + 4, "size", 60); //날짜 (i)에 해당하는 col_1
								this.div_work.grd_cmgList.setFormatColProperty((2*i) + 5, "size", 60); //날짜 (i)에 해당하는 col_2
							}
						}else{
							//해당 년월의 말일 이후의 날짜 COLUMN SIZE 0
							if(i > 27){
								this.div_work.grd_cmgList.setFormatColProperty((2*i) + 4, "size", 0);
								this.div_work.grd_cmgList.setFormatColProperty((2*i) + 5, "size", 0);
							}
						}
					}
				}
			break;
			
		}
	}
}
