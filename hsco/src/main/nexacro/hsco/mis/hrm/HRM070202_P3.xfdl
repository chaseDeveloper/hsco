    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : HRM070202_P1
 * 화면명   : 관용차량선택
 * 화면설명 : 관용차량선택을 선택한다.
 * 작성일   : 2017.05.12
 * 작성자   : 이수지
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 *   
 ***********************************************************************
 */
/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
// 업무에 필요한 공통 라이브러리 파일을 추가할 수 있다.
// TODO

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 this.fv_objForm;

this.isAuth = false;
 
 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.HRM070202_P1_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.ds_reqstList.clearData();
	this.ds_reqstList.addRow();
	this.ds_caralcFlwpsg.clearData();
	
	this.fv_objForm = this.parent.arg_0;
	
	this.div_EMP_DRVER.fn_setBind("ds_reqstList","DRVER_EMPNO","DRVER_EMPNM");
		
	this.ds_cond.setColumn(0, "BSRP_YEAR" 		,this.parent.arg_1);
	this.ds_cond.setColumn(0, "BSRP_SEQ" 		,this.parent.arg_2);
	this.ds_cond.setColumn(0, "EMPNO" 			,this.parent.arg_3);
	this.fn_transaction("selectBsrpVhcle");
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/
/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBsrpVhcle": 				// 관용차량정보 조회
			var sController   	= "hsco/mis/hrm/HRM070202/selectBsrpVhcle.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_reqstList=output1 ds_caralcFlwpsg=output2";
			break;
		case "tbhrmReqstSave": 				// 저장(CU)
			var sController   	= "hsco/mis/hrm/HRM070202/tbhrmReqstSave.do";
			var sInDatasets   	= "output=ds_reqstList:U input2=ds_caralcFlwpsg:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{	trace(strSvcId);
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(nErrorCode != 0) {
	  this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	if (nErrorCode != 0){
	} else {
		switch (strSvcId) {	
			case "selectBsrpVhcle": 				// 관용차량정보 조회
				if(this.ds_reqstList.getRowCount()<1){
					this.ds_reqstList.copyData(this.fv_objForm.ds_popup_reqstList);
					this.ds_caralcFlwpsg.copyData(this.fv_objForm.ds_popup_caralcFlwpsg);
				}
				break;
			case "tbhrmReqstSave": 					// 저장하기
				this.gfn_message("success.처리.성공", null, null, null, this.fn_popupCallback);
				break;
		}
	}
}

/*
 * 함수명  	: fn_PopupCallback
 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_PopupCallback();
 */
this.fn_popupCallback = function(sPopupId, sReturn)
{
	var ds_Popup = new Dataset();
	var loadCnt = ds_Popup.loadXML(sReturn);
	if(loadCnt) {
		if(sPopupId == "addFlwpsg") {
			var nRow = this.ds_caralcFlwpsg.addRow();
			this.ds_caralcFlwpsg.setColumn(nRow, "FLWPSG_EMPNO"    , ds_Popup.getColumn(0, "EMPNO"));
			this.ds_caralcFlwpsg.setColumn(nRow, "EMPNM"    , ds_Popup.getColumn(0, "EMPNM"));
			this.ds_caralcFlwpsg.setColumn(nRow, "FLWPSG_DEPT_CODE", ds_Popup.getColumn(0, "DEPT_CODE"));
			this.ds_caralcFlwpsg.setColumn(nRow, "DEPT_NM"   , ds_Popup.getColumn(0, "DEPT_NM"));
			this.ds_caralcFlwpsg.setColumn(nRow, "JSSFC_SE"   , ds_Popup.getColumn(0, "JSSFC_NM"));
		}else if(sPopupId == "vhclePop"){
			this.ds_reqstList.setColumn(0, "VHCLE_CODE" , ds_Popup.getColumn(0, "VHCLE_CODE"));
			this.ds_reqstList.setColumn(0, "VHCLE_NM"   , ds_Popup.getColumn(0, "VHCLE_NM"));
		}
	}
}

