 */
this.fn_sanctionPopup = function(){

	var content = '';
	
	if(this.ds_bodBiCn.rowcount > 0){
		for(var i=0; i < this.ds_bodBiCn.rowcount; i++){
			var confmAt = this.ds_bodBiCn.getColumn(i, "CONFM_AT");	// 의결여부
			
			if(confmAt == '1'){
				var biNm = this.ds_bodBiCn.getColumn(i, "BI_NM");	// 의안건명
				content += '○ ' + biNm + String.fromCharCode(13) + String.fromCharCode(10);
			}		
		}
	}
	
    var parm = {
		arg_0 : this,
		arg_1 : content,
		arg_2 : this.ds_bodMaster.getColumn(this.ds_bodMaster.rowposition, "BOD_MANAGE_NO"),	// SANCTN_NO
