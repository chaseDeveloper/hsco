/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
    if(Ex.util.isUpdated(this.ds_bodMaster) || Ex.util.isUpdated(this.ds_bodAtdrn) || Ex.util.isUpdated(this.ds_bodBiCn)){
	    if (!this.gfn_message("confirm.변경.취소.여부")) {
	        return false;
	    }
	}
	
	this.ds_bodMaster.clearData();
	this.ds_bodAtdrn.clearData();
	this.ds_bodBiCn.clearData();
	this.ds_fileList.clearData();
	
	this.ds_bodMaster.addRow();
	this.ds_bodMaster.setColumn(0, "BOD_SE", "");
	this.ds_bodMaster.setColumn(0, "BEGIN_TIME1", "00");
	this.ds_bodMaster.setColumn(0, "BEGIN_TIME2", "00");
	this.ds_bodMaster.setColumn(0, "END_TIME1", "00");
	this.ds_bodMaster.setColumn(0, "END_TIME2", "00");
	this.ds_bodMaster.setColumn(0, "BOD_DE", this.dateUtils.today());
	/*this.ds_bodMaster.setColumn(0, "SANCTN_KND", '0');*/

	this.ds_cond.setColumn(0, "BOD_MANAGE_NO", null);
	
	this.fn_transaction("selectBodNmstList");	
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete= function()
{
	//2016.08.10
	var SANCTN_STS = this.ds_bodMaster.getColumn(this.ds_bodMaster.rowposition, "SANCTN_STS");
	switch (SANCTN_STS) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");					
		return;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return;
		
		default:
	}	
	
	if ( this.gfn_message("confirm.삭제여부") ) {
		this.ds_fileList.clearData();
		this.ds_bodAtdrn.clearData();
		this.ds_bodBiCn.clearData();
		this.ds_bodMaster.deleteAll();
		
		this.ds_cond.setColumn(0, "BOD_MANAGE_NO", null);
		this.fn_save();
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if ( this.gfn_message("confirm.취소여부") ) {
		if(this.ds_bodMaster.getRowType(this.ds_bodMaster.rowposition) == Dataset.ROWTYPE_INSERT){
			this.ds_bodAtdrn.clearData();
		}else {
			this.ds_bodAtdrn.reset();		                                 //DATASET ROLLBACK
			this.ds_bodAtdrn.applyChange();	                             	 //DATASET COMMIT
		}
		
		this.ds_bodMaster.reset();		                                 //DATASET ROLLBACK
		this.ds_bodMaster.applyChange();	                             //DATASET COMMIT
		this.ds_bodBiCn.reset();		                                 //DATASET ROLLBACK
		this.ds_bodBiCn.applyChange();	                            	 //DATASET COMMIT		
		return;
	}
}

