/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	// 데이터셋 클리어
	this.ds_bodMaster.clearData();
	
	this.fn_transaction("select");
	

}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_bodMaster) || Ex.util.isUpdated(this.ds_bodAtdrn) || Ex.util.isUpdated(this.ds_bodBiCn) || Ex.util.isUpdated(this.ds_fileList)) {	
		// validation 체크(월간계획)
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_bodMaster, this.ds_validation)) {
			return;
		}
		//this.ds_bodMaster.getRowType(this.ds_bodMaster.rowposition);
		this.fn_transaction("save");
	} else {	
	    this.gfn_message("comm.데이터.변경.없음");
	}

}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "select":    		// 위원회 조회
			var sController   	= "hsco/mis/hrm/HRM100102/selectBodMaster.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_bodMaster=output1 ds_bodAtdrn=output2 ds_bodBiCn=output3";
			break;
		case "save":    		// 위원회 저장
			var sController   	= "hsco/mis/hrm/HRM100102/saveBodMaster.do";
			var sInDatasets   	= "input1=ds_bodMaster:U input2=ds_bodAtdrn:A input3=ds_bodBiCn:U";
			var sOutDatasets  	= "ds_cond_temp=output1";
			break;
		case "selectFileList":   // 첨부파일 조회
			var sController   	= "hsco/mis/hrm/HRM100102/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_fileList=output1";
			break;
		case "selectBodNmstList": // 위원회 명단 조회               
			var sController   	= "hsco/mis/hrm/HRM100102/selectBodNmstList.do";    
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_bodAtdrn=output1";
			break;
			
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "select":	//  조회
				this.ds_cond02.clearData();
				this.ds_cond02.addRow();
				this.ds_cond02.setColumn(0, "FILE_SN", this.ds_bodMaster.getColumn(this.ds_bodMaster.rowposition, "FILE_SN"));
				this.fn_transaction("selectFileList");  // 첨부파일 조회
				break;
			case "save":	//  저장
			    this.gfn_message("success.처리.성공");
			    
				if(this.ds_cond_temp.rowcount > 0){
					var bodNo = this.ds_cond_temp.getColumn(0, "BOD_MANAGE_NO");
					
					if(bodNo){
						this.ds_cond.setColumn(0, "BOD_MANAGE_NO", bodNo);
						this.ds_cond.setColumn(0, "BOD_CD_SE", this.ds_bodMaster.getColumn(0,"BOD_CD_SE"));
						this.fn_search();
					} else {
						this.ds_cond.setColumn(0, "BOD_MANAGE_NO", null);
					}
				} else {
					this.ds_cond.setColumn(0, "BOD_MANAGE_NO", null);
				}
				
				this.ds_bodMaster.clearData();
				this.ds_bodAtdrn.clearData();
				this.ds_bodBiCn.clearData();
				this.ds_fileList.clearData();
				
				break;
			case "selectBodNmstList": // 위원회 명단 조회
				var cnt = this.ds_bodAtdrn.rowcount;
				this.ds_bodMaster.setColumn(0, "TOT_HMV_CO", cnt);
				this.ds_bodMaster.setColumn(0, "ATEND_HMV_CO", cnt);
				
				this.fn_cudSetup();
			    break;
			case "selectFileList":   // 첨부파일 조회
				this.grd_cmmnAtchmnfl.setCellProperty("Head", 0, "text", "0");
				this.fn_cudSetup();	// 전자결재 상태에 따른 수정여부 설정
				break;
		}
	}
}


 /*
 * 함수명  	: fn_PopBsnsCallback
 * 설명 		: 팝업 콜백 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                sReturn  : 팝업에서 넘어오는 데이터
 * return Type 	: None
 * 사용법		: this.fn_PopBsnsCallback();
 */
this.fn_PopCallback = function(sPopupId, sReturn)
{
    if(sPopupId == "HRM100102_p02")
    {		
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(!nRowCnt){
			
		} else {
			if(this.ds_bodMaster.rowcount == 0){
				this.ds_bodMaster.addRow();
				this.ds_bodMaster.setColumn(0, "BOD_CD_SE", "");
				this.ds_bodMaster.setColumn(0, "BOD_SE", "");
				this.ds_bodMaster.setColumn(0, "BEGIN_TIME1", "00");
				this.ds_bodMaster.setColumn(0, "BEGIN_TIME2", "00");
				this.ds_bodMaster.setColumn(0, "END_TIME1", "00");
				this.ds_bodMaster.setColumn(0, "END_TIME2", "00");				
			}
			
			if(this.ds_bodAtdrn.rowcount > 1) {
				var ds_bodAtdrnCopy = new Dataset();
				ds_bodAtdrnCopy.copyData(this.ds_bodAtdrn);
				
				for(i=0; i<nRowCnt; i++){
					var strExctvNo01 = dsRtn.getColumn(i,"EXCTV_NO");
					var strDplctYn = "N";
					
					for(r=0; r<ds_bodAtdrnCopy.rowcount; r++) {
						var strExctvNo02 = ds_bodAtdrnCopy.getColumn(r, "EXCTV_NO");
						if(strExctvNo01 == strExctvNo02) {
							r = ds_bodAtdrnCopy.rowcount;
							strDplctYn = "Y"
						}
					}
					
					if(strDplctYn == "N") {
						var nRow = this.ds_bodAtdrn.addRow();
						this.ds_bodAtdrn.setColumn(nRow, "BOD_CD_SE", this.ds_bodMaster.getColumn(0, "BOD_CD_SE"));
						this.ds_bodAtdrn.setColumn(nRow, "BOD_MANAGE_NO", this.ds_bodMaster.getColumn(0, "BOD_MANAGE_NO"));
						this.ds_bodAtdrn.setColumn(nRow, "EXCTV_NO", dsRtn.getColumn(i,"EXCTV_NO"));
						this.ds_bodAtdrn.setColumn(nRow, "OFCPS_NM", dsRtn.getColumn(i,"OFCPS_NM"));
						this.ds_bodAtdrn.setColumn(nRow, "NM"      , dsRtn.getColumn(i,"NM"));
						this.ds_bodAtdrn.setColumn(nRow, "CTTPC1"  , dsRtn.getColumn(i,"CTTPC1S")+"-"+dsRtn.getColumn(i,"CTTPC1M")+"-"+dsRtn.getColumn(i,"CTTPC1E"));										
					}
				}								
			} else {
				for(i=0; i<nRowCnt; i++){
					var nRow = this.ds_bodAtdrn.addRow();
					this.ds_bodAtdrn.setColumn(nRow, "BOD_CD_SE", this.ds_bodMaster.getColumn(0, "BOD_CD_SE"));
					this.ds_bodAtdrn.setColumn(nRow, "BOD_MANAGE_NO", this.ds_bodMaster.getColumn(0, "BOD_MANAGE_NO"));
					this.ds_bodAtdrn.setColumn(nRow, "EXCTV_NO", dsRtn.getColumn(i,"EXCTV_NO"));
					this.ds_bodAtdrn.setColumn(nRow, "OFCPS_NM", dsRtn.getColumn(i,"OFCPS_NM"));
					this.ds_bodAtdrn.setColumn(nRow, "NM"      , dsRtn.getColumn(i,"NM"));
					this.ds_bodAtdrn.setColumn(nRow, "CTTPC1"  , dsRtn.getColumn(i,"CTTPC1S")+"-"+dsRtn.getColumn(i,"CTTPC1M")+"-"+dsRtn.getColumn(i,"CTTPC1E"));
				}
			}
			
			this.ds_bodMaster.setColumn(0, "ATEND_HMV_CO", this.ds_bodAtdrn.rowcount);
		}
	}
	
	if(sPopupId == "deptCode_P01") {
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			this.ds_bodBiCn.setColumn(this.ds_bodBiCn.rowposition, "PROPSE_DEPT_CODE" , dsRtn.getColumn(0, "DEPT_CODE"));
			this.ds_bodBiCn.setColumn(this.ds_bodBiCn.rowposition, "PROPSE_DEPT_NM" ,   dsRtn.getColumn(0, "DEPT_NM"));
		}
	}	

	if(sPopupId == "HRM100102_P01") {
		
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			this.ds_cond.setColumn(0, "BOD_MANAGE_NO" ,dsRtn.getColumn(0, "BOD_MANAGE_NO"));
			this.fn_search();
		}
	}	
	
	if(sPopupId == "HRM100102_P03") {
		
		if(sReturn){
			this.ds_bodMaster.set_updatecontrol(false);
			this.ds_bodMaster.setColumn(this.ds_bodMaster.rowposition, "SANCTN_NO", sReturn);
			this.ds_bodMaster.set_updatecontrol(true);
		}
		
		this.fn_search();
	}
} 

