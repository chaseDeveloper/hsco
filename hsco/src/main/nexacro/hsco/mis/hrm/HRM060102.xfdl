		this.ds_workEvlTagter_02.setColumn(nRow, "EVL_MAN_2_EMPNM", "");
		this.ds_workEvlTagter_02.setColumn(nRow, "CNFRMR_EMPNM",    "");
		this.ds_workEvlTagter_02.setColumn(nRow, "EVL_STEP_SE",     "02");
	}
	// 3단계 평정자 삭제
	for(var fRow = arrSelectedDs.length-1; fRow >= 0; fRow--) {
		var datasetRow = arrSelectedDs[fRow];
		this.ds_workEvlTagter_03.deleteRow(datasetRow);
	}
}


