		return false;
	}
	
	var arrSelectedDs = this.tab_tbhrmPereva.tabpage4.grd_workEvlTagter_03.getSelectedDatasetRows();
	if(this.comUtils.isNull(arrSelectedDs.length)) {
		var errMsg = "2단계 대상자로 선정할 사원을 선택하여 주시기 바랍니다.";
		alert(errMsg);
		return false;
	}
	
	// 2단계 평정자 추가
	for(var fRow = 0; fRow < arrSelectedDs.length; fRow++) {
		var datasetRow = arrSelectedDs[fRow];
		var nRow = this.ds_workEvlTagter_02.addRow();
		this.ds_workEvlTagter_02.copyRow(nRow, this.ds_workEvlTagter_03, datasetRow);
