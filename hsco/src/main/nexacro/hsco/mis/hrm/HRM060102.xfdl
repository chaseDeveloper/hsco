    <Script type="xscript5.0"><![CDATA[
 /***********************************************************************
 * Script Include 
************************************************************************/
 
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
************************************************************************/

this.HRM060102_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}

this.HRM060102_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
	
	// 공통코드
	var comboParams = [
		["ds_hrm001" , "HRM001", "Y", "A", "B"],		// 직종 구분
		["ds_hrm002"  ,"HRM002", "Y", "A", "B"],		// 직급 구분
		["ds_hrm049", "HRM049", "Y", "D", "B"],
		["ds_hrm095", "HRM095", "Y", "D", "B"]
	];
	
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	var callback = function (svcId, errorCode, errorMsg) {
		var evlYear = this.comUtils.getClientDate("YYYY");
		var htSe    = nexacro.toNumber(this.comUtils.getClientDate("MM"),0,0,0) < 7 ? "01" : "02";
		
		this.ds_cond.set_enableevent(false);
		this.ds_cond.setColumn(nRow, "EVL_YEAR", evlYear);
		this.ds_cond.setColumn(nRow, "HT_SE",    htSe   );
		this.ds_cond.setColumn(nRow, "TRGET_COMBO", "ALL" );
		
		this.ds_cond.set_enableevent(true);
		
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
	
}


/***********************************************************************
 * Form/Dataset 오브젝트 초기화
************************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */ 
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// ..
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
 * Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회버튼 이벤트 처리         
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_workEvlPd.clearData();
	this.ds_workEvlExcl.clearData();
	this.ds_workEvlTagter.clearData();
	this.ds_workEvlTagter_02.clearData();
	this.ds_workEvlTagter_03.clearData();
	
	this.fn_transaction("selectWorkEvl");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장버튼 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (!this.comUtils.isDatasetUpdated(this.ds_workEvlPd)        && 
		!this.comUtils.isDatasetUpdated(this.ds_workEvlExcl)      && 
		!this.comUtils.isDatasetUpdated(this.ds_workEvlTagter)    && 
		!this.comUtils.isDatasetUpdated(this.ds_workEvlTagter_02) && 
		!this.comUtils.isDatasetUpdated(this.ds_workEvlTagter_03)		) {
			this.gfn_message("comm.데이터.변경.없음");
			return false;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_workEvlPd, this.ds_validation)) {
		this.tab_tbhrmPereva.set_tabindex(0);
		return;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_workEvlExcl, this.ds_validation)) {
		this.tab_tbhrmPereva.set_tabindex(1);
		return;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_workEvlTagter, this.ds_validation)) {
		this.tab_tbhrmPereva.set_tabindex(2);
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		this.fn_transaction("saveWorkEvl");
	}
}



/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectWorkEvl":
			var sController   	= "hsco/mis/hrm/HRM060102/selectWorkEvl.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_workEvlPd=output1 ds_workEvlExcl=output2 ds_workEvlTagter=output3 ds_workEvlTagter_02=output4 ds_workEvlTagter_03=output5";
			break;
		case "saveWorkEvl":
			var sController   	= "hsco/mis/hrm/HRM060102/saveWorkEvl.do";
			var sInDatasets   	= "input1=ds_workEvlPd:U input2=ds_workEvlExcl:U input3=ds_workEvlTagter:U input4=ds_workEvlTagter_02:U input5=ds_workEvlTagter_03:U";
			var sOutDatasets  	= "";
			break;
		case "saveAutoWorkEvlExcl":
			var sController   	= "hsco/mis/hrm/HRM060102/saveAutoWorkEvlExcl.do";
			var sInDatasets   	= "input1=ds_condEmp";
			var sOutDatasets  	= "";
			break;
		case "saveAutoWorkEvlTagter":
			var sController   	= "hsco/mis/hrm/HRM060102/saveAutoWorkEvlTagter.do";
			var sInDatasets   	= "input1=ds_condEmp";
			var sOutDatasets  	= "";
			break;
		case "saveAutoCalc":
			var sController   	= "hsco/mis/hrm/HRM060102/saveAutoCalc.do";
			var sInDatasets   	= "input1=ds_condEmp";
			var sOutDatasets  	= "";
			break;
		case "addExclEmp": //select tp3
			var sController   	= "hsco/mis/hrm/HRM060102/addExclEmp.do";
			var sInDatasets   	= "input1=ds_condEmp";
			var sOutDatasets  	= "ds_selectExclEmp=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
 * Transaction Callback 영역
***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectWorkEvl":  //select
				// 검색조건으로 쓰려고 만든거 같으니, 일단 냅두긴 하겠지만....
				this.ds_condEmp.copyData(this.ds_workEvlPd);
				
				// 평정년도 및 구분의 값 유무를 판단하여 실제 DB상에 데이터가 존재하는지 판단
				var oriEvlYear = this.ds_workEvlPd.getColumn(0, "ORI_EVL_YEAR");
				var oriHtSe    = this.ds_workEvlPd.getColumn(0, "ORI_HT_SE");
				if(this.comUtils.isNull(oriEvlYear) && this.comUtils.isNull(oriEvlYear)) {
					this.ds_workEvlPd.set_updatecontrol(false);
					this.ds_workEvlPd.setRowType(0, Dataset.ROWTYPE_INSERT);
					this.ds_workEvlPd.set_updatecontrol(true);
				}
				break;
			case "saveWorkEvl": case "saveAutoWorkEvlExcl": case "saveAutoWorkEvlTagter": case "saveAutoCalc":
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "addExclEmp":
				var nRow = this.ds_workEvlExcl.addRow();
				this.ds_workEvlExcl.copyRow(nRow, this.ds_selectExclEmp, 0);
				break;
		}
	}
}


 /*
 * 함수명  	: fn_popupCallback
 * 설명 		: Popup Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_popupCallback = function(sPopupId, sReturn)
{
	var ds_Popup = new Dataset();
	var loadCnt = ds_Popup.loadXML(sReturn);
	if(loadCnt) {
		if(sPopupId == "empPop") {
            var EmpCode   = ds_Popup.getColumn(0, "EMPNO");
			this.ds_condEmp.setColumn(0, "EMPNO",     EmpCode);
			this.fn_transaction("addExclEmp");
		} else if(sPopupId == "EVL_MAN_1_EMPNO") {
			var empno = ds_Popup.getColumn(0, "EMPNO");
			var empnm = ds_Popup.getColumn(0, "EMPNM");
			var etRow = this.ds_workEvlTagter.rowposition;
			this.ds_workEvlTagter.setColumn(etRow, "EVL_MAN_1_EMPNO", empno);
			this.ds_workEvlTagter.setColumn(etRow, "EVL_MAN_1_EMPNM", empnm);
			this.ds_workEvlTagter.setColumn(etRow, "CHK_UPD", "변경");			// 동일한 사람을 다시 선택했더라도, 데이터셋 상태를 UPDATE로 유지하기 위함
		} else if(sPopupId == "EVL_MAN_2_EMPNO") {
			var empno = ds_Popup.getColumn(0, "EMPNO");
			var empnm = ds_Popup.getColumn(0, "EMPNM");
            var etRow = this.ds_workEvlTagter.rowposition;
			this.ds_workEvlTagter.setColumn(etRow, "EVL_MAN_2_EMPNO", empno);
			this.ds_workEvlTagter.setColumn(etRow, "EVL_MAN_2_EMPNM", empnm);
			this.ds_workEvlTagter.setColumn(etRow, "CHK_UPD", "변경");			// 동일한 사람을 다시 선택했더라도, 데이터셋 상태를 UPDATE로 유지하기 위함
		} else if(sPopupId == "CNFRMR_EMPNO") {
			var empno = ds_Popup.getColumn(0, "EMPNO");
			var empnm = ds_Popup.getColumn(0, "EMPNM");
            var etRow = this.ds_workEvlTagter.rowposition;
			this.ds_workEvlTagter.setColumn(etRow, "CNFRMR_EMPNO", empno);
			this.ds_workEvlTagter.setColumn(etRow, "CNFRMR_EMPNM", empnm);
			this.ds_workEvlTagter.setColumn(etRow, "CHK_UPD", "변경");			// 동일한 사람을 다시 선택했더라도, 데이터셋 상태를 UPDATE로 유지하기 위함
		}
	}
}


/**********************************************************************
* Dataset 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 검색조건 Dataset 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged(obj,e);
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(!this.comUtils.isNull(e.oldvalue)) {
		this.fn_search();
	}
}



/**********************************************************************
* Tab 관련 이벤트 처리
***********************************************************************/



/**********************************************************************
* 탭2 제외자
***********************************************************************/

/*
 * 함수명  	: tab_tbhrmPereva_tabpage2_btn_targetDcsn_onclick
 * 설명 		: '제외자 확정' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_tbhrmPereva_tabpage2_btn_targetDcsn_onclick(obj,e);
 */
this.tab_tbhrmPereva_tabpage2_btn_targetDcsn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_workEvlPd, this.ds_validation)) {
		this.tab_tbhrmPereva.set_tabindex(0);
		return false;
	}
	
	if (this.comUtils.isDatasetUpdated(this.ds_workEvlPd)) {
		var errMsg = "기간설정 탭의 내용을 저장하신 후 진행하여 주시기 바랍니다.";
		alert(errMsg);
		this.tab_tbhrmPereva.set_tabindex(0);
		return false;
	}
	
	if(this.gfn_message("comm.행위여부", "제외자를 모두 삭제하고, 자동계산하여 재선정")){
		this.fn_transaction("saveAutoWorkEvlExcl");
	}
}

/*
 * 함수명  	: tab_tbhrmPereva_tabpage2_btn_add_onclick
 * 설명 		: '행추가' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_tbhrmPereva_tabpage2_btn_add_onclick(obj,e);
 */
this.tab_tbhrmPereva_tabpage2_btn_add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_popup("empPop", 400, 400, "사원조회팝업", "", "com::empno_P01.xfdl", "fn_popupCallback", true);
}


/*
 * 함수명  	: tab_tbhrmPereva_tabpage2_btn_del_onclick
 * 설명 		: '행삭제' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_tbhrmPereva_tabpage2_btn_del_onclick(obj,e);
 */
this.tab_tbhrmPereva_tabpage2_btn_del_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(!this.gfn_message("confirm.삭제여부")) {
		return false;	// 아니오
	}
	this.ds_workEvlExcl.deleteRow(this.ds_workEvlExcl.rowposition);
}


/*
 * 함수명  	: tab_tbhrmPereva_tabpage2_btn_cancel_onclick
 * 설명 		: '행취소' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_tbhrmPereva_tabpage2_btn_cancel_onclick(obj,e);
 */
this.tab_tbhrmPereva_tabpage2_btn_cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.comUtils.isDatasetUpdated(this.ds_workEvlExcl)){
		if(this.gfn_message("confirm.취소여부")){  	
			this.ds_workEvlExcl.reset();
		}
	}
}



/**********************************************************************
* 탭3 대상자
***********************************************************************/

/*
 * 함수명  	: tab_tbhrmPereva_tabpage3_btn_targetDcsn_onclick
 * 설명 		: '대상자확정' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_tbhrmPereva_tabpage3_btn_targetDcsn_onclick(obj,e);
 */
this.tab_tbhrmPereva_tabpage3_btn_targetDcsn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_workEvlPd, this.ds_validation)) {
		this.tab_tbhrmPereva.set_tabindex(0);
		return false;
	}
	
	if (this.comUtils.isDatasetUpdated(this.ds_workEvlPd)) {
		var errMsg = "기간설정 탭의 내용을 저장하신 후 진행하여 주시기 바랍니다.";
		alert(errMsg);
		this.tab_tbhrmPereva.set_tabindex(0);
		return false;
	}
	
	if(this.gfn_message("comm.행위여부", "대상자 및 평정단계를 모두 삭제하고, 자동계산하여 재선정")){
		this.fn_transaction("saveAutoWorkEvlTagter");
	}
}

/*
 * 함수명  	: tab_tbhrmPereva_tabpage3_btn_targetDcsn_onclick
 * 설명 		: '대상자재계산' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_tbhrmPereva_tabpage3_btn_targetDcsn_onclick(obj,e);
 */
this.tab_tbhrmPereva_tabpage3_btn_calc_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_workEvlTagter.getRowCount() == 0) {
		alert("대상자가 존재하지 않습니다. 최초선정 이후 재계산이 가능합니다");
		return false;
	}
	
	if(this.gfn_message("comm.행위여부", "현재 조회된 모든 대상자의 평정자/확인자를 변경")){
		this.fn_transaction("saveAutoCalc");
	}
}


/*
 * 함수명  	: tab_tbhrmPereva_tabpage3_grd_workEvlTagter_onexpandup
 * 설명 		: Grid expand 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_tbhrmPereva_tabpage3_grd_workEvlTagter_onexpandup(obj,e);
 */
this.tab_tbhrmPereva_tabpage3_grd_workEvlTagter_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if (this.comUtils.isDatasetUpdated(this.ds_workEvlTagter_02) || this.comUtils.isDatasetUpdated(this.ds_workEvlTagter_03)) {
		var errMsg = "[평정단계 변경] 탭의 내용을 저장하신 후 진행하여 주시기 바랍니다.";
		alert(errMsg);
		this.tab_tbhrmPereva.set_tabindex(3);
		return false;
	}
	
	if(e.col == 11) {
		this.gfn_popup("EVL_MAN_1_EMPNO", 400, 400, "평정자(1차)선택", "", "com::empno_P01.xfdl", "fn_popupCallback", true);
	} else if(e.col == 12) {
