 * 설명 		: 조회 이벤트 처리(수요조사 목록 조회)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search_sub = function()
{
	this.ds_dmandExaminList.clearData();
	this.ds_operMethodOpinion.clearData();
	this.ds_hopeNewEdc.clearData();
	this.ds_hopeTruedu.clearData();
	this.ds_envrnImprvmOpionion.clearData();
	this.ds_hopeTrueduInstt.clearData();
	this.ds_hopeInstrctr.clearData();
	this.ds_grupEdcPlan.clearData();
	this.fn_transaction("selectDmandExaminList");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{	
	try{
		// update check
		if (   !this.comUtils.isDatasetUpdated(this.ds_operMethodOpinion)		// 운영방안의견
		    && !this.comUtils.isDatasetUpdated(this.ds_hopeNewEdc)				// 희망신규교육
		    && !this.comUtils.isDatasetUpdated(this.ds_hopeTruedu)				// 희망위탁교육
		    && !this.comUtils.isDatasetUpdated(this.ds_envrnImprvmOpionion)		// 환경개선의견
		    && !this.comUtils.isDatasetUpdated(this.ds_hopeTrueduInstt)			// 희망위탁교육기관
		    && !this.comUtils.isDatasetUpdated(this.ds_hopeInstrctr)			// 희망강사
		    && !this.comUtils.isDatasetUpdated(this.ds_grupEdcPlan)				// 단체교육계획
		   ) {
			this.gfn_message("comm.데이터.변경.없음");
			return false;
		}
		
		// validation check
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_hopeNewEdc, this.ds_validation)) {				// 탭2 : 희망신규교육
			this.tab_demandExamin.set_tabindex(1);
			return false;
		}
		else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hopeTruedu, this.ds_validation)) {			// 탭3 : 희망위탁교육
			this.tab_demandExamin.set_tabindex(2);
			return false;
		}
		else if(!this.gfn_checkValidation_NoUpdChk(this.ds_envrnImprvmOpionion, this.ds_validation)) {	// 탭4 : 환경개선의견
			this.tab_demandExamin.set_tabindex(3);
			return false;
		}
		else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hopeTrueduInstt, this.ds_validation)) {		// 탭5 : 희망위탁교육기관
			this.tab_demandExamin.set_tabindex(4);
			return false;
		}
		else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hopeInstrctr, this.ds_validation)) {			// 탭6 : 희망강사
			this.tab_demandExamin.set_tabindex(5);
			return false;
		}
		else if(!this.gfn_checkValidation_NoUpdChk(this.ds_grupEdcPlan, this.ds_validation)) {			// 탭7 : 단체교육계획
			this.tab_demandExamin.set_tabindex(6);
			return false;
		}
	}catch(e) {
		alert(e.message);
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		saveRow = this.ds_dmandExaminList.rowposition;
		this.fn_transaction("save");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectDmandExaminList":	// 수요조사 목록 조회
			var sController   	= "hsco/mis/hrm/HRM080202/selectDmandExaminList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_dmandExaminList=output1";
			break;
		case "selectDmandExaminOpinion":		// 수요조사의견 조회
			var sController   	= "hsco/mis/hrm/HRM080202/selectDmandExaminOpinion.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_operMethodOpinion=output1 ds_hopeNewEdc=output2 ds_hopeTruedu=output3 ds_envrnImprvmOpionion=output4 ds_hopeTrueduInstt=output5 ds_hopeInstrctr=output6 ds_grupEdcPlan=output7";
			break;
		case "save":			// 저장
			var sController   	= "hsco/mis/hrm/HRM080202/save.do";
			var sInDatasets   	= "input1=ds_operMethodOpinion:U input2=ds_hopeNewEdc:U input3=ds_hopeTruedu:U input4=ds_envrnImprvmOpionion:U input5=ds_hopeTrueduInstt:U input6=ds_hopeInstrctr:U input7=ds_grupEdcPlan:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(nErrorCode != 0) {
	  this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	if (nErrorCode != 0){
	} else {
		switch (strSvcId) {
			case "selectDmandExaminList":	// 수요조사 조회
				var rCnt = this.ds_dmandExaminList.rowcount;
				if(rCnt > -1) {
					var dmandExaminNo     = this.ds_dmandExaminList.getColumn(0, "DMAND_EXAMIN_NO");
					var yearDmandExaminNo = this.ds_dmandExaminList.getColumn(0, "YEAR_DMAND_EXAMIN_NO");
					this.ds_cond.setColumn(0, "DMAND_EXAMIN_NO",      dmandExaminNo    );
					this.ds_cond.setColumn(0, "YEAR_DMAND_EXAMIN_NO", yearDmandExaminNo);
				}
				break;
			case "selectDmandExaminOpinion":	// 수요조사의견 조회
				var year = this.ds_cond.getColumn(0, "YEAR");
				var str = "상기의견 등의 작성내용은 ";
				str += year;
				str += "년도 교육훈련계획 수립 및 예산 편성시 참고자료로 활용될 예정입니다.";
				this.tab_demandExamin.tabpage1.Static2.set_text(str);
				
				// 버튼 사용여부
				var fBtn = this.inpYn == "1" ? true : false;
				this.tab_demandExamin.tabpage1.btn_Cancel.set_visible(fBtn);
				this.tab_demandExamin.tabpage2.btn_Add.set_visible(fBtn);
				this.tab_demandExamin.tabpage2.btn_Delete.set_visible(fBtn);
				this.tab_demandExamin.tabpage2.btn_Cancel.set_visible(fBtn);
				this.tab_demandExamin.tabpage3.btn_Add.set_visible(fBtn);
				this.tab_demandExamin.tabpage3.btn_Delete.set_visible(fBtn);
				this.tab_demandExamin.tabpage3.btn_Cancel.set_visible(fBtn);
				this.tab_demandExamin.tabpage4.btn_Add.set_visible(fBtn);
				this.tab_demandExamin.tabpage4.btn_Delete.set_visible(fBtn);
				this.tab_demandExamin.tabpage4.btn_Cancel.set_visible(fBtn);
				this.tab_demandExamin.tabpage5.btn_Add.set_visible(fBtn);
				this.tab_demandExamin.tabpage5.btn_Delete.set_visible(fBtn);
				this.tab_demandExamin.tabpage5.btn_Cancel.set_visible(fBtn);
				this.tab_demandExamin.tabpage6.btn_Add.set_visible(fBtn);
				this.tab_demandExamin.tabpage6.btn_Delete.set_visible(fBtn);
				this.tab_demandExamin.tabpage6.btn_Cancel.set_visible(fBtn);
				this.tab_demandExamin.tabpage7.btn_Add.set_visible(fBtn);
				this.tab_demandExamin.tabpage7.btn_Delete.set_visible(fBtn);
				this.tab_demandExamin.tabpage7.btn_Cancel.set_visible(fBtn);
				break;
			case "save":					// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}

