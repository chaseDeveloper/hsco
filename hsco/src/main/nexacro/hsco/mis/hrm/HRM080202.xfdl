this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 검색조건 바인딩
	this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	this.div_search.div_srchDept.fn_set_onlyLast(true);
	
	// 초기 검색조건
	this.ds_cond.set_enableevent(false);
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	var year = this.comUtils.getClientDate("YYYY");
	var deptCode = this.gfn_getDeptId();
	var deptNm   = this.gfn_getDeptName();
 	this.ds_cond.setColumn(nRow, "YEAR",      year    );	// 당해년도
 	this.ds_cond.setColumn(nRow, "DEPT_CODE", deptCode);	// 본인부서코드
 	this.ds_cond.setColumn(nRow, "DEPT_NM",   deptNm  );	// 본인부서명
	this.ds_cond.set_enableevent(true);
	
	// 공통코드
	var comboParams = [
		["ds_hrm035", "HRM035", "Y", "D", "B"]	// ds_hrm035 : 교육대상
	   ,["ds_hrm036", "HRM036", "Y", "D", "B"]	// ds_hrm036 : 교육방법(희망신규교육)
	   ,["ds_hrm040", "HRM040", "Y", "D", "B"]	// ds_hrm036 : 교육환경구분
	   ,["ds_hrm041", "HRM041", "Y", "D", "B"]	// ds_hrm036 : 교육방법(집단교육)
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 공통코드와 연계된 초기검색조건 설정
		// 수정가능여부를 판별하기위해 권한그룹 체크
		if(this.gfn_authGrpId("HRM_EDC") != -1) {			// 교육담당자
			this.chargerAt = true;
		}
		
		// 수정가능한 경우 부서변경 가능
		if(this.chargerAt){
			this.div_search.div_srchDept.set_enable(true);
			this.ds_cond.setColumn(nRow, "DEPT_CODE", "000000"        );		// 부서코드
			this.ds_cond.setColumn(nRow, "DEPT_NM",   "화성도시공사");		// 부서명
		}
		// 본인의 부서 고정
		else{
			//해당 부서만
			this.ds_cond.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId()  );	// 부서코드
			this.ds_cond.setColumn(nRow, "DEPT_NM",   this.gfn_getDeptName());	// 부서명
		}
		
		this.fn_search_sub();	// 수요조사조회
	};
	this.gfn_comboLoad(comboParams, callback);
}

