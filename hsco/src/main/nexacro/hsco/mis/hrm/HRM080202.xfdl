/*
 * 함수명  	: fn_print
 * 설명 		: 인쇄
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
 */
this.fn_print = function ()
{
	var nRow = this.ds_dmandExaminList.rowposition;
	if(nRow > -1) {
		var year          = this.ds_dmandExaminList.getColumn(nRow, "YEAR");
		var dmandExaminNo = this.ds_dmandExaminList.getColumn(nRow, "DMAND_EXAMIN_NO");
		var dmandExaminNm = this.ds_dmandExaminList.getColumn(nRow, "DMAND_EXAMIN_NM");
		var deptCode      = this.ds_cond.getColumn(0, "DEPT_CODE");
		var deptNm        = this.ds_cond.getColumn(0, "DEPT_NM");
		
		if(this.comUtils.isNull(deptCode)) {
			this.gfn_message("info.처리불가", "부서코드를 입력하여 주십시오.");
			return false;
		}
		
		this.ds_condPrint.clearData();
		var cpRow = this.ds_condPrint.addRow();
		this.ds_condPrint.setColumn(cpRow, "YEAR",            year         );
		this.ds_condPrint.setColumn(cpRow, "DMAND_EXAMIN_NO", dmandExaminNo);
		this.ds_condPrint.setColumn(cpRow, "DEPT_CODE",       deptCode     );
		this.ds_condPrint.setColumn(cpRow, "DEPT_NM",         deptNm       );
		
		if(this.gfn_message("confirm.삭제.진행", year + "년 " + dmandExaminNm + "에 대한 " + deptNm + "의 의견을 인쇄합니다."))	{
			saveRow = this.ds_dmandExaminList.rowposition;
			this.gfn_ReportPopup(this, "MIS/HRM/HRM080202_R01.crf", this.ds_condPrint);
		}
	}
	else {
		this.gfn_message("info.처리불가", "선택된 수요조사가 없습니다.");
		return false;
	}
}

this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{	
	if(e.columnid == "YEAR") {
		if(Ex.util.isUpdated(this.ds_operMethodOpinion)
		   || Ex.util.isUpdated(this.ds_hopeNewEdc)
		   || Ex.util.isUpdated(this.ds_hopeTruedu)
		   || Ex.util.isUpdated(this.ds_envrnImprvmOpionion)
		   || Ex.util.isUpdated(this.ds_hopeTrueduInstt)
		   || Ex.util.isUpdated(this.ds_hopeInstrctr)
		   || Ex.util.isUpdated(this.ds_grupEdcPlan)
		   ) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				obj.setColumn(e.row, "YEAR", e.oldvalue);
				return false;	// 아니오
			}
		}
		this.fn_search_sub();
	}
	else if(e.columnid == "YEAR_DMAND_EXAMIN_NO") {
		if(Ex.util.isUpdated(this.ds_operMethodOpinion)
		   || Ex.util.isUpdated(this.ds_hopeNewEdc)
		   || Ex.util.isUpdated(this.ds_hopeTruedu)
		   || Ex.util.isUpdated(this.ds_envrnImprvmOpionion)
		   || Ex.util.isUpdated(this.ds_hopeTrueduInstt)
		   || Ex.util.isUpdated(this.ds_hopeInstrctr)
		   || Ex.util.isUpdated(this.ds_grupEdcPlan)
		   ) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				obj.setColumn(e.row, "YEAR_DMAND_EXAMIN_NO", e.oldvalue);
				return false;	// 아니오
			}
		}
		var yearDmandExaminNo = e.newvalue;
		var delRow = this.ds_dmandExaminList.findRow("YEAR_DMAND_EXAMIN_NO", yearDmandExaminNo);
		this.inpYn = this.ds_dmandExaminList.getColumn(delRow, "INP_YN") == "1" ? true : false;
		trace("delRow : " + delRow);
		trace("inpYn : " + this.inpYn);
		
		this.fn_search();
	}
}

