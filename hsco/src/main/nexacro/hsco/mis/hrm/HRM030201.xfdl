this.ds_gnfdDtlsList_onvaluechanged = function(obj:Dataset,  e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "GNFD_SE") {
		if(e.newvalue == "024") {
			this.div_dtls.div_srcls.set_visible(true);
		} else {
			this.div_dtls.div_srcls.set_visible(false);
		}
	}
	
	if(e.columnid == "DSLRC_CODE") {
		var fRow = this.ds_dscplStdrList.findRow("DSLRC_CODE", e.newvalue);
		if(fRow > -1) {
			var addMonth = this.ds_dscplStdrList.getColumn(fRow, "LMTT_APPLC_MONTH_CO");
			var bfYear  = obj.getColumn(e.row, "BEFORE_SRCLS_YY");
			var bfMonth = obj.getColumn(e.row, "BEFORE_SRCLS_MT");
			
			var afMonth = (bfMonth + addMonth) % 12;
			var afYear  = parseInt(bfYear) + parseInt((bfMonth + addMonth)/12);
			if(afMonth == 0) {
				afMonth = 12;
				afYear  = afYear - 1;
			}
			obj.setColumn(e.row, "SRCLS_YY", afYear);
			obj.setColumn(e.row, "SRCLS_MT", afMonth);
		}
	}
}
