/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/

 /*
 * 함수명  	: fn_search
 * 설명 		: 인사발령관리 발령기본정보 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.ds_gnfdDtlsList.clearData();
	this.fn_transaction("selectTbhrmGnfdDtlsList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (!this.comUtils.isDatasetUpdated(this.ds_gnfdDtlsList) ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_gnfdDtlsList, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.fn_transaction("saveTbhrmGnfdCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectTbhrmGnfdDtlsList": // 인사발령관리 발령목록 불러오기
			var sController   	= "hsco/mis/hrm/HRM030201/tbhrmGnfdDtlsList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_gnfdDtlsList=output1";
			break;
		case "selectEmpnoInfo": // 신규 입력 사원 선택시 해당 사원의 현재 정보 조회
			var sController   	= "hsco/mis/hrm/HRM030201/selectEmpnoInfo.do";
			var sInDatasets   	= "input1=ds_cond_empno";
			var sOutDatasets  	= "ds_empno=output1";
			break;
		case "saveTbhrmGnfdCUD": // 인사발령관리 발령상세 CUD
			var sController   	= "hsco/mis/hrm/HRM030201/tbhrmGnfdCUD.do";
			var sInDatasets   	= "input1=ds_gnfdDtlsList:U";
			var sOutDatasets  	= "";
			break;
		case "selectTbhrmGnfdProcess": // 인사발령관리 발령처리
			var sController   	= "hsco/mis/hrm/HRM030201/tbhrmGnfdProcess.do";
			var sInDatasets   	= "input1=ds_gnfdDtlsList:U";
			var sOutDatasets  	= "";
			break;
		case "selectTbhrmGnfdProcessCancel": // 인사발령관리 발령처리취소
			var sController   	= "hsco/mis/hrm/HRM030201/tbhrmGnfdProcessCancel.do";
			var sInDatasets   	= "input1=ds_gnfdDtlsList:U";
			var sOutDatasets  	= "";
			break;	
		case "selectDscplStdrList":		
			var sController   	= "/hsco/mis/hrm/HRM040302/selectDscplStdrList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_dscplStdrList=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectTbhrmGnfdDtlsList": // 인사발령관리 발령목록 불러오기
				break;	
			case "saveTbhrmGnfdCUD": // 인사발령관리 발령상세 CUD
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectEmpnoInfo": // 신규 입력 사원 선택시 해당 사원의 현재 정보 조회
				var beforeDeptCd = null;
				var beforeDeptNm = null;
				var beforeBizplcCd = null;
				var beforeBizplcNm = null;
				var beforeJssfcSe = null;
				var beforeClsfSe = null;
				var beforeOfcpsSe = null;
				var beforeJblnSe = null;
				var beforeSrclsSe = null;
				var beforeSttusSe = null;
				// === 추가 2018.03.15
				var cnwkYycnt = null;		// 근속년수
				var cnwkYycntStdrMt = null;	// 근속년수기준월
				var salarySe = null;		// 급여구분
				var srclsPromtStdrMt = null;// 호봉기준월
				var srclsPromtStdrYy = null;// 호봉승급기준년
				var cnwkYycntStdrYy = null; // 근속년수기준년
				
				if(this.ds_empno.rowcount > 0){
					beforeDeptCd = this.ds_empno.getColumn(0, "BEFORE_DEPT_CODE");
					beforeDeptNm = this.ds_empno.getColumn(0, "BEFORE_DEPT_NM");
					beforeBizplcCd = this.ds_empno.getColumn(0, "BEFORE_BIZPLC_CODE");
					beforeBizplcNm = this.ds_empno.getColumn(0, "BEFORE_BIZPLC_NM");
					beforeJssfcSe = this.ds_empno.getColumn(0, "BEFORE_JSSFC_SE");
					beforeClsfSe = this.ds_empno.getColumn(0, "BEFORE_CLSF_SE");
					beforeOfcpsSe = this.ds_empno.getColumn(0, "BEFORE_OFCPS_SE");
					beforeJblnSe = this.ds_empno.getColumn(0, "BEFORE_JBLN_SE");
