 /*
 * 함수명  	: fn_cancel
 * 설명 		: 취소버튼 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if(!Ex.util.isUpdated(this.ds_gnfdDtlsList)){
		this.gfn_message("comm.내역.변경.없음");
		return;
	}
	
	if ( this.gfn_message("confirm.취소여부") ) {
		this.ds_gnfdDtlsList.reset();
		this.ds_gnfdDtlsList.applyChange();
	}
}

 /*
 * 함수명  	: fn_delete
 * 설명 		: 인사발령관리 삭제버튼 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete= function()
{
	var rowposition = this.ds_gnfdDtlsList.rowposition;
	var dcsnAt = this.ds_gnfdDtlsList.getColumn(rowposition, "DCSN_AT");
	var sanctnSts = this.ds_gnfdDtlsList.getColumn(rowposition, "SANCTN_STS");
	
	if(dcsnAt == "1"){
		this.gfn_message("fail.삭제.불가", "발령처리가 완료된 건은"); 
	}
	else{
		// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
		// 결재상태가 기안전 외는 신규로 다시 진행.
		
		if(sanctnSts != "0") {
			this.gfn_message("fail.삭제.불가","결재가 진행된 건은");
		} else {
			if(this.gfn_message("confirm.삭제여부")){
				this.ds_gnfdDtlsList.set_enableevent(false);
				this.ds_gnfdDtlsList.deleteRow(rowposition);
				this.ds_gnfdDtlsList.set_enableevent(true);
			}
		}
	}
}

