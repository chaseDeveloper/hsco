 /*
 * 함수명  	: fn_gnfdProcess
 * 설명 		: 인사발령관리 발령처리/취소버튼 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_gnfdProcess();
 */
this.fn_gnfdProcess = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 3){
		var nRow = this.ds_gnfdDtlsList.rowposition;
		
		if(nRow > -1){
			
			var sanctnKnd = this.ds_gnfdDtlsList.getColumn(nRow, "SANCTN_STS");	// 결재상태
			/* TODO 전자결재 연계 후 주석 해제 필요.*/
// 			if(sanctnKnd != "5"){
// 				this.gfn_message("info.진행가능","결재가 완료된");
// 				return false;
// 			}
			
			if(this.comUtils.isDatasetUpdated(this.ds_gnfdDtlsList)) {
				if(!this.gfn_message("confirm.변경.취소.여부")) {
					return false;	// 아니오
				} else {
					this.ds_gnfdDtlsList.reset();
					this.ds_gnfdDtlsList.applyChange();
				}
			}
			
// 			if(!Ex.util.isUpdated(this.ds_gnfdDtlsList)){
// 				this.gfn_message("comm.내역.변경.없음");
// 				return;
// 			}
			var dcsnAt = this.ds_gnfdDtlsList.getColumn(nRow, "DCSN_AT");	// 확정여부
			if(dcsnAt == '1'){
				// 발령취소
				if(this.gfn_message("comm.행위여부", "발령취소"))	{
					this.ds_gnfdDtlsList.setColumn(nRow, "DCSN_AT", "0");
					this.fn_transaction("selectTbhrmGnfdProcessCancel");
				}
			} else {
				// 발령처리
				if(this.gfn_message("comm.행위여부", "발령처리"))	{
					this.ds_gnfdDtlsList.setColumn(nRow, "DCSN_AT", "1");
					this.fn_transaction("selectTbhrmGnfdProcess");
				}
			}
		}
	}
}

