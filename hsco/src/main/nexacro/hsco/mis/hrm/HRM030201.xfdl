 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "bizplc") {
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);
		if(nRowCnt){
			var nRow = this.ds_gnfdDtlsList.rowposition;
			this.ds_gnfdDtlsList.setColumn(nRow, "BIZPLC_CODE", dsRtn.getColumn(0, "BIZPLC_CODE"));
			this.ds_gnfdDtlsList.setColumn(nRow, "BIZPLC_NM",   dsRtn.getColumn(0, "BIZPLC_NM"));
		}
	}
	
	if(sPopupId == "bf_bizplc") {
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);
		if(nRowCnt){
			var nRow = this.ds_gnfdDtlsList.rowposition;
			this.ds_gnfdDtlsList.setColumn(nRow, "BEFORE_BIZPLC_CODE", dsRtn.getColumn(0, "BIZPLC_CODE"));
			this.ds_gnfdDtlsList.setColumn(nRow, "BEFORE_BIZPLC_NM",   dsRtn.getColumn(0, "BIZPLC_NM"));
		}
	}
	
	if(sPopupId == "search") {
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);
		if(nRowCnt){
			var nRow = this.ds_gnfdDtlsList.rowposition;
			this.ds_gnfdDtlsList.setColumn(nRow, "EMPNO",   dsRtn.getColumn(0, "EMPNO"));
			this.ds_gnfdDtlsList.setColumn(nRow, "EMPNM",   dsRtn.getColumn(0, "EMPNM"));
		}
	}
}

