 /*
 * 함수명  	: fn_print2
 * 설명 		: 인사발령대장 출력 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print2();
 */
this.fn_print2 = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var prtSe = this.ds_cond.getColumn(0, "PRT_SE");	
	if(this.comUtils.isNull(prtSe)){
		alert("출력구분을 선택해주세요");
		return false;
	}
	
	
	this.ds_condPrint.clearData();
	var prtGnfdSeq = "";
	var prtEmpno = "";
	
	
	for(var fRow = 0; fRow < this.ds_gnfdDtlsList.getRowCount(); fRow++) {
		var chkVal = this.ds_gnfdDtlsList.getColumn(fRow, "PRT_CHK");
		if(chkVal == '1') {
			prtGnfdSeq += (",'" + this.ds_gnfdDtlsList.getColumn(fRow , "GNFD_SEQ") + "'");
			prtEmpno += (",'" + this.ds_gnfdDtlsList.getColumn(fRow , "EMPNO") + "'");
		}
	}
	
	if(prtGnfdSeq == ""){
		alert("출력선택된 발령건이 없습니다.");
		return false;
	}
	
	this.ds_condPrint.addRow();
	this.ds_condPrint.setColumn(0, "GNFD_SEQ", prtGnfdSeq);
	this.ds_condPrint.setColumn(0, "EMPNO", prtEmpno);
	this.ds_condPrint.setColumn(0, "NTICE_MAN", this.fn_getEmpno());
	
	switch(prtSe){
		case "1":
			this.gfn_ReportPopup(this, "MIS/HRM/HRM030201_R01.crf", this.ds_condPrint);
		break;
		case "2":
			this.gfn_ReportPopup(this, "MIS/HRM/HRM030201_R02.crf", this.ds_condPrint);
		break;
		case "3":
			this.gfn_ReportPopup(this, "MIS/HRM/HRM030201_R03.crf", this.ds_condPrint);
		break;
	}
}

