this.ds_resveList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "USE_BGNDE") {
		//this.ds_resveList.setColumn(e.row, "REQST_DE", e.newvalue);
	}
	if(e.columnid == "USE_BGNDE" || e.columnid == "USE_ENDDE") {
		var s_USE_BGNDE = this.ds_resveList.getColumn(e.row, "USE_BGNDE");
		var s_USE_ENDDE = this.ds_resveList.getColumn(e.row, "USE_ENDDE");
				
		if(s_USE_BGNDE == "" || s_USE_ENDDE == "") {
			this.ds_resveList.setColumn(e.row, "MDWK_DAYCNT",0);
			this.ds_resveList.setColumn(e.row, "WKEND_DAYCNT",0);
			return;
		}		
		
		if(s_USE_BGNDE > s_USE_ENDDE) {
			alert("시작일이 종료일이후 일자입니다.");
			return;
		}
		
		var d_USE_BGNDE = new Date(parseInt(s_USE_BGNDE.substring(0,4),10),parseInt(s_USE_BGNDE.substring(4,6),10)-1,parseInt(s_USE_BGNDE.substring(6,8),10));
		var d_USE_ENDDE = new Date(parseInt(s_USE_ENDDE.substring(0,4),10),parseInt(s_USE_ENDDE.substring(4,6),10)-1,parseInt(s_USE_ENDDE.substring(6,8),10));
		
		var n_MDWK_DAYCNT = 0;
		var n_WKEND_DAYCNT = 0;
		while(true){
			var n_day = d_USE_BGNDE.getDay();
			if( n_day == 0 || n_day == 6 ) {
				n_WKEND_DAYCNT++;
			} else {
				n_MDWK_DAYCNT++;
			}
			
			d_USE_BGNDE.addDate(1);
			if(d_USE_BGNDE>=d_USE_ENDDE) break;
		}
		
		
		this.ds_resveList.setColumn(e.row, "MDWK_DAYCNT", n_MDWK_DAYCNT);
		this.ds_resveList.setColumn(e.row, "WKEND_DAYCNT", n_WKEND_DAYCNT);
	}
	else if(e.columnid == "SPORT_AMOUNT" || e.columnid == "RUM_CHRGE") {
		var s_SPORT_AMOUNT = this.ds_resveList.getColumn(e.row, "SPORT_AMOUNT");
		var s_RUM_CHRGE = this.ds_resveList.getColumn(e.row, "RUM_CHRGE");
		if(s_SPORT_AMOUNT == "" || s_SPORT_AMOUNT == null) s_SPORT_AMOUNT = "0";
		if(s_RUM_CHRGE == "" || s_RUM_CHRGE == null) s_RUM_CHRGE = "0";
		var n_AMT_SUM = parseInt(s_SPORT_AMOUNT) + parseInt(s_RUM_CHRGE);
		this.ds_resveList.setColumn(e.row, "AMT_SUM", n_AMT_SUM);
	}
	if(e.columnid == "CHK") {
		var s_SELF_CNFIRM_AT = obj.getColumn(e.row, "CONFM_AT");
		var s_CHK = obj.getColumn(e.row, "CHK");
		
		if(s_SELF_CNFIRM_AT == "1" && s_CHK == "1") {
			alert("확인완료건은 선택할 수 없습니다.");
			obj.setColumn(e.row, "CHK", "0");
		}
	}
}

this.div_work_grd_resveList_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	this.fn_pop();
}


this.fn_pop= function()
{
	var nLeft = system.clientToScreenX(this, 10);
	var nTop  = system.clientToScreenY(this, 10);     
	var newChild = new ChildFrame;
    newChild.init("Popup1", "absolute", nLeft, nTop, 400, 400, null, null , "com::empno_P01.xfdl");
	newChild.set_openalign("center middle");
	newChild.set_dragmovetype("all");  
	newChild.set_showtitlebar(true);
    newChild.showModal(this.getOwnerFrame(), {deptCd:this.ds_resveCond.getColumn(0, "DEPT_CODE")} , this , "fn_callback1");
}

this.fn_callback1 = function(sPopupId, sReturn)
{
	if(sPopupId == "Popup1")
	{	
		var dsEmpno = new Dataset();
		nRowCnt = dsEmpno.loadXML(sReturn);
        if(nRowCnt){
			this.ds_resveList.setColumn(this.ds_resveList.rowposition,"APPLCNT_EMPNO",dsEmpno.getColumn(0, "EMPNO"));
			this.ds_resveList.setColumn(this.ds_resveList.rowposition,"APPLCNT_EMPNM",dsEmpno.getColumn(0, "EMPNM"));
			//this.ds_resveList.setColumn(this.ds_resveList.rowposition,"RSPOFC_SE",dsEmpno.getColumn(0, "RSPOFC_SE"));
			this.ds_resveList.setColumn(this.ds_resveList.rowposition,"DEPT_CODE",dsEmpno.getColumn(0, "DEPT_CODE"));
			this.ds_resveList.setColumn(this.ds_resveList.rowposition,"DEPT_NM",dsEmpno.getColumn(0, "DEPT_NM"));
        }
	}
}
