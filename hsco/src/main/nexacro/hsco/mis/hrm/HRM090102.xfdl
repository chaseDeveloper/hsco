 */
this.div_work_btn_rowInsert02_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rlRow = this.ds_resveList.rowposition;
	var reqstDe      = this.ds_resveList.getColumn(rlRow, "REQST_DE");
	var applcntEmpno = this.ds_resveList.getColumn(rlRow, "APPLCNT_EMPNO");
	var registDT     = this.ds_resveList.getColumn(rlRow, "REGIST_DT");
	
	if(!registDT) {
		this.gfn_message("errors.값확인.요청", "유효한 예약신청내역이|예약신청내역을 다시");
		return false;
	}
	else {
		this.ds_resveDtlCond.setColumn(0, 'REQST_DE'     , reqstDe);
		this.ds_resveDtlCond.setColumn(0, 'APPLCNT_EMPNO', applcntEmpno);
		this.fn_transaction("getDtlInfo");
		var nRow = this.ds_resveDtl.addRow();
		this.ds_resveDtl.setColumn(nRow, "REQST_DE", reqstDe);
		this.ds_resveDtl.setColumn(nRow, "APPLCNT_EMPNO", applcntEmpno);
	}
}

