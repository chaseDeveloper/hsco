/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/


/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(콘도예약신청정보)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(예약신청내역)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_resveCond, this.ds_validation)) {
		return;
	}
	this.ds_resveList.clearData();
	this.fn_transaction("selectResveList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_resveList) && !this.comUtils.isDatasetUpdated(this.ds_resveDtl)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(예약신청내역)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_resveList, this.ds_validation)) {
		return;
	}
	// validation 체크(예약신청상세)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_resveDtl, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_resveList.rowposition;
		this.fn_transaction("saveResveCUD");
	}
}


/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "getCndmnmList":				// 콘도현황 조회
			var sController   	= "/hsco/mis/hrm/HRM090102/getCndmnmList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_cndmnmList=output1";
			break;
		case "selectResveList":				// 콘도예약신청 조회
			var sController   	= "/hsco/mis/hrm/HRM090102/selectResveList.do";
			var sInDatasets   	= "input1=ds_resveCond";
			var sOutDatasets  	= "ds_resveList=output1";
			break;
		case "saveResveCUD":	 			// 콘도예약신청 저장
			var sController   	= "/hsco/mis/hrm/HRM090102/saveResveCUD.do";
			var sInDatasets   	= "input1=ds_resveList:U input2=ds_resveDtl:U";
			var sOutDatasets  	= "";
			break;
		case "selectResveDtlList":	 		// 콘도예약신청 상세 조회
			var sController   	= "/hsco/mis/hrm/HRM090102/selectResveDtlList.do";
			var sInDatasets   	= "input1=ds_resveDtlCond";
			var sOutDatasets  	= "ds_resveDtl=output1";
			break;
		case "getDtlInfo":	 				// 콘도예약신청 입력시 자동입력 정보 조회
			var sController   	= "/hsco/mis/hrm/HRM090102/getDtlInfo.do";
			var sInDatasets   	= "input1=ds_resveDtlCond";
			var sOutDatasets  	= "ds_dtlInfo=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "getCndmnmList":		// 조회(콘도현황)
				this.fn_search();
				break;
			case "selectResveList":		// 조회(콘도예약신청정보)
				if(this.saveRow > -1){
					this.ds_resveList.set_rowposition(this.saveRow);
					this.saveRow = -1;
					this.fn_transaction("selectResveDtlList");
				}
				break;
			case "saveResveCUD": 		// 저장(콘도예약신청)
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectResveDtlList":	// 조회(콘도예약신청상세정보)
				break;
			case "getDtlInfo":			// 조회(콘도예약신청상세 자동입력정보)
				var nRow = this.ds_resveDtl.rowposition;
				this.ds_resveDtl.setColumn(nRow, "BGNDE", this.ds_dtlInfo.getColumn(0, "BGNDE"));
				this.ds_resveDtl.setColumn(nRow, "ENDDE", this.ds_dtlInfo.getColumn(0, "ENDDE"));
				this.ds_resveDtl.setColumn(nRow, "MDWK_DAYCNT", this.ds_dtlInfo.getColumn(0, "MDWK_DAYCNT"));
				this.ds_resveDtl.setColumn(nRow, "WKEND_DAYCNT", this.ds_dtlInfo.getColumn(0, "WKEND_DAYCNT"));
				break;
		}
	}
}

