/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	// 수정가능여부 체크
	if(this.updateYn != 'Y'){
		this.gfn_message("info.처리불가", this.message);
		return;
	}
	
	if (this.gfn_message("confirm.취소여부")) {
		
		this.ds_workScreAcmslt.reset();			//DATASET ROLLBACK
		this.ds_workScreAcmslt.applyChange();		//DATASET COMMIT
		
		this.ds_jobPrtnDtlsAcmsltList.reset();			//DATASET ROLLBACK
		this.ds_jobPrtnDtlsAcmsltList.applyChange();		//DATASET COMMIT
		
		this.ds_acmsltPd.reset();			//DATASET ROLLBACK
		this.ds_acmsltPd.applyChange();		//DATASET COMMIT
		return;
	}
}

