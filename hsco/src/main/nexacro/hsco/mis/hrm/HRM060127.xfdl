	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "HRM");
	this.File_UpDownManager.init(fileConfig);
}

/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	
	// 공통코드
	var comboParams = [ ["ds_hrm049",        "HRM049", 1, ""], 
						["ds_hrm053",        "HRM053", 1, ""], 
						["ds_hrm054",        "HRM054", 1, ""]
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		this.div_search.sta_registText.set_text("");
		
		this.div_search.div_EMP.fn_setBind("ds_cond","EMPNO","EMPNM");
		this.div_HOPE_DEPT1.fn_setBind("ds_workScreAcmslt", "HOPE_DEPT1", "HOPE_DEPT1_NM");
		this.div_HOPE_DEPT2.fn_setBind("ds_workScreAcmslt", "HOPE_DEPT2", "HOPE_DEPT2_NM");
		
		// 인사결재자("HRM_ADMIN") 체크
		if(this.gfn_authGrpId("HRM_ADMIN") == -1 && this.gfn_authGrpId("HRM_EVL") == -1) {
			//this.div_search.div_EMP.set_enable(true);
			
			this.div_search.div_EMP.div_sear.edt_emplnm.set_readonly(true);
			this.div_search.div_EMP.div_sear.edt_empno.set_readonly(true);
			this.div_search.div_EMP.div_sear.edt_emplnm.set_cssclass("edt_WF_Readonly");
			this.div_search.div_EMP.div_sear.edt_empno.set_cssclass("edt_WF_Readonly");
			this.div_search.div_EMP.div_sear.search.set_visible(false);
		}
		
		
		this.ds_cond.setColumn(nRow, "EVL_YEAR", this.comUtils.getClientDate("YYYY"));		// 당해년도
		this.ds_cond.setColumn(nRow, "HT_SE", this.comUtils.getClientDate("MM") <= '06' ? '01' : '02');
		
		this.ds_cond.setColumn(nRow, "EMPNO", this.fn_getEmpno());
		this.ds_cond.setColumn(nRow, "EMPNM", this.fn_getEmpnm());
		this.ds_cond.setColumn(nRow, "ACMSLT_SE", "02");
		
		this.fn_transaction("selectSearchList");		// (검색조건에 해당하는 사원의 가장 마지막 근무평정 기간을 초기 검색조건으로 설정)
	};
	
	this.gfn_comboLoad (comboParams, callback);
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/


/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(등급별인원비율)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.ds_workScreAcmslt.clearData();
	this.edt_evlScore.set_value("");
	this.edt_evlRank.set_value("");
	
	var year = this.ds_cond.getColumn(0, "EVL_YEAR");
	if(year >= 2020) {
		this.ds_cond.setColumn(0, "YEAR_SE", "02");
	} else {
		this.ds_cond.setColumn(0, "YEAR_SE", "01");
	}
	
	this.fn_transaction("selectEmpChk");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 수정가능여부 체크
	if(this.updateYn != 'Y'){
		this.gfn_message("info.처리불가", this.message);
		return;
	}
	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_workScreAcmslt)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}else{
		if(this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("saveWorkScreAcmslt");
		}
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// 최초 조회(검색조건에 해당하는 사원의 가장 마지막 근무평정 기간을 초기 검색조건으로 설정)
		case "selectSearchList":	
			var sController   	= "/hsco/mis/hrm/HRM060126/selectSearchList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_condTemp=output1";
			break;
		// 제외 대상자 목록 조회
		case "selectEmpChk":	
			var sController   	= "/hsco/mis/hrm/HRM060126/selectEmpChk.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_empChk=output1";
			break;
		case "selectWorkScreAcmslt":		//조회
			var sController   	= "/hsco/mis/hrm/HRM060127/selectWorkScreAcmslt.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_workScreAcmslt=output1 ds_jobPrtnDtlsAcmsltList=output2 ds_acmsltPd=output3 ds_work_evl_trgter=output4"
			break;
		case "saveWorkScreAcmslt":			//저장
			var sController   	= "/hsco/mis/hrm/HRM060127/saveWorkScreAcmslt.do";
			var sInDatasets   	= "input1=ds_workScreAcmslt:A";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/hrm/HRM060127/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_fileList=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			// 최초 조회(검색조건에 해당하는 사원의 가장 마지막 근무평정 기간을 초기 검색조건으로 설정)
			case "selectSearchList":		
				
				if(this.ds_condTemp.rowcount == 0) {					
					this.updateYn = 'N';
					
					this.gfn_message("info.처리불가", "해당하는 근무평가 기간이 없습니다.");
					this.message = "현재 입력된 사원은 대상자가 아닙니다.";
						
					this.Combo01.set_cssclass("cmb_WF_Readonly");
					this.Combo02.set_cssclass("cmb_WF_Readonly");
					this.edt_pc.set_cssclass("edt_WF_Readonly");
					this.Edit34.set_cssclass("edt_WF_Readonly");
					this.Edit00.set_cssclass("edt_WF_Readonly");
					this.TextArea01.set_cssclass("txt_WF_Readonly");
					this.div_HOPE_DEPT1.set_enable(false);
					this.div_HOPE_DEPT2.set_enable(false);
					
					this.Combo01.set_readonly(this.updateYn == 'Y' ? false : true);
					this.Combo02.set_readonly(this.updateYn == 'Y' ? false : true);
					this.edt_pc.set_readonly(this.updateYn == 'Y' ? false : true);
					this.Edit34.set_readonly(this.updateYn == 'Y' ? false : true);
					this.Edit00.set_readonly(this.updateYn == 'Y' ? false : true);
					this.TextArea01.set_readonly(this.updateYn == 'Y' ? false : true);
					this.chk_nowAtitdEtc.set_readonly(this.updateYn == 'Y' ? false : true);
					
					return;
				}
				this.ds_cond.setColumn(0, "EVL_YEAR", this.ds_condTemp.getColumn(0, "EVL_YEAR"));
				this.ds_cond.setColumn(0, "HT_SE", this.ds_condTemp.getColumn(0, "HT_SE"));
				
				this.fn_search();
				break;
			case "selectEmpChk":
				// 대상자 목록 조회				
				if(this.ds_empChk.getColumn(0, "EMP_CNT") == 0) {
					this.ds_workScreAcmslt.clearData();
					this.ds_jobPrtnDtlsAcmsltList.clearData();
					this.ds_acmsltPd.clearData();
					this.ds_fileList.clearData();
					
					this.div_search.sta_registText.set_text("");
					
					this.updateYn = 'N';
					
					this.gfn_message("info.처리불가", "현재 입력된 사원은 대상자가 아닙니다.");
					this.message = "현재 입력된 사원은 대상자가 아닙니다.";
						
					this.Combo01.set_cssclass("cmb_WF_Readonly");
					this.Combo02.set_cssclass("cmb_WF_Readonly");
					this.edt_pc.set_cssclass("edt_WF_Readonly");
					this.Edit34.set_cssclass("edt_WF_Readonly");
					this.Edit00.set_cssclass("edt_WF_Readonly");
					this.TextArea01.set_cssclass("txt_WF_Readonly");
					this.div_HOPE_DEPT1.set_enable(false);
					this.div_HOPE_DEPT2.set_enable(false);
					
					this.Combo01.set_readonly(this.updateYn == 'Y' ? false : true);
					this.Combo02.set_readonly(this.updateYn == 'Y' ? false : true);
					this.edt_pc.set_readonly(this.updateYn == 'Y' ? false : true);
					this.Edit34.set_readonly(this.updateYn == 'Y' ? false : true);
					this.Edit00.set_readonly(this.updateYn == 'Y' ? false : true);
					this.TextArea01.set_readonly(this.updateYn == 'Y' ? false : true);
					this.chk_nowAtitdEtc.set_readonly(this.updateYn == 'Y' ? false : true);
					
					return;
				}
				
				this.fn_transaction("selectWorkScreAcmslt");
				break;
			case "selectWorkScreAcmslt":
				// 등록기간 텍스트 설정
				var text = "";
				if(this.ds_acmsltPd.rowcount > 0){
					var bgnDe = this.dateUtils.format(this.ds_acmsltPd.getColumn(0, "INDVDL_BGNDE"),"yyyy-mm-dd");
					var endDe = this.dateUtils.format(this.ds_acmsltPd.getColumn(0, "INDVDL_ENDDE"),"yyyy-mm-dd");
					text = "(등록기간 : " + bgnDe + " ~ " + endDe + ")";
					
					this.updateYn = this.ds_acmsltPd.getColumn(0, "UPDATE_YN");
					
					var dcsnDe = this.ds_acmsltPd.getColumn(0, "DCSN_DE");
					var today = this.gfn_today();
					
					if(!this.comUtils.isNull(dcsnDe) && (dcsnDe <= '20180301') ){
						if(this.ds_work_evl_trgter.rowcount > 0){
							var evlScore = this.ds_work_evl_trgter.getColumn(0, "TOT_EVL_SCORE");
							var evlRank = this.ds_work_evl_trgter.getColumn(0, "SCORE_RNK");
							
							this.edt_evlScore.set_value(evlScore);
							this.edt_evlRank.set_value(evlRank);
						}
					}
					
				}else {
					text = "(등록기간 지정안됨.)"
					
					this.updateYn = 'N';
				}
				
				this.div_search.sta_registText.set_text(text);
				
				//alert(this.updateYn);
				// 수정 가능 여부에 따른 알림 설정
				if(this.updateYn == 'Y'){
					this.Combo01.set_cssclass("");
					this.Combo02.set_cssclass("");
					this.edt_pc.set_cssclass("");
					this.Edit34.set_cssclass("");
					this.Edit00.set_cssclass("");
					this.TextArea01.set_cssclass("");
					this.div_HOPE_DEPT1.set_enable(true);
					this.div_HOPE_DEPT2.set_enable(true);
				} else {
					this.gfn_message("info.처리불가", "등록 기간이 아닙니다.");
					this.message = "등록 기간이 아닙니다.";
					
					this.Combo01.set_cssclass("cmb_WF_Readonly");
					this.Combo02.set_cssclass("cmb_WF_Readonly");
					this.edt_pc.set_cssclass("edt_WF_Readonly");
					this.Edit34.set_cssclass("edt_WF_Readonly");
					this.Edit00.set_cssclass("edt_WF_Readonly");
					this.TextArea01.set_cssclass("txt_WF_Readonly");
					this.div_HOPE_DEPT1.set_enable(false);
					this.div_HOPE_DEPT2.set_enable(false);
				}
				
				this.Combo01.set_readonly(this.updateYn == 'Y' ? false : true);
				this.Combo02.set_readonly(this.updateYn == 'Y' ? false : true);
				this.edt_pc.set_readonly(this.updateYn == 'Y' ? false : true);
				this.Edit34.set_readonly(this.updateYn == 'Y' ? false : true);
				this.Edit00.set_readonly(this.updateYn == 'Y' ? false : true);
				this.TextArea01.set_readonly(this.updateYn == 'Y' ? false : true);
				this.chk_nowAtitdEtc.set_readonly(this.updateYn == 'Y' ? false : true);
				
				if(this.ds_workScreAcmslt.rowposition > -1){
					this.grd_cmmnAtchmnfl.setCellProperty("Head", 0, "text", "0");
					this.ds_cond.setColumn(0, "FILE_SN", this.ds_workScreAcmslt.getColumn(this.ds_workScreAcmslt.rowposition, "FILE_SN"));
					
					this.fn_transaction("selectFileList");
				}else{
					this.ds_fileList.clearData();
				}
				break;
			case "saveWorkScreAcmslt":
				this.fn_search();
				break;
			case "selectFileList":
				break;
		}
	}
}


