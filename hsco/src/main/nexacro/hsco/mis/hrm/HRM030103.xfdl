 * 설명 		: Popup Call 이후 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	var dsRtn = new Dataset();
	nRowCnt = dsRtn.loadXML(sReturn);
	
	if(!nRowCnt){
		
	} else {
		
		switch (sPopupId) {
			case "HRM010102_P1":	// 사업소
				var nRow = this.ds_hr_mastr.rowposition;
				this.ds_hr_mastr.setColumn(nRow, "BIZPLC_CODE", dsRtn.getColumn(0, "BIZPLC_CODE"));
				this.ds_hr_mastr.setColumn(nRow, "BIZPLC_NM",   dsRtn.getColumn(0, "BIZPLC_NM"));
				break;
		}		
	}
}


/**********************************************************************
* 사진 관련 이벤트 처리
***********************************************************************/
/*
 * 함수명  	: btn_photo_onclick
 * 설명 		: 사진첨부 버튼 클릭 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_photo_onclick();
 */
this.btn_photo_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_hr_mastr.rowposition >= 0){
		if(this.ds_hr_mastr.getRowType(this.ds_hr_mastr.rowposition) != 2){
			if(this.ds_fileList.rowcount < 1){
				
				this.mCallback = function() {
					this.fn_autoFileSn(
						 "MIS"									// 대분류   (SYS_SE)
						,"HRM"									// 업무구분 (JOB_SE)
						, this.File_UpDownManager	            // fileManager 오브젝트
						, this.ds_fileList		                // fileObject
						, this.ds_hr_mastr	            		// fileSn을 불러오거나 반영해야 하는 컬럼
						, "TBHRM_HR_MASTR"         				// fileSn을 저장할 테이블
						, "FILE_SN"								// fileSn 컬럼명이 다를 경우를 대비
						, ["EMPNO"]							// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
					);
				}
				
				//this.gfn_message("info.저장.초과", "이미지 파일만", null, null, mCallback);
					this.mCallback();
				
			} else {
				this.gfn_message("info.저장.초과", "최대 1개 까지만");
			}
		} else {
			this.gfn_message("info.진행가능", "신규 사원은 저장");
		}
	} else {
		this.gfn_message("info.항목.선택.알림", "사원");
	}
}

/*
 * 함수명  	: btn_del_photo_onclick
 * 설명 		: 사진삭제 버튼 클릭 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_del_photo_onclick();
 */
this.btn_del_photo_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var fileSn = this.ds_hr_mastr.getColumn(this.ds_hr_mastr.rowposition, "FILE_SN");
	if(!fileSn){
		this.gfn_message("info.파일정보.없음");
	} else {
		if ( this.gfn_message("confirm.삭제여부") ) {
			this.ds_fileList.deleteAll();
			
			//this.div_empInfo.img_photo.set_image("URL('theme://images/image_sample.png')");
			
			this.fn_transaction("deleteFile");
			
			
		}
	}
}

