/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : false,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess,	// 전송 성공 이벤트 핸들러
		onError : this.fn_onFileError,
		allowTypes : ["jpg","jpeg","gif","png"]	// 선택 허용 파일유형
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "HRM");
	this.File_UpDownManager.init(fileConfig);
	
	var dsTemp1 = new Dataset();
	var loadCnt1 = dsTemp1.loadXML(this.gfn_getLinkParam("mis_hrm::HRM030103.xfdl"));		// dsTemp1 : 인사정보
	
	if(loadCnt1) {
		
		var empno   = dsTemp1.getColumn(0, "EMPNO"        );	// 사원번호
		var empnm   = dsTemp1.getColumn(0, "EMPNM"        );	// 사원번호
		var fileSn  = dsTemp1.getColumn(0, "FILE_SN"      );	// 첨부파일(사진)
		this.type   = dsTemp1.getColumn(0, "TYPE"      );	    // 링크유형 I:신규 / U:조회
		
		if(this.type == "U"){
			this.ds_cond.clearData();
			var nRow = this.ds_cond.addRow();
			this.ds_cond.setColumn(nRow, "EMPNO", empno);
			this.ds_cond.setColumn(nRow, "EMPNM", empnm);
			this.ds_cond.setColumn(nRow, "FILE_SN", fileSn);
		}
	} else {
		this.ds_cond.clearData();
		var nRow = this.ds_cond.addRow();
		this.ds_cond.setColumn(nRow, "EMPNO", this.gfn_getEmpNo());
		this.ds_cond.setColumn(nRow, "EMPNM", this.gfn_getUserName());
	}
	
	// 해당 화면에 대한 파라미터 삭제 (해당에 대한 공통함수가 없으므로 직접 구현)
	var strExpr1 = "PAGE_URL=='mis_hrm::HRM030103.xfdl'";
	var findRow1 = application.gds_mdiParam.findRowExpr(strExpr1);
	application.gds_mdiParam.deleteRow(findRow1);
}




