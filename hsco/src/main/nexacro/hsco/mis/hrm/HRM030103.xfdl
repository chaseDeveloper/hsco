	   ,["ds_hrm015", "HRM015", "Y", "S", "B"]	// ds_hrm015 : 혈액형구분
	   ,["ds_hrm016", "HRM016", "Y", "S", "B"]	// ds_hrm016 : 학위종별/구분
	   ,["ds_hrm017", "HRM017", "Y", "S", "B"]	// ds_hrm017 : 자격증등급
