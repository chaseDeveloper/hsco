***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectHrMastr": // 인사마스터 직원목록 불러오기
			var sController   	= "hsco/mis/hrm/HRM030103/selectHrMastr.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_hr_mastr=output1";
			sOutDatasets       += " ds_hr_family=output2";			// 탭2  : 가족사항
			sOutDatasets  	   += " ds_hr_acdmcr=output3";			// 탭3  : 학력사항
			sOutDatasets  	   += " ds_hr_qualf=output4";			// 탭4  : 자격사항
			sOutDatasets  	   += " ds_hr_career=output5";			// 탭5  : 경력사항
			//sOutDatasets  	   += " ds_hr_bsrp=output7";			// 탭7  : 국외출장현황
			sOutDatasets  	   += " ds_hr_gnfd=output8";			// 탭8  : 발령사항
			sOutDatasets  	   += " ds_hr_rwardDtls=output9";		// 탭9  : 포상내역
			sOutDatasets  	   += " ds_hr_dscplDsps=output10";		// 탭10 : 징계내역
			sOutDatasets  	   += " ds_hr_fggg=output11";		// 탭11 : 외국어
			break;
		case "selectPhoto":   // 사진 파일 조회
			var sController   	= "hsco/mis/hrm/HRM030103/selectPhoto.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_fileList=output1";
			break;
		case "saveHrMastr":		// 저장
			var sController   	= "hsco/mis/hrm/HRM030103/saveHrMastr.do";
			var sOutDatasets  	= "";
			var sInDatasets   	= "input1=ds_hr_mastr:U";
			// 탭2 : 가족사항
			sInDatasets  	   += " input2=ds_hr_family:U";
			// 탭3 : 학력사항
			sInDatasets  	   += " input3=ds_hr_acdmcr:U";
			// 탭4 : 자격사항
			sInDatasets  	   += " input4=ds_hr_qualf:U";
			// 탭5 : 경력사항
			sInDatasets  	   += " input5=ds_hr_career:U";
			// 탭11: 외국어
			sInDatasets  	   += " input6=ds_hr_fggg:U";
			break;
		case "selectFnncInsttCode":	// 금융기관 코드정보
			var sController   	= "hsco/mis/hrm/HRM030103/selectFnncInsttCode.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fnncInstt=output1";
			break;
		case "deleteFile":
			var sController  = "hsco/cmm/file/deleteFile.do";
			var sInDatasets  = "input1=ds_fileList:U";
			var sOutDatasets = "ds_fileList=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

/**********************************************************************
 * Transaction Callback 영역
***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectHrMastr":
				// 첨부파일번호 존재 확인
				var fileSn = this.ds_hr_mastr.getColumn(0, "FILE_SN");
				if(fileSn){
					this.ds_cond.setColumn(0, "FILE_SN", fileSn);
					this.fn_photo_search();
				}
				break;
			case "selectPhoto":
				// 첨부파일(사진) 표시 실행
				if(this.ds_fileList.rowcount > 0){
					this.fn_photo_view();
				}
				break;
			case "saveHrMastr":
				this.gfn_message("success.처리.성공");
				if(this.delYn == "N"){
					this.div_empInfo.edt_EMPNO.set_cssclass("edt_WF_Readonly");
					this.div_empInfo.edt_EMPNO.set_readonly(true);
					this.fn_search();
				} else{
					// 삭제 처리 후 신규 등록 상태로 변경
					this.div_empInfo.edt_EMPNO.set_cssclass("edt_WF_Essential");
					this.div_empInfo.edt_EMPNO.set_readonly(false);
					
					this.ds_hr_mastr.clearData();
					var nRow = this.ds_hr_mastr.addRow();
					this.ds_hr_mastr.setColumn(nRow, "JSSFC_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "HR_CLSF_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "STTUS_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "OFCPS_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "JBLN_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "BDP_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "MLTDTCS_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "MSCL_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "CURIMPT_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "CLSS_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "SALARY_SE", "");
					this.ds_hr_mastr.setColumn(nRow, "SALARY_PYMNT_BANK_SE", "");
					
					this.ds_cond.clearData();
					this.delYn = "N";
				}
				break;
			case "selectFnncInsttCode":		// 금융기관 코드정보
				this.ds_fnncInstt.insertRow(0);
				this.ds_fnncInstt.setColumn(0, "FNNC_INSTT_CODE", "");
				this.ds_fnncInstt.setColumn(0, "FNNC_INSTT_CODE_NM", "선택");
				break;
			case "deleteFile":				
				// 인사기본정보 데이터셋의 상태가 삭제일 경우, 저장 트랜잭션 실행
				if(this.delYn == "Y"){
					this.fn_transaction("saveHrMastr");
				}else{
					this.gfn_message("success.처리.성공");
				}
				break;
			
		}
	}
}

