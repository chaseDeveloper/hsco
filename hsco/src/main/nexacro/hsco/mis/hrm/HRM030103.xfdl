/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	try{
		// update check
		if (   !this.comUtils.isDatasetUpdated(this.ds_hr_mastr)		// 기본정보
			&& !this.comUtils.isDatasetUpdated(this.ds_hr_family)		// 탭2 : 가족사항
			&& !this.comUtils.isDatasetUpdated(this.ds_hr_acdmcr)		// 탭3 : 학력사항
			&& !this.comUtils.isDatasetUpdated(this.ds_hr_qualf)		// 탭4 : 자격사항
			&& !this.comUtils.isDatasetUpdated(this.ds_hr_career)		// 탭5 : 경력사항
			&& !this.comUtils.isDatasetUpdated(this.ds_hr_fggg)			// 탭11 : 외국어
		   ) {
			this.gfn_message("comm.데이터.변경.없음");
			return false;
		}
		
		
		if((this.gfn_authGrpId("HRM_ADMIN") == -1)) {
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_hr_mastr, this.ds_validation2)) {			// 기본정보
				return false;
			}
		} else {
			// validation check
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_hr_mastr, this.ds_validation)) {			// 기본정보
				this.tab_hrInfo.set_tabindex(0);
				return false;
			}
			else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hr_family, this.ds_validation)) {	// 탭2 : 가족사항
				this.tab_hrInfo.set_tabindex(1);
				return false;
			}
			else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hr_acdmcr, this.ds_validation)) {	// 탭3 : 학력사항
				this.tab_hrInfo.set_tabindex(2);
				return false;
			}
			else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hr_qualf, this.ds_validation)) {		// 탭4 : 자격사항
				this.tab_hrInfo.set_tabindex(3);
				return false;
			}
			else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hr_career, this.ds_validation)) {	// 탭5 : 경력사항
				this.tab_hrInfo.set_tabindex(4);
				return false;
			}
			else if(!this.gfn_checkValidation_NoUpdChk(this.ds_hr_fggg, this.ds_validation)) {	// 탭11 : 외국어
				this.tab_hrInfo.set_tabindex(9);
				return false;
			}
		}
	}catch(e) {
		alert(e.message);
	}
	
	if(this.ds_hr_mastr.rowposition > -1){
		// save
		if(this.gfn_message("confirm.저장.여부"))	{
			
			this.ds_cond.clearData();
			this.ds_cond.addRow();
			
			this.ds_cond.setColumn(0, "EMPNO", this.ds_hr_mastr.getColumn(0, "EMPNO"));
			this.ds_cond.setColumn(0, "EMPNM", this.ds_hr_mastr.getColumn(0, "EMPNM"))
			this.fn_transaction("saveHrMastr");
		}
	}
}


