/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	if(this.gfn_authGrpId("HRM_ADMIN") == -1) {
		alert("일반사용자의 경우, 인사정보를 삭제할 수 없습니다.");
		return false;
	}
	
	var nRow = this.ds_hr_mastr.rowposition;
	if(nRow > -1){
		if(!this.gfn_message("confirm.삭제여부")) {
			return false;	// 아니오
		}
		var fileSn = this.ds_hr_mastr.getColumn(nRow, "FILE_SN");
		
		this.delYn = "Y";
		this.ds_hr_mastr.deleteRow(nRow);
		
		this.ds_hr_family.clearData();
		this.ds_hr_acdmcr.clearData();
		this.ds_hr_qualf.clearData();
		this.ds_hr_career.clearData();
		
		// 첨부파일이 있을 경우, 첨부파일 삭제 후 사원삭제 진행
		if(!fileSn){
			this.fn_transaction("saveHrMastr");
		} else {
			//this.div_empInfo.img_photo.set_image("URL('theme://images/image_sample.png')");
			
			this.ds_fileList.deleteRow(0);
			this.fn_transaction("deleteFile");
		}
	}
	
	
}



