/*
 * 함수명  	: ds_evlDtls_oncolumnchanged
 * 설명 		: 데이터 변경 전 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_evlDtls_oncolumnchanged(obj,e);
 */
this.ds_evlDtls_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{	
	var evlSe = obj.getColumn(e.row, "EVL_SE");

	var val0 = !this.comUtils.isNull(obj.getColumn(e.row, "CODE_0")) ? nexacro.toNumber(obj.getColumn(e.row, "CODE_0")) : 0;
	var val1 = !this.comUtils.isNull(obj.getColumn(e.row, "CODE_1")) ? nexacro.toNumber(obj.getColumn(e.row, "CODE_1")) : 0;
	var val2 = !this.comUtils.isNull(obj.getColumn(e.row, "CODE_2")) ? nexacro.toNumber(obj.getColumn(e.row, "CODE_2")) : 0;
	var val3 = !this.comUtils.isNull(obj.getColumn(e.row, "CODE_3")) ? nexacro.toNumber(obj.getColumn(e.row, "CODE_3")) : 0;
	var val4 = !this.comUtils.isNull(obj.getColumn(e.row, "CODE_4")) ? nexacro.toNumber(obj.getColumn(e.row, "CODE_4")) : 0;
	
	// 라디오버튼 역할
	if(e.columnid == "CODE_0") {
		
		// 가점은 해제 됨
		if(evlSe != "2" && e.newvalue == "0") {
			if(val1 == 0 && val2 == 0 && val3 == 0 && val4 == 0) {
				return false;
			}
		}
		
	}else if(e.columnid == "CODE_1") {
			
		// 체크 해제
		if(evlSe != "2" && e.newvalue == "0") {
			if(val0 == 0 && val2 == 0 && val3 == 0 && val4 == 0) {
				return false;
			}
		}
	
	}else if(e.columnid == "CODE_2") {
		
		// 체크 해제
		if(evlSe != "2" && e.newvalue == "0") {
			if(val1 == 0 && val0 == 0 && val3 == 0 && val4 == 0) {
				return false;
			}
		}
	
	}else if(e.columnid == "CODE_3") {
		// 체크 해제
		if(evlSe != "2" && e.newvalue == "0") {
			if(val1 == 0 && val2 == 0 && val0 == 0 && val4 == 0) {
				return false;
			}
		}
	
	}else if(e.columnid == "CODE_4") {
		// 체크 해제
		if(evlSe != "2" && e.newvalue == "0") {
			if(val1 == 0 && val2 == 0 && val3 == 0 && val0 == 0) {
				return false;
			}
		}
	}
// 	// 평정위원점수 변경
 	else if(e.columnid == "EVL_SCORE") {
		
		var chkArray = [val0, val1, val2, val3, val4];
		var chkCol = "";
		
		for(var i = 0; i < chkArray.length; i++) {

			if(chkArray[i] == "1") {
				chkCol = "VAL"+i;
				break;
			}
		}
		
		// 평정기준점수 가져오기
		var evlIemSe   = !this.comUtils.isNull(obj.getColumn(e.row, "EVL_IEM_SE")) ? nexacro.toNumber(obj.getColumn(e.row, "EVL_IEM_SE")) : 0;
		var codeRow = this.ds_hrm074.findRow("CODE", evlIemSe);
		var chkVal = !this.comUtils.isNull(this.ds_hrm074.getColumn(codeRow, chkCol)) ? nexacro.toNumber(this.ds_hrm074.getColumn(codeRow, chkCol)) : 0;
		var codeNm = !this.comUtils.isNull(this.ds_hrm074.getColumn(codeRow, "CO")) ? nexacro.toNumber(this.ds_hrm074.getColumn(codeRow, chkCol)) : 0;

		// 평정기준점수 '부족' 의 값은 예외적으로 0~8 까지임.
		if(chkCol != "VAL4") {
			if((e.newvalue > chkVal) || (e.newvalue < (chkVal-2))) {
				this.gfn_message("info.처리불가","평정기준점수에 맞는 점수를 입력해주세요.");
				obj.setColumn(obj.rowposition, "EVL_SCORE", e.oldvalue);
				return false;
			}
		}else {
			if(e.newvalue < 0 || e.newvalue > 8) {
				this.gfn_message("info.처리불가","평정기준점수에 맞는 점수를 입력해주세요.");
				obj.setColumn(obj.rowposition, "EVL_SCORE", e.oldvalue);
				return false;
			}
		}
		
 	}
}


/*
 * 함수명  	: ds_evlDtls_oncolumnchanged
 * 설명 		: 데이터 변경 후 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_evlDtls_oncolumnchanged(obj,e);
 */
this.ds_evlDtls_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var evlIemSe    = !this.comUtils.isNull(obj.getColumn(e.row, "EVL_IEM_SE")) ? nexacro.toNumber(obj.getColumn(e.row, "EVL_IEM_SE")) : 0;
	var evlManCo    = this.ds_scrproEvlMfcmm.rowcount;
	var codeRow		= this.ds_hrm074.findRow("CODE", evlIemSe);

	// 라디오버튼 역할
 	if(e.columnid == "CODE_0") {
		
		if(e.newvalue == "1") {
			obj.setColumn(e.row, "CODE_1", "0");
			obj.setColumn(e.row, "CODE_2", "0");
			obj.setColumn(e.row, "CODE_3", "0");
			obj.setColumn(e.row, "CODE_4", "0");		
			
			var val = this.ds_hrm074.getColumn(codeRow, "VAL0");
			obj.setColumn(e.row, "EVL_SCORE", val);
			obj.setColumn(e.row, "EVL_CODE" , val);
		
		}else if(e.newvalue == "0") {
			obj.setColumn(e.row, "EVL_SCORE", "0");
			obj.setColumn(e.row, "EVL_CODE" , "0");
		}
		
	}else if(e.columnid == "CODE_1") {
		
		if(e.newvalue == "1") {
			obj.setColumn(e.row, "CODE_0", "0");
			obj.setColumn(e.row, "CODE_2", "0");
			obj.setColumn(e.row, "CODE_3", "0");
			obj.setColumn(e.row, "CODE_4", "0");		
		
			var val = this.ds_hrm074.getColumn(codeRow, "VAL1");
			obj.setColumn(e.row, "EVL_SCORE", val);
			obj.setColumn(e.row, "EVL_CODE" , val);

		}else if(e.newvalue == "0") {
			obj.setColumn(e.row, "EVL_SCORE", "0");
			obj.setColumn(e.row, "EVL_CODE" , "0");
		}
 	}else if(e.columnid == "CODE_2") {

		if(e.newvalue == "1") {
			obj.setColumn(e.row, "CODE_1", "0");
			obj.setColumn(e.row, "CODE_0", "0");
			obj.setColumn(e.row, "CODE_3", "0");
			obj.setColumn(e.row, "CODE_4", "0");		
		
			var val = this.ds_hrm074.getColumn(codeRow, "VAL2");
			obj.setColumn(e.row, "EVL_SCORE", val);
			obj.setColumn(e.row, "EVL_CODE" , val);
		}else if(e.newvalue == "0") {
			obj.setColumn(e.row, "EVL_SCORE", "0");
			obj.setColumn(e.row, "EVL_CODE" , "0");
		}
		
 	}else if(e.columnid == "CODE_3") {
		
		if(e.newvalue == "1") {
			obj.setColumn(e.row, "CODE_1", "0");
			obj.setColumn(e.row, "CODE_2", "0");
			obj.setColumn(e.row, "CODE_0", "0");
			obj.setColumn(e.row, "CODE_4", "0");		
		
			var val = this.ds_hrm074.getColumn(codeRow, "VAL3");
			obj.setColumn(e.row, "EVL_SCORE", val);
			obj.setColumn(e.row, "EVL_CODE" , val);
		}else if(e.newvalue == "0") {
			obj.setColumn(e.row, "EVL_SCORE", "0");
			obj.setColumn(e.row, "EVL_CODE" , "0");

		}
 	
 	}else if(e.columnid == "CODE_4") {
	
		if(e.newvalue == "1") {
			obj.setColumn(e.row, "CODE_1", "0");
			obj.setColumn(e.row, "CODE_2", "0");
			obj.setColumn(e.row, "CODE_3", "0");
			obj.setColumn(e.row, "CODE_0", "0");		
		
			var val = this.ds_hrm074.getColumn(codeRow, "VAL4");
			obj.setColumn(e.row, "EVL_SCORE", val);
			obj.setColumn(e.row, "EVL_CODE" , val);
		}else if(e.newvalue == "0") {
			obj.setColumn(e.row, "EVL_SCORE", "0");
			obj.setColumn(e.row, "EVL_CODE" , "0");
		}
 	}

	// 항목 변경에 대한 공통적인 기능
	if(e.columnid == "CODE_0" || e.columnid == "CODE_1" || e.columnid == "CODE_2" || e.columnid == "CODE_3" || e.columnid == "CODE_4") {
		
		var nRow = this.ds_evlInfo.rowposition;
		// 타평정위원 평정점수 합
		var excSumEvlScore = !this.comUtils.isNull(this.ds_evlInfo.getColumn(nRow, "EXC_SUM_EVL_SCORE")) ? nexacro.toNumber(this.ds_evlInfo.getColumn(nRow, "EXC_SUM_EVL_SCORE")) : 0;

		// 합계, 평균
		var sumEvlScore = obj.getSum("EVL_SCORE");
		
		var nRow = this.ds_evlInfo.rowposition;
		
		
		var empmnYear = this.ds_evlDtls.getColumn(e.row, "EMPMN_YEAR");
		var odrSe = this.ds_evlDtls.getColumn(e.row, "ODR_SE");
		var apyexmNo = this.ds_evlDtls.getColumn(e.row, "APYEXM_NO");
		
		this.ds_evlInfo.setColumn(nRow, "EMPMN_YEAR"	, empmnYear);
		this.ds_evlInfo.setColumn(nRow, "ODR_SE"	    , odrSe);
		this.ds_evlInfo.setColumn(nRow, "APYEXM_NO"	    , apyexmNo);
		
		this.ds_evlInfo.setColumn(nRow, "TOT_EVL_SCORE"	, sumEvlScore);
		this.ds_evlInfo.setColumn(nRow, "AVG_EVL_SCORE" , nexacro.round((excSumEvlScore+sumEvlScore)/evlManCo, 2));
	}
	
	// 소계(expr) 이 바로 적용되지 않는 문제 enableredraw 함수로 해결
	this.grd_evlDtls.set_enableredraw(false);
	this.grd_evlDtls.set_enableredraw(true);
}


/***********************************************************************
 * dataset 관련 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: ds_ecnySportGnrlList_onrowposchanged
 * 설명 		: ds_ecnySportGnrlList데이터셋 row 변경시 (마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_ecnySportGnrlList_onrowposchanged();
 */
this.ds_ecnySportGnrlList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1){
		// 이미지
		var nRow = this.ds_ecnySportGnrlList.rowposition;
		
		this.ds_cond_file.clearData();
		this.ds_cond_file.addRow();
		this.ds_cond_file.setColumn(0, "FILE_SN", this.ds_ecnySportGnrlList.getColumn(nRow, "PHOTO_FILE_ID"));

		this.ds_fileList.clearData();
		this.fn_transaction("selectAtchFile");
				
		// 평가내역
		var empmnYear = this.ds_cond.getColumn(0, "EMPMN_YEAR");
		var odrSe     = this.ds_cond.getColumn(0, "ODR_SE");
		var mfcmmNo  = this.ds_cond.getColumn(0, "MFCMM_NO");
		
		var nRow = this.ds_ecnySportGnrlList.rowposition;
		var apyexmNo   = this.ds_ecnySportGnrlList.getColumn(nRow, "RL_APYEXM_NO");
	
		this.ds_cond2.clearData();
		var nRow = this.ds_cond2.addRow();
		
		this.ds_cond2.setColumn(nRow, "EMPMN_YEAR", empmnYear);
		this.ds_cond2.setColumn(nRow, "ODR_SE"    , odrSe);
		this.ds_cond2.setColumn(nRow, "MFCMM_NO"  , mfcmmNo);
		this.ds_cond2.setColumn(nRow, "APYEXM_NO" , apyexmNo);
				
		this.fn_transaction("selectIntrvwEvlIEemList");
	}
}


