 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{

   // 데이터셋 미변경 체크
    if (!this.comUtils.isDatasetUpdated(this.ds_evlDtls)) {
        this.gfn_message("comm.데이터.변경.없음")
        return false;
    }

	// validation
   	for(var i=0; i<this.ds_evlDtls.rowcount; i++){
  		if(this.ds_evlDtls.getColumn(i, "EVL_IEM_SE") != '999') {
  		 	if(this.ds_evlDtls.getColumn(i, "EVL_SE") != "2") {
				if(this.ds_evlDtls.getColumn(i,"CODE_0")=="0" && this.ds_evlDtls.getColumn(i,"CODE_1")=="0" && this.ds_evlDtls.getColumn(i,"CODE_2")=="0"
				&& this.ds_evlDtls.getColumn(i,"CODE_3")=="0" && this.ds_evlDtls.getColumn(i,"CODE_4")=="0"){
					this.gfn_message("info.처리불가","평가하지 않은 항목이 있습니다.");
					return false;
				}
			}
  		}else {
 			this.ds_evlDtls.deleteRow(i);
 		}
   	}


    if(this.gfn_message("confirm.저장.여부")) {
        this.saveRow = this.ds_ecnySportGnrlList.rowposition;
        this.fn_transaction("intrvwEvlCU");
    
    }else {
		// 저장하지 않으면 지웠던 소계 생성
		this.ds_evlDtls.set_enableevent(false);
		this.ds_evlDtls.set_updatecontrol(false);
		
		var nRow = this.ds_evlDtls.findRow("EVL_SE", "2");
		// 자원봉사활동 요소 명칭
// 		this.ds_evlDtls.insertRow(nRow);
