this.fn_cancel = function()
{
	// 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_evlDtls)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }

	if (this.gfn_message("confirm.취소여부")) {
		this.ds_evlDtls.reset();			//DATASET ROLLBACK
		this.ds_evlDtls.applyChange();		//DATASET COMMIT
		return;
	}	
}
 /*
