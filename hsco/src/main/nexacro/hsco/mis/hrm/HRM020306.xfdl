/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// ========= 검색조건 =========
		case "selectEmpmnYearCombo": 		// 채용년도 조회(검색조건)
			//trace("selectEmpmnYearCombo");
			var sController   	= "hsco/mis/hrm/HRM020201/empmnYearComboList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_empmnYearList=output1";
			break;
		case "selectOdrSeCombo":			// 차수, 채용공고 조회(검색조건)
			//trace("selectOdrSeCombo");
			var sController   	= "hsco/mis/hrm/HRM020201/odrSeComboList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_odrSeList=output1";
			break;
		case "selectScrproEvlMfcmmList": 	// 평가위원 조회(검색조건)
			//trace("selectScrproEvlMfcmmList");
			var sController   	= "hsco/mis/hrm/HRM020305/scrproEvlMfcmmList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_scrproEvlMfcmm=output1";
			break;
		// =============================
		case "selectEcnySportGnrlList":	// 대상자 목록 조회
			var sController   	= "hsco/mis/hrm/HRM020306/selectEcnySportGnrlList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_ecnySportGnrlList=output1";
			break;
		case "selectIntrvwEvlIEemList":	//	면접세부정보 조회
			var sController   	= "hsco/mis/hrm/HRM020306/selectIntrvwEvlIEemList.do";
			var sInDatasets   	= "input1=ds_cond2";
			var sOutDatasets  	= "ds_evlDtls=output1 ds_evlInfo=output2";
			break;
		case "intrvwEvlCU":				// 면접 평가내역 생성 및 수정
			var sController   	= "hsco/mis/hrm/HRM020306/intrvwEvlCU.do";
			var sInDatasets   	= "input1=ds_evlDtls:U input2=ds_evlInfo:U";
			var sOutDatasets  	= "";
			break;
	        // ======== 첨부파일 관련 transaction ========
        case "selectAtchFile":			// 첨부파일 조회
            var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
            var sInDatasets  = "input1=ds_cond_file";
            var sOutDatasets = "ds_fileList=output1";
            break;

	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


