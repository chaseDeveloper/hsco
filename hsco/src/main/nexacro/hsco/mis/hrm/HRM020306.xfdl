 				this.fn_search();
			case "selectEcnySportGnrlList": // 대상자목록 불러오기
				
				break;
			case "selectIntrvwEvlIEemList": // 면접심사 내역 불러오기
				if(this.saveRow > -1){
					this.ds_ecnySportGnrlList.set_rowposition(this.saveRow);
