				}
				this.ds_evlDtls.set_enableevent(false);
				this.ds_evlDtls.set_updatecontrol(false);
 				var nRow = this.ds_evlDtls.findRow("EVL_SE", "2");
// 				// 자원봉사활동 요소 명칭
//  				this.ds_evlDtls.insertRow(nRow);
//  				this.ds_evlDtls.setColumn(nRow, "EVL_SE"    , "2");
//  				this.ds_evlDtls.setColumn(nRow, "EVL_IEM_SE", "999");
//  				this.ds_evlDtls.setColumn(nRow, "EVL_NM"    , "");
//  				this.ds_evlDtls.setColumn(nRow, "CODE_0"    , "금장");
//  				this.ds_evlDtls.setColumn(nRow, "CODE_1"    , "은장");
//  				this.ds_evlDtls.setColumn(nRow, "CODE_2"    , "동장");
//  				this.ds_evlDtls.setColumn(nRow, "CODE_3"    , "해바라기");
//  				this.ds_evlDtls.setColumn(nRow, "CODE_4"    , "-");
//  				this.ds_evlDtls.setColumn(nRow, "EVL_SCORE" , "0");		// 밸리데이션 체크를 위해
				// 소계
 				this.ds_evlDtls.insertRow(nRow);
 				this.ds_evlDtls.setColumn(nRow, "EVL_SE"    , "1");
 				this.ds_evlDtls.setColumn(nRow, "EVL_IEM_SE", "999");
 				this.ds_evlDtls.setColumn(nRow, "EVL_NM"    , "소	 계");
 				this.ds_evlDtls.setColumn(nRow, "EVL_SCORE" , "0");		// 밸리데이션 체크를 위해
				this.ds_evlDtls.set_updatecontrol(true);
				this.ds_evlDtls.set_enableevent(true);
				
			break;
			case "intrvwEvlCU": //면접심사내역 저장하기
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;	
			// ======== 첨부파일 관련 transaction ========
