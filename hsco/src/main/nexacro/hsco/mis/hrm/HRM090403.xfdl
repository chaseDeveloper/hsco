		this.ds_reqstList.setColumn(nRow, "CARALC_ENDDE"  	 , this.gfn_today());
		this.ds_reqstList.setColumn(nRow, "CARALC_BEGIN_HOUR"  	 , "00");
		this.ds_reqstList.setColumn(nRow, "CARALC_BEGIN_MIN"  	 , "00");
