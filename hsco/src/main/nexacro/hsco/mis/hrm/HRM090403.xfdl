 /*
 * 함수명  	: fn_PopBsnsCallback
 * 설명 		: 팝업 콜백 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                sReturn  : 팝업에서 넘어오는 데이터
 * return Type 	: None
 * 사용법		: this.fn_PopBsnsCallback();
 */
this.fn_PopCallback = function(sPopupId, sReturn)
{
	var dsRtn = new Dataset();
	nRowCnt = dsRtn.loadXML(sReturn);
		
    if(sPopupId == "condVhcle"){
		this.ds_cond.setColumn(0, "VHCLE_NM", dsRtn.getColumn(0, "VHCLE_NM"));
		this.ds_cond.setColumn(0, "VHCLE_CODE", dsRtn.getColumn(0, "VHCLE_CODE"));
    }
     
    if(sPopupId == "grdVhcle"){
		var nRow = this.ds_reqstList.rowposition;
		this.ds_reqstList.setColumn(nRow, "VHCLE_CODE", dsRtn.getColumn(0, "VHCLE_CODE"));
		this.ds_reqstList.setColumn(nRow, "VHCLE_NO", dsRtn.getColumn(0, "VHCLE_NO"));
		this.ds_reqstList.setColumn(nRow, "VHCLE_NM",   dsRtn.getColumn(0, "VHCLE_NM"));
    }
    
    if(sPopupId == "empnoPopup"){
		var empno = dsRtn.getColumn(0, "EMPNO");
		if(!this.comUtils.isNull(empno)){
			// 운전자 == 동승자 중복체크
			if(empno == this.ds_reqstList.getColumn(this.ds_reqstList.rowposition, "DRVER_EMPNO")){
				this.gfn_message("info.항목.중복.재입력.요청", "운전자와 동승자가")
				return;
			}
			
			// 동승자 == 동승자 중복체크
			var fRow = this.ds_caralcFlwpsg.findRow("FLWPSG_EMPNO" ,empno);
			if(fRow > -1){
				this.gfn_message("info.항목.중복.재입력.요청", "동승자가")
				return;
			}
		}
		var nRow = this.ds_caralcFlwpsg.rowposition;
		this.ds_caralcFlwpsg.setColumn(nRow, "FLWPSG_EMPNO", dsRtn.getColumn(0, "EMPNO"));
		this.ds_caralcFlwpsg.setColumn(nRow, "EMPNM", dsRtn.getColumn(0, "EMPNM"));
		this.ds_caralcFlwpsg.setColumn(nRow, "FLWPSG_DEPT_CODE",   dsRtn.getColumn(0, "DEPT_CODE"));
		this.ds_caralcFlwpsg.setColumn(nRow, "DEPT_NM",   dsRtn.getColumn(0, "DEPT_NM"));
    }
}

