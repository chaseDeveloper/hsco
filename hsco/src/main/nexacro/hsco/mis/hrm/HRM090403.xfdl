	} else {
		this.gfn_message("info.저장.초과", "신규건은 1건씩");
	}
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{	
	var rowposition = this.ds_reqstList.rowposition;
	var rowCnt = this.ds_reqstList.rowcount;
	
	if(rowposition > -1){
		var bsrpYn = this.ds_reqstList.getColumn(rowposition, "BSRP_YN");
		var sanctnSts = this.ds_reqstList.getColumn(rowposition, "SANCTN_STS");
		
		if(bsrpYn != "1"){
		// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
		// 결재상태가 기안전 외는 신규로 다시 진행.
			if(sanctnSts == "0" || this.comUtils.isNull(sanctnSts)){
				
				if(this.gfn_message("confirm.삭제여부")){
					this.ds_caralcFlwpsg.clearData();
					this.ds_reqstList.deleteRow(rowposition);
					
					if(rowCnt != rowposition+1){
						this.ds_reqstList.set_rowposition(-1);
						this.ds_reqstList.set_rowposition(rowposition);
					}
				}
				
			} else {
				this.gfn_message("fail.삭제.불가", "결재가 진행되어");
			}
		} else {
			this.gfn_message("fail.삭제.불가", "출장배차 건은 현재화면에서");
		}
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_reqstList.reset();			//DATASET ROLLBACK
		this.ds_reqstList.applyChange();	//DATASET COMMIT
		
		if(this.ds_reqstList.rowcount){
			this.ds_reqstList.set_rowposition(-1);
			this.ds_reqstList.set_rowposition(0);
		}
		
		return;
	}
}


