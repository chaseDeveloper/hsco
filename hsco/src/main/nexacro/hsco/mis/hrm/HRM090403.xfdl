***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(차량정비정보)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(차량정비정보)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.ds_caralcFlwpsg.clearData();
	this.ds_reqstList.clearData();	
	//this.fn_transaction("tbhrmReqstList");
	this.fn_transaction("selectVhcleCarAlcList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{	
	// update check
	if (   !this.comUtils.isDatasetUpdated(this.ds_reqstList)		// 배차신청목록
		&& !this.comUtils.isDatasetUpdated(this.ds_caralcFlwpsg)	// 동승자목록
	   ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_reqstList, this.ds_validation)) {
		return false;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_caralcFlwpsg, this.ds_validation)) {
		return false;
	}
		
	if(this.gfn_message("confirm.저장.여부"))	{
		
		this.saveRow = this.ds_reqstList.rowposition;
		this.fn_transaction("tbhrmReqstSave");
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectVhcleCarAlcList": 	// 차량별 배차 현황 조회
			var sController   	= "hsco/mis/hrm/HRM090403/selectVhcleCarAlcList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_vhcleList=output1";
			break;
		case "tbhrmReqstList":	// 배차신청 정보 조회
			var sController   	= "/hsco/mis/hrm/HRM090403/tbhrmReqstList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_reqstList=output1";
			break;
		case "selectCralcFlwpsg":	// 동승자 조회
			var sController   	= "/hsco/mis/hrm/HRM090403/selectCralcFlwpsg.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_caralcFlwpsg=output1";
			break;
		case "tbhrmReqstSave":	 // 저장
			var sController   	= "/hsco/mis/hrm/HRM090403/tbhrmReqstSave.do";
			var sInDatasets   	= "input1=ds_reqstList:U input2=ds_caralcFlwpsg:U";
			var sOutDatasets  	= "";
			break;
		
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectVhcleCarAlcList": 	// 차량별 배차 현황 조회
				this.fn_transaction("tbhrmReqstList");
				break;
			case "tbhrmReqstList":	// 조회(배차신청 정보)
				if(this.saveRow > -1){
					this.ds_reqstList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "selectCralcFlwpsg": 	// 동승자조회
				break;
			case "tbhrmReqstSave":	// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}
