 * 사용법		: this.ds_reqstList_canrowposchange();
 */
this.ds_reqstList_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	//발령사항 목록의 업데이트 여부
	if(this.comUtils.isDatasetUpdated(this.ds_caralcFlwpsg)){
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;
		}
	}
}

/*
 * 함수명  	: ds_reqstList_onrowposchanged
 * 설명 		: 배차신청목록 데이터 셋 onrowposchanged 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_reqstList_onrowposchanged();
 */
this.ds_reqstList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_caralcFlwpsg.clearData();
	
	if(e.newrow > -1){
		
		var seq = obj.getColumn(e.newrow, "CARALC_SEQ");
		var sanctnSts = obj.getColumn(e.newrow, "SANCTN_STS");
		var bsrpYn = obj.getColumn(e.newrow, "BSRP_YN");
		
		this.sanctnSts = this.comUtils.isNull(sanctnSts) ? "0" : sanctnSts;
		this.bsrpYn = this.comUtils.isNull(bsrpYn) ? "0" : bsrpYn;
		
		if(!this.comUtils.isNull(seq)){
			this.ds_cond_sub.clearData();
			var nRow = this.ds_cond_sub.addRow();
			
			this.ds_cond_sub.setColumn(nRow, "CARALC_SEQ", seq);
			this.fn_transaction("selectCralcFlwpsg");
		}
	}
}

 /*
 * 함수명  	: ds_reqstList_cancolumnchange
 * 설명 		: 배차신청 목록 데이터 셋 oncolumnchanged 이벤트 처리
 * params  		: none
 * return Type 	: None
 * 사용법		: this.ds_reqstList_cancolumnchange();
 */
this.ds_reqstList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "CARALC_BGNDE" || e.columnid == "CARALC_ENDDE"
	   || e.columnid == "CARALC_BEGIN_HOUR" || e.columnid == "CARALC_BEGIN_MIN"
	   || e.columnid == "CARALC_END_HOUR" || e.columnid == "CARALC_END_MIN") {
	   
		var sd = this.ds_reqstList.getColumn(e.row, "CARALC_BGNDE");			//시작일
		var ed = this.ds_reqstList.getColumn(e.row, "CARALC_ENDDE");			//종료일
		var stH = this.ds_reqstList.getColumn(e.row, "CARALC_BEGIN_HOUR");		//시작시간(시)
		var stM = this.ds_reqstList.getColumn(e.row, "CARALC_BEGIN_MIN");		//시작시간(분)
		var etH = this.ds_reqstList.getColumn(e.row, "CARALC_END_HOUR");		//종료시간(시)
		var etM = this.ds_reqstList.getColumn(e.row, "CARALC_END_MIN");			//종료시간(분)
		
		switch (e.columnid) {
			case "CARALC_BGNDE" :
				sd = e.newvalue;
				break;
			case "CARALC_ENDDE" :
				ed = e.newvalue;
				break;
			case "CARALC_BEGIN_HOUR" :
				stH = e.newvalue;
				break;
			case "CARALC_BEGIN_MIN" :
				stM = e.newvalue;
				break;
			case "CARALC_END_HOUR" :
				etH = e.newvalue;
				break;
			case "CARALC_END_MIN" :
				etM = e.newvalue;
				break;
		}
		
		if(!this.comUtils.isNull(sd) && !this.comUtils.isNull(ed) 
		   && !this.comUtils.isNull(stH) && !this.comUtils.isNull(stM)
		   && !this.comUtils.isNull(etH) && !this.comUtils.isNull(etM)){
		   
			var startDate = new Date(sd.substr(0,4)+"/"+sd.substr(4,2)+"/"+sd.substr(6,2)+" "+stH+":"+stM+":"+"00");
			var endDate  = new Date(ed.substr(0,4)+"/"+ed.substr(4,2)+"/"+ed.substr(6,2)+" "+etH+":"+etM+":"+"00");
			var tmp = endDate.getTime() - startDate.getTime();
			
			if(tmp < 0){
				this.gfn_message("errors.종료일자.시작일자.이상", "배차|배차");
				obj.setColumn(e.row, e.columnid, e.oldvalue);
			}
		}
	}
}


 /*
 * 함수명  	: btn_Add_R_onclick
 * 설명 		: 동승자 목록 행추가 버튼 이벤트 처리
 * params  		: none
 * return Type 	: None
 * 사용법		: this.btn_Add_R_onclick();
 */
this.btn_Add_R_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rowposition = this.ds_reqstList.rowposition;
	
	if(rowposition > -1){
		var sanctnSts = this.ds_reqstList.getColumn(rowposition, "SANCTN_STS");
		var bsrpYn = this.ds_reqstList.getColumn(rowposition, "BSRP_YN");
		
		if(bsrpYn != "1"){
		// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
		// 결재상태가 기안전 외는 신규로 다시 진행.
			if(sanctnSts == "0" || this.comUtils.isNull(sanctnSts)){
				
				var nRow = this.ds_caralcFlwpsg.addRow();
				
				var seq = this.ds_reqstList.getColumn(rowposition, "CARALC_SEQ");
				this.ds_caralcFlwpsg.setColumn(nRow, "CARALC_SEQ", seq);
				
			} else {
				this.gfn_message("fail.수정.불가", "결재가 진행되어");
			}
		} else {
			this.gfn_message("fail.수정.불가", "출장배차 건은");
		}
	}
}

 /*
 * 함수명  	: btn_Del_R_onclick
 * 설명 		: 동승자 목록 행삭제 버튼 이벤트 처리
 * params  		: none
 * return Type 	: None
 * 사용법		: this.btn_Del_R_onclick();
 */
this.btn_Del_R_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rowposition = this.ds_reqstList.rowposition;
	
	if(rowposition > -1){
		var sanctnSts = this.ds_reqstList.getColumn(rowposition, "SANCTN_STS");
		var bsrpYn = this.ds_reqstList.getColumn(rowposition, "BSRP_YN");
		
		if(bsrpYn != "1"){
		// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
		// 결재상태가 기안전 외는 신규로 다시 진행.
			if(sanctnSts == "0" || this.comUtils.isNull(sanctnSts)){
				
				if(this.gfn_message("confirm.삭제여부")){
					
					this.ds_caralcFlwpsg.deleteRow(this.ds_caralcFlwpsg.rowposition);
				}
				
			} else {
				this.gfn_message("fail.삭제.불가", "결재가 진행되어");
			}
		} else {
			this.gfn_message("fail.삭제.불가", "출장배차 건은 현재화면에서");
		}
	}
}

