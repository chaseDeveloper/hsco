	// 서브 조회
	if(e.newrow > -1) {
		this.ds_condSub.clearData();
		var nRow = this.ds_condSub.addRow();
		var evlYear = obj.getColumn(e.newrow, "EVL_YEAR");
		var htSe   = obj.getColumn(e.newrow, "HT_SE");
		var empno  = obj.getColumn(e.newrow, "EMPNO");
		
		this.ds_condSub.setColumn(nRow, "EVL_YEAR", evlYear);
		this.ds_condSub.setColumn(nRow, "HT_SE", htSe);
		this.ds_condSub.setColumn(nRow, "EMPNO", empno);
		
		this.fn_searchSub();
	}
}

