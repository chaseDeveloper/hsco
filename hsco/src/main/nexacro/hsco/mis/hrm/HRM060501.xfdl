 */
 this.fn_save = function(){
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_aprsrList)) {
		this.gfn_message("comm.데이터.변경.없음")
		return false;
	}
	
	// validation 체크(다면평가 평가자)
//	if(!this.gfn_checkValidation_NoUpdChk(this.ds_aprsrList, this.ds_validation)) {
//		return;
//	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		this.saveRow = this.ds_aprsrList.rowposition;
		this.fn_transaction("saveAprDetail");
	}
 }

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(평가자, 피평가자)
 * params  		: None
