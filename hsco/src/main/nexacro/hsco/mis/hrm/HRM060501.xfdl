this.fn_PopupCallback = function(sPopupId, sReturn)
{
	if(sPopupId == "empnoPop")
	{	
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		
		if(loadCnt) {
			var temp1 = dsTemp.getColumn(0, "EMPNO");
			var temp2 = dsTemp.getColumn(0, "EMPNM");
			var temp3 = dsTemp.getColumn(0, "CLSF_NM");
			var temp4 = dsTemp.getColumn(0, "JSSFC_NM");
			//var temp7 = dsTemp.getColumn(0, "GRADE");
			
			var nRow = this.ds_aprsrList.rowposition;
			this.ds_aprsrList.setColumn(nRow, "APRSR_EMPNO", temp1);
			this.ds_aprsrList.setColumn(nRow, "EMPNM", temp2);
			this.ds_aprsrList.setColumn(nRow, "CLSF_NM", temp3);
			this.ds_aprsrList.setColumn(nRow, "JSSFC_NM", temp4);
			//this.ds_aprsrList.setColumn(nRow, "GRADE", temp7);
			
		}
		
	}
}


this.btn_RowDelete_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	if(this.ds_aprsrList.rowcount > 0) {
		if (this.gfn_message("confirm.삭제여부")) {
			var cRow = this.ds_aprsrList.rowposition;
			this.ds_aprsrList.setColumn(cRow, "APRSR_EMPNO",null);
			this.ds_aprsrList.setColumn(cRow, "CLSF_NM", null);
			this.ds_aprsrList.setColumn(cRow, "JSSFC_NM", null);
			this.ds_aprsrList.setColumn(cRow, "EMPNM", null);
			this.ds_aprsrList.setColumn(cRow, "PRSEN_AT", null);
			}
	}
}

this.btn_random_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var sSvcID			= "selectRandomList"
	var sController   	= "/hsco/mis/hrm/HRM060501/selectRandomList.do";
	var sInDatasets   	= "";
	var sOutDatasets  	= "ds_random=output1";

	Ex.core.tran(this, sSvcID, sController, sInDatasets, sOutDatasets, "", "", "", false);
	

	var sDataset = "ds1=ds_random";
	var params = {};
	
	this.fn_jxlsDownload('HRM060501.xlsx', "다면평가_평가자.xlsx", sDataset, params);
}
