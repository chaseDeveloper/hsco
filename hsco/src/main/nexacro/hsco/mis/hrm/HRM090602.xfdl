 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var nRow = this.ds_ctsnnList.addRow();
	this.ds_ctsnnList.setColumn(nRow, "REQST_DE"    , this.gfn_today());
	this.ds_ctsnnList.setColumn(nRow, "PROGRS_STTUS", "001");
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	var errMsg = "";
	
	for(var i = 0; i < this.ds_ctsnnList.rowcount; i++) {
		var chk   = this.ds_ctsnnList.getColumn(i, "CHK");
		var sttus = this.ds_ctsnnList.getColumn(i, "PROGRS_STTUS");
		
		if(chk == '1') {
			if(sttus == '002' || sttus == '003') {
				if(errMsg.length > 0) {
					errMsg += ", " + (i+1);
				}
				else {
					errMsg += (i+1);
				}
			}
		}
	}
	
	if(errMsg.length > 0) {
		this.gfn_message("fail.삭제.불가", errMsg + "행은 이미 확인/종료되어");
		return false;
	}
	else {	
		this.gfn_delMultiRow(this.ds_ctsnnList);
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if(this.gfn_message("confirm.취소여부")) {
		this.ds_ctsnnList.reset();			//DATASET ROLLBACK
		this.ds_ctsnnList.applyChange();	//DATASET COMMIT
		return;
	}
}

