/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.checkDs = [
  this.ds_ctsnnList
];
this.saveRow = -1;

this.isAuth = false;
/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.HRM090602_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.HRM090602_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	this.isAuth = (this.gfn_authcode("005")>-1)||(this.gfn_authcode("112")>-1);// 인사(005), 총무(112) 이면 true
	//this.isAuth = false;
	
	this.fn_loadCombo();
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.init_Form();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	// SMS 전송대상에서, 부서 선택불가 설정
	this.div_work.div_deptCode.fn_setReadonly(true);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
	this.gfn_initCondDs(this, this.div_search);
}


/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	var comboParams = [
			["ds_relateSe"   , "_HRM013", "1", "D"],		// 가족관계
			["ds_ofcpsSe"    , "HRM003", "1", "D"],		// 직위구분
			["ds_ctsnnSe"    , "_HRM254", "1", "D"],		// 경조구분
			["ds_progrsSttus", "_HRM255", "1", "D"]		// 경조상태
		];
	
	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: 공통 callback
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callbackAfter();
 */
this.fn_callbackAfter = function ()
{

	this.div_search.div_EMP.fn_setBind("ds_cond","EMPNO","EMPNM");
	this.div_search.div_DEPT.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	if(!this.isAuth) {
		this.div_search.div_DEPT.fn_set_enable(false);
		
		this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
		this.ds_cond.setColumn(0, "DEPT_NM", this.gfn_getDeptName());
		
		this.div_search.div_EMP.fn_set_enable(false);

		this.ds_cond.setColumn(0, "EMPNO", this.fn_getEmpno());
		this.ds_cond.setColumn(0, "EMPNM", this.fn_getEmpnm());
	}
	
	var today = this.gfn_today();
	var startDe = this.dateUtils.addMonth(today, -1, 's');
	this.ds_cond.setColumn(0, "END_DE", today);
	this.ds_cond.setColumn(0, "BGN_DE", startDe);
	this.ds_cond.setColumn(0, "DE_SE", "001");
	this.div_work.div_sear.edt_applcntEmpNo.set_cssclass("edt_WF_Essential");
	this.div_work.div_sear.edt_applcntEmpNm.set_cssclass("edt_WF_Essential");
	this.div_work.div_sear00.edt_trgterEmpNo.set_cssclass("edt_WF_Essential");
	this.div_work.div_sear00.edt_trgterEmpNm.set_cssclass("edt_WF_Essential");
	this.fn_search();
}

/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/


/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(경조사안내 내역)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(기준년월)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.ds_ctsnnList.clearData();
	this.fn_transaction("tbhrmCtsnnList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_ctsnnList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(경조사안내)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_ctsnnList, this.ds_validation)) {
		return;
	}
	
	for(var clRow = 0; clRow < this.ds_ctsnnList.rowcount; clRow++) {
		var reqstDe = this.ds_ctsnnList.getColumn(clRow, "REQST_DE");
		reqstDe = this.dateUtils.addDate(reqstDe, 7);		// 비교 날짜(신청날짜+7일)
		reqstDe = this.dateUtils.format(reqstDe, 'yyyymmdd');
		var ctsnnBgnDe = this.ds_ctsnnList.getColumn(clRow, "CTSNN_BGNDE");
		var ctsnnEndDe = this.ds_ctsnnList.getColumn(clRow, "CTSNN_ENDDE");
		var ctsnnSe = this.ds_ctsnnList.getColumn(clRow, "CTSNN_SE");
		var sttus = this.ds_ctsnnList.getColumn(clRow, "PROGRS_STTUS");
		var rowType = this.ds_ctsnnList.getRowType(clRow);
		
		if(reqstDe >= ctsnnBgnDe && (ctsnnSe == '001' || ctsnnSe == '002') && (rowType == "2" || rowType == "4")) {
			this.gfn_message("info.처리불가", "경사신청은 7일 이전에만 가능합니다.");
			return false;
		}
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_ctsnnList.rowposition;
		this.fn_transaction("tbhrmCtsnnSave");
	}
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "tbhrmCtsnnList":	// 경조사안내 내역 조회
			var sController   	= "/hsco/mis/hrm/HRM090602/tbhrmCtsnnList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_ctsnnList=output1";
			break;
		case "tbhrmCtsnnSave":	 // 저장
			var sController   	= "/hsco/mis/hrm/HRM090602/tbhrmCtsnnSave.do";
			var sInDatasets   	= "input1=ds_ctsnnList:U";
			var sOutDatasets  	= "";
			break;
		case "smsTrgterR":		// SMS발송
			var sController   	= "/hsco/mis/hrm/HRM090602/smsTrgterR.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_smsTrgter=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "tbhrmCtsnnList":	// 조회(경조사안내 내역)
				if(this.saveRow > -1){
					this.ds_ctsnnList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "tbhrmCtsnnSave":	// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "smsTrgterR":		//SMS발송
					var ctsnnDetail = this.ds_ctsnnList.getColumn(this.ds_ctsnnList.rowposition, "CTSNN_DETAIL");  // 경조사 상세내용
					var param = {
						 ds      : this.ds_smsTrgter
						,CONTENT : ctsnnDetail
						,JOB_SE : "HRM"
					};
					this.gfn_popup("smsPopup", "", "", "문자발송", param, "com::COM010700_P01.xfdl", null, true);
				break;
		}
	}
}

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "empnoPop") {
		var dsEmpNo = new Dataset();
		var loadCnt = dsEmpNo.loadXML(sReturn);
		if(loadCnt) {
			var clRow = this.ds_ctsnnList.rowposition;
			var empNo = dsEmpNo.getColumn(0, "EMPNO");
			var empNm = dsEmpNo.getColumn(0, "EMPNM");

			this.ds_ctsnnList.setColumn(clRow, "APPLCNT_EMPNO", empNo);			// 신청자 사번
			this.ds_ctsnnList.setColumn(clRow, "APPLCNT_EMPNM", empNm);			// 신청자 사원명
		}
	}
}

 /*
 * 함수명  	: fn_popupCallBack2
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack2 = function(sPopupId, sReturn)
{
	if(sPopupId == "empnoPop2") {
		var dsEmpNo = new Dataset();
		var loadCnt = dsEmpNo.loadXML(sReturn);
		if(loadCnt) {
			var clRow    = this.ds_ctsnnList.rowposition;
			var empNo    = dsEmpNo.getColumn(0, "EMPNO");
			var empNm    = dsEmpNo.getColumn(0, "EMPNM");
			var deptCode = dsEmpNo.getColumn(0, "DEPT_CODE");
			var deptNm   = dsEmpNo.getColumn(0, "DEPT_NM");
			var ofcpsSe  = dsEmpNo.getColumn(0, "OFCPS");

			this.ds_ctsnnList.setColumn(clRow, "TRGTER_EMPNO"    , empNo);		// 대상자 사번
			this.ds_ctsnnList.setColumn(clRow, "TRGTER_EMPNM"    , empNm);		// 대상자 사원명
			this.ds_ctsnnList.setColumn(clRow, "TRGTER_DEPT_CODE", deptCode);	// 대상자 부서코드
			this.ds_ctsnnList.setColumn(clRow, "TRGTER_DEPT_NM"  , deptNm);		// 대상자 부서명
			this.ds_ctsnnList.setColumn(clRow, "TRGTER_OFCPS_SE" , ofcpsSe);	// 대상자 직위
		}
	}
}

