      <BindItem id="item19" compid="div_dtls.cmb_insttCode" propid="value" datasetid="ds_dylrr" columnid="BANK_CODE"/>
      <BindItem id="item21" compid="div_dtls.edt_insttNm" propid="value" datasetid="ds_dylrr" columnid="DPSTR_NM"/>
      <BindItem id="item22" compid="div_search.edt_salaryDeptCode" propid="value" datasetid="ds_cond" columnid="DEPT_CODE"/>
      <BindItem id="item23" compid="div_search.edt_salaryDeptNm" propid="value" datasetid="ds_cond" columnid="DEPT_NM"/>
