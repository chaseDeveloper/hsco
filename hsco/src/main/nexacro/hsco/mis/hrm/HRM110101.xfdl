    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * Script Include 
************************************************************************/

include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";


/***********************************************************************
 * 화면 변수 선언부
************************************************************************/
var nRowPos = 0;



/***********************************************************************
 * onInit / onLoad 영역
************************************************************************/
this.HRM110101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();
	
	// DataSet 초기화
	this.fn_init_dataset();
	
 	// 수정가능여부를 판별하기위해 권한그룹 체크
	if(this.gfn_authGrpId("DEV_GRP") != -1) {			// 개발자
		this.chargerAt = true;
	}
	if(this.chargerAt){
	
	}
	
	var comboParams = [
	    ["ds_hrm033",   "HRM033", "Y", "D", "B"]	// ds_hrm033 : 과세구분
	   ,["ds_hrm064",   "HRM064", "Y", "D", "B"]	// ds_hrm064 : 사용여부
	];
	var callback = function (svcId, errorCode, errorMsg) {
		this.ds_cond.clearData();
		var nRow = this.ds_cond.addRow();
		
		this.ds_cond.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId()  );	// 부서코드
		this.ds_cond.setColumn(nRow, "DEPT_NM",   this.gfn_getDeptName());	// 부서명
		this.ds_cond.setColumn(nRow, "USE_AT", '1');
		this.ds_cond.set_enableevent(true);
		
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}



/***********************************************************************
 * Form/Dataset 오브젝트 초기화
************************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */ 
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_fnncInsttCode.clearData();
	this.ds_dylrr.clearData();
	
    this.fn_transaction("selectDylrr");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (!this.comUtils.isDatasetUpdated(this.ds_dylrr)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_dylrr, this.ds_validation)) {
		return false;
	}
	
	// TODO : 주민등록번호 정합성 체크함수로 대체
	for(var fRow = 0; fRow < this.ds_dylrr.getRowCount(); fRow++) {
		if(this.ds_dylrr.getRowType(fRow) != Dataset.ROWTYPE_NORMAL) {
			var findSpace = this.ds_dylrr.getColumn(fRow, "IHIDNUM").indexOf(' ');
			if(findSpace > -1) {
				var errMsg = "[일용직근로자 " + (fRow + 1) + "번째 데이터]" + "\n";
				errMsg    += "주민등록번호 자릿수에 공백이 존재합니다"
				alert(errMsg);
				return false;
			}
		}
	}
	
	// 주거래계좌 선택 확인
	var cnt = 0;
	
	// save
	if(this.gfn_message("confirm.저장.여부"))	{
		nRowPos = this.ds_dylrr.rowposition;
		this.fn_transaction("saveDylrr");
	}
}



/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectDylrr":
			var sController   	= "hsco/mis/hrm/HRM110101/selectDylrr.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_dylrr=output1 ds_fnncInsttCode=output2";
			break;
		case "saveDylrr":
			var sController   	= "hsco/mis/hrm/HRM110101/saveDylrr.do";
			var sInDatasets   	= "input1=ds_dylrr:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}



/**********************************************************************
 * Transaction Callback 영역
***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0') {
		switch (strSvcId) {
			case "selectDylrr":
				if(this.ds_dylrr.getRowCount() > 0 && nRowPos > -1){
					this.ds_dylrr.set_rowposition(nRowPos);
					nRowPos = -1;
				}
				break;
			case "saveDylrr":
				this.fn_search();
				break;
		}
	}
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 급여지급부서 선택
	if(sPopupId == "salaryDeptPopup") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var salaryDeptCode = dsTemp.getColumn(0, "SALARY_DEPT_CODE");	// 급여지급부서코드
			var salaryDeptNm   = dsTemp.getColumn(0, "SALARY_DEPT_NM");		// 급여지급부서명
			
			var dRow    = this.ds_dylrr.rowposition;
			this.ds_dylrr.setColumn(dRow, "SALARY_DEPT_CODE", salaryDeptCode);
			this.ds_dylrr.setColumn(dRow, "SALARY_DEPT_NM"  , salaryDeptNm);
		}
	}
	
	// 급여지급부서 선택
	if(sPopupId == "salaryDeptCondPopup") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var salaryDeptCode = dsTemp.getColumn(0, "SALARY_DEPT_CODE");	// 급여지급부서코드
			var salaryDeptNm   = dsTemp.getColumn(0, "SALARY_DEPT_NM");		// 급여지급부서명
			
			var dRow    = this.ds_dylrr.rowposition;
			this.ds_cond.setColumn(dRow, "DEPT_CODE", salaryDeptCode);
			this.ds_cond.setColumn(dRow, "DEPT_NM"  , salaryDeptNm);
		}
	}
}



/***********************************************************************
* Transaction 영역 끝
************************************************************************/



/**********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
***********************************************************************
*
* 1. Dataset 관련 이벤트 처리
* 2. Button 관련 이벤트 처리
*
***********************************************************************/



/**********************************************************************
* 공통함수 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var nRow = this.ds_dylrr.addRow();
	this.ds_dylrr.setColumn(nRow, "TAXT_SE", '1');			// 과세구분 : 1(일반과세자)
	this.ds_dylrr.setColumn(nRow, "USE_AT",  '1');			// 사용여부 : 1(사용)
	this.ds_dylrr.setColumn(nRow, "MNBS_ACNUT_AT",  '1');	// 주계좌여부 : 1
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	if(!this.gfn_message("confirm.삭제여부")) {
		return false;	// 아니오
	}
	
	var grpRow = this.ds_dylrr.rowposition;
	this.ds_dylrr.deleteRow(grpRow)
}


/*
 * 함수명  	: fn_ca1ncel
 * 설명 		: 취소 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if(Ex.util.isUpdated(this.ds_dylrr)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	
	this.ds_dylrr.reset();
	
	this.ds_dylrr.applyChange();
}


/**********************************************************************
* Button 관련 이벤트 처리
***********************************************************************/
/*
 * 함수명  	: div_dtls_btn_salaryDept_onclick
 * 설명 		: '급여지급부서 팝업' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_dtls_btn_salaryDept_onclick(obj,e);
 */
this.div_dtls_btn_salaryDept_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_popup("salaryDeptPopup", 903, 842, "급여부서", null, "mis_sal::SAL030103_P01.xfdl", "fn_popupCallBack", true);
}

/*
 * 함수명  	: div_search_btn_salaryDept_onclick
 * 설명 		: '급여지급부서 팝업' 버튼 클릭 후 이벤트 처리(검색조건)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_search_btn_salaryDept_onclick(obj,e);
 */
this.div_search_btn_salaryDept_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_popup("salaryDeptCondPopup", 903, 842, "급여부서", null, "mis_sal::SAL030103_P01.xfdl", "fn_popupCallBack", true);
}


/**********************************************************************
* Dataset 관련 이벤트 처리
***********************************************************************/


/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged(obj,e);
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	this.fn_search();
}
