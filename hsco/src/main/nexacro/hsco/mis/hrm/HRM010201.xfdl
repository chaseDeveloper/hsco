 */
this.ds_orgcht_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "APPLC_DE") {
		var newVal = e.newvalue;
		var findRow = obj.findRow(e.columnid, e.newvalue);
