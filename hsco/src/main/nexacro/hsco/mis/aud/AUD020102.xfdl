/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 this.checkDs = [
	 this.ds_dclzDtlsList
	,this.ds_bsrpDtlsList
];
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.AUD020102_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}
/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.AUD020102_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.div_search.cal_bgnDe.set_value(this.gfn_today().substring(0,6) + '01');
	this.div_search.cal_endDe.set_value(this.gfn_today());
	this.div_search.div_EMP.fn_setBind("ds_cond","EMPNO","EMPNM");
	this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	var comboParams = [["ds_bsrpSe",  "HRM241", "1", "전체"]  //출장구분
					  ,["ds_ofcpsSe", "HRM021", "1", ""]      //직위구분
					  ,["ds_dclzSe",  "SAL009", "1", ""],     //근태구분
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	if(this.div_work.tab_hrmInfo.tabindex==2){
		//출퇴근대장 조회시 출퇴근정보 기간 안내를 visible
		this.div_work.sta_cmgGuide.set_visible(true);
	}else{
		this.div_work.sta_cmgGuide.set_visible(false);
	}
	
	switch(this.div_work.tab_hrmInfo.tabindex)
	{
		case 0:
			//휴가(근태)정보 조회
			this.fn_transaction("selectDclzDtlsList");
		break;
		case 1:
			//출장정보 조회
			this.fn_transaction("selectBsrpDtlsList");
		break;
		case 2:
			//초과근무정보 조회
			this.fn_transaction("selectEmpList");
		break;
		case 3:
			//부당사례조회
			this.ds_injstcNtcn.clearData();
			this.fn_transaction("selectInjstcNtcn");
		break;
	}
}

/***********************************************************************
* 상단 취소처리
************************************************************************/
this.fn_cancel = function()
{	
	// 데이터셋 수정 여부체크
	if(this.comUtils.isDatasetUpdated(this.ds_ntcnTrget)){
		this.ds_ntcnTrget.reset();
	}
}

/**********************************************************************
* tabpage 변경 시
***********************************************************************/ 
this.div_work_AUD020102_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	this.fn_search();
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		//근태정보 목록 조회
		case "selectDclzDtlsList":
			var sController   	= "hsco/mis/aud/AUD020102/selectDclzDtlsList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_dclzDtlsList=output01";
		break;		
		//출장정보 목록 조회
		case "selectBsrpDtlsList" :
		    var sController   	= "hsco/mis/aud/AUD020102/selectBsrpDtlsList.do";
			var sInDatasets   	= "input03=ds_cond";
			var sOutDatasets  	= "ds_bsrpDtlsList=output03";
		break;
		//사원목록 조회
		case "selectEmpList":
			var sController   	= "hsco/mis/aud/AUD020102/selectEmpList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_empList=output1";
		break;
		// 월별초과근무조회
		case "selectMtOvtmwrkList":
			var sController   	= "hsco/mis/aud/AUD020102/selectMtOvtmwrkList.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_mtOvtmwrkList=output1";
			break;
		// 초과근무상세내역 조회
		case "selectIndvdlOvtmwrkList":
			var sController   	= "hsco/mis/aud/AUD020102/selectIndvdlOvtmwrkList.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_indvdlOvtmwrkList=output1";
			break;
		//부당사례 조회
		case "selectInjstcNtcn":
			var sController   	= "hsco/mis/aud/AUD020102/selectInjstcNtcn.do";
			var sInDatasets   	= "input06=ds_cond";
			var sOutDatasets  	= "ds_injstcNtcn=output06";
		break;
	}
	var sArgs = "";
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs);
// 	if(strSvcId == "selectInjstcNtcn") {
// 		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs, null, null, false);
// 	}
// 	else {
// 		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs);
// 	}
}

/**********************************************************************
* Transaction Callback 영역
***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	}else {		
		switch (strSvcId) {
			//조회 콜백
			case "selectCmgList" :
				if(this.ds_cmgList.getRowCount() == 0) {
					this.gfn_message("info.데이터.없음");
				}else{
					for(var i =1 ; i <= 31 ; i ++){
						//trace(this.comUtils.isNull(this.ds_cmgList.getColumn(0 , "DAY"+i))); 
						if(!this.comUtils.isNull(this.ds_cmgList.getColumn(0 , "DAY"+i)))
						{
							//HEAD 요일 설정
							//trace(i+ "일23(" + this.ds_cmgList.getColumn(0 , "DAY"+i) +  ")");
							this.div_work.tab_hrmInfo.T03.grd_cmgList.setCellProperty("Head", 5+i, "text", i+ "일(" + this.ds_cmgList.getColumn(0 , "DAY"+i) +  ")" );
							
							if(i > 27){ //조회조건에 따라 row가 0일 경우 날짜 컬럼이 모두 사라지는 경우 방지 
										//모두 사라질 경우 재 조회 시 그리드 생성에 시간 소요 多
							
							//28일 부터 월말 COULMN SIZE 60 
								this.div_work.tab_hrmInfo.T03.grd_cmgList.setFormatColProperty((2*i) + 4, "size", 60); //날짜 (i)에 해당하는 col_1
								this.div_work.tab_hrmInfo.T03.grd_cmgList.setFormatColProperty((2*i) + 5, "size", 60); //날짜 (i)에 해당하는 col_2
							}
						}else{
							//해당 년월의 말일 이후의 날짜 COLUMN SIZE 0
							if(i > 27){
								this.div_work.tab_hrmInfo.T03.grd_cmgList.setFormatColProperty((2*i) + 4, "size", 0);
								this.div_work.tab_hrmInfo.T03.grd_cmgList.setFormatColProperty((2*i) + 5, "size", 0);
							}
						}
					}
				}
				break;
		}
	}
}
/**********************************************************************
* grid header click sort
***********************************************************************/ 
this.div_work_Grid_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.gfn_gridSort(obj, e);
}


/**************************************************************************
* 사원목록 행변경시
**************************************************************************/
this.ds_empList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_mtOvtmwrkList.clearData();
	this.ds_indvdlOvtmwrkList.clearData();
	
	if(e.newrow > -1) {
		this.ds_condSub.clearData();
		var nRow = this.ds_condSub.addRow();
		var pRow = this.ds_empList.rowposition;
		var ovtmwrkYear = this.ds_cond.getColumn(0, "BGNDE").substring(0,4);
		var empno       = this.ds_empList.getColumn(pRow, "EMPNO");
		
		this.ds_condSub.setColumn(0, "OVTMWRK_YEAR", ovtmwrkYear);
		this.ds_condSub.setColumn(0, "EMPNO"       , empno);
		this.fn_transaction("selectMtOvtmwrkList");
	}
}

/**************************************************************************
* 월별초과근무 행변경시
**************************************************************************/
this.ds_mtOvtmwrkList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_indvdlOvtmwrkList.clearData();
	
	if(e.newrow > -1) {
		this.ds_condSub2.clearData();
		var nRow = this.ds_condSub2.addRow();
		var pRow = this.ds_empList.rowposition;
		var ovtmwrkYm = obj.getColumn(e.newrow, "OVTMWRK_YM");
		var empno     = this.ds_empList.getColumn(pRow, "EMPNO");

		this.ds_condSub2.setColumn(0, "OVTMWRK_YM", ovtmwrkYm);
		this.ds_condSub2.setColumn(0, "EMPNO"     , empno);
		this.fn_transaction("selectIndvdlOvtmwrkList");
	}
}
