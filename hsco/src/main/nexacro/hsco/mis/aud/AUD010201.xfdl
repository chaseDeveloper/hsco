    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 감사관리
 * 02. 화면명   : AUD010201.xfdl
 * 03. 화면설명 : 일상감사관리 테이블을 관리한다.
 * 04. 작성일   : 2015.07.02
 * 05. 작성자   : 이지영
 * 06. 수정이력 : 2015.07.02 최초생성
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */
 /***********************************************************************
 * Script Include 
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
this.chargerAt = false;

/***********************************************************************************
* 설명	: oninit 영역
***********************************************************************************/
this.AUD010201_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}
/***********************************************************************
 * 설명: 폼로딩시 처리
 ************************************************************************/
this.AUD010201_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// todo..
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();	// 조회 데이터셋 초기화	
	this.ds_cond.addRow();	// 조회 데이터셋 행추가
	this.div_SearchArea.div_dept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	this.ds_cond.setColumn(0, "REQUST_BGN_DE", this.gfn_today().substring(0,4) + '0101');
	this.ds_cond.setColumn(0, "REQUST_END_DE", this.gfn_today());
	
	var comboParams = [["ds_aud014" , "AUD014", 1, ""],			// 공감법분류
	                   ["ds_aud015" , "AUD015", 1, ""], 		// 기관별분류
	                   ["ds_aud014S", "AUD014", 1, "전체"],		// 공감법분류(조회)
	                   ["ds_aud015S", "AUD015", 1, "전체"],		// 기관별분류(조회)
	                   ["ds_aud016" , "AUD016", 1, ""],			// 감사결과
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
	 	// 권한그룹 체크
		if(this.gfn_authGrpId("AUD_ADMIN") != -1) {			// 감사담당자
			this.chargerAt = true;
		}
		
		if(this.chargerAt) {
			this.div_SearchArea.div_dept.set_enable(true);
		}
		else {
			this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
			this.ds_cond.setColumn(0, "DEPT_NM",   this.gfn_getDeptName());
		}
		
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "AUD");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
 * 설명: 조회버튼 클릭시 처리
 ************************************************************************/
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// validation 체크(요청일자)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.DS_EDAY_AUDIT.clearData();
	this.ds_fileList.clearData();
	
	this.fn_transaction("selectEdayAuditList");
}

/***********************************************************************************
* 설명	: 상단 저장버튼 클릭시
***********************************************************************************/
this.fn_save = function()
{
	//접수 이전 상태일 경우 접수일자를 오늘 날짜로 세팅, 접수가 된 경우는 접수일자 유지, 내용 수정
	if(this.comUtils.isNull(this.DS_EDAY_AUDIT.getColumn(this.DS_EDAY_AUDIT.rowposition, "RCEPT_DE")))
	{
		this.DS_EDAY_AUDIT.setColumn(this.DS_EDAY_AUDIT.rowposition, "RCEPT_DE", this.gfn_today());
	}
	//validation check		
	if(!this.gfn_checkValidation(this.DS_EDAY_AUDIT, this.ds_validation)){
		return;
	} 
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.DS_EDAY_AUDIT.rowposition;
		this.fn_transaction("edayAuditListU");
	}
}

/***********************************************************************
 * 설명: 상단 취소버튼 클릭시
 ************************************************************************/
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.DS_EDAY_AUDIT))
	{
		if(this.gfn_message("confirm.취소여부")){ //모든 내용을 복원 하시겠습니까?
			this.DS_EDAY_AUDIT.reset();		//DATASET ROLLBACK
			this.DS_EDAY_AUDIT.applyChange();	//DATASET COMMIT
			return;
		}	
	}else{
		this.gfn_message("comm.내역.변경.없음");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		//상단 조회
		case "selectEdayAuditList" :
			// 초기화
			var sController   	= "hsco/mis/aud/AUD010201/selectEdayAuditList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_EDAY_AUDIT=output01";
		break;

		//상단 저장 버튼
		case "edayAuditListU" :
			var sController   	= "hsco/mis/aud/AUD010201/edayAuditListU.do";
			var sInDatasets   	= "input02=DS_EDAY_AUDIT:U";
			var sOutDatasets  	= "";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_EDAY_AUDIT.getColumn(this.DS_EDAY_AUDIT.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 

	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}else{
		//상단 저장 콜백 
		if (strSvcId == "edayAuditListU")
		{
			this.gfn_message("success.처리.성공");
			this.fn_search();
		}
		
		//상단 조회 콜백
		if (strSvcId == "selectEdayAuditList")
		{
			//저장트랜잭션 직전 rowposition값으로 이동
			if(save_row != -1)
			{
				this.DS_EDAY_AUDIT.set_rowposition(save_row);
				save_row = -1;
			}
		}
	}
}

/***********************************************************************************
* 설명	: 일상감사요청목록 행 변경 시
***********************************************************************************/
this.DS_EDAY_AUDIT_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_fileList.clearData();
	
	if(e.newrow > -1) {
		var readonly = obj.getColumn(e.newrow, "READONLY");
		
		if(readonly == '1') {
			this.div_Cont.edt_adtorDeptNm.set_cssclass("edt_WF_Readonly");
			this.div_Cont.edt_adtorEmpNo.set_cssclass("edt_WF_Readonly");
			this.div_Cont.edt_adtorEmpNm.set_cssclass("edt_WF_Readonly");
			this.div_Cont.txt_rm.set_cssclass("txt_WF_Readonly");
		}
		else {
			this.div_Cont.edt_adtorDeptNm.set_cssclass("edt_WF_Essential");
			this.div_Cont.edt_adtorEmpNo.set_cssclass("edt_WF_Essential");
			this.div_Cont.edt_adtorEmpNm.set_cssclass("edt_WF_Essential");
			this.div_Cont.txt_rm.set_cssclass("txt_WF_Essential");
		}
		
		this.fn_transaction("selectAtchFile");
	}
}

/***********************************************************************************
* 설명	: GRID HEAD 정렬
***********************************************************************************/
this.div_Cont_grd_EDAY_AUDIT_LIST_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.gfn_gridSort(obj, e); // Grid 데이터 정렬
}

/***********************************************************************************
* 설명	: 검색버튼 클릭
***********************************************************************************/
this.div_sear_search_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var parm = {
	     arg_0      : this
	};
	this.gfn_popup("Popup1", 0, 0, "", parm, "com::empno_P01.xfdl", "fn_callback5", true);
}

/***********************************************************************************
* 설명	: 사원선택 팝업 콜백
***********************************************************************************/
this.fn_callback5 = function(sPopupId, sReturn)
{
	var cRow = this.DS_EDAY_AUDIT.rowposition;
	
	if(sPopupId == "Popup1")
	{
        var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var empno    = dsTemp.getColumn(0, "EMPNO");
			var empnm    = dsTemp.getColumn(0, "EMPNM");
			var deptCode = dsTemp.getColumn(0, "DEPT_CODE");
			var deptNm   = dsTemp.getColumn(0, "DEPT_NM");
			this.DS_EDAY_AUDIT.setColumn(cRow, "ADTOR_EMPNO"    , empno);		// 감사자사번
			this.DS_EDAY_AUDIT.setColumn(cRow, "ADTOR_EMPNM"    , empnm);		// 감사자명
			this.DS_EDAY_AUDIT.setColumn(cRow, "ADTOR_DEPT_CODE", deptCode);	// 감사자부서코드
			this.DS_EDAY_AUDIT.setColumn(cRow, "ADTOR_DEPT_NM"  , deptNm);		// 감사자부서명
		}
	}
}

/***********************************************************************
 * 설명 : 첨부파일 선택다운로드 버튼클릭 시
 ***********************************************************************/
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
		return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}

/***********************************************************************
 * 설명 : 첨부파일 그리드 헤드 클릭 시 
 ***********************************************************************/
this.grd_cmmnAtchmnfl_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0)
	{	
		//모든 체크박스 선택
		this.gfn_setGridCheckAll(obj, e);
	}else {
		//선택한 헤드에 따른 정렬
		this.gfn_gridSort(obj, e);
	}
}

/***********************************************************************
 * 설명 : 조회조건 변경시
 ***********************************************************************/
this.DS_SEARCH_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "PUBLIC_AUDIT_CL") {
		this.ds_aud015S.filter("String(VAL1).indexOf('" + e.newvalue + "') >= 0 || CODE == ''");
		obj.setColumn(e.row, "INSTT_ACCTO_CL", "");
	}
}

/*
 * 함수명  	: fn_get_imgUrl
 * 설명 		: 전자결재종류 이미지 경로 리턴 함수
 * params  		: 대상 전자결재 상태
 * return Type 	: 전자결재 종류 이미지 경로
 * 사용법		: this.fn_get_imgUrl(sanctnSts)
 */
this.fn_get_imgUrl = function(sanctnSts)
{
	return this.gfn_getSanctnImgUrl(sanctnSts);
}

