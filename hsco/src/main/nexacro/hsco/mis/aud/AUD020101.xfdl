{
	// ..
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.AUD020101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.div_search.cal_bgnTransDate.setFocus(true);
	
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.div_search.div_dept.fn_setBind("ds_cond01","DEPT_CODE","DEPT_NM");
	
	this.ds_cond01.clearData();
	this.ds_cond01.addRow();
	
	var bgnde = this.gfn_today().substring(0,6)+'01';
	var endde = this.gfn_today();
	
	this.ds_cond01.setColumn(0, "BGN_TRANS_DATE", bgnde);
	this.ds_cond01.setColumn(0, "END_TRANS_DATE", endde);
	
	this.fn_search();
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	// ..
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond01, this.ds_validation)) {
		return;
	}
	
	this.ds_confmDtls.clearData();
	this.ds_cardPartclr.clearData();
	this.fn_transaction("selectcatsTmpApporvalList");
}

/**********************************************************************
* 특이사항목록 저장버튼 클릭시
***********************************************************************/ 
this.fn_save = function()
{
	if(this.gfn_message("confirm.저장.여부")){
		this.fn_transaction("cardPartclrMatterCUD");
	}
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//법인카드사용내역목록 조회
		case "selectcatsTmpApporvalList":
			var sController   	= "hsco/mis/aud/AUD020101/selectCatsTmpApprovalList.do";
			var sInDatasets   	= "input01=ds_cond01";
			var sOutDatasets  	= "ds_confmDtls=output01";
		break;
		
		//특이사항 조회
		case "selectCardPartclrMatterList":
			var sController   	= "hsco/mis/aud/AUD020101/selectCardPartclrMatterList.do";
			var sInDatasets   	= "input02=ds_cond01";
			var sOutDatasets  	= "ds_cardPartclr=output02";
		break;
			
		//특이사항 등록
		case "cardPartclrMatterCUD":
			var sController   	= "hsco/mis/aud/AUD020101/cardPartclrMatterCUD.do";
			var sInDatasets   	= "input03=ds_cardPartclr:U";
			var sOutDatasets  	= "";
		break;
		//알림대상 조회
		case "selectNtcnTrget":
			var sController   	= "hsco/mis/aud/AUD020101/selectNtcnTrget.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_ntcnTrget=output1";
		break;	
		//알림대상 추가삭제
		case "ntcnTrgetCUD":
			var sController   	= "hsco/mis/aud/AUD020101/ntcnTrgetCUD.do";
			var sInDatasets   	= "input1=ds_ntcnTrget:U";
			var sOutDatasets  	= "";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

/**********************************************************************
* Transaction Callback 영역
***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{

	if(nErrorCode != 0) {
		 this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
    }else{
		switch (strSvcId) {	
			//특이사항 저장/수정/삭제	
			case "cardPartclrMatterCUD":
				this.fn_transaction("ntcnTrgetCUD");
			break;
			
			//법인카드사용목록 조회 콜백
			case "selectcatsTmpApporvalList":
				this.fn_transaction("selectCardPartclrMatterList");
			break;
			//알림대상
			case "ntcnTrgetCUD":
				this.fn_transaction("selectCardPartclrMatterList");
			break;
			//특이사항조회
			case "selectCardPartclrMatterList":
				this.fn_transaction("selectNtcnTrget");
			break;
		}
	}
	
}
/**********************************************************************
* 특이사항 등록 버튼 클릭시
***********************************************************************/ 
this.div_work_Button61_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	var catsTmpRow = this.ds_confmDtls.rowposition;
	var fail_count = 0;
	
	if(this.ds_confmDtls.getRowType(catsTmpRow)=='16'){
		fail_count = 1;
		this.gfn_message("info.처리불가","해당 내역은 이동할 수 없습니다.");
	}
		
	for(var i = 0 ; i < this.ds_cardPartclr.rowcount ; i++)
	{
		if(this.ds_confmDtls.getColumn(catsTmpRow, "CARD_NM") == this.ds_cardPartclr.getColumn(i, "CARD_NM") 
		&& this.ds_confmDtls.getColumn(catsTmpRow, "APPR_DATE") == this.ds_cardPartclr.getColumn(i, "APPR_DATE")
		&& this.ds_confmDtls.getColumn(catsTmpRow, "APPR_TIME") == this.ds_cardPartclr.getColumn(i, "APPR_TIME")
		&& this.ds_confmDtls.getColumn(catsTmpRow, "CHAIN_NM") == this.ds_cardPartclr.getColumn(i, "CHAIN_NM"))
		{
			fail_count = 1;
			this.gfn_message("comm.데이터.기등록.존재","해당 사용내역");
		}
	}
	if(fail_count == 0)
	{
		nRow = this.ds_cardPartclr.addRow();		
		this.ds_cardPartclr.copyRow(nRow, this.ds_confmDtls, this.ds_confmDtls.rowposition);
		this.ds_confmDtls.set_rowposition(this.ds_confmDtls.rowposition + 1);
	}
}

/**********************************************************************
* 상단 삭제 버튼 클릭시(특이사항 삭제)
***********************************************************************/ 
this.fn_delete = function()
{
	if(this.gfn_message("confirm.삭제여부")){
		nRow = this.ds_cardPartclr.rowposition;
		this.ds_cardPartclr.deleteRow(nRow);
	}
}
/**********************************************************************
* 특이사항 (▲) 버튼
***********************************************************************/ 
this.div_work_Button60_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_cardPartclr.deleteRow(this.ds_cardPartclr.rowposition);
}
/**********************************************************************
* 상단 취소 버튼 (특이사항 취소)
***********************************************************************/ 
this.fn_cancel = function()
{
	if(Ex.util.isUpdated(this.ds_cardPartclr)){
    	if(this.gfn_message("confirm.취소여부")){  //모든 내용을 복원 하시겠습니까?
		    this.ds_cardPartclr.reset();		//DATASET ROLLBACK
		    this.ds_cardPartclr.applyChange();	//DATASET COMMIT
		return;
	    }	
    }else{
		this.gfn_message("comm.내역.변경.없음");
    }
}

