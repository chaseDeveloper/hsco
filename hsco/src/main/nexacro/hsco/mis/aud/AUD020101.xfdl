/***********************************************************************
* Popup Callback
************************************************************************/
this.fn_popupCallBack = function(strID, variant)
{	
	if(!variant) {
		return false;	// 팝업에서 값을 선택하지 않았을때, 이후 동작은 발생해서는 안됨
	}
	
	var dsTemp = new Dataset();
	dsTemp.loadXML(variant);
	
	switch(strID) {
		case "empnoPopup":	//거래처조회 팝업 콜백
			var nRow = this.ds_ntcnTrget.addRow();
			this.ds_ntcnTrget.setColumn(nRow, "EMPNO", dsTemp.getColumn(0, "EMPNO"));
			this.ds_ntcnTrget.setColumn(nRow, "EMPNM", dsTemp.getColumn(0, "EMPNM"));
		break;
	}
}

/***************************************************
* 법인카드 특이사항 일괄 이동 처리
* 설명 : 사용내역중 검색 조건에 해당하는 건을 일괄 특이사항 처리
***************************************************/
this.div_work_div_search2_btn_batchMove_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.comUtils.isNull(this.div_work.div_search2.msk_sTime.value)){
		this.div_work.div_search2.msk_sTime.setFocus(true);
		this.gfn_message("info.처리불가", "승인 시작 시간을 입력해주세요.");
		return false;
	}
	
	if(this.comUtils.isNull(this.div_work.div_search2.msk_eTime.value)){
		this.div_work.div_search2.msk_eTime.setFocus(true);
		this.gfn_message("info.처리불가", "승인 종료 시간을 입력해주세요.");
		return false;
	}
	
	var fail_count = 0;
	var oRowCnt = this.ds_confmDtls.rowcount;
	var chkInt = 0;
	var suc;
	var sBranchType = this.div_work.div_search2.edt_branchType.value;
	
	
	//this.gfn_message("info.처리불가", "이동 처리를 시작합니다.");
	alert("특이사항 내역 이동을 시작합니다.");
	for(var i=0; i < oRowCnt; i++){
		var dt = this.ds_confmDtls.getColumn(i, "APPR_DATE");
		var tm = this.ds_confmDtls.getColumn(i, "APPR_TIME");
		
		//승인일자가 있는 행
		if(!this.comUtils.isNull(dt)){
			dt = dt.substr(0,4)+"/"+dt.substr(4,2)+"/"+dt.substr(6,2);
			tm = tm.substr(0,4);
			//var objDate = new Date("2008/10/11");
			var objDate = new Date(dt);
			var yo = objDate.getDay();	// 6 (토요일)
			
			//중복 데이터 검증용
			var cardNm = this.ds_cardPartclr.findRow("CARD_NM", this.ds_confmDtls.getColumn(i, "CARD_NM"));
			var apprDt = this.ds_cardPartclr.findRow("APPR_DATE", this.ds_confmDtls.getColumn(i, "APPR_DATE"));
			var apprTm = this.ds_cardPartclr.findRow("APPR_TIME", this.ds_confmDtls.getColumn(i, "APPR_TIME"));
			var chanNm = this.ds_cardPartclr.findRow("CHAIN_NM", this.ds_confmDtls.getColumn(i, "CHAIN_NM"));

			if(cardNm > -1 && apprDt > -1 && apprTm > -1 && chanNm > -1){
				this.gfn_message("info.처리불가", (i+1)+"행에 중복된 데이터가 존재합니다.");
				return false;
			}
			
			// 업종 비교
			if(!this.comUtils.isNull(sBranchType)) {
				var branchType = this.ds_confmDtls.getColumn(i, "BRANCHTYPE");
				var nIndex = branchType.indexOf(sBranchType);
				
				if(nIndex < 0) {
					continue;
				}
			}
			
			//검색시간과 승인시간 비교
			if(this.div_work.div_search2.msk_sTime.value <= tm 
			|| this.div_work.div_search2.msk_eTime.value >= tm){
				//휴일 체크시
				if(this.div_work.div_search2.chk_vacation.value==1){
					if(yo==0 || yo==6){
						var nRow = this.ds_cardPartclr.addRow();		
						suc = this.ds_cardPartclr.copyRow(nRow, this.ds_confmDtls, i);
						chkInt++;
					}
				}else{
					var nRow = this.ds_cardPartclr.addRow();		
					suc = this.ds_cardPartclr.copyRow(nRow, this.ds_confmDtls, i);
					chkInt++;
				}
			}
		}
	}
	
	if(suc == true){
		this.gfn_message("info.처리불가", "복사 완료되었습니다.");
	}
	
	if(chkInt==0){
		this.gfn_message("info.처리불가", "해당 조건에 맞는 내역이 없습니다.");
	}else{
		this.gfn_message("info.처리불가", chkInt+"건의 특이사항 복사를 완료했습니다.");
	}
}

