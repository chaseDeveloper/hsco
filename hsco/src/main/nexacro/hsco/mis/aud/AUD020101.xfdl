this.div_work_btn_smsSend_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var sRow = this.ds_ntcnTrget.rowposition;
	
	var MBTLNO = this.ds_ntcnTrget.getColumn( sRow ,"TELNO_2");
	var NM = this.ds_ntcnTrget.getColumn( sRow, "EMPNM");
	this.ds_smsList.clearData();
		
	if(!this.comUtils.isNullEmpty(MBTLNO)){
		var nRow = this.ds_smsList.addRow();
		this.ds_smsList.setColumn(nRow , "MBTLNO", MBTLNO);
		this.ds_smsList.setColumn(nRow , "NM", NM);
	}	
	
	var CONTENT = "테스트 문자 메세지 발송";
	var param = {
		CONTENT : CONTENT,
		JOB_SE : "AUD", 
		ds : this.ds_smsList
	};
		
	this.gfn_popup("COM010700_P01.xfdl"
			   , 800
			   , 500
			   , "SMS발송"
			   , param
			   , "com::COM010700_P01.xfdl"
			   , "sms_callback");
}

