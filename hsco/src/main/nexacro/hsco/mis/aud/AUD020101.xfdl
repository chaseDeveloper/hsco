************************************************************************/
this.grd_cprCard_oncelldblclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	
	var decsnNo = this.ds_confmDtls.getColumn(this.ds_confmDtls.rowposition, "DECSN_NO");
	if(this.comUtils.isNull(decsnNo)){
		//지출등록된 건만 팝업호출
		return;
	}

	this.ds_decsnNo.clearData();
	this.ds_decsnNo.addRow();
	
	var decsnYear = this.ds_confmDtls.getColumn(this.ds_confmDtls.rowposition, "DECSN_YEAR");
	var decsnNo   = this.ds_confmDtls.getColumn(this.ds_confmDtls.rowposition, "DECSN_NO");
	
	this.ds_decsnNo.setColumn(0, "DECSN_YEAR", decsnYear);
	this.ds_decsnNo.setColumn(0, "DECSN_NO", decsnNo);
	
	var args = {		
		 arg_0      			: this
		,trgFormId				: "AUD020101"												// 팝업을 호출하는 화면의 ID
		,anactKnd				: "03"														// 결의서구분(03:지출결의서)
		,decsnYear				: this.ds_decsnNo.getColumn(0, "DECSN_YEAR")				// 결의년도
		,decsnNo				: this.ds_decsnNo.getColumn(0, "DECSN_NO")					// 결의번호
	};
				
	this.gfn_popup("expDsnPopup", "902", "745", "결의서", args, "mis_acc::ACC020203_P10.xfdl", "fn_popupCallback", true);
}

