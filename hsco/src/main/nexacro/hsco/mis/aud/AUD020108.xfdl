 * 함수명  	: fn_search
 * 설명 		: div 내부 공통함수 호출(조회)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search(obj,e);
 */
this.fn_search = function()
{
	this.div_lnk.fn_search();
}


