/*
 * 함수명  	: fn_sanction
 * 설명 		: '결재' 버튼 클릭 후
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_sanction();
 */
this.fn_sanction = function()
{
	if(Ex.util.isUpdated(this.DS_OFLNE_CVPL)){
		this.gfn_message("comm.정보.저장.요청","먼저 수정한 정보를");
	}else{
		switch(this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition, "SANCTN_STTUS")){		
		case "T05":
		case "T07":
		case "T11":
		case "T14":
		case "T15":
			if(this.gfn_message("comm.행위여부","접수 및 처리내용을 수정하고 내부결재 재상신을 진행") ){
				this.DS_OFLNE_CVPL.setColumn(this.DS_OFLNE_CVPL.rowposition, "SANCTN_NO", "");
				this.fn_setreadonly(false);
				break;
			}else{
				break;
			}
		default:
			var cvplRow = this.DS_OFLNE_CVPL.rowposition;
			
			var innerSanctnMdlId = "AUD-100064"; // 내부결재모형ID
			var sysSeCode = "SYS";
			var jobSe = "MIS";
			var svcId = "GetInnerSanctnId";
			if(this.ds_recomInfo.getRowCount() ==0) this.ds_recomInfo.addRow();
			
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0, "RCEPT_NO", this.DS_OFLNE_CVPL.getColumn(cvplRow, "RCEPT_NO"));
			
			var key = "AUD100064" + "^" + this.DS_OFLNE_CVPL.getColumn(0, "RCEPT_NO");
			this.ds_recomInfo.setColumn(0, "JOB_KEY", key);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_MDL_ID", innerSanctnMdlId);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_ID", this.DS_OFLNE_CVPL.getColumn(cvplRow, "SANCTN_NO"));
			this.ds_recomInfo.setColumn(0, "SYS_SE_CODE", sysSeCode);
			this.ds_recomInfo.setColumn(0, "JOB_SE", jobSe);
			
			this.fn_ManageInnerSanction(innerSanctnMdlId, sysSeCode, jobSe, svcId);
		break;
		}
	}
}
/***********************************************************************
 * 설명: 오프라인 민원 리스트 셀 영역 변경 시
 * 변경 값이 있을 경우 저장여부 확인, 없을 경우 부서정보 조회
 ************************************************************************/
this.div_Cont_grd_OFLNE_CVPL_onselectchanged = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.DS_OFLNE_CVPL_DEPT.clearData();
	 //변경된 감사계획목록이 저장되어있을 경우(신규/추가 작업 중 영역 변경 제외)
	if(this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition, "RCEPT_NO") > 0 )
	{
		//DS_AUDIT_COMPOSI, DS_AUDIT_DEPT_TABLE 의 값이 변경되었을 경우
		if( (Ex.util.isUpdated(this.DS_OFLNE_CVPL_DEPT)) ) 
		{
			if(this.gfn_message("confirm.변경.저장.여부")){	
				this.fn_transaction("OfflinCvplDeptListCUD");
			}else{
				this.fn_transaction("selectCvplDeptList");
			}
		}else{
		//변경 값이 없을경우
			this.fn_transaction("selectCvplDeptList");
		}
	}
}

/***********************************************************************************
* 설명	: 부서 및 담당자  행추가 버튼 클릭시
***********************************************************************************/
this.div_Cont_btn_Add_Row_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	//오프라인신고목록이 등록되었을 경우에만 행추가
	if(!this.comUtils.isNull(this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition, "RCEPT_NO")))
	{
		var nRow = this.DS_OFLNE_CVPL_DEPT.addRow();
		
		//감사계획관리 테이블의 AUDIT_PLAN_NO, AUDIT_PLAN_YEAR 값을 대상부서 정보에 세팅.
		this.DS_OFLNE_CVPL_DEPT.setColumn(nRow,"RCEPT_DE",this.gfn_today());
		this.DS_OFLNE_CVPL_DEPT.setColumn(nRow,"RCEPT_NO",this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition, "RCEPT_NO"));
		
		//행추가시 팝업 호출 2016.07.01
		this.grd_OFLNE_CVPL_DEPT_oncellclick();
	}
	else{
		if(this.DS_OFLNE_CVPL.rowposition > -1)
		{
			this.gfn_message("comm.정보.저장.요청","먼저 오프라인신고목록");
		}else{
			this.gfn_message("comm.항목.선택.요청","오프라인신고목록");
		}
	}
}
/***********************************************************************************
* 설명	: 부서 및 담당자 행삭제 버튼 클릭시
******************************************************.*****************************/
this.div_Cont_btn_Del_Row_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
