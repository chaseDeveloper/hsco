		this.DS_OFLNE_CVPL.deleteRow(nRow);
	}else{
		switch(this.DS_OFLNE_CVPL.getColumn(nRow, "SANCTN_STTUS")){
			case "T05":
			case "T07":
			case "T11":
			case "T12":
			case "T14":
			case "T15":
				this.gfn_message("comm.결재.완료");
			break;
			default:
				this.gfn_message("comm.결재.진행");
			break;
		}
	}
}

