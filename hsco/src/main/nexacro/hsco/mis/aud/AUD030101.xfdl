/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "audAdmin":
			var sController   	= "hsco/com/sym/aum/SYM020200/getListGrpUser.do";
			var sInDatasets   	= "input1=ds_audAdmin";
			var sOutDatasets  	= "ds_audAdmin=output1";
		break;
		//상단 조회
		case "selectOffCvplist":
			// 초기화
			//this.DS_OFLNE_CVPL.clearData();
			this.DS_OFLNE_CVPL_DEPT.clearData();
			this.ds_fileList.clearData();
			var sController   	= "hsco/mis/aud/AUD030101/selectOffCvplist.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_OFLNE_CVPL=output01";
		break;
		
		//상단 저장
		case "OffCvplistCUD":
			var testData;
			var sController   	= "hsco/mis/aud/AUD030101/OffCvplistCUD.do";
			var sInDatasets   	= "input01=ds_uc input02=DS_OFLNE_CVPL:U";
			var sOutDatasets  	= "";
			trace(testData);
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	
		//부서및 담당자정보조회
		case "selectCvplDeptList" :		
			this.DS_OFLNE_CVPL_DEPT.clearData();
			this.DS_SEARCH2.clearData();
			var nRow = this.DS_SEARCH2.addRow();
			this.DS_SEARCH2.setColumn(nRow, "RCEPT_NO" ,this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition,'RCEPT_NO'));		
			var sController   	= "hsco/mis/aud/AUD030101/selectCvplDeptList.do";
			var sInDatasets   	= "input03=DS_SEARCH2";
			var sOutDatasets    = "DS_OFLNE_CVPL_DEPT=output03";
		break;
    
		//부서저장 트랜잭션
		case "OfflinCvplDeptListCUD":
			var sController   	= "hsco/mis/aud/AUD030101/OfflinCvplDeptListCUD.do";	
			var sInDatasets   	= "input04=DS_OFLNE_CVPL_DEPT:U";
			var sOutDatasets  	= "";
		break;
		
		//내부결재 관련 트랜잭션
		case "saveInnerSanc": // 내부결재 저장
			var sController   	= "hsco/mis/aud/AUD030101/saveInnerSanc.do";
			var sInDatasets   	= "input1=ds_cond02:U";
			var sOutDatasets  	= "";
			break;
		case "GetInnerSanctnId": // 내부결재id 조회
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
		break;
		case "innerSanctnUbiDataset": // 시스템연계 내부결재보고서 정보 조회
			var sController   	= "hsco/mis/aud/AUD030101/innerSanctnUbiDataset.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";	//양식 정해지면 output데이터셋 만들어서 추가예정
		break;
		//최초 내부결재 팝업 호출 시 저장버튼을 눌러 시작되었는지, 닫기버튼을 눌러 취소를 했는지 여부check
		case "selectInnerSanctnMasterCheck"	:
			var sController   	= "hsco/mis/aud/AUD010101/selectInnerSanctnMasterCheck.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_innerSanctnChek=output1";
		break;
		case "CONFMAT":
			var sController   	= "hsco/mis/aud/AUD030101/confmU.do";	
			var sInDatasets   	= "input1=ds_confmAt:U";
			var sOutDatasets  	= "";
		break;
		
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}else{
		switch(strSvcId)
		{
			case "audAdmin":
				this.fn_search();
			break;
			//오프라인민원저장콜백
			case "OffCvplistCUD":
				this.fn_transaction("OfflinCvplDeptListCUD");
			break;
			case "selectCvplDeptList":
			break;
			//부서정보 저장 콜백
			case "OfflinCvplDeptListCUD":
				this.fn_search();
			break;
			
			//상단 조회 콜백
			case "selectOffCvplist":
				//저장트랜잭션 직전 rowposition값으로 이동
				var save_row = this.DS_OFLNE_CVPL.findRow("RCEPT_NO",save_pk);
				if(save_row != -1)
				{
					this.DS_OFLNE_CVPL.set_rowposition(save_row);
					save_pk = -1;
				}
			break;
			//내부결재 트랜잭션 콜백
			case "GetInnerSanctnId":
				this.ds_cond02.clearData();
				this.ds_cond02.addRow();
				this.ds_cond02.setColumn(0, "RCEPT_NO", this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition, "RCEPT_NO"));
				this.ds_cond02.setColumn(0, "INNER_SANCTN_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
				this.fn_popup_InnerSanctionDrft();
				break;
			case "innerSanctnUbiDataset":
				break;
			case "selectInnerSanctnMasterCheck":
				if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
				{
					this.fn_transaction("saveInnerSanc");
				}
				break;
			case "saveInnerSanc":
				this.fn_transaction("incExpElseCashMastrList");
				break;
			case "CONFMAT":
				this.DS_OFLNE_CVPL.set_updatecontrol(false);
				var nConfm = Number(this.DS_OFLNE_CVPL.getColumn(this.DS_OFLNE_CVPL.rowposition, "CONFM_AT"));
				//0 -> 1, 1-> 0
				this.DS_OFLNE_CVPL.setColumn(this.DS_OFLNE_CVPL.rowposition, "CONFM_AT", -(nConfm-1));
				this.DS_OFLNE_CVPL.set_updatecontrol(true);
				//2016.07.01
				this.gfn_message("success.처리.성공");
			break;
		}
	}
}

