/*
 * 함수명  	: fn_setreadonly
 * 설명 		: 진행중인 결재건인 경우, 수정할 수 없도록 한다.
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_setreadonly();
 */ 
this.fn_setreadonly = function(flag){
	//flag == true ? readOnly : readOnly
	var cssflag = ["Readonly", ""];
	var flagNumber=(flag==true?0:1);
		
	
	//첨부파일 버튼 visible 여부
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].btn_delFiles.set_visible(!flag);
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].btn_uploadFiles.set_visible(!flag);
	//처리결과_제목 readonly 여부
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].edt_PROCESS_SJ.set_readonly(flag);
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].edt_PROCESS_SJ.set_cssclass("edt_WF_"+cssflag[flagNumber]);
	//처리결과_내용 readonly 여부
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].txt_PROCESS_CN.set_readonly(flag);
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].txt_PROCESS_CN.set_cssclass("txt_WF_"+cssflag[flagNumber]);
	//조치구분 readonly 여부
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].rdo_MANAGT_SE.set_readonly(flag);
	this.div_Cont.AUD030103.tabpages[this.div_Cont.AUD030103.tabindex].rdo_MANAGT_SE.set_cssclass("rdo_WF_"+cssflag[flagNumber]);
}



