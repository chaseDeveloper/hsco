	var findRow = this.ds_fileList2.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
		return false;
	}
	
	for(var fRow = this.ds_fileList2.getRowCount()-1; fRow >= 0; fRow--) {
		var chkVal = this.ds_fileList2.getColumn(fRow, "CHK");
		if(chkVal == '1') {
			this.ds_fileList2.deleteRow(fRow);
		}
	}
	var strSvcId   	 = "deleteFile";
	var sController  = "hsco/cmm/file/deleteFile.do";
	var sInDatasets  = "input1=ds_fileList2:U";
	var sOutDatasets = "ds_fileList2=output1";
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
