/*
 * 함수명  	: fn_sanction
 * 설명 		: '결재' 버튼 클릭 후
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_sanction();
 */
this.fn_sanction = function()
{
	var cRow = this.DS_ONLINE_CVPL.rowposition;
	if(cRow>-1){
		switch(this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "SANCTN_STTUS")){
		case "T05":
		case "T07":
		case "T11":
		case "T14":
		case "T15":
			if(this.gfn_message("comm.행위여부","접수 및 처리내용을 수정하고 내부결재 재상신을 진행") ){
				this.DS_ONLINE_CVPL.setColumn(this.DS_ONLINE_CVPL.rowposition, "INNER_SANCTN_NO", "");
				break;
			}else{
				break;
			}
		default:
			var innerSanctnMdlId = "AUD-100065"; // 내부결재모형ID
			var sysSeCode = "SYS";
			var jobSe = "MIS";
			var svcId = "GetInnerSanctnId";
			if(this.ds_recomInfo.getRowCount() ==0) this.ds_recomInfo.addRow();
			

			var key = "AUD100065" + "^" + this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "RCEPT_SN");
			this.ds_recomInfo.setColumn(0, "JOB_KEY", key);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_MDL_ID", innerSanctnMdlId);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_ID", this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "INNER_SANCTN_NO"));
			
			this.ds_recomInfo.setColumn(0, "SYS_SE_CODE", sysSeCode);
			this.ds_recomInfo.setColumn(0, "JOB_SE", jobSe);
			this.fn_ManageInnerSanction(innerSanctnMdlId, sysSeCode, jobSe, svcId);
		break;
		}
	}else{
		this.gfn_message("comm.그리드.행선택.요청");
	}
}

/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if(strSvcId =="selectOnCvpResultlist")
	{ 
		//저장트랜잭션 직전 rowposition값으로 이동
		if(save_row != -1)
		{
			this.DS_ONLINE_CVPL.set_rowposition(save_row);
			save_row = -1;
		}
	}
	
	if(strSvcId == "selectAtchFile")
	{
		this.fn_transaction("selectAtchFile2");
	}
	
	//로그인 사용자 정보
	if(strSvcId == "selectUserInfo")
	{
		var telNo = this.ds_session.getColumn(0, "TELNO_1");
		this.ds_chrctrCnInfo.clearData();
		this.ds_chrctrCnInfo.addRow();
		this.ds_chrctrCnInfo.setColumn(0, "SEND_TEL_NO", telNo);
		this.fn_transaction("audAdmin");
	}
	if(strSvcId == "audAdmin")
	{
		this.fn_search();
	}
	
	//내부결재 트랜잭션 콜백
	if(strSvcId == "GetInnerSanctnId"){
		this.ds_cond02.clearData();
		this.ds_cond02.addRow();
		this.ds_cond02.setColumn(0, "RCEPT_SN", this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "RCEPT_SN"));
		this.ds_cond02.setColumn(0, "INNER_SANCTN_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
		this.fn_popup_InnerSanctionDrft();
	}
	if(strSvcId == "selectInnerSanctnMasterCheck"){
		if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
		{
			this.fn_transaction("saveInnerSanc");
		}
	}
	if(strSvcId == "saveInnerSanc"){
		this.fn_transaction("selectOnCvpResultlist");
	}
	if (strSvcId == "sendEmail")
	{
		this.gfn_message("success.이메일발송.성공");
	}
	if (strSvcId == "CONFMAT")
	{
		this.DS_ONLINE_CVPL.set_updatecontrol(false);
		var nConfm = Number(this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "CONFM_AT"));
		//0 -> 1, 1-> 0
		this.DS_ONLINE_CVPL.setColumn(this.DS_ONLINE_CVPL.rowposition, "CONFM_AT", -(nConfm-1));
		this.DS_ONLINE_CVPL.set_updatecontrol(true);
		
		//2016.07.01
		this.gfn_message("success.처리.성공");
	}
}


