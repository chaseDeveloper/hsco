/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		//로그인 사용자 정보 조회
		case "selectUserInfo" :			
			var sController   	= "hsco/mis/cus/CUS000000/selectUserInfo.do";
			var sInDatasets   	= "input01=ds_cond01";
			var sOutDatasets  	= "ds_session=output01";
		break;
		//감사결재자 조회
		case "audAdmin":
			var sController   	= "hsco/com/sym/aum/SYM020200/getListGrpUser.do";
			var sInDatasets   	= "input1=ds_audAdmin";
			var sOutDatasets  	= "ds_audAdmin=output1";
		break;
		//상단 조회
	    case "selectOnCvpResultlist" :
			this.DS_ONLINE_CVPL.clearData();
			this.ds_fileList.clearData();
			this.ds_fileList2.clearData();
			// 조회조건
			var tabnumber = this.div_Cont.AUD030103.tabindex + 1;
			this.ds_cond.setColumn(0,"RCEPT_SE",     tabnumber);	
			var sController   	= "hsco/mis/aud/AUD030103/selectOnCvpResultlist.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_ONLINE_CVPL=output01";
		break;
		
		//상단 저장
		case "rceptOnCvplResultU":
			var sController   	= "hsco/mis/aud/AUD030103/rceptOnCvplResultU.do";
			var sInDatasets   	= "input02=DS_ONLINE_CVPL:U";
			var sOutDatasets  	= "";		
		break;
			
		//접수 첨부파일조회
		case "selectAtchFile":
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;

		//조치결과 첨부파일 조회
		case "selectAtchFile2":
			this.ds_fileList2.clearData();
			this.ds_fileList2.addRow();
			this.ds_fileList2.setColumn(0, "FILE_SN", this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "MANAGT_ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList2";
			var sOutDatasets = "ds_fileList2=output1";
		break;	
		
		//내부결재 관련 트랜잭션
		case "saveInnerSanc": // 내부결재 저장
			var sController   	= "hsco/mis/aud/AUD030103/saveInnerSanc.do";
			var sInDatasets   	= "input1=ds_cond02:U";
			var sOutDatasets  	= "";
			break;
		case "GetInnerSanctnId": // 내부결재id 조회   
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
		break;
		case "innerSanctnUbiDataset": // 시스템연계 내부결재보고서 정보 조회
			var sController   	= "hsco/mis/aud/AUD030103/innerSanctnUbiDataset.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";	//양식 정해지면 output데이터셋 만들어서 추가예정
		break;
		//최초 내부결재 팝업 호출 시 저장버튼을 눌러 시작되었는지, 닫기버튼을 눌러 취소를 했는지 여부check
		case "selectInnerSanctnMasterCheck"	:
			var sController   	= "hsco/mis/aud/AUD010101/selectInnerSanctnMasterCheck.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_innerSanctnChek=output1";
		break;
		case "CONFMAT":
			var sController   	= "hsco/mis/aud/AUD030103/confmU.do";	
			var sInDatasets   	= "input1=ds_confmAt:U";
			var sOutDatasets  	= "";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

