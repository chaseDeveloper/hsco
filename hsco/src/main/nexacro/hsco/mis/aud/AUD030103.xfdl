this.sendEmail = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var tabIndex = this.div_Cont.AUD030103.tabindex;

	var nRow = this.DS_ONLINE_CVPL.rowposition;
	if(nRow<0){
		this.gfn_message("info.항목.선택.알림", "수신자");
