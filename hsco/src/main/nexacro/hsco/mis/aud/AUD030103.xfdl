      }
      
      if(this.gfn_message("comm.행위여부","이메일을 발송") ){
		  this.ds_email.clearData();
		  this.ds_email.addRow();
		  this.ds_email.setColumn(0, "RECV_EMAIL", email);
		  this.ds_email.setColumn(0, "RECV_NM"   , this.DS_ONLINE_CVPL.getColumn(nRow, "APLCNT_NM"));
		  this.ds_email.setColumn(0, "SEND_EMAIL", "hsco@hsco.or.kr");   // 보내는 사람 메일주소로 변경
		  this.ds_email.setColumn(0, "SEND_NM", "화성도시공사");       // 보내는 사람 이름으로 변경
		  this.ds_email.setColumn(0, "TITLE", "[화성도시공사] 문의하신 온라인신고 결과입니다.");
		  this.ds_email.setColumn(0, "SUBJECT", this.DS_ONLINE_CVPL.getColumn(nRow, "PROCESS_SJ"));
		  this.ds_email.setColumn(0, "CONTENTS", this.DS_ONLINE_CVPL.getColumn(nRow, "PROCESS_CN"));
		  this.ds_email.setColumn(0, "FILE_SN", this.DS_ONLINE_CVPL.getColumn(nRow, "MANAGT_ATCH_FILE"));
		  // 발송
		  Ex.core.tran(this, "sendEmail", "hsco/mis/mis/MIS000000/sendEmail.do", "input1=ds_email", "", "", "fn_callBack"); 
      }
}

