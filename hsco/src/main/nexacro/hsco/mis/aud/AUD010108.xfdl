    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 감사관리
 * 02. 화면명   : AUD010108.xfdl
 * 03. 화면설명 : 내외부감사관리, 수범사례관리 테이블을 관리한다
 * 04. 작성일   : 2015.06.30
 * 05. 작성자   : 이지영
 * 06. 수정이력 : 2015.06.30 최초생성
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *    2015.07.03   이지영   수정(직원 팝업 추가)
 ***********************************************************************
 */
 /***********************************************************************
 * Script Include 
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
	 this.DS_PRGN_CASE_LIST
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
/***********************************************************************
 * 설명: ONINIT 영역
 ************************************************************************/
this.AUD010108_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * 설명: 폼로딩시 처리
 ************************************************************************/
this.AUD010108_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_SearchArea.spn_year.setFocus(true);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();   // 조회 데이터셋 초기화	
	this.ds_cond.addRow();      // 조회 데이터셋 행추가
	this.ds_cond.setColumn(0, "AUDIT_BGNYEAR", this.gfn_today().substring(0,4) - 5);
	this.ds_cond.setColumn(0, "AUDIT_ENDYEAR", this.gfn_today().substring(0,4));
	
	var comboParams = [["DS_AUD_INSTT",     "AUD002", 1, "전체"],
	                   ["DS_AUD_KND",       "AUD003", 1, "전체"], 
	                   ["DS_AUD_CL",        "AUD004", 1, "전체"],
	                   ["DS_AUDIT_ED_AT",   "AUD018", 1, ""],
	                   ["DS_AUD037",        "AUD037", "Y", "A", "B"],	// 감사구분(조회조건)
	                   ["DS_AUD037G",       "AUD037", "Y", "D", "B"],	// 감사구분
	                   ["DS_OFCPS_SE",      "HRM003", 1, ""],
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "AUD");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
 * 설명: 조회 처리
 ************************************************************************/
this.fn_search= function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	this.fn_transaction("selectInExAdList");
}
/***********************************************************************************
* 설명	: 내외부감사목록 셀영역 변경시.
***********************************************************************************/
this.DS_INEX_AUDIT_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.DS_PRGN_CASE_LIST.clearData();
	this.ds_fileList.clearData();
	if(e.newrow > -1) {
		this.fn_transaction("selectPrgnCaseList");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		
		//상단 내외부감사 조회 트랜잭션
		case "selectInExAdList" :
			// 초기화
			this.DS_INEX_AUDIT.clearData();
			this.DS_PRGN_CASE_LIST.clearData();
			this.ds_fileList.clearData();
			var sController   	= "hsco/mis/aud/AUD010105/selectInExAdList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_INEX_AUDIT=output01";
		break;
		
		//수범사례조회
		case "selectPrgnCaseList":
			this.DS_PRGN_CASE_LIST.clearData();
			this.ds_fileList.clearData();
			this.DS_SEARCH2.clearData();              // 조회 데이터셋 초기화
			var nRow = this.DS_SEARCH2.addRow();
			//조회조건
			this.DS_SEARCH2.setColumn(nRow,"AUDIT_NO",  this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_PLAN_NO"));
			this.DS_SEARCH2.setColumn(nRow,"AUDIT_YEAR",this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_PLAN_YEAR"));

			var sController   	= "hsco/mis/aud/AUD010108/selectPrgnCaseList.do";
			var sInDatasets   	= "input02=DS_SEARCH2";
			var sOutDatasets  	= "DS_PRGN_CASE_LIST=output02";
		break;
		
		//수범사례저장
		case "PrgnCaseListCUD":
			var sController   	= "hsco/mis/aud/AUD010108/PrgnCaseListCUD.do";
			var sInDatasets   	= "input03=DS_PRGN_CASE_LIST:U";
			var sOutDatasets  	= "";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_PRGN_CASE_LIST.getColumn(this.DS_PRGN_CASE_LIST.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}
/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}else{
		//수범사례 저장 콜백
		if (strSvcId == "PrgnCaseListCUD") 
		{
			this.fn_transaction("selectPrgnCaseList")
		}
		//수범사례 조회 콜백
		if (strSvcId == "selectPrgnCaseList")
		{
			//저장트랜잭션 직전 rowposition값으로 이동
			if(save_row != -1)
			{
				this.DS_PRGN_CASE_LIST.set_rowposition(save_row);
				save_row = -1;
			}
		}
	}
}

/***********************************************************************************
* 설명	: 상단 신규버튼 클릭 시
***********************************************************************************/
this.fn_insert = function()
{
	if(this.DS_INEX_AUDIT.rowposition > -1) {
		this.DS_PRGN_CASE_LIST.set_enableevent(false);
		nRow = this.DS_PRGN_CASE_LIST.addRow();
		this.ds_fileList.clearData();
		this.DS_PRGN_CASE_LIST.set_enableevent(true);
		this.DS_PRGN_CASE_LIST.setColumn(nRow,"AUDIT_NO",  this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_PLAN_NO"));
		this.DS_PRGN_CASE_LIST.setColumn(nRow,"AUDIT_YEAR",this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_PLAN_YEAR"));
	}
	else {
		this.gfn_message("comm.항목.선택.요청","내외부감사목록");
	}
}
/***********************************************************************************
* 설명	: 상단 삭제버튼 클릭 시
***********************************************************************************/
this.fn_delete = function()
{
	if(this.gfn_message("confirm.삭제여부")){
		var nRow = this.DS_PRGN_CASE_LIST.rowposition;
		this.DS_PRGN_CASE_LIST.deleteRow(nRow);	
	}
}
/***********************************************************************************
* 설명	: 상단 취소버튼 클릭 시
***********************************************************************************/
this.fn_cancel = function()
{
	 if(Ex.util.isUpdated(this.DS_PRGN_CASE_LIST)){
    	if(this.gfn_message("confirm.취소여부")){
	        //모든 내용을 복원 하시겠습니까?
		    this.DS_PRGN_CASE_LIST.reset();		//DATASET ROLLBACK
		    this.DS_PRGN_CASE_LIST.applyChange();	//DATASET COMMIT
		return;
	    }	
    }else{
		this.gfn_message("comm.내역.변경.없음");
    }
}
/***********************************************************************************
* 설명	: 행저장 버튼 클릭 시
***********************************************************************************/
this.fn_save = function()
{
	//validation check		
	if(!this.gfn_checkValidation(this.DS_PRGN_CASE_LIST, this.ds_validation)){
		return;
	} 	
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.DS_PRGN_CASE_LIST.rowposition;
		this.fn_transaction("PrgnCaseListCUD");
	}
}


/***********************************************************************************
* 설명	: 직원 버튼(팝업) 클릭 시
***********************************************************************************/
this.div_Cont_btn_Empl_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	if(this.DS_PRGN_CASE_LIST.rowposition > -1){
		 var nLeft = system.clientToScreenX(this, 10);
		 var nTop  = system.clientToScreenY(this, 10);

		 var newChild = new ChildFrame;
		 newChild.init("Popup1"
					, "absolute", nLeft, nTop, 400, 400, null, null
					, "com::empno_P01.xfdl");

		 newChild.set_openalign("center middle");
		 newChild.set_dragmovetype("all");  

		 newChild.showModal(this.getOwnerFrame()
						 , {}
						 , this
						 , "fn_callback2");
    }else{ 
		this.gfn_message("comm.항목.선택.요청","수범사례목록");
    }
}


/***********************************************************************************
* 설명	: 사원선택 팝업 콜백
***********************************************************************************/
this.fn_callback2 = function(sPopupId, sReturn)
{
	if(sPopupId == "Popup1")
	{
		this.ds_rtn_1.clearData();
		this.ds_rtn_1.loadXML(sReturn);trace(sReturn);trace("OFCPS_SE : " + this.ds_rtn_1.getColumn(this.ds_rtn_1.rowposition, 'OFCPS_SE'));
		if(this.ds_rtn_1.rowcount > 0){
			this.DS_PRGN_CASE_LIST.setColumn(this.DS_PRGN_CASE_LIST.rowposition, 'DEPT_CODE',  this.ds_rtn_1.getColumn(this.ds_rtn_1.rowposition, 'DEPT_CODE'));
			this.DS_PRGN_CASE_LIST.setColumn(this.DS_PRGN_CASE_LIST.rowposition, 'OFCPS_SE', this.ds_rtn_1.getColumn(this.ds_rtn_1.rowposition, 'OFCPS_SE'));
			this.DS_PRGN_CASE_LIST.setColumn(this.DS_PRGN_CASE_LIST.rowposition, 'DEPT_NM',    this.ds_rtn_1.getColumn(this.ds_rtn_1.rowposition, 'DEPT_NM'));
			this.DS_PRGN_CASE_LIST.setColumn(this.DS_PRGN_CASE_LIST.rowposition, 'EMPNO',      this.ds_rtn_1.getColumn(this.ds_rtn_1.rowposition, 'EMPNO'));
			this.DS_PRGN_CASE_LIST.setColumn(this.DS_PRGN_CASE_LIST.rowposition, 'EMPNM',      this.ds_rtn_1.getColumn(this.ds_rtn_1.rowposition, 'EMPNM'));
		}
	}
}

/***********************************************************************************
* 설명	: 수범사례 행 변경 시 첨부파일 조회
***********************************************************************************/
this.DS_PRGN_CASE_LIST_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_fileList.clearData();
	if(e.newrow > -1) {
		this.fn_transaction("selectAtchFile");
	}
}

/***********************************************************************
 * 설명 : 첨부파일 선택 삭제 버튼클릭 시
 ***********************************************************************/
this.btn_delFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
		return false;
	}
	
	for(var fRow = this.ds_fileList.getRowCount()-1; fRow >= 0; fRow--) {
		var chkVal = this.ds_fileList.getColumn(fRow, "CHK");
		if(chkVal == '1') {
			this.ds_fileList.deleteRow(fRow);
		}
	}
	var strSvcId   	 = "deleteFile";
	var sController  = "hsco/cmm/file/deleteFile.do";
	var sInDatasets  = "input1=ds_fileList:U";
	var sOutDatasets = "ds_fileList=output1";
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
/***********************************************************************
 * 설명 : 첨부파일 업로드 버튼클릭 시
 ***********************************************************************/
this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
	this.fn_autoFileSn(
		 "MIS"									// 대분류   (SYS_SE)
		,"AUD"									// 업무구분 (JOB_SE)
		, this.File_UpDownManager	            // fileManager 오브젝트
		, this.ds_fileList		                // fileObject
		, this.DS_PRGN_CASE_LIST	           	// fileSn을 불러오거나 반영해야 하는 컬럼
		, "TBAUD_PRGN_CASE"		                // fileSn을 저장할 테이블
		, "ATCH_FILE"							// fileSn 컬럼명이 다를 경우를 대비
		, ["PRGN_CASE_SN", "AUDIT_NO","AUDIT_YEAR"]	// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
	);
}
/***********************************************************************
 * 설명 : 첨부파일 선택다운로드 버튼클릭 시
 ***********************************************************************/
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
		return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}

/***********************************************************************
 * 설명 : 첨부파일 그리드 헤드 클릭 시 
 ***********************************************************************/
this.grd_cmmnAtchmnfl_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0)
	{	
		//모든 체크박스 선택
		this.gfn_setGridCheckAll(obj, e);
	}else {
		//선택한 헤드에 따른 정렬
		this.gfn_gridSort(obj, e);
	}
}

this.DS_INEX_AUDIT_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(Ex.util.isUpdated(this.DS_PRGN_CASE_LIST)){
		if(!this.gfn_message("confirm.변경.취소.여부")){  	
			return false; //유지
		}else{	
			return true;
		}
	}
}
