/**********************************************************************
* call Transaction 영역
***********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	
		case "selectOnCvplist":
			// 초기화
			this.DS_ONLINE_CVPL.clearData();
			this.ds_fileList.clearData();
			// 조회조건 		
			var tabnumber = this.div_Cont.AUD030102.tabindex + 1;
			this.ds_cond.setColumn(0,"RCEPT_SE",    tabnumber);
			var sController   	= "hsco/mis/aud/AUD030102/selectOnCvplist.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_ONLINE_CVPL=output01";
		break;
	
		//온라인민원 접수 트랜잭션
		case "rceptOnCvplCUD":
			var sController   	= "hsco/mis/aud/AUD030102/rceptOnCvplCUD.do";
			var sInDatasets   	= "input01=DS_ONLINE_CVPL:U";
			var sOutDatasets  	= "";
		break;
		
		//온라인민원 이첩 트랜잭션
		case "TrfntfCUD":
			var sController   	= "hsco/mis/aud/AUD030102/TrfntfCUD.do";
			var sInDatasets   	= "input01=DS_ONLINE_CVPL:U";
			var sOutDatasets  	= "";
		break;
						
		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.AUD030102.T01.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.div_Cont.AUD030102.T02.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.div_Cont.AUD030102.T03.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.div_Cont.AUD030102.T04.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectOnCvplist": // ListMst 불러오기
				for(var i = 0 ; i <= this.DS_ONLINE_CVPL.rowcount; i ++)
				{
					this.DS_ONLINE_CVPL.set_updatecontrol(false);
					if(this.comUtils.isNull(this.DS_ONLINE_CVPL.getColumn(i, "MANAGT_SE"))){
						this.DS_ONLINE_CVPL.setColumn(i, "MANAGT_SE", 4)
					}
					this.DS_ONLINE_CVPL.set_updatecontrol(true);
				}
				// 저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.DS_ONLINE_CVPL.set_rowposition(save_row);
					save_row = -1;
				}
			break;
			case "rceptOnCvplCUD": 	// 접수CUD
				this.fn_search();	
				break;
				
			case "TrfntfCUD": 		// 이첩CUD
				this.fn_search();
				break;
		}
	}
}
