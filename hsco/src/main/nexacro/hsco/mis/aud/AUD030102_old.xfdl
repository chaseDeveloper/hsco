    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 감사관리
 * 02. 화면명   : AUD030102.xfdl
 * 03. 화면설명 : 온라인신고관리 테이블을 관리한다.
 * 04. 작성일   : 2015.07.06
 * 05. 작성자   : 이지영
 * 06. 수정이력 : 2015.07.06 최초생성
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *    2015.07.07   이지영   최초
 ***********************************************************************
 */
 /***********************************************************************
 * Script Include 
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
	 this.DS_ONLINE_CVPL
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
/***********************************************************************
 * 설명: oninit 영역
 ************************************************************************/
this.AUD030102_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * 설명: 폼로딩시 처리
 ************************************************************************/
this.AUD030102_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_SearchArea.cal_BGN_RCEPT_DE.setFocus(true);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();   // 조회 데이터셋 초기화	
	this.ds_cond.addRow();      // 조회 데이터셋 행추가
	
	this.ds_cond.setColumn(0, "BGN_RCEPT_DE", this.gfn_today().substring(0,4) + '0101');
	this.ds_cond.setColumn(0, "END_RCEPT_DE", this.gfn_today());
	var comboParams = [["DS_MANAGT_SE",        "AUD024", 1, "전체"],
	                   ["DS_RCEPT_AT",         "AUD023", 1, "전체"],
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "AUD");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
 * 설명: 조회버튼 클릭시 처리
 ************************************************************************/
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_transaction("selectOnCvplist");
}

/***********************************************************************************
* 설명	: 접수버튼 클릭시
***********************************************************************************/
this.div_Cont_AUD030102_T01_btn_Rcept_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.DS_ONLINE_CVPL.rowposition > -1)
	{
		if(this.DS_ONLINE_CVPL.getColumn(this.DS_ONLINE_CVPL.rowposition, 'RCEPT_AT') == 2)
		{
			this.gfn_message("comm.접수.완료");
		}else{
			//접수여부를 접수(2)상태로
			this.DS_ONLINE_CVPL.setColumn(this.DS_ONLINE_CVPL.rowposition, 'RCEPT_AT', '2');	// 접수구분 : 접수완료
			this.DS_ONLINE_CVPL.setColumn(this.DS_ONLINE_CVPL.rowposition, 'MANAGT_SE', '1');	// 조치구분 : 처리중
		//현재 rowpostion값을 저장
		save_row = this.DS_ONLINE_CVPL.rowposition;
			this.fn_transaction("rceptOnCvplCUD");
		}
	}else{
		this.gfn_message("comm.데이터.선택.없음");
	}
}

