    }	
}
/***********************************************************************************
* 감사계획목록 행 변경 전 
***********************************************************************************/
this.DS_AUDIT_PLAN_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	 //변경된 감사계획목록이 저장되어있을 경우(신규/추가 작업 중 영역 변경 제외)
	if(this.DS_AUDIT_PLAN.rowcount > 0 )
	{
		//DS_AUDIT_COMPOSI, DS_AUDIT_DEPT_TABLE 의 값이 변경되었을 경우
		if( (Ex.util.isUpdated(this.DS_AUDIT_COMPOSI)) || (Ex.util.isUpdated(this.DS_AUDIT_DEPT_TABLE)) )
		{	
			if(!this.gfn_message("confirm.변경.취소.여부")){  	
				return false; //유지
			}
			else{
				return true;
			}
		}
	}
}

/***********************************************************************************
* grid header click sort
***********************************************************************************/
this.div_Cont_grd_AUDIT_TABLE_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.gfn_gridSort(obj, e);	
}


/***********************************************************************
 * 설명 : 첨부파일 선택 삭제 버튼클릭 시
 ***********************************************************************/
this.btn_delFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
		return false;
	}
	
	for(var fRow = this.ds_fileList.getRowCount()-1; fRow >= 0; fRow--) {
		var chkVal = this.ds_fileList.getColumn(fRow, "CHK");
		if(chkVal == '1') {
			this.ds_fileList.deleteRow(fRow);
		}
	}
	var strSvcId   	 = "deleteFile";
	var sController  = "hsco/cmm/file/deleteFile.do";
	var sInDatasets  = "input1=ds_fileList:U";
	var sOutDatasets = "ds_fileList=output1";
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
/***********************************************************************
 * 설명 : 첨부파일 업로드 버튼클릭 시
 ***********************************************************************/
this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//감사계획목록이 선택되었을 경우에만 행추가 
	if(this.DS_AUDIT_PLAN.rowcount < 1 ){
		this.gfn_message("comm.항목.선택.요청","감사계획목록");
	}
	else{
		if(this.DS_AUDIT_PLAN.getColumn(this.DS_AUDIT_PLAN.rowposition, "AUDIT_PLAN_NO") > 0){
			// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
			this.fn_autoFileSn(
				 "MIS"									// 대분류   (SYS_SE)
				,"AUD"									// 업무구분 (JOB_SE)
				, this.File_UpDownManager	            // fileManager 오브젝트
				, this.ds_fileList		                // fileObject
				, this.DS_AUDIT_PLAN	             	// fileSn을 불러오거나 반영해야 하는 컬럼
				, "TBAUD_AUDIT_PLAN"					// fileSn을 저장할 테이블
				, "ATCH_FILE"							// fileSn 컬럼명이 다를 경우를 대비
				, ["AUDIT_PLAN_NO", "AUDIT_PLAN_YEAR"]	// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
			);
		}
		else{
			this.gfn_message("comm.정보.저장.요청","먼저 감사계획목록");
		}
	}
}
/***********************************************************************
 * 설명 : 첨부파일 선택다운로드 버튼클릭 시
 ***********************************************************************/
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
		return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}

/***********************************************************************
 * 설명 : 첨부파일 그리드 헤드 클릭 시 
 ***********************************************************************/
this.grd_cmmnAtchmnfl_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0)
	{	
		//모든 체크박스 선택
		this.gfn_setGridCheckAll(obj, e);
	}else {
		//선택한 헤드에 따른 정렬
		this.gfn_gridSort(obj, e);
	}
}


/***********************************************************************
 * 설명 : 감사계획목록 컬럼 변경 시
 ***********************************************************************/
this.DS_AUDIT_PLAN_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "AUDIT_PLAN_INSTT") {
		var cRow = this.DS_AUDIT_COMPOSI.rowcount;
		if(cRow > 0) {
			var auditPlanInsttNm = this.div_Cont.grd_AUDIT_TABLE.getCellText(this.DS_AUDIT_PLAN.rowposition, 2);
			for(var i = 0; i < cRow; i++) {
				this.DS_AUDIT_COMPOSI.setColumn(i, "AUDIT_INSTT_CODE", e.newvalue);
				this.DS_AUDIT_COMPOSI.setColumn(i, "AUDIT_INSTT_NM"  , auditPlanInsttNm);
			}
		}
	}
	if(e.columnid == "CONFM_AT") {
		if(e.newvalue == '1') {
			this.div_Cont.txt_PRGN_LIST.set_readonly(true);
			this.div_Cont.txt_PRGN_LIST.set_cssclass("txt_WF_Readonly");
			this.div_Cont.txt_prvyyAuditAcmslt.set_readonly(true);
			this.div_Cont.txt_prvyyAuditAcmslt.set_cssclass("txt_WF_Readonly");
			
			this.div_Cont.btn_delFiles.set_enable(false);
			this.div_Cont.btn_uploadFiles.set_enable(false);
			
			this.div_Cont.btn_Add_Row01.set_enable(false);
			this.div_Cont.btn_Del_Row01.set_enable(false);
			this.div_Cont.btn_Can_Row01.set_enable(false);
			
			this.div_Cont.btn_Add_Row02.set_enable(false);
			this.div_Cont.btn_Del_Row02.set_enable(false);
			this.div_Cont.btn_Can_Row02.set_enable(false);
		}
		// 수정불가(승인후)
		else {
			this.div_Cont.txt_PRGN_LIST.set_readonly(false);
			this.div_Cont.txt_PRGN_LIST.set_cssclass("");
			this.div_Cont.txt_prvyyAuditAcmslt.set_readonly(false);
			this.div_Cont.txt_prvyyAuditAcmslt.set_cssclass("");
			
			this.div_Cont.btn_delFiles.set_enable(true);
			this.div_Cont.btn_uploadFiles.set_enable(true);
			
			this.div_Cont.btn_Add_Row01.set_enable(true);
			this.div_Cont.btn_Del_Row01.set_enable(true);
			this.div_Cont.btn_Can_Row01.set_enable(true);
			
			this.div_Cont.btn_Add_Row02.set_enable(true);
			this.div_Cont.btn_Del_Row02.set_enable(true);
			this.div_Cont.btn_Can_Row02.set_enable(true);
		}
	}
}

/***********************************************************************
 * 설명 : 계획승인 버튼 클릭시
 ***********************************************************************/
this.div_Cont_btn_dcsn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var dRow = this.DS_AUDIT_DEPT_TABLE.rowcount;
	var cRow = this.DS_AUDIT_COMPOSI.rowcount;
	var pRow = this.DS_AUDIT_PLAN.rowposition;
	var confmAt = this.DS_AUDIT_PLAN.getColumn(pRow, "CONFM_AT");
	var auditAt = this.DS_AUDIT_PLAN.getColumn(pRow, "AUDIT_AT");
	
	// 데이터셋 수정 여부체크
	if (   this.comUtils.isDatasetUpdated(this.DS_AUDIT_PLAN)
	    || this.comUtils.isDatasetUpdated(this.DS_AUDIT_DEPT_TABLE)
	    || this.comUtils.isDatasetUpdated(this.DS_AUDIT_COMPOSI)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;
		}
	}
	
	if(dRow < 1) {
		this.gfn_message("info.처리불가", "감사대상부서 정보가 존재하지 않습니다.\n감사대상부서 정보를 입력하여주십시오.");
		return false;
	}
	if(cRow < 1) {
		this.gfn_message("info.처리불가", "감사인 정보가 존재하지 않습니다.\n감사인 정보를 입력하여주십시오.");
		return false;
	}
	if(confmAt == '1') {
		this.gfn_message("info.처리불가", "이미 승인된 계획입니다.");
		return false;
	}
	if(auditAt == '1') {
		this.gfn_message("info.처리불가", "이미 종결된 계획입니다.");
		return false;
	}
	
	if(this.gfn_message("comm.행위여부", "현재 선택한 감사계획을 승인")){
		this.fn_transaction("updateConfmAt");
	}
}
