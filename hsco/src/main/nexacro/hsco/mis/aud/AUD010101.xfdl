/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리()
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.DS_AUDIT_PLAN) && !this.comUtils.isDatasetUpdated(this.DS_AUDIT_DEPT_TABLE) && !this.comUtils.isDatasetUpdated(this.DS_AUDIT_COMPOSI)) {
		this.gfn_message("comm.내역.변경.없음");
		return false;
	}
	
	// validation 체크(감사목록)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AUDIT_PLAN, this.ds_validation)) {
		return;
	}
	// validation 체크(관련부서)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AUDIT_DEPT_TABLE, this.ds_validation)) {
		return;
	}
	// validation 체크(감사인목록)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AUDIT_COMPOSI, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.DS_AUDIT_PLAN.rowposition;
		
		//2016.06.30 해당컬럼 삭제시 소스에서 삭제 또는 주석처리
		for(i=0 ; i < this.DS_AUDIT_COMPOSI.rowcount; i++){		
			this.DS_AUDIT_COMPOSI.setColumn(i, "AUDIT_INSTT_CODE",this.DS_AUDIT_PLAN.getColumn(this.DS_AUDIT_PLAN.rowposition, "AUDIT_PLAN_INSTT"));
			var strText = this.div_Cont.grd_AUDIT_TABLE.getCellText(0, 2);
			//alert("감사기관 = " + strText)
			this.DS_AUDIT_COMPOSI.setColumn(i, "AUDIT_INSTT_NM",strText);
		}
		
		this.fn_transaction("AuditPlanListCUD");
	}
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectAuditPlanList":
			var sController   	= "hsco/mis/aud/AUD010101/selectAuditPlanList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_AUDIT_PLAN=output01";
			break;
			
		// sub데이터 조회
		case "selectAuditR":
			// 부모값
			var apRow  = this.DS_AUDIT_PLAN.rowposition
			var planNo = this.DS_AUDIT_PLAN.getColumn(apRow, "AUDIT_PLAN_NO");
			var planYr = this.DS_AUDIT_PLAN.getColumn(apRow, "AUDIT_PLAN_YEAR");
			
			// 검색조건
			this.DS_SEARCH2.clearData();
			var nRow = this.DS_SEARCH2.addRow();
			this.DS_SEARCH2.setColumn(nRow,"AUDIT_PLAN_NO",   planNo);
			this.DS_SEARCH2.setColumn(nRow,"AUDIT_PLAN_YEAR", planYr);
			
			var sController   	= "hsco/mis/aud/AUD010101/selectAuditR.do";
			var sInDatasets   	= "input05=DS_SEARCH2";
			var sOutDatasets  	= "DS_AUDIT_DEPT_TABLE=output05 DS_AUDIT_COMPOSI=output06";
			break;
			
		// 저장
		case "AuditPlanListCUD":
			var sController   	= "hsco/mis/aud/AUD010101/AuditPlanListCUD.do";
			var sInDatasets   	= "input1=DS_AUDIT_PLAN:U input2=DS_AUDIT_DEPT_TABLE:U input3=DS_AUDIT_COMPOSI:U";
			var sOutDatasets  	= "";
			break;
			
		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_AUDIT_PLAN.getColumn(this.DS_AUDIT_PLAN.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
			break;
		case "updateConfmAt":
			var sController   	= "hsco/mis/aud/AUD010101/updateConfmAt.do";	
			var sInDatasets   	= "input1=DS_SEARCH2";
			var sOutDatasets  	= "";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
/**********************************************************************
* Transaction Callback 영역
***********************************************************************/
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "AuditPlanListCUD": 	// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectAuditR":
				this.fn_transaction("selectAtchFile");
				break;
			case "selectAuditPlanList":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.DS_AUDIT_PLAN.set_rowposition(save_row);
					save_row = -1;
				}
				break;
			case "updateConfmAt":
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}

