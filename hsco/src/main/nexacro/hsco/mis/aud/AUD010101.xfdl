    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID    : AUD010101.xfdl
 * 화면명    : 감사계획관리
 * 화면설명  : 감사계획관리 테이블을 관리한다.
 * 작성일    : 2015.06.09
 * 작성자    : 이지영
 * 수정이력  : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    2015.07.27   이지영   수정
				-CUD 트랜잭션 콜백형태에서 한번에 날리는 형태로 수정    
 ***********************************************************************/
/**********************************************************************
* Script Include 
***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/**********************************************************************
* 화면 변수 선언부
**********************************************************************/
this.parentRow = -1;      
this.checkDs = [
	 this.DS_AUDIT_PLAN
	,this.DS_AUDIT_DEPT_TABLE
	,this.DS_AUDIT_COMPOSI
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
/***********************************************************************
* onInit 영역
***********************************************************************/
this.AUD010101_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();	
	// DataSet 초기화
	this.fn_init_dataset();
}
/***********************************************************************
* onLoad 영역
***********************************************************************/
this.AUD010101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_SearchArea.spn_year.setFocus(true);	
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
 	this.fn_misFormInit(this);
 	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	this.ds_cond.setColumn(nRow, "AUDIT_PLAN_BGNYEAR", this.gfn_today().substring(0,4) - 5);
	this.ds_cond.setColumn(nRow, "AUDIT_PLAN_ENDYEAR", this.gfn_today().substring(0,4));
	this.DS_DEPT.clearData();
   
	// 공통코드
	var comboParams = [  ["DS_AUD_INSTTS",        "AUD002", 1, "전체"] //감사기관
						,["DS_AUD_KNDS",          "AUD003", 1, "전체"] //감사종류
						,["DS_AUD_CLS",           "AUD004", 1, "전체"] //감사분류
						,["DS_COND_AUD036_DSPTH", "AUD036", 1, "전체"] //감사통보
						,["DS_AUD_INSTT",         "AUD002", 1, ""] //감사기관
						,["DS_AUD_KND",           "AUD003", 1, ""] //감사종류
						,["DS_AUD_CL",            "AUD004", 1, ""] //감사분류
						,["DS_AUD036_DSPTH",      "AUD036", 1, ""] //감사통보
						,["DS_AUD037",            "AUD037", 1, "전체"] //감사구분
						,["DS_AUD037G",           "AUD037", 1, ""] //감사구분
						,["ds_sacnctnSttusSe",    "COM032", 1, ""]
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}
/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	
	  var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "AUD");
   this.File_UpDownManager.init(fileConfig);
}

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리()
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// validation 체크(세부사업)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	// clear Datasets
	this.DS_AUDIT_PLAN.clearData();
	this.DS_AUDIT_COMPOSI.clearData();
	this.DS_AUDIT_DEPT_TABLE.clearData();
