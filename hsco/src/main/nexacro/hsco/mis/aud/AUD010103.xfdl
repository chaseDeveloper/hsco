    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 감사관리
 * 02. 화면명   : AUD010103.xfdl
 * 03. 화면설명 : 내외부감사관리,내외부감사실시통보관리 테이블을 관리한다.
 * 04. 작성일   : 2015.06.16
 * 05. 작성자   : 이지영
 * 06. 수정이력 : 2015.06.16 최초생성
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *    2015.06.16   이지영   최초
 ***********************************************************************
 */
 /***********************************************************************
 * Script Include 
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
	 this.DS_INEX_AUDIT_OPRTN
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
/***********************************************************************
 * 설명 : onInit 영역
 ***********************************************************************/
this.AUD010103_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * 설명 : onload 영역
 ***********************************************************************/
this.AUD010103_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_SearchArea.spn_year.setFocus(true);	
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);		// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();     // 조회 데이터셋 초기화	
	this.ds_cond.addRow();		// 조회 데이터셋 행추가
	this.ds_cond.setColumn(0, "AUDIT_BGNYEAR", this.gfn_today().substring(0,4) - 5);
	this.ds_cond.setColumn(0, "AUDIT_ENDYEAR", this.gfn_today().substring(0,4));
	var comboParams = [["DS_AUD_INSTT",    "AUD002", 1, "전체"],
	                   ["DS_AUD_KND",      "AUD003", 1, "전체"], 
	                   ["DS_AUD_CL",       "AUD004", 1, "전체"],
	                   ["DS_AUDIT_ED_AT",  "AUD018", 1, ""],
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
    var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "AUD");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
 * 설명: 조회 처리
 ************************************************************************/
this.fn_search= function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	// 초기화
	this.fn_transaction("selectInExAdList");
}

/***********************************************************************
 * 설명: 내외부감사관리 셀 영역 변경 전
 ************************************************************************/
this.DS_INEX_AUDIT_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
    if(Ex.util.isUpdated(this.DS_INEX_AUDIT_OPRTN)){
		if(!this.gfn_message("confirm.변경.취소.여부")){  	
			return false; //유지
		}else{
			this.DS_INEX_AUDIT_OPRTN.reset();
			return true;
		}
	}
}
/***********************************************************************
 * 설명: 내외부감사관리 셀 영역 변경 후
 ************************************************************************/
this.DS_INEX_AUDIT_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.DS_INEX_AUDIT_OPRTN.clearData();
	this.ds_fileList.clearData();
	
	if(e.newrow > -1) {
		this.fn_transaction("selectIEAOList");
	}
}

/***********************************************************************************
* 설명	: 상단 신규버튼 클릭시 
***********************************************************************************/
this.fn_insert = function()
{
	if(this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_ED_AT") == '2')
	{
		this.gfn_message("comm.종결","감사");
	}else {
		this.DS_INEX_AUDIT_OPRTN.set_enableevent(false);
		this.ds_fileList.clearData();
		var nRow = this.DS_INEX_AUDIT_OPRTN.addRow();
		
		//내외부감사목록테이블의 AUDIT_NO, AUDIT_YEAR 값을 감사실시통보 정보에 세팅한다.		
		var pRow = this.DS_INEX_AUDIT.rowposition;
		var dspthDe = this.gfn_today();
		var auditPlanNo   = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_PLAN_NO");
		var auditPlanYear = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_PLAN_YEAR");
		var auditBgnDe    = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_BGNDE");
		var auditEndDe    = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_ENDDE");
		
		this.DS_INEX_AUDIT_OPRTN.setColumn(nRow, "DSPTH_DE",    dspthDe);
		this.DS_INEX_AUDIT_OPRTN.setColumn(nRow, "AUDIT_NO",    auditPlanNo);
		this.DS_INEX_AUDIT_OPRTN.setColumn(nRow, "AUDIT_YEAR",  auditPlanYear);
		this.DS_INEX_AUDIT_OPRTN.setColumn(nRow, "AUDIT_BGNDE", auditBgnDe);
		this.DS_INEX_AUDIT_OPRTN.setColumn(nRow, "AUDIT_ENDDE", auditEndDe);
		this.DS_INEX_AUDIT_OPRTN.set_enableevent(true);
	}
}
/***********************************************************************************
* 설명	: 상단 삭제 버튼 클릭시
***********************************************************************************/
this.fn_delete = function()
{
	if(this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_ED_AT") == '2')
	{
		this.gfn_message("comm.종결","감사");
	}
	else
	{
		/*
		 * 0 : 기안전
		 * 1 : 기안
		 * 2 : 반송
		 * 3 : 취소
		 * 4 : 중간결재
		 * 5 : 결재완료
		 */
		var sanctnSts = this.DS_INEX_AUDIT_OPRTN.getColumn(this.DS_INEX_AUDIT_OPRTN.rowposition, "SANCTN_STS");
		
		// 기안전, 반송, 취소일 경우 삭제 가능
		if(sanctnSts == '0' || sanctnSts == '2' || sanctnSts == '3' || this.comUtils.isNull(sanctnSts)){
			if(this.gfn_message("confirm.삭제여부"))
			{
				var nRow01 = this.DS_INEX_AUDIT_OPRTN.rowposition;
				this.DS_INEX_AUDIT_OPRTN.deleteRow(nRow01);
				var nRow02 = this.DS_INEX_AUDIT_OPRTN.rowposition;
				
				if(nRow01 == nRow02){
					this.DS_INEX_AUDIT_OPRTN.set_rowposition(-1);
					this.DS_INEX_AUDIT_OPRTN.set_rowposition(nRow02);
				}
			}
		}else {
			this.gfn_message("info.처리불가","결재 상태를 확인하시기 바랍니다.");
		}
	}
}

/***********************************************************************
 * 설명: 상단 취소 버튼 클릭시
 ************************************************************************/
this.fn_cancel = function()
{
    if(Ex.util.isUpdated(this.DS_INEX_AUDIT_OPRTN)){
	    if(this.gfn_message("confirm.취소여부")){ //모든 내용을 복원 하시겠습니까?
		    this.DS_INEX_AUDIT_OPRTN.reset();		//DATASET ROLLBACK
		    this.DS_INEX_AUDIT_OPRTN.applyChange();	//DATASET COMMIT
		    return;
	    }	
    }else{
		this.gfn_message("comm.내역.변경.없음");
    }
}    

/***********************************************************************
 * 설명: 상단저장버튼 클릭시
 ************************************************************************/
this.fn_save = function()
{
	//validation check		
	if(!this.gfn_checkValidation(this.DS_INEX_AUDIT_OPRTN, this.ds_validation)){
		return;
	} 
	
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.DS_INEX_AUDIT_OPRTN.rowposition;
		this.fn_transaction("IEAOListCUD");
	}
}

/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}else{
		//감사실시통보 저장 콜백
		if (strSvcId == "IEAOListCUD")
		{
			this.gfn_message("success.처리.성공");
			this.fn_transaction("selectIEAOList");
		}
		if (strSvcId == "selectIEAOList")
		{
			//저장트랜잭션 직전 rowposition값으로 이동
			if(save_row != -1)
			{
				this.DS_INEX_AUDIT_OPRTN.set_rowposition(save_row);
				save_row = -1;
			}
		}
		if (strSvcId == "approve")
		{
			var oUrl = this.ds_approve.getColumn(0,"O_URL");
			system.execBrowser(oUrl);
			this.fn_transaction("selectIEAOList");
		}
	}
}

/***********************************************************************************
* grid header click sort
***********************************************************************************/
this.Grid_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.gfn_gridSort(obj, e);	
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//내외부감사목록 조회
		case "selectInExAdList":
			this.DS_INEX_AUDIT_OPRTN.clearData();
			this.ds_fileList.clearData();
			var sController   	= "hsco/mis/aud/AUD010103/selectInExAdList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_INEX_AUDIT=output01";
		break;
			
		//감사실시통보 저장
		case "IEAOListCUD":
			// 감사실시통보 정보가 생성되면 저장 시 종결여부를 '대기' 상태로 변경
			// 감사실시통보를 하지 않으면 계속 '대기' 상태인건가?
			this.DS_INEX_AUDIT.setColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_ED_AT", "1");
			var sController   	= "hsco/mis/aud/AUD010103/IEAOListCUD.do";
			var sInDatasets   	= "input03=DS_INEX_AUDIT_OPRTN:U input04=DS_INEX_AUDIT:U";
			var sOutDatasets  	= "";
		break;
		
		//감사실시통보 조회
		case "selectIEAOList":
		    this.DS_INEX_AUDIT_OPRTN.clearData();
			this.ds_fileList.clearData();
		    this.DS_SEARCH2.clearData();              // 조회 데이터셋 초기화
			var nRow = this.DS_SEARCH2.addRow();
			//조회조건
			this.DS_SEARCH2.setColumn(nRow,"AUDIT_NO",  this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_NO"));
			this.DS_SEARCH2.setColumn(nRow,"AUDIT_YEAR",this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_YEAR"));
			var sController   	= "hsco/mis/aud/AUD010103/selectIEAOList.do";
			var sInDatasets   	= "input02=DS_SEARCH2";
			var sOutDatasets  	= "DS_INEX_AUDIT_OPRTN=output02";
		break;		

		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_INEX_AUDIT_OPRTN.getColumn(this.DS_INEX_AUDIT_OPRTN.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
		case "approve":
			var sController   	= "hsco/mis/aud/AUD010103/approve.do";	
			var sInDatasets   	= "input1=ds_approve";
			var sOutDatasets  	= "ds_approve=output1";
		break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/***********************************************************************
* 감사실시통보 행 변경시 첨부파일 조회
************************************************************************/
this.DS_INEX_AUDIT_OPRTN_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var auditEdAt = this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "AUDIT_ED_AT");
	var bEnable   = this.fn_getEnable(e.newrow);
	
	if(bEnable) {
		this.div_Cont.btn_delFiles.set_enable(true);
		this.div_Cont.btn_uploadFiles.set_enable(true);
	}
	else {
		this.div_Cont.btn_delFiles.set_enable(false);
		this.div_Cont.btn_uploadFiles.set_enable(false);
	}
	
	this.fn_transaction("selectAtchFile");
}

