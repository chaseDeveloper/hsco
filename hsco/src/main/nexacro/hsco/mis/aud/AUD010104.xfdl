/***********************************************************************
 * 설명 : 첨부파일 업로드 성공시
 ***********************************************************************/
this.fn_onFileSuccess = function(){
	//this.gfn_message("success.처리.성공");
	
	if(this.div_Cont.AUD010104.tabindex == 1) {			// 감사자료
		this.ds_condFile1.setColumn(0, "FILE_SN", this.DS_AUDIT_DTA.getColumn(this.DS_AUDIT_DTA.rowposition, "ATCH_FILE"));
		this.fn_transaction("selectFileList1");
	}
	else if(this.div_Cont.AUD010104.tabindex == 2) {	// 확인서
		this.ds_condFile2.setColumn(0, "FILE_SN", this.DS_INEX_AUDIT.getColumn(this.DS_INEX_AUDIT.rowposition, "ATCH_FILE2"));
		this.fn_transaction("selectFileList2");
	}
	else if(this.div_Cont.AUD010104.tabindex == 3) {	// 현지조치요구서
		this.ds_condFile3.setColumn(0, "FILE_SN", this.DS_AMD.getColumn(this.DS_AMD.rowposition, "ATCH_FILE"));
		this.fn_transaction("selectFileList3");
	}
}

