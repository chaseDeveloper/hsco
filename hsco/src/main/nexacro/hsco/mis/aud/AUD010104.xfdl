/*
 * 함수명  	: fn_getEnable
 * 설명 		: 수정가능여부를 판별하는 함수
 * params  		: 행번호
 * return Type 	: 수정가능여부
 * 사용법		: this.fn_getEnable(nRow)
 */
this.fn_getEnable = function(nRow)
{
	var pRow = this.DS_INEX_AUDIT.rowposition;
	var auditEdAt = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_ED_AT");
	var rtn = false;
	
	if(this.div_Cont.AUD010104.tabindex == 0) {	// 감사일지
		if(auditEdAt != '2') {
			rtn = true;
		}
	}
	else if(this.div_Cont.AUD010104.tabindex == 1) {	// 감사자료
		if(auditEdAt != '2') {
			rtn = true;
		}
	}
	else if(this.div_Cont.AUD010104.tabindex == 2) {	// 확인서
		
	}
	else if(this.div_Cont.AUD010104.tabindex == 3) {	// 현지조치요구서
		var confmAt = this.DS_AMD.getColumn(nRow, "CONFM_AT");
		if(confmAt != '1') {
			rtn = true;
		}
		else {
			rtn = false;
		}
	}
	
	if(auditEdAt == '2') {
		rtn = false;
	}
	
	return rtn;
}

// 행추가 버튼 클릭시
this.fn_btnAdd = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow = this.DS_INEX_AUDIT.rowposition;
	var auditEdAt = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_ED_AT");
	
	if(pRow < 0) {
		this.gfn_message("comm.항목.선택.요청","내외부감사목록");
		return false;
	}
	if(auditEdAt == '2') {
		this.gfn_message("comm.종결","내외부감사");
		return false;
	}
	
	var auditPlanNo   = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_PLAN_NO");
	var auditPlanYear = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_PLAN_YEAR");
	var auditDe       = this.gfn_today();
	
	
	if(this.div_Cont.AUD010104.tabindex == 0) {			// 감사일지
		var cRow = this.DS_AUDIT_DIARY.addRow();
		this.DS_AUDIT_DIARY.setColumn(cRow, "AUDIT_NO"  , auditPlanNo);
		this.DS_AUDIT_DIARY.setColumn(cRow, "AUDIT_YEAR", auditPlanYear);
		this.DS_AUDIT_DIARY.setColumn(cRow, "AUDIT_DE"  , auditDe);
	}
	else if(this.div_Cont.AUD010104.tabindex == 1) {	// 감사자료
		var cRow = this.DS_AUDIT_DTA.addRow();
		this.DS_AUDIT_DTA.setColumn(cRow, "AUDIT_NO"  , auditPlanNo);
		this.DS_AUDIT_DTA.setColumn(cRow, "AUDIT_YEAR", auditPlanYear);
		this.DS_AUDIT_DTA.setColumn(cRow, "REGIST_DT" , auditDe);
	}
	else if(this.div_Cont.AUD010104.tabindex == 3) {	// 현지조치요구서
		var cRow = this.DS_AMD.addRow();
		this.DS_AMD.setColumn(cRow, "AUDIT_NO"   , auditPlanNo);
		this.DS_AMD.setColumn(cRow, "AUDIT_YEAR" , auditPlanYear);
		this.DS_AMD.setColumn(cRow, "OPERTN_YEAR", auditPlanYear);
		this.DS_AMD.setColumn(cRow, "ENABLE_AT"  , '1');
		this.DS_AMD.setColumn(cRow, "READONLY"   , '0');
		this.DS_AMD.setColumn(cRow, "CSS_CLASS"  , 'txt_WF_Essential');
	}
}

// 행삭제 버튼 클릭시
this.fn_btnDelete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow = this.DS_INEX_AUDIT.rowposition;
	var auditEdAt = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_ED_AT");
	
	if(pRow < 0) {
		this.gfn_message("comm.항목.선택.요청","내외부감사목록");
		return false;
	}
	if(auditEdAt == '2') {
		this.gfn_message("comm.종결","내외부감사");
		return false;
	}
	
	if(this.div_Cont.AUD010104.tabindex == 0) {			// 감사일지		
		if(this.gfn_message("confirm.삭제여부")){
			var cRow = this.DS_AUDIT_DIARY.rowposition;
			this.DS_AUDIT_DIARY.deleteRow(cRow);		
		}
	}
	else if(this.div_Cont.AUD010104.tabindex == 1) {	// 감사자료
		if(this.gfn_message("confirm.삭제여부")){
			var cRow = this.DS_AUDIT_DTA.rowposition;
			this.DS_AUDIT_DTA.deleteRow(cRow);		
		}
	}
	else if(this.div_Cont.AUD010104.tabindex == 3) {	// 현지조치요구서
		if(this.gfn_message("confirm.삭제여부")){
			var cRow = this.DS_AMD.rowposition;
			this.DS_AMD.deleteRow(cRow);		
		}
	}
}

// 행취소 버튼 클릭시
this.fn_btnCancle = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.div_Cont.AUD010104.tabindex == 0) {			// 감사일지
		if(Ex.util.isUpdated(this.DS_AUDIT_DIARY)) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
		}
		this.DS_AUDIT_DIARY.reset();		//DATASET ROLLBACK
		this.DS_AUDIT_DIARY.applyChange();	//DATASET COMMIT
	}
	else if(this.div_Cont.AUD010104.tabindex == 1) {	// 감사자료
		if(Ex.util.isUpdated(this.DS_AUDIT_DTA)) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
		}
		this.DS_AUDIT_DTA.reset();		//DATASET ROLLBACK
		this.DS_AUDIT_DTA.applyChange();	//DATASET COMMIT
	}
	else if(this.div_Cont.AUD010104.tabindex == 3) {	// 현지조치요구서
		if(Ex.util.isUpdated(this.DS_AMD)) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
		}
		this.DS_AMD.reset();		//DATASET ROLLBACK
		this.DS_AMD.applyChange();	//DATASET COMMIT
	}
}

/**********************************************************************
* 현지조치요구서 소관부서 버튼 클릭시
***********************************************************************/
this.div_Cont_AUD010104_T04_grd_ACPLCMANAGT_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(e.col == 6) {	// 소관부서
		var parm = {
			 arg_0      : this
		};
		this.gfn_popup("deptPop", 400, 400, "", parm, "com::deptCode_P01.xfdl", "fn_popupCallBack", true);
	}
}

/**********************************************************************
* 처분요구서 그리드내 부서 버튼 콜백
***********************************************************************/
this.fn_popupCallBack = function(sPopupId, sReturn)
{	
	if(sPopupId == "deptPop") {	// 소관부서
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		
 		if(loadCnt) {
			var deptCode = dsTemp.getColumn(0, "DEPT_CODE");
			var deptNm   = dsTemp.getColumn(0, "DEPT_NM");
 			
 			var nRow = this.DS_AMD.rowposition;
 			this.DS_AMD.setColumn(nRow, "JRSD_DEPT"   , deptCode);
 			this.DS_AMD.setColumn(nRow, "JRSD_DEPT_NM", deptNm);
 		}
	}
}

this.DS_AUDIT_DIARY_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "AUDIT_DEPT_CODE") {
		var fRow = this.ds_targetDept.findRow("DEPT_CODE", e.newvalue);
		var deptNm = this.ds_targetDept.getColumn(fRow, "DEPT_NM");
		obj.setColumn(e.row, "AUDIT_DEPT_NM", deptNm);
	}
}
