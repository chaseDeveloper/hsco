/**********************************************************************
* call Transaction 영역
***********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectInExAdList":			// 내외부감사조회
			var sController		= "hsco/mis/aud/AUD010104/selectInExAdList.do";
			var sInDatasets		= "input01=ds_cond";
			var sOutDatasets	= "DS_INEX_AUDIT=output01";
			break;
		case "selectAuditProgrsManage":		// 조회(탭)
			var sController		= "hsco/mis/aud/AUD010104/selectAuditProgrsManage.do";
			var sInDatasets		= "input1=DS_SEARCH2 input2=ds_condFile2";
			var sOutDatasets	= "DS_AUDIT_DIARY=output1 DS_AUDIT_DTA=output2 ds_fileList2=output3 DS_AMD=output4 ds_targetDept=output5 ds_composi=output6";
			break;
		case "selectFileList1":				// 첨부파일 조회(감사자료)
			this.ds_fileList1.clearData();
			var sController		= "hsco/mis/aud/AUD010104/selectFileList1.do";
			var sInDatasets		= "input1=ds_condFile1";
			var sOutDatasets	= "ds_fileList1=output1";
			break;
		case "selectFileList2":				// 첨부파일 조회(회신확인서)
			this.ds_fileList2.clearData();
			var sController		= "hsco/mis/aud/AUD010104/selectFileList2.do";
			var sInDatasets		= "input1=ds_condFile2";
			var sOutDatasets	= "ds_fileList2=output1";
			break;
		case "selectFileList3":				// 첨부파일 조회(화면확인서)
			this.ds_fileList3.clearData();
			var sController		= "hsco/mis/aud/AUD010104/selectFileList3.do";
			var sInDatasets		= "input1=ds_condFile3";
			var sOutDatasets	= "ds_fileList3=output1";
			break;
		case "save":						// 저장(감사일지, 감사자료, 현지조치요구서)
			var sController   	= "hsco/mis/aud/AUD010104/save.do";
			var sInDatasets   	= "input1=DS_AUDIT_DIARY:U input2=DS_AUDIT_DTA:U input3=DS_AMD:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
/**********************************************************************
* Transaction Callback 영역
***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectInExAdList":			// 내외부감사조회
				if(this.save_row != -1)
				{
					this.DS_INEX_AUDIT.set_rowposition(this.save_row);
					this.save_row = -1;
				}
				break;
			case "selectAuditProgrsManage" :	// 조회(탭)
				if(this.save_row1 != -1)
				{
					this.DS_AUDIT_DIARY.set_rowposition(this.save_row1);
					this.save_row1 = -1;
				}
				if(this.save_row2 != -1)
				{
					this.DS_AUDIT_DTA.set_rowposition(this.save_row2);
					this.save_row2 = -1;
				}
				if(this.save_row4 != -1)
				{
					this.DS_AMD.set_rowposition(this.save_row4);
					this.save_row4 = -1;
				}
				break;
			case "selectFileList1":      		// 첨부파일 조회
				this.ds_fileList1.filter("USE_AT == '1'");
				break;
			case "selectFileList2":      		// 첨부파일 조회
				this.ds_fileList2.filter("USE_AT == '1'");
				break;
			case "selectFileList3":     		 // 첨부파일 조회
				this.ds_fileList3.filter("USE_AT == '1'");
				break;
			case "save":						// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}	


/**********************************************************************
* T02 감사자료 행 변경시 첨부파일 조회 
***********************************************************************/
this.DS_AUDIT_DTA_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_fileList1.clearData();
	
	if(e.newrow > -1) {	
		var pRow = this.DS_INEX_AUDIT.rowposition;
		var auditEdAt = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_ED_AT");
		
		if(auditEdAt == '2') {
			this.div_Cont.AUD010104.T02.btn_delFiles.set_enable(false);
			this.div_Cont.AUD010104.T02.btn_uploadFiles.set_enable(false);
		}
		else {
			this.div_Cont.AUD010104.T02.btn_delFiles.set_enable(true);
			this.div_Cont.AUD010104.T02.btn_uploadFiles.set_enable(true);
		}
		
		this.ds_condFile1.clearData();
		var nRow = this.ds_condFile1.addRow();
		var fileSn = obj.getColumn(e.newrow, "ATCH_FILE");
		this.ds_condFile1.setColumn(nRow, "FILE_SN", fileSn);
		this.fn_transaction("selectFileList1");
	}
}

/**********************************************************************
* T04 현지조치요구서 행 변경시 첨부파일 조회 
***********************************************************************/
this.DS_AMD_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{	
	this.ds_fileList3.clearData();
	
	if(e.newrow > -1) {	
		var enableAt = obj.getColumn(e.reason, "ENABLE_AT");
		
		if(enableAt == '0') {
			this.div_Cont.AUD010104.T04.btn_delFiles.set_enable(false);
			this.div_Cont.AUD010104.T04.btn_uploadFiles.set_enable(false);
		}
		else {
			this.div_Cont.AUD010104.T04.btn_delFiles.set_enable(true);
			this.div_Cont.AUD010104.T04.btn_uploadFiles.set_enable(true);
		}
		
		this.ds_condFile3.clearData();
		var nRow = this.ds_condFile3.addRow();
		var fileSn = obj.getColumn(e.newrow, "ATCH_FILE");
		this.ds_condFile3.setColumn(nRow, "FILE_SN", fileSn);
		this.fn_transaction("selectFileList3");
	};
}

/***********************************************************************
 * 설명 : 첨부파일 선택 삭제 버튼클릭 시
 ***********************************************************************/
this.btn_delFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = -1;
	
	if(this.div_Cont.AUD010104.tabindex == 1) {			// 감사일지
		findRow = this.ds_fileList1.findRow("CHK", "1");
		if(findRow < 0) {
			this.gfn_message("comm.데이터.선택.없음");
			return;
		}
		
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		
		for(var fRow = this.ds_fileList1.getRowCount()-1; fRow >= 0; fRow--) {
			var chkVal = this.ds_fileList1.getColumn(fRow, "CHK");
			if(chkVal == '1') {
				this.ds_fileList1.deleteRow(fRow);
			}
		}
		
		var sInDatasets  = "input1=ds_fileList1:U";
		var sOutDatasets = "ds_fileList1=output1";
	}
	else if(this.div_Cont.AUD010104.tabindex == 2) {	// 감사자료
		findRow = this.ds_fileList2.findRow("CHK", "1");
		if(findRow < 0) {
			this.gfn_message("comm.데이터.선택.없음");
				return;
		}
		
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		
		for(var fRow = this.ds_fileList2.getRowCount()-1; fRow >= 0; fRow--) {
			var chkVal = this.ds_fileList2.getColumn(fRow, "CHK");
			if(chkVal == '1') {
				this.ds_fileList2.deleteRow(fRow);
			}
		}
		
		var sInDatasets  = "input1=ds_fileList2:U";
		var sOutDatasets = "ds_fileList2=output1";
	}
	else if(this.div_Cont.AUD010104.tabindex == 3) {	// 현지조치요구서
		findRow = this.ds_fileList3.findRow("CHK", "1");
		if(findRow < 0) {
			this.gfn_message("comm.데이터.선택.없음");
				return;
		}
		
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		
		for(var fRow = this.ds_fileList3.getRowCount()-1; fRow >= 0; fRow--) {
			var chkVal = this.ds_fileList3.getColumn(fRow, "CHK");
			if(chkVal == '1') {
				this.ds_fileList3.deleteRow(fRow);
			}
		}
		
		var sInDatasets  = "input1=ds_fileList3:U";
		var sOutDatasets = "ds_fileList3=output1";
	}
	
	var strSvcId   	 = "deleteFile";
	var sController  = "hsco/cmm/file/deleteFile.do";
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/***********************************************************************
 * 설명 : 첨부파일 업로드 버튼클릭 시
 ***********************************************************************/
this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow = this.DS_INEX_AUDIT.rowposition;
	var auditEdAt = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_ED_AT");
	
	if(pRow < 0) {
		this.gfn_message("comm.항목.선택.요청","내외부감사목록");
		return false;
	}
	if(auditEdAt == '2') {
		this.gfn_message("comm.종결","내외부감사");
		return false;
	}
	
	switch(this.div_Cont.AUD010104.tabindex)
	{
		case 1 :	
			// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
			this.fn_autoFileSn(
				 "MIS"									// 대분류   (SYS_SE)
				,"AUD"									// 업무구분 (JOB_SE)
				, this.File_UpDownManager	            // fileManager 오브젝트
				, this.ds_fileList1		                // fileObject
				, this.DS_AUDIT_DTA	                	// fileSn을 불러오거나 반영해야 하는 컬럼
				, "TBAUD_AUDIT_DTA"		                // fileSn을 저장할 테이블
				, "ATCH_FILE"							// fileSn 컬럼명이 다를 경우를 대비
				, ["DTA_SN", "AUDIT_NO","AUDIT_YEAR"]	// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
			);
		break;
			
		case 2 :	//회신확인서
			// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
			this.fn_autoFileSn(
				 "MIS"										    // 대분류   (SYS_SE)
				,"AUD"										    // 업무구분 (JOB_SE)
				, this.File_UpDownManager	         	        // fileManager 오브젝트
				, this.ds_fileList2		                        // fileObject
				, this.DS_INEX_AUDIT                   			// fileSn을 불러오거나 반영해야 하는 컬럼
				, "TBAUD_AUDIT_PLAN"		            		// fileSn을 저장할 테이블
				, "ATCH_FILE2"							        // fileSn 컬럼명이 다를 경우를 대비
				, ["AUDIT_PLAN_NO","AUDIT_PLAN_YEAR"]			// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
			);
		break;
		
		case 3 :	//화면확인서
			// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
			this.fn_autoFileSn(
				 "MIS"										    // 대분류   (SYS_SE)
				,"AUD"										    // 업무구분 (JOB_SE)
				, this.File_UpDownManager	         	        // fileManager 오브젝트
				, this.ds_fileList3		                        // fileObject
				, this.DS_AMD	                    			// fileSn을 불러오거나 반영해야 하는 컬럼
				, "TBAUD_ACPLC_MANAGT_DMDDOC"		            // fileSn을 저장할 테이블
				, "ATCH_FILE"							        // fileSn 컬럼명이 다를 경우를 대비
				, ["ACPLC_MANAGT_SN", "AUDIT_NO","AUDIT_YEAR"]	// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
			);
		break;
	}
}

/***********************************************************************
 * 설명 : 첨부파일 선택다운로드 버튼클릭 시
 ***********************************************************************/
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = -1;
	
	if(this.div_Cont.AUD010104.tabindex == 1) {			// 감사일지
		findRow = this.ds_fileList1.findRow("CHK", "1");
		if(findRow < 0) {
			this.gfn_message("comm.데이터.선택.없음");
			return;
		}
		
		for(var fRow = 0; fRow < this.ds_fileList1.getRowCount(); fRow++) {
			var fileSn    = this.ds_fileList1.getColumn(fRow, "FILE_SN");
			var fileOrder = this.ds_fileList1.getColumn(fRow, "FILE_ORDR");
			var fileName  = this.ds_fileList1.getColumn(fRow, "ORGINL_FILE_NM");
			var chk       = this.ds_fileList1.getColumn(fRow, "CHK");
			if(chk == '1') {
				this.File_UpDownManager.download(fileSn, fileOrder, fileName);
			}
		}
	}
	else if(this.div_Cont.AUD010104.tabindex == 2) {	// 감사자료
		findRow = this.ds_fileList2.findRow("CHK", "1");
		if(findRow < 0) {
			this.gfn_message("comm.데이터.선택.없음");
			return;
		}
		
		for(var fRow = 0; fRow < this.ds_fileList2.getRowCount(); fRow++) {
			var fileSn    = this.ds_fileList2.getColumn(fRow, "FILE_SN");
			var fileOrder = this.ds_fileList2.getColumn(fRow, "FILE_ORDR");
			var fileName  = this.ds_fileList2.getColumn(fRow, "ORGINL_FILE_NM");
			var chk       = this.ds_fileList2.getColumn(fRow, "CHK");
			if(chk == '1') {
				this.File_UpDownManager.download(fileSn, fileOrder, fileName);
			}
		}
	}
	else if(this.div_Cont.AUD010104.tabindex == 3) {	// 현지조치요구서
		findRow = this.ds_fileList3.findRow("CHK", "1");
		if(findRow < 0) {
			this.gfn_message("comm.데이터.선택.없음");
			return;
		}
		
		for(var fRow = 0; fRow < this.ds_fileList3.getRowCount(); fRow++) {
			var fileSn    = this.ds_fileList3.getColumn(fRow, "FILE_SN");
