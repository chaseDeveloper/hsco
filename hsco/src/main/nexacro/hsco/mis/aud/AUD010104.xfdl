    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 감사관리
 * 02. 화면명   : AUD010104.xfdl
 * 03. 화면설명 : 내외부감사관리, 감사일지관리, 감사자료관리, 감사확인서관리, 
				   관련자조서, 현지조치요구서관리 테이블을 관리한다.
 * 04. 작성일   : 2015.06.17
 * 05. 작성자   : 이지영
 * 06. 수정이력 : 2015.06.17 최초생성
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */
 /***********************************************************************
 * Script Include 
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.File_UpDownManager = null;
this.chargerAt = false;
this.save_row  = -1;	// 내외부감사목록
this.save_row1 = -1;	// 감사일지
this.save_row2 = -1;	// 감사자료
this.save_row4 = -1;	// 현지조치요구서

/***********************************************************************
 * 설명 : onInit 영역
 ***********************************************************************/
this.AUD010104_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}

/***********************************************************************
 * 설명: 폼로딩시 처리
 ************************************************************************/
this.AUD010104_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);	
	
	this.ds_cond.clearData();   // 조회 데이터셋 초기화	
	this.ds_cond.addRow();      // 조회 데이터셋 행추가
	this.ds_cond.setColumn(0, "AUDIT_BGNYEAR", this.gfn_today().substring(0,4) - 5);
	this.ds_cond.setColumn(0, "AUDIT_ENDYEAR", this.gfn_today().substring(0,4));
	
	// 공통코드
	var comboParams = [
	    ["DS_AUD_INSTT"     , "AUD002", "Y", "A", "B"]	// 감사기관
	   ,["DS_AUD_KND"       , "AUD003", "Y", "A", "B"]	// 감사종류
	   ,["DS_AUD_CL"        , "AUD004", "Y", "A", "B"]	// 감사분류
	   ,["DS_AUDIT_ED_AT"   , "AUD018", "Y", "D", "B"]	// 종결여부
	   ,["DS_DSPS_SE"       , "AUD030", "Y", "D", "B"]	// 처분종류
	   ,["DS_AUDIT_DTA_SE"  , "AUD031", "Y", "D", "B"]	// 감사자료구분
	   ,["DS_AUD037"        , "AUD037", "Y", "A", "B"] 	// 감사구분(조회조건)
	   ,["DS_AUD037G"       , "AUD037", "Y", "D", "B"] 	// 감사구분
	   ,["ds_sacnctnSttusSe", "COM032", "Y", "D", "B"]	// 결재상태(전자결재상태)
	];
	var callback = function (svcId, errorCode, errorMsg) {		
		// 수정가능여부를 판별하기위해 권한그룹 체크
		if((this.gfn_authGrpId("AUD_ADMIN") != -1)) {			// 감사결재자
			this.chargerAt = true;
		}
		
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	  var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "AUD");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
 * 설명: 조회버튼 클릭시 처리
 ************************************************************************/
this.fn_search= function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	// 데이터셋 초기화
	this.DS_INEX_AUDIT.clearData();
	this.DS_AUDIT_DIARY.clearData();
	this.DS_AUDIT_DTA.clearData();
	this.DS_AMD.clearData();
	this.ds_fileList1.clearData();
	this.ds_fileList2.clearData();
	this.ds_fileList3.clearData();
	this.fn_transaction("selectInExAdList");
}

/***********************************************************************
 * 설명: 상단 행취소 버튼
 ************************************************************************/
this.fn_cancel= function()
{
	if(   Ex.util.isUpdated(this.DS_AUDIT_DIARY)			// 감사일지
	   || Ex.util.isUpdated(this.DS_AUDIT_DTA)				// 감사자료
	   || Ex.util.isUpdated(this.DS_AMD)) {					// 현지조치요구서
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
		
		this.DS_AUDIT_DIARY.reset();		//DATASET ROLLBACK
		this.DS_AUDIT_DIARY.applyChange();	//DATASET COMMIT
		this.DS_AUDIT_DTA.reset();			//DATASET ROLLBACK
		this.DS_AUDIT_DTA.applyChange();	//DATASET COMMIT
		this.DS_AMD.reset();				//DATASET ROLLBACK
		this.DS_AMD.applyChange();			//DATASET COMMIT
	}
}

/***********************************************************************
 * 설명: 상단 저장 버튼
 ************************************************************************/
this.fn_save= function()
{
	// update check
	if (   !this.comUtils.isDatasetUpdated(this.DS_AUDIT_DIARY)		// 탭1 : 감사일지
	    && !this.comUtils.isDatasetUpdated(this.DS_AUDIT_DTA)		// 탭2 : 감사자료
	    && !this.comUtils.isDatasetUpdated(this.DS_AMD)				// 탭4 : 현지조치요구서
	   ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(감사일지)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AUDIT_DIARY, this.ds_validation)) {
		this.div_Cont.AUD010104.set_tabindex(0);
		return false;
	}
	// validation 체크(감사자료)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AUDIT_DTA, this.ds_validation)) {
		this.div_Cont.AUD010104.set_tabindex(1);
		return false;
	}
	// validation 체크(현지조치요구서)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AMD, this.ds_validation)) {
		this.div_Cont.AUD010104.set_tabindex(3);
		return false;
	}
	
	// save
	if(this.gfn_message("confirm.저장.여부"))	{
		this.save_row  = this.DS_INEX_AUDIT.rowposition;
		this.save_row1 = this.DS_AUDIT_DIARY.rowposition;
		this.save_row2 = this.DS_AUDIT_DTA.rowposition;
		this.save_row4 = this.DS_AMD.rowposition;
		this.fn_transaction("save");
	}
}

/***********************************************************************
 * 설명: 내외부감사목록 행변경 전
 ************************************************************************/
 this.DS_INEX_AUDIT_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(   Ex.util.isUpdated(this.DS_AUDIT_DIARY)	// 감사일지
	   || Ex.util.isUpdated(this.DS_AUDIT_DTA)		// 감사자료
	   || Ex.util.isUpdated(this.DS_AMD)) {			// 현지조치요구서
		
		if(!this.gfn_message("confirm.변경.취소.여부")){  	
			return false; //유지
		}
		this.DS_AUDIT_DIARY.reset();		//DATASET ROLLBACK
		this.DS_AUDIT_DIARY.applyChange();	//DATASET COMMIT
		this.DS_AUDIT_DTA.reset();			//DATASET ROLLBACK
		this.DS_AUDIT_DTA.applyChange();	//DATASET COMMIT
		this.DS_AMD.reset();				//DATASET ROLLBACK
		this.DS_AMD.applyChange();			//DATASET COMMIT
	}
}
/***********************************************************************
 * 설명: 내외부감사목록 행변경 후
 ************************************************************************/
this.DS_INEX_AUDIT_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.DS_AUDIT_DIARY.clearData();
	this.DS_AUDIT_DTA.clearData();
	this.DS_AMD.clearData();
	this.ds_composi.clearData();
	this.ds_targetDept.clearData();
	this.ds_fileList1.clearData();
	this.ds_fileList2.clearData();
	this.ds_fileList3.clearData();
	
	if(e.newrow > -1) {
		var auditEdAt = obj.getColumn(e.newrow, "AUDIT_ED_AT");
		
		if(auditEdAt == '2') {
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST.set_readonly(true);
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST00.set_readonly(true);
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST01.set_readonly(true);
			
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST.set_cssclass("txt_WF_Readonly");
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST00.set_cssclass("txt_WF_Readonly");
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST01.set_cssclass("txt_WF_Readonly");
			
			this.div_Cont.AUD010104.T03.btn_delFiles.set_enable(false);
			this.div_Cont.AUD010104.T03.btn_uploadFiles.set_enable(false);
		}
		else {
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST.set_readonly(false);
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST00.set_readonly(false);
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST01.set_readonly(false);
			
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST.set_cssclass("txt_WF_Essential");
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST00.set_cssclass("");
			this.div_Cont.AUD010104.T01.txt_PRGN_LIST01.set_cssclass("");
			
			this.div_Cont.AUD010104.T03.btn_delFiles.set_enable(true);
			this.div_Cont.AUD010104.T03.btn_uploadFiles.set_enable(true);
		}
		
		this.DS_SEARCH2.clearData();
		var nRow = this.DS_SEARCH2.addRow();
		this.ds_condFile2.clearData();
		var fRow = this.ds_condFile2.addRow();
		var auditPlanNo   = obj.getColumn(e.newrow, "AUDIT_PLAN_NO");
		var auditPlanYear = obj.getColumn(e.newrow, "AUDIT_PLAN_YEAR");
		var fileSn        = obj.getColumn(e.newrow, "ATCH_FILE2");
		this.DS_SEARCH2.setColumn(nRow, "AUDIT_NO"  , auditPlanNo);
		this.DS_SEARCH2.setColumn(nRow, "AUDIT_YEAR", auditPlanYear);
		this.ds_condFile2.setColumn(fRow, "FILE_SN", fileSn);
		
		this.fn_transaction("selectAuditProgrsManage");
	}
}


