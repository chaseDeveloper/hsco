/***********************************************************************************
* 설명	: 점수 입력 시 
***********************************************************************************/
this.DS_INTGTY_MLG_REGIST_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
 	if(e.columnid=="DEPT_CODE"){
 		var deptCode = obj.getColumn(e.row, "DEPT_CODE");
 		
 		if(!this.comUtils.isNull(deptCode)) {
			obj.setColumn(0, "MLG_SCORE"    , "");
			obj.setColumn(0, "UPLMT_SCORE"  , "");
			obj.setColumn(0, "CSBY_ALLOT"   , "");
			obj.setColumn(0, "MLG_SCORE_TOT", "");
			obj.setColumn(0, "MLG_SE", "");
			obj.setColumn(0, "MLG_KND", "");
			obj.setColumn(0, "MLG_DETAIL", "");
 		}
 	}
			
	if(e.columnid == "MLG_SCORE"){	
		// 성명 클릭 후 셋팅되는 점수에 대해서 동분야 총점, 배점, 상한점수와 비교해 적절한 점수인지 판단
		var empRow = e.row;
		var s_mlgScoreTot = obj.getColumn(empRow, "MLG_SCORE_TOT");
		var s_mlgScore    = e.newvalue;
		var s_mxmmValue   = obj.getColumn(empRow, "MXMM_VALUE");
		var s_val_tot     = nexacro.toNumber(s_mlgScore) + nexacro.toNumber(s_mlgScoreTot);
		var s_csbyAllot   = obj.getColumn(empRow, "CSBY_ALLOT");
		
		if(!this.validationChk(s_mlgScore, s_mxmmValue, s_mlgScoreTot, s_val_tot, s_csbyAllot)) {
			obj.setColumn(empRow, "MLG_SCORE", 0);
			return;
		}
 	}
 
	return true;
}


