/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		//상세조회
		case "MlgRegistDeptListR":
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistDeptListR.do";
			var sInDatasets   	= "input06=DS_SEARCH2";
			var sOutDatasets  	= "DS_INTGTY_MLG_REGIST=output06";
		break;
		// 동분야 총점
		case "selectMlgScoreTot":
			var sController   	= "hsco/mis/aud/AUD040102/selectMlgScoreTot.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_mlgScoreTot=output1";
		break;
		//저장버튼 클릭시
		case "mlgRegistDeptCU" :
			//적용일시로 적용년도 구하기
			var applc_year = this.div_Cont.cal_APPLC_DE.value.toDateString().substr(-4);
			this.DS_INTGTY_MLG_REGIST.setColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "APPLC_YEAR" , applc_year);
			//부서별, 개인별 구분 부서별(2)셋팅
			this.DS_INTGTY_MLG_REGIST.setColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "DEPT_INDVDL_SE" , deptIndvdlSe);
			
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistDeptCU.do";
			var sInDatasets   	= "input05=DS_INTGTY_MLG_REGIST:U";
			var sOutDatasets  	= "";
			var sArgs = "";	
		break;
		
		//신규버튼 클릭시 이전정보 저장
		case "mlgRegistDeptC2":
			//적용일시로 적용년도 구하기
			var applc_year = this.div_Cont.cal_APPLC_DE.value.toDateString().substr(-4);
			this.DS_INTGTY_MLG_REGIST.setColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "APPLC_YEAR" , applc_year);
			//부서별, 개인별 구분 부서별(2)셋팅
			this.DS_INTGTY_MLG_REGIST.setColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "DEPT_INDVDL_SE" , deptIndvdlSe);
			
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistDeptCU.do";
			var sInDatasets   	= "input05=DS_INTGTY_MLG_REGIST:U";
			var sOutDatasets  	= "";
			var sArgs = "";	
		break;
		
		//청렴마일리지 목록 삭제
		case "mlgRegistDeptD" :
			this.ds_deleteInfo.clearData();
			this.ds_deleteInfo.addRow();
			this.ds_deleteInfo.setColumn(0, "APPLC_YEAR", this.DS_INTGTY_MLG_REGIST.getColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "APPLC_YEAR"));
			this.ds_deleteInfo.setColumn(0, "MLG_SN",     this.DS_INTGTY_MLG_REGIST.getColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "MLG_SN"));
			this.ds_deleteInfo.setColumn(0, "MLG_CODE",   this.DS_INTGTY_MLG_REGIST.getColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "MLG_CODE"));
			this.ds_deleteInfo.setColumn(0, "MLG_SE",     this.DS_INTGTY_MLG_REGIST.getColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "MLG_SE"));	
			
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistDeptD.do";
			var sInDatasets   	= "input07=ds_deleteInfo";
			var sOutDatasets  	= "";
			var sArgs = "";	
		break;	
		
	//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_INTGTY_MLG_REGIST.getColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
		//청렴 마일리지 부서 개인별 점수 합계
		case "getScoreIndvdl":
			this.DS_SEARCH4.clearData();
			var applc_year = this.div_Cont.cal_APPLC_DE.value.toDateString().substr(-4);
			var nRow = this.DS_SEARCH4.addRow();
			this.DS_SEARCH4.setColumn(nRow, "APPLC_YEAR", applc_year);
			this.DS_SEARCH4.setColumn(nRow, "MLG_CODE", this.DS_INTGTY_MLG_REGIST.getColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "MLG_CODE"));
			this.DS_SEARCH4.setColumn(nRow, "DEPT_CODE", this.DS_INTGTY_MLG_REGIST.getColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "DEPT_CODE"));
			
			var sController   	= "hsco/mis/aud/AUD040102/getScoreIndvdl.do";
			var sInDatasets   	= "input08=DS_SEARCH4";
			var sOutDatasets  	= "DS_SUM_SCORE=output08";
			var sArgs = "";	
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			case "selectMlgScoreTot":
				
				var mlgScoreTot = this.ds_mlgScoreTot.getColumn(0, "MLG_SCORE_TOT");
				this.DS_INTGTY_MLG_REGIST.setColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "MLG_SCORE_TOT", mlgScoreTot);
			break;		
			case "mlgRegistDeptD":
				this.close();
			break;
			case "mlgRegistDeptCU":
				this.close();
			break;
			
			case "mlgRegistDeptC2":
				this.DS_INTGTY_MLG_REGIST.clearData();
				nRow = this.DS_INTGTY_MLG_REGIST.addRow();
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "APPLC_DE" , this.gfn_today());
			break;
			//상세조회시 첨부파일 조회
			case "MlgRegistDeptListR":
				this.fn_transaction("selectAtchFile");
			break;
			
			//청렴 마일리지 부서 개인별 점수 합계
			case "getScoreIndvdl":
				this.DS_INTGTY_MLG_REGIST.setColumn(this.DS_INTGTY_MLG_REGIST.rowposition, "MLG_SCORE", this.DS_SUM_SCORE.getColumn(0, "SCORE"));
			break;
		}
	}
}

