***********************************************************************************/
this.div_SearchArea_btn_Save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.DS_EMPNO.getRowCount() < 1){
		this.gfn_message("comm.항목.필수","등록대상");
		return false;
	}
	
	//적용일시로 적용년도 구하기

	var applc_year = this.div_Cont.cal_APPLC_DE.value.toDateString().substr(-4);

	save_at = "Y"; //제외 대상 여부 체크
	for(var i = 0 ; i <= this.DS_MLG_EXCL_TRGET.rowcount ; i++){
		for(var j = 0; j < this.DS_EMPNO.rowcount; j++){
			if(applc_year == this.DS_MLG_EXCL_TRGET.getColumn(i, "APPLC_YEAR") 
				&& this.DS_EMPNO.getColumn(j, "EMPNO") == this.DS_MLG_EXCL_TRGET.getColumn(i, "EMPNO"))	
			{
				save_at = "N";
				this.gfn_message("comm.제외대상.기등록");
				return false;
			}
			
		}
	}
	
	//validation check		
	if(!this.gfn_checkValidation(this.DS_EMPNO, this.ds_validation)){
		return;
	}
	
	for(var i = 0; i < this.DS_EMPNO.rowcount; i++) {
			
		var s_mlgScoreTot = this.DS_EMPNO.getColumn(i, "MLG_SCORE_TOT");
		var s_mlgScore    = this.DS_EMPNO.getColumn(i, "MLG_SCORE");
		var s_mxmmValue   = this.DS_EMPNO.getColumn(i, "MXMM_VALUE");
		var s_val_tot     = nexacro.toNumber(s_mlgScore) + nexacro.toNumber(s_mlgScoreTot);
		var s_csbyAllot   = this.DS_EMPNO.getColumn(i, "CSBY_ALLOT");
		if(!this.validationChk(s_mlgScore, s_mxmmValue, s_mlgScoreTot, s_val_tot, s_csbyAllot, i)) {
			return false;
		}
	}
	
	
	if(this.gfn_message("confirm.저장.여부")) {
		this.fn_transaction("mlgRegistListCU");
	}

}
