this.fn_callback6 = function(sPopupId, sReturn)
{
	if(sPopupId == "Popup44")
	{
		this.ds_rtn_2.clearData();
        nRowCnt = this.ds_rtn_2.loadXML(sReturn);
        if(!nRowCnt){
            this.ds_rtn_2.addRow();
        } else {
			var cRow = this.DS_EMPNO.rowposition;
			this.DS_EMPNO.setColumn(cRow, "MLG_SE",      this.ds_rtn_2.getColumn(0, "MLG_SE"));
			this.DS_EMPNO.setColumn(cRow, "MLG_KND",     this.ds_rtn_2.getColumn(0, "MLG_KND"));
			this.DS_EMPNO.setColumn(cRow, "MLG_DETAIL",  this.ds_rtn_2.getColumn(0, "MLG_DETAIL"));
			this.DS_EMPNO.setColumn(cRow, "MLG_CODE",    this.ds_rtn_2.getColumn(0, "MLG_CODE"));
			this.DS_EMPNO.setColumn(cRow, "MLG_UPPER_CODE",    this.ds_rtn_2.getColumn(0, "MLG_UPPER_CODE"));
			this.DS_EMPNO.setColumn(cRow, "MXMM_VALUE", this.ds_rtn_2.getColumn(0, "MXMM_VALUE"));
			// 컬럼이 있으니 저장은 하지만 보여지진 않음
			this.DS_EMPNO.setColumn(cRow, "UPLMT_SCORE", this.ds_rtn_2.getColumn(0, "MXMM_VALUE"));
			this.DS_EMPNO.setColumn(cRow, "CSBY_ALLOT",  this.ds_rtn_2.getColumn(0, "CSBY_ALLOT"));
			this.DS_EMPNO.setColumn(cRow, "MXMM_UPPER_VALUE",  this.ds_rtn_2.getColumn(0, "MXMM_UPPER_VALUE"));
			this.DS_EMPNO.setColumn(cRow, "DDC_MXMM_UPPER_VALUE_AT",  this.ds_rtn_2.getColumn(0, "DDC_MXMM_UPPER_VALUE_AT"));
			
			this.div_Cont.div_sear.search.set_visible(true);
			this.div_Cont.btn_addEmp.set_text("사원추가");
			this.div_Cont.btn_addEmp.set_visible(true);
        }
	}else if(sPopupId == "IntgtyScorePopup"){
			var dsTemp = new Dataset();
			var loadCnt = dsTemp.loadXML(sReturn);
			if(loadCnt) {
				var nRow = this.DS_EMPNO.rowposition;
				for(var i=0; i < dsTemp.rowcount; i++){
					//같은데이터 확인
					var fRow = this.DS_EMPNO.findRowExpr("EMPNO =='"+dsTemp.getColumn(i, "EMPNO")+
													"'&&MLG_SCORE =='"+dsTemp.getColumn(i, "CRTFC_TIME")+
													"'&&RM == '"+dsTemp.getColumn(i, "EDC_NM")+"'");
					for(var j = 0 ; j <= this.DS_MLG_EXCL_TRGET.rowcount ; j++)
					{
						var applc_year = this.div_Cont.cal_APPLC_DE.value.toDateString().substr(-4);
						if(applc_year == this.DS_MLG_EXCL_TRGET.getColumn(j, "APPLC_YEAR") 
							&& dsTemp.getColumn(i, "EMPNO") == this.DS_MLG_EXCL_TRGET.getColumn(j, "EMPNO"))
						{
							//제외대상이 있으면 건너뛰고 추가함
							fRow = 0;
						}
					}
					
					if(fRow>-1){
						//같은내용이 이미 있거나 제외대상이면 건너뛰고 추가함
						continue;
					}
					var empRow = this.DS_EMPNO.addRow();
					this.DS_EMPNO.setColumn(empRow, "EMPNO"	    , dsTemp.getColumn(i, "EMPNO"));
					this.DS_EMPNO.setColumn(empRow, "EMPNM"	    , dsTemp.getColumn(i, "EMPNM"));
					this.DS_EMPNO.setColumn(empRow, "DEPT_CODE" , dsTemp.getColumn(i, "DEPT_CODE"));
					this.DS_EMPNO.setColumn(empRow, "DEPT_NM"   , dsTemp.getColumn(i, "DEPT_NM"));
					this.DS_EMPNO.setColumn(empRow, "MLG_SCORE" , dsTemp.getColumn(i, "CRTFC_TIME"));
					this.DS_EMPNO.setColumn(empRow, "RM"        , dsTemp.getColumn(i, "EDC_NM"));
					this.DS_EMPNO.setColumn(empRow, "JSSFC_NM"  , dsTemp.getColumn(j, "JSSFC_NM"));
					this.DS_EMPNO.setColumn(empRow, "HR_CLSF_NM", dsTemp.getColumn(j, "HR_CLSF_NM"));
					this.DS_EMPNO.setColumn(empRow, "OFCPS_NM"  , dsTemp.getColumn(j, "OFCPS_NM"));
					this.DS_EMPNO.setColumn(empRow, "JBLN_NM"	, dsTemp.getColumn(j, "JBLN_NM"));
				
				}
				this.DS_INTGTY_MLG_CURROW.clearData();
				this.DS_INTGTY_MLG_CURROW.addRow();
				this.DS_INTGTY_MLG_CURROW.copyRow(0, this.DS_EMPNO, this.DS_EMPNO.rowcount-1);
			}
	}
}
