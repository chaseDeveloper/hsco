			this.DS_SEARCH2.setColumn(nRow, "MLG_SN",     this.parent.val_mlg_seq_no);
			this.DS_SEARCH2.setColumn(nRow, "APPLC_YEAR", this.parent.val_applc_year);
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistListR.do";
			var sInDatasets   	= "input03=DS_SEARCH2";
			var sOutDatasets  	= "DS_EMPNO=output03";
		break;
		//제외대상목록
		case "selectExclTrgetList":
			var sController   	= "hsco/mis/aud/AUD040102_P02/selectExclTrgetList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "DS_MLG_EXCL_TRGET=output03";
		break;				
// 		// 저장 가능한 사원 리스트
// 		case "selectRegistEmpList":
// 			var sController   	= "hsco/mis/aud/AUD040102/selectMlgScore.do";
// 			var sInDatasets   	= "input1=ds_cond2";
// 			var sOutDatasets  	= "ds_registEmpList=output1";
// 		break;
// 		
		// 동분야 총점
		case "selectMlgScoreTot":
			this.ds_mlgScoreTot.clearData();
			var sController   	= "hsco/mis/aud/AUD040102/selectMlgScoreTot.do";
			var sInDatasets   	= "input1=ds_cond2";
			var sOutDatasets  	= "ds_mlgScoreTot=output1";
		break;
		//저장버튼 클릭시
		case "mlgRegistListCU" :
			//alert(this.DS_EMPNO.getColumn(this.DS_EMPNO.rowposition,"MXMM_VALUE"));
			var applc_year = this.div_Cont.cal_APPLC_DE.value.toDateString().substr(-4);
			this.DS_EMPNO.setColumn(this.DS_EMPNO.rowposition, "APPLC_YEAR" , applc_year);
			//부서별, 개인별 구분 개인별(1)셋팅
			this.DS_EMPNO.setColumn(this.DS_EMPNO.rowposition, "DEPT_INDVDL_SE" , "1");
			//alert(this.DS_EMPNO.getRowType(0));
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistListCU.do";
			var sInDatasets   	= "input02=DS_EMPNO:U";
			var sOutDatasets  	= "";
		break;		
		
		case "mlgRegistListD":
			//삭제 트랜잭션
			this.ds_deleteInfo.clearData();
			this.ds_deleteInfo.addRow();
			this.ds_deleteInfo.setColumn(0, "APPLC_YEAR", this.DS_EMPNO.getColumn(this.DS_EMPNO.rowposition, "APPLC_YEAR"));
			this.ds_deleteInfo.setColumn(0, "MLG_SN",     this.DS_EMPNO.getColumn(this.DS_EMPNO.rowposition, "MLG_SN"));
			this.ds_deleteInfo.setColumn(0, "MLG_CODE",   this.DS_EMPNO.getColumn(this.DS_EMPNO.rowposition, "MLG_CODE"));
			this.ds_deleteInfo.setColumn(0, "MLG_SE",     this.DS_EMPNO.getColumn(this.DS_EMPNO.rowposition, "MLG_SE"));	
			
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistListD.do";
			var sInDatasets   	= "input08=ds_deleteInfo";
			var sOutDatasets  	= "";
			var sArgs = "";	
		break;
		
		case "mlgRegistListC2":
			//신규버튼클릭시 저장트랜잭션
			var applc_year = this.div_Cont.cal_APPLC_DE.value.toDateString().substr(-4);
			this.DS_EMPNO.setColumn(this.DS_EMPNO.rowposition, "APPLC_YEAR" , applc_year);
			//부서별, 개인별 구분 개인별(1)셋팅
			this.DS_EMPNO.setColumn(this.DS_EMPNO.rowposition, "DEPT_INDVDL_SE" , "1");
			//alert(this.DS_EMPNO.getRowType(0));
			var sController   	= "hsco/mis/aud/AUD040102/mlgRegistListCU.do";
			var sInDatasets   	= "input02=DS_EMPNO:U";
			var sOutDatasets  	= "";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_EMPNO.getColumn(this.DS_EMPNO.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

