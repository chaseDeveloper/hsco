/***********************************************************************************
* 설명	: 엑셀파일을 업로드 콜백
***********************************************************************************/	
this.excelImport_callBack1 = function()
{
	if(this.ds_import.rowcount > 1){
		var nRow;
		this.DS_INTGTY_MLG_REGIST.updatecontrol = false;
		for(i=1; i< this.ds_import.rowcount; i++) {
			
			var col8  = this.ds_import.getColumn(i, "Column8");		//적용일자
			var col9  = this.ds_import.getColumn(i, "Column9");		//코드
			var col10 = this.ds_import.getColumn(i, "Column10");	//구분
			
			if(!this.comUtils.isNull(col8) && !this.comUtils.isNull(col9) && !this.comUtils.isNull(col10)) {
				nRow = this.DS_INTGTY_MLG_REGIST.addRow();
							
				//부서 확인
				var deptFindRow = this.ds_deptCode.findRow("DEPT_NM", this.ds_import.getColumn(i, "Column1"));
				if(deptFindRow > -1) {
					var code = this.ds_deptCode.getColumn(deptFindRow, "DEPT_CODE");
					this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "DEPT_CODE", code);
				}
				
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "EMPNO",      this.ds_import.getColumn(i, "Column2"));
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "EMPNM",      this.ds_import.getColumn(i, "Column3"));	
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "JSSFC_NM",   this.ds_import.getColumn(i, "Column4"));
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "HR_CLSF_NM", this.ds_import.getColumn(i, "Column5"));
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "OFCPS_NM",   this.ds_import.getColumn(i, "Column6"));
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "JBLN_NM",    this.ds_import.getColumn(i, "Column7"));
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "APPLC_DE",   this.ds_import.getColumn(i, "Column8"));
				
				// 날짜 확인
				var ApplcDe = this.DS_INTGTY_MLG_REGIST.getColumn(nRow, "APPLC_DE");				
				var ApplcDe2 = ApplcDe.replace(/[-]/g, "");
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "APPLC_DE",       ApplcDe2);
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "APPLC_YEAR",     ApplcDe2.substring(0, 4));
