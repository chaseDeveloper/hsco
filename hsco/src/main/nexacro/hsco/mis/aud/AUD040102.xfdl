this.validationChk = function(s_mlgScore, s_mxmmValue, s_mlgScoreTot, s_val_tot, s_csbyAllot, i) {
 	 	 	
 	var mlgScore = s_mlgScore;
 	var mxmmValue = s_mxmmValue;
 	var mlgScoreTot = s_mlgScoreTot;
 	var val_tot = s_val_tot;
 	var csbyAllot = s_csbyAllot;
 	var rowTxt = nexacro.toNumber(nexacro.toNumber(i)+1)+"행의 ";

 	trace("mlgScore : "+mlgScore+", mxmmValue : "+mxmmValue+", mlgScoreTot : "+mlgScoreTot+", val_tot : "+val_tot+", csbyAllot : "+csbyAllot);
 		
 	//배점이 양수 일 경우
	if(csbyAllot >= 0)
 	{
 		// 1. 점수가 음수일 경우
 		if(mlgScore < 0 ) 
 		{
 			//trace("1-1 배점이 양수, 점수가 배점보다 클 경우  mlgScore : "+mlgScore);
 			this.gfn_message("errors.항목값.범위.이상", rowTxt+"점수|0");
 			return false;
 		}
 		
 		// 2. 점수가 배점보다 클 경우
 		if(mlgScore > nexacro.toNumber(csbyAllot))
 		{
 			//trace("1-2 배점이 양수, 점수가 배점보다 클 경우  mlgScore : "+mlgScore+", csbyAllot : "+csbyAllot);
 			this.gfn_message("errors.항목값.범위.이하",rowTxt+"점수|배점");
 			return false;
 		}
 		
 		// 3. 동분야총점 + 점수가 상한점수보다 클 경우
 		if(val_tot > mxmmValue)
 		{
 			//trace("1-3 배점이 양수, 점수가 상한점수보다 클 경우  val_tot : "+val_tot+", mxmmValue : "+mxmmValue);
 			this.gfn_message("errors.항목값.범위.이하",rowTxt+"점수와 동분야총점의 합산점수|상한점수");
 			return false;
 		}
 	
 	//배점이 음수 일 경우 
 	}else if(csbyAllot < 0){
 		// 1. 점수가 양수일 경우
 		if(mlgScore > 0 )
 		{
			//trace("2-1 배점이 음수, 점수가 양수일 경우  mlgScore : "+mlgScore);
 			this.gfn_message("errors.항목값.범위.미만",rowTxt+"점수|0");
 			return false;
 		}
 		
 		// 2. 점수가 배점보다 작을 경우 
 		if(mlgScore < csbyAllot)
 		{
			//trace("2-2 배점이 음수, 점수가 배점보다 작을 경우  mlgScore : "+mlgScore+", csbyAllot : "+csbyAllot);
 			this.gfn_message("errors.항목값.범위.이상",rowTxt+"점수|배점");
 			return false;
 		}
 		
 		// 3. 동분야총점 + 점수가 상한점수보다 작을 경우
 		if(val_tot < mxmmValue)
 		{
			//trace("2-3 배점이 음수, 분야총점 + 점수가 상한점수보다 작을 경우  val_tot : "+val_tot+", mxmmValue : "+mxmmValue);
 			this.gfn_message("errors.항목값.범위.이상",rowTxt+"점수와 동분야총점의 합산점수|상한점수");
 			return false;
 		}
	}	
	return true;
}
