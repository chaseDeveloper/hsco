/***********************************************************************
 * 설명: 상단 저장버튼 클릭시
 ************************************************************************/
this.fn_save = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 엑셀 업로드 시 입력할 점수에 대한 검증
	for(var i = 0; i < this.DS_INTGTY_MLG_REGIST.rowcount; i++) {
		
		// 중복데이터 찾기 위한 값
		var applcYear = this.DS_INTGTY_MLG_REGIST.getColumn(i, "APPLC_YEAR");
		var mlgCode   = this.DS_INTGTY_MLG_REGIST.getColumn(i, "MLG_CODE");
		var mlgSe     = this.DS_INTGTY_MLG_REGIST.getColumn(i, "MLG_SE");
		var empno     = this.DS_INTGTY_MLG_REGIST.getColumn(i, "EMPNO");
		var deptCode  = this.DS_INTGTY_MLG_REGIST.getColumn(i, "DEPT_CODE");
		var deptIndvdlSe  = this.DS_INTGTY_MLG_REGIST.getColumn(i, "DEPT_INDVDL_SE");
		
		
		// 검증위한 값
		var s_mlgScore = this.DS_INTGTY_MLG_REGIST.getColumn(i, "MLG_SCORE");
		var s_mxmmValue = this.ds_mlgScoreTotList.getColumn(i, "MXMM_VALUE");
		var s_mlgScoreTot = this.ds_mlgScoreTotList.getColumn(i, "MLG_SCORE_TOT");
		var s_val_tot = nexacro.toNumber(s_mlgScore) + nexacro.toNumber(s_mlgScoreTot);
		var s_csbyAllot = this.ds_mlgScoreTotList.getColumn(i, "CSBY_ALLOT");
		var searchRow = nexacro.toNumber(nexacro.toNumber(i)+1);
					
		// 부서, 사원의 중복된 데이터가 있다면 입력된 점수를 합산하여 비교해야 함(-_-)
		// [개인별] 
		if(deptIndvdlSe == "1") {
		
			var dupRow = this.DS_INTGTY_MLG_REGIST.findRowExpr("APPLC_YEAR == '"+applcYear+"' && MLG_CODE == '"+mlgCode+"' && MLG_SE == '"+mlgSe+"' && EMPNO == '"+empno+"'", searchRow);
			if(dupRow > -1) {
				s_mlgScoreTot += nexacro.toNumber(this.ds_mlgScoreTotList.getColumn(dupRow, "MLG_SCORE"));
				if(s_mlgScoreTot > s_mxmmValue) {
					this.gfn_message("errors.항목값.범위.이하","적용년도, 코드, 분야, 사원이 중복된 "+(i+1)+"행과 "+(dupRow+1)+"행의 점수의 합 + 동분야총점의 합산점수|상한점수");
					return false;
				}
			}
		// [부서별] 
		}else if(deptIndvdlSe == "2") {
			
			var dupRow = this.DS_INTGTY_MLG_REGIST.findRowExpr("APPLC_YEAR == '"+applcYear+"' && MLG_CODE == '"+mlgCode+"' && MLG_SE == '"+mlgSe+"' && DEPT_CODE == '"+deptCode+"'", searchRow);
			if(dupRow > -1) {
				s_mlgScoreTot += nexacro.toNumber(this.ds_mlgScoreTotList.getColumn(dupRow, "MLG_SCORE"));
				if(s_mlgScoreTot > s_mxmmValue) {
					this.gfn_message("errors.항목값.범위.이하","적용년도, 코드, 분야, 부서가 중복된 "+(i+1)+"행과 "+(dupRow+1)+"행의 점수의 합 + 동분야총점의 합산점수|상한점수");
					return false;
				}
			}
		}

			
		if(!this.validationChk(s_mlgScore, s_mxmmValue, s_mlgScoreTot, s_val_tot, s_csbyAllot, i)) {
			return false;
		}	
	}
	
	// 엑셀 업로드 후 검증 트랜잭션 때문에 rowtype이 1로 변경되었음, 2로 다시 변경
	if(this.ds_mlgScoreTotList.rowcount > 0) {
		this.DS_INTGTY_MLG_REGIST.set_updatecontrol(false);
		for(var i = 0; i < this.DS_INTGTY_MLG_REGIST.rowcount; i++) {
			this.DS_INTGTY_MLG_REGIST.setRowType(i, 2);
		}
		this.DS_INTGTY_MLG_REGIST.set_updatecontrol(true);
	}
	
 	if(this.gfn_message("confirm.저장.여부")){ 
 		this.fn_transaction("MlgRegistListCUD");
 	}
}

