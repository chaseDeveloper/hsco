/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		
		case "selectMlgKnd":
			var sController   	= "hsco/mis/aud/AUD040102/selectMlgKnd.do";
			var sInDatasets   	= "input01=ds_cond00";
			var sOutDatasets  	= "DS_MLGKND=output01";
		break;
		
		case "MlgRegistListCUD":
			var sController   	= "hsco/mis/aud/AUD040102/MlgRegistListCUD.do";
			var sInDatasets   	= "input02=DS_INTGTY_MLG_REGIST:U";
			var sOutDatasets  	= "";
		break;
		
		case "selectMlgRegistDeptList":
			var sController   	= "hsco/mis/aud/AUD040102/selectMlgRegistDeptList.do";
			var sInDatasets   	= "input04=ds_cond";
			var sOutDatasets  	= "DS_INTGTY_MLG_REGIST=output04";
		break;
		
		case "selectMlgRegistList":
			var sController   	= "hsco/mis/aud/AUD040102/selectMlgRegistList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_INTGTY_MLG_REGIST=output01";
		break;
		case "insertMlgRegistOfTraing":
			var sController   	= "hsco/mis/aud/AUD040102/insertMlgRegistOfTraing.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_INTGTY_MLG_REGIST=output01";
		break;
		case "selectDeptCode":	// 부서 조회
			this.ds_deptCond.clearData();
			this.ds_deptCond.addRow();
			this.ds_deptCond.setColumn(0, "DEPT_CNM", "");
			this.ds_deptCond.setColumn(0, "USE_AT"  , "%");
			var sController   	= "hsco/mis/hrm/HRM010101/selectDeptList.do";
			var sInDatasets   	= "input1=ds_deptCond";
			var sOutDatasets  	= "ds_deptCode=output1";
		break;
		case "selectMlgScore":
			var sController   	= "hsco/mis/aud/AUD040102/selectMlgScore.do";
			var sInDatasets   	= "input1=DS_INTGTY_MLG_REGIST";
			var sOutDatasets  	= "ds_mlgScoreTotList=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode == 0){
		
		switch (strSvcId) {
			case "selectMlgKnd":
				this.fn_transaction("selectDeptCode");
			break;
			case "MlgRegistListCUD":
				this.fn_select_mlg_regist_list();
			break;
			case "insertMlgRegistOfTraing":
				alert("처리되었습니다.");
				this.fn_select_mlg_regist_list();
			break;
			case "selectMlgScore":
							


			break;
		}
	} else {		
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	}
}

