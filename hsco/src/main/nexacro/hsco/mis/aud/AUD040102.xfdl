		var empno = this.div_SearchArea.div_EMP.GetValue("EMPNO");
	}
	if(this.div_SearchArea.div_DEPT.GetValue("DEPT_CODE")){
		var dept_code = this.div_SearchArea.div_DEPT.GetValue("DEPT_CODE");
	}
		
	if(this.div_Cont.AUD040102.tabindex == 0)
	{
		// 초기화
