			var deptFindRow = this.ds_deptCode.findRow("DEPT_NM", this.ds_import2.getColumn(i, "Column1"));
			if(deptFindRow > -1) {
				var code = this.ds_deptCode.getColumn(deptFindRow, "DEPT_CODE");
				this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "DEPT_CODE", code);
				}

			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "APPLC_DE",      this.ds_import2.getColumn(i, "Column2"));
			// 날짜 확인
			var ApplcDe = this.DS_INTGTY_MLG_REGIST.getColumn(nRow, "APPLC_DE");				
			var ApplcDe2 = ApplcDe.replace(/[-]/g, "");
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "APPLC_DE",       ApplcDe2);
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "MLG_CODE",       this.ds_import2.getColumn(i, "Column3"));
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "MLG_SE",         this.ds_import2.getColumn(i, "Column4"));
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "MLG_KND",        this.ds_import2.getColumn(i, "Column5"));
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "MLG_DETAIL",     this.ds_import2.getColumn(i, "Column6"));
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "MLG_SCORE",      this.ds_import2.getColumn(i, "Column7"));
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "RM",             this.ds_import2.getColumn(i, "Column8"));
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "DEPT_INDVDL_SE", "2");
			this.DS_INTGTY_MLG_REGIST.setColumn(nRow, "APPLC_YEAR",     ApplcDe2.substring(0, 4));
			this.DS_INTGTY_MLG_REGIST.setRowType(nRow, Dataset.ROWTYPE_INSERT);
		}
		this.DS_INTGTY_MLG_REGIST.updatecontrol = true;
		this.fn_transaction("selectMlgScore");
	}	
}	
