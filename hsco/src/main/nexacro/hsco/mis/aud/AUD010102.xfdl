 ***********************************************************************
 *   
 ***********************************************************************
 */
 /***********************************************************************
 * Script Include 
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
//저장시 rowposition값 저장할 변수
var save_row = -1;

/***********************************************************************
 * 설명 : onInit 영역
 ***********************************************************************/
this.AUD010102_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}
/***********************************************************************
 * 설명: onLoad 영역
 ************************************************************************/ 
this.AUD010102_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.div_SearchArea.spn_year.setFocus(true);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();   // 조회 데이터셋 초기화	
	this.ds_cond.addRow();      // 조회 데이터셋 행추가
	this.ds_cond.setColumn(0, "AUDIT_BGNYEAR", this.gfn_today().substring(0,4) - 5);
	this.ds_cond.setColumn(0, "AUDIT_ENDYEAR", this.gfn_today().substring(0,4));
	var comboParams = [["DS_AUD_INSTT",    "AUD002", 1, "전체"],
	                   ["DS_AUD_KND",      "AUD003", 1, "전체"], 
	                   ["DS_AUD_CL",       "AUD004", 1, "전체"],
	                   ["DS_AUDIT_ED_AT",  "AUD018", 1, ""],
	                   ["DS_AUD_INSTT_G",  "AUD002", "Y", "D", "B"],
	                   ["DS_AUD_KND_G",    "AUD003", "Y", "D", "B"],
	                   ["DS_AUD_CL_G",     "AUD004", "Y", "D", "B"],
	                   ["ds_com051",       "COM051", "Y", "D", "B"],
	                   ["ds_aud036",       "AUD036", "Y", "D", "B"],
	                   ["ds_aud037",       "AUD037", "Y", "A", "B"],
	                   ["ds_aud037G",      "AUD037", "Y", "D", "B"]
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	
}

/***********************************************************************
 * 설명: 조회버튼 클릭시 처리
 ************************************************************************/
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
// 	 //내외부감사관리목록 중 저장되지 않은 정보가 있을 때 조회버튼 클릭시
//     if(Ex.util.isUpdated(this.DS_INEX_AUDIT)){
// 	    if(this.gfn_message("confirm.변경.저장.여부")){
// 			save_row = this.DS_INEX_AUDIT.rowposition;
// 	      	this.fn_transaction("InExAudListCUD");
// 	    }else{
// 			//변경은 되었지만 저장을 아니오 눌렀을 경우 조회만.
// 			this.fn_transaction("selectInExAdList");
// 		}
// 	}else{
// 		//변경되지 않았을 경우 조회만.
// 	this.fn_transaction("selectInExAdList");	
// 	}
	this.fn_transaction("selectInExAdList");
}


/************************************************************************************
* 설명 : 년도 콤보박스 선택 시 조회
************************************************************************************/
this.div_SearchArea_cmb_AUDIT_YEAR_onitemchanged = function(obj:Combo, e:nexacro.ComboCloseUpEventInfo)
{
	this.fn_search();
}

/************************************************************************************
* 설명 : 상단 신규버튼 클릭시
************************************************************************************/
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow  = this.DS_INEX_AUDIT.addRow();
	var year  = this.gfn_today().substring(0,4);
	var today = this.gfn_today();
	
	this.DS_INEX_AUDIT.setColumn(nRow, "AUDIT_PLAN_YEAR", year);
	this.DS_INEX_AUDIT.setColumn(nRow, "AUDIT_BGNDE"    , today);
	this.DS_INEX_AUDIT.setColumn(nRow, "AUDIT_ENDDE"    , today);
	this.DS_INEX_AUDIT.setColumn(nRow, "PLAN_REGIST_AT" , '0');
	this.DS_INEX_AUDIT.setColumn(nRow, "CONFM_AT2"      , '0');
	this.DS_INEX_AUDIT.setColumn(nRow, "DSPTH_AT"       , '0');
	this.DS_INEX_AUDIT.setColumn(nRow, "AUDIT_ED_AT"    , '0');
}


/***********************************************************************
 * 설명: 상단 취소버튼 클릭시
 ************************************************************************/
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.DS_INEX_AUDIT) || Ex.util.isUpdated(this.DS_AUDIT_COMPOSI))
	{
		if(this.gfn_message("confirm.취소여부")){  //모든 내용을 복원 하시겠습니까?
			this.DS_INEX_AUDIT.reset();		//DATASET ROLLBACK
			this.DS_INEX_AUDIT.applyChange();	//DATASET COMMIT
			this.DS_AUDIT_COMPOSI.reset();		//DATASET ROLLBACK
			this.DS_AUDIT_COMPOSI.applyChange();	//DATASET COMMIT
			return;
		}	
	}else{
		this.gfn_message("comm.내역.변경.없음");
	}
}


/***********************************************************************
 * 설명: 상단 삭제버튼 클릭시
 ************************************************************************/
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow = this.DS_INEX_AUDIT.rowposition;
	var planRegistAt = this.DS_INEX_AUDIT.getColumn(pRow, "PLAN_REGIST_AT");
	var auditEdAt    = this.DS_INEX_AUDIT.getColumn(pRow, "AUDIT_ED_AT");
	
	if(pRow < 0 ) {
		this.gfn_message("comm.항목.선택.요청","내외부감사목록");
		return false;
	}
	
	if(planRegistAt == '1') {
		this.gfn_message("info.처리불가","계획등록이 아닌 건만 삭제할 수 있습니다.");
		return false;
	}
	
	// 승인된 건은 삭제불가
	if(auditEdAt != "0") {
		this.gfn_message("fail.삭제.불가","종결여부가 대가가 아닌 건은");
		return false;
	}
	else {
		if(this.gfn_message("confirm.삭제여부")){
			var dRow = this.DS_INEX_AUDIT.rowposition;
			this.DS_INEX_AUDIT.deleteRow(dRow);
			this.DS_INEX_AUDIT.set_rowposition(dRow);
		}
	}
}

/***********************************************************************
 * 설명: 상단 저장버튼 클릭시
 ************************************************************************/
this.fn_save = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 수정 여부체크
	if (   !this.comUtils.isDatasetUpdated(this.DS_INEX_AUDIT)
	    && !this.comUtils.isDatasetUpdated(this.DS_AUDIT_DEPT_TABLE)
	    && !this.comUtils.isDatasetUpdated(this.DS_AUDIT_COMPOSI)) {
		this.gfn_message("comm.내역.변경.없음");
		return false;
	}
	
	//validation check(내외부감사)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_INEX_AUDIT, this.ds_validation)) {
		return false;
	}
	//validation check(감사대상부서)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AUDIT_DEPT_TABLE, this.ds_validation)) {
		return false;
	}
	//validation check(감사인)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_AUDIT_COMPOSI, this.ds_validation)) {
		return false;
	}
	
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.DS_INEX_AUDIT.rowposition;
		this.fn_transaction("InExAudListCUD");
	}
}


/**********************************************************************
* call Transaction 영역
***********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "InExAudListCUD":		//내외부감사 등록
			var sController   	= "hsco/mis/aud/AUD010102/InExAudListCUD.do";
			var sInDatasets   	= "input02=DS_INEX_AUDIT:U input1=DS_AUDIT_COMPOSI:U input2=DS_AUDIT_DEPT_TABLE:U";
			var sOutDatasets  	= "";
			break;
		case "selectInExAdList":	//내외부감사 조회 
			this.DS_INEX_AUDIT.clearData();
			var sController   	= "hsco/mis/aud/AUD010102/selectInExAdList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_INEX_AUDIT=output01";
			break;
		case "selectAdtor":			// 감사인 조회 
			var sController   	= "hsco/mis/aud/AUD010102/selectAdtor.do";
			var sInDatasets   	= "input1=DS_SEARCH_SUB";
			var sOutDatasets  	= "DS_AUDIT_COMPOSI=output1 DS_AUDIT_DEPT_TABLE=output2";
			break;
		case "getAdtor":			// 감사인가져오기
			var sController   	= "hsco/mis/aud/AUD010102/getAdtor.do";
			var sInDatasets   	= "input1=DS_SEARCH_SUB";
			var sOutDatasets  	= "";
			break;
		case "getTrgetDept":		// 감사대상부서가져오기
			var sController   	= "hsco/mis/aud/AUD010102/getTrgetDept.do";
			var sInDatasets   	= "input1=DS_SEARCH_SUB";
			var sOutDatasets  	= "";
			break;
		case "updateConfmAt2":		// 등록승인
			var sController   	= "hsco/mis/aud/AUD010102/updateConfmAt2.do";	
			var sInDatasets   	= "input1=DS_SEARCH_SUB";
			var sOutDatasets  	= "";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if(nErrorCode == 0) {
		switch (strSvcId) {
			case "InExAudListCUD":		// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectInExAdList":	// 조회(내외부감사목록)
				if(save_row != -1) {
					this.DS_INEX_AUDIT.set_rowposition(save_row);
					save_row = -1;
				}
				break;
			case "selectAdtor":			// 조회(감사인)
				break;
			case "getAdtor":			// 감사인가져오기
				this.gfn_message("success.처리.성공");
				this.fn_transaction("selectAdtor");
				break;
			case "getTrgetDept":		// 감사대상부서가져오기
				this.gfn_message("success.처리.성공");
				this.fn_transaction("selectAdtor");
				break;
			case "updateConfmAt2":		// 등록승인
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}

