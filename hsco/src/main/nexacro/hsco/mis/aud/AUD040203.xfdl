***********************************************************************/

/**********************************************************************
* 사용자 정의 함수
***********************************************************************/

this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "YM") {
		this.fn_transaction("selectSjList");
	}else if(e.columnid == "SJ") {
		this.fn_search();
	}
}
