/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명      : initForm
 * 설명         : Form을 초기화한다.
 * params          : None
 * return Type     : None
 * 사용법        : this.initForm();
 */
this.fn_init_form = function()
{
	trace("fn_init_form");
	this.gfn_initForm(this);
	this.fn_misFormInit(this);

	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "QUSTNR_SE", "01"); //설문구분(01 : 감사)
	this.ds_cond.setColumn(0, "YM"              , this.dateUtils.format(this.gfn_today(),"yyyymm"));
	this.ds_cond.setColumn(0, "QUSTNR_MANAGE_SN", "");

	// 공통코드와 연계된 초기검색조건 설정
	// 수정가능여부를 판별하기위해 권한그룹 체크
	if(this.gfn_authGrpId("AUD_ADMIN") != -1) {			// 감사결재자
		this.chargerAt = true;
	}

	// 수정가능한 경우 부서변경 가능
	if(this.chargerAt){
	}
 
}


/*
 * 함수명      : initDataset
 * 설명         : Dataset을 초기화한다
 * params          : None
 * return Type     : None
 * 사용법        : this.initDataset();
 */
this.fn_init_dataset = function()
{
