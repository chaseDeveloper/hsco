this.AUD040203_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	trace("AUD040203_onload");
	
	this.fn_init_form();
	this.fn_init_dataset();
    
 	this.div_search.div_ym.fn_setBind("ds_cond", "YM");
   
	
	//this.fn_transaction("selectSjList");
