 *    
 ***********************************************************************/
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
var approval_row = this.parent.APP_ROW;
var approval_info = this.parent.APP_INFO;
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.AUD020101_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}
/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.AUD020101_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	this.ds_tempDataset.loadXML(approval_info);
	this.ds_catsTmpApproval.clearData();
	this.ds_catsTmpApproval.addRow();
	this.ds_catsTmpApproval.copyRow(0, this.ds_tempDataset, approval_row);
}

/***********************************************************************
 * Form 오브젝트 초기화
 ***********************************************************************/ 
this.fn_init_form = function()
{
	this.gfn_initForm(this);	
	this.fn_misFormInit(this);
}

/***********************************************************************
 * Dataset 오브젝트 초기화
 ***********************************************************************/ 
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);	
}

/**********************************************************************
* 법인카드사용내역 상세조회
***********************************************************************/ 
this.selectCatsTmpApprovalR = function()
{
	this.ds_cond01.clearData();
	var nRow = this.ds_cond01.addRow();
	
	this.ds_cond01.setColumn(nRow, "SEQ",this.approval_seq_no);

	this.fn_transaction("selectcatsTmpApporvalR");
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//부서코드 조회
		//법인카드사용내역목록 조회
		case "selectcatsTmpApporvalR":
			var sController   	= "hsco/mis/aud/AUD020101/selectcatsTmpApporvalR.do";
			var sInDatasets   	= "input03=ds_cond01";
			var sOutDatasets  	= "ds_catsTmpApprovalR=output03";
			break;

	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

/**********************************************************************
 * 닫기버튼 클릭시
 **********************************************************************/ 
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}
]]></Script>
