***********************************************************************************/
this.DS_MNGD_RCEPT_PROCESS_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow>=0){
		this.fn_setreadonly();
		this.fn_transaction("selectAtchFile");
	}
}

