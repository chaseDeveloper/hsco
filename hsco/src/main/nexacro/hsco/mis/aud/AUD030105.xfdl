/***********************************************************************
 * 설명: 조회버튼 클릭시 처리
 ************************************************************************/
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_transaction("selectMngdRceptlist");
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	

		//감사결재자 조회
		case "audAdmin":
			var sController   	= "hsco/com/sym/aum/SYM020200/getListGrpUser.do";
			var sInDatasets   	= "input1=ds_audAdmin";
			var sOutDatasets  	= "ds_audAdmin=output1";
		break;
		//상단 조회
	    case "selectMngdRceptlist" :
			this.ds_fileList.clearData();
			this.DS_MNGD_RCEPT_PROCESS.clearData();
			var sController   	= "hsco/mis/aud/AUD030105/selectMngdRceptlist.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "DS_MNGD_RCEPT_PROCESS=output01";
		break;
		
		//상단 저장
		case "MngdRceptlistCUD":
			var sController   	= "hsco/mis/aud/AUD030105/MngdRceptlistCUD.do";
			var sInDatasets   	= "input01=ds_uc input02=DS_MNGD_RCEPT_PROCESS:U";
			var sOutDatasets  	= "";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_Cont.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_MNGD_RCEPT_PROCESS.getColumn(this.DS_MNGD_RCEPT_PROCESS.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}else{
		if(strSvcId =="audAdmin")
		{
			this.fn_search();
		}
		if(strSvcId =="MngdRceptlistCUD")
		{ 
			this.fn_transaction("selectMngdRceptlist");
		}
		if(strSvcId == "selectMngdRceptlist")
		{
			//저장트랜잭션 직전 rowposition값으로 이동
			var save_row = this.DS_MNGD_RCEPT_PROCESS.findRow("RCEPT_SN",save_pk);
			if(save_row != -1)
			{
				this.DS_MNGD_RCEPT_PROCESS.set_rowposition(save_row);
				save_pk = -1;
			}
		}
	}
}
/**********************************************************************
 * 설명: 상단 신규버튼 클릭시
 ************************************************************************/
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.DS_MNGD_RCEPT_PROCESS.addRow();
	this.DS_MNGD_RCEPT_PROCESS.setColumn(nRow, "RCEPT_DE",     this.gfn_today());
	this.DS_MNGD_RCEPT_PROCESS.setColumn(nRow, "MNGD_RCEPT_DE",this.gfn_today());
	this.DS_MNGD_RCEPT_PROCESS.setColumn(nRow, "RCEPTER_EMPNM",this.gfn_getUserName());
 
 	if(!this.chargerAt) {
 	 	this.DS_MNGD_RCEPT_PROCESS.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId() );
 		this.DS_MNGD_RCEPT_PROCESS.setColumn(nRow, "DEPT_NM"  , this.gfn_getDeptName());
 		this.DS_MNGD_RCEPT_PROCESS.setColumn(nRow, "EMPNO"    ,this.fn_getEmpno());
 		this.DS_MNGD_RCEPT_PROCESS.setColumn(nRow, "EMPNM"    ,this.fn_getEmpnm());
 	}
}

/***********************************************************************
 * 설명: 상단 삭제버튼 클릭시
 ************************************************************************/
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.DS_MNGD_RCEPT_PROCESS.rowcount > 0) {
		var nRow = this.DS_MNGD_RCEPT_PROCESS.rowposition;
		
		var confmAt = this.DS_MNGD_RCEPT_PROCESS.getColumn(nRow, "CONFM_AT");
		if(confmAt == "1") {
			alert("처리된 건은 삭제할 수 없습니다.");	
			return false;
		}
		
		if(this.gfn_message("confirm.삭제여부")){
			this.DS_MNGD_RCEPT_PROCESS.deleteRow(nRow);
		}
	}
}

/***********************************************************************
 * 설명: 상단 취소버튼 클릭시
 ************************************************************************/
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.DS_MNGD_RCEPT_PROCESS)){
	    if(this.gfn_message("confirm.취소여부")){  //모든 내용을 복원 하시겠습니까?
		    this.DS_MNGD_RCEPT_PROCESS.reset();		//DATASET ROLLBACK
		    this.DS_MNGD_RCEPT_PROCESS.applyChange();	//DATASET COMMIT
		    return;
	    }
	}else{
		this.gfn_message("comm.내역.변경.없음");
	}
}

/***********************************************************************
 * 설명: 상단 저장버튼 클릭시
 ************************************************************************/
this.fn_save = function(obj:Button,  e:nexacro.ClickEventInfo)
{
// 	if(!this.chargerAt){
// 		//부여된 권한이 없습니다. => 메시지 알림 2016.07.01
// 		this.gfn_message("info.권한.허가목록.없음");
// 		return false;
// 	}
		
	//validation check		
	if(!this.gfn_checkValidation(this.DS_MNGD_RCEPT_PROCESS, this.ds_validation)){
		return;
	} 
	if(this.gfn_message("confirm.저장.여부")){
		this.fn_transaction("MngdRceptlistCUD");
	}
}


/***********************************************************************
* 조회기간 변경 시
************************************************************************/
this.DS_SEARCH_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "BGN_RCEPT_DE")
	{
		if(e.newvalue > this.div_SearchArea.cal_END_RCEPT_DE.value) 
		{
			this.gfn_message("errors.시작일자.종료일자.이하","|");
			return false;
		}
		return true;
	}
	//종료일 변경시
	if(e.columnid == "END_RCEPT_DE")
	{
		if(e.newvalue < this.div_SearchArea.cal_BGN_RCEPT_DE.value) 
		{
			this.gfn_message("errors.종료일자.시작일자.이상","|");
			return false;
		}
		return true;
	}
}

