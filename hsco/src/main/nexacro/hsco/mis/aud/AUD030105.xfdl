/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();   // 조회 데이터셋 초기화	
	this.ds_cond.addRow();      // 조회 데이터셋 행추가
	this.ds_cond.setColumn(0, "BGN_RCEPT_DE" ,this.gfn_today().substring(0,4) + '0101');
	this.ds_cond.setColumn(0, "END_RCEPT_DE" ,this.gfn_today());
	this.ds_cond.setColumn(0, "EMPNO" ,this.fn_getEmpno());
	this.ds_cond.setColumn(0, "EMPNM" ,this.fn_getEmpnm());
	
	// 문자전송 dataset 세팅
	this.ds_sendMssage.clearData();
	var nRow = this.ds_sendMssage.addRow();
	var userId       = 'hsuco7765';
	var scheduleType = '1';
	var subject      = '금품등접수처리';
	var callback     = '010-2090-8176';			// 발신번호
	var destCount    = '1';
	var destInfo     = '김형태^010-2090-8176';	// 수신자 정보
	var msgType      = '0';
	var mmsMsg       = '금품등접수처리가 신규 등록되었습니다. 확인 부탁드립니다.';
	var contentCount = '0';
	var contentData  = '';
	
	this.ds_sendMssage.setColumn(nRow, "USER_ID"      , userId);
	this.ds_sendMssage.setColumn(nRow, "SCHEDULE_TYPE", scheduleType);
	this.ds_sendMssage.setColumn(nRow, "SUBJECT"      , subject);
	this.ds_sendMssage.setColumn(nRow, "CALLBACK"     , callback);
	this.ds_sendMssage.setColumn(nRow, "DEST_COUNT"   , destCount);
	this.ds_sendMssage.setColumn(nRow, "DEST_INFO"    , destInfo);
	this.ds_sendMssage.setColumn(nRow, "MSG_TYPE"     , msgType);
	this.ds_sendMssage.setColumn(nRow, "MMS_MSG"      , mmsMsg);
	this.ds_sendMssage.setColumn(nRow, "CONTENT_COUNT", contentCount);
	this.ds_sendMssage.setColumn(nRow, "CONTENT_DATA" , contentData);
	
	
	// 공통코드와 연계된 초기검색조건 설정
	// 수정가능여부를 판별하기위해 권한그룹 체크
	if(this.gfn_authGrpId("DEV_GRP") != -1) {			// 개발자
		this.chargerAt = true;
	}
	if(this.gfn_authGrpId("AUD_TIMHDER") != -1) {		// 감사팀장!!!!
		this.chargerAt = true;
	}
		
	// 수정가능한 경우 사원변경, 처리결과 입력 가능
	if(this.chargerAt){
		this.div_SearchArea.div_EMP.set_enable(true);
		this.div_Cont.btn_uploadFiles.set_enable(true);
		this.div_Cont.btn_delFiles.set_enable(true);
	}
}

