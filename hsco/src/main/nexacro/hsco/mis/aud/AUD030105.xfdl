	}else {
		//선택한 헤드에 따른 정렬
		this.gfn_gridSort(obj, e);
	}
}




/***********************************************************************
 * 권한, 승인여부에 따라 오브젝트를 제어
 ***********************************************************************/	

/*
 * 함수명  	: fn_setreadonly
 * 설명 		: 권한, 승인여부에 따라 오브젝트를 제어한다.
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_setreadonly();
 */  
this.fn_setreadonly = function(){
	
	var nRow = this.DS_MNGD_RCEPT_PROCESS.rowposition;
	var confmAt = this.DS_MNGD_RCEPT_PROCESS.getColumn(nRow, "CONFM_AT");
	var authAt = this.DS_MNGD_RCEPT_PROCESS.getColumn(nRow, "AUTH_AT");	// 본인작성
	var registerId = this.DS_MNGD_RCEPT_PROCESS.getColumn(nRow, "REGISTER_ID");
	
	var cssflag = ["Readonly", "Essential"];
	var flag = true;

	// 권한 있음
	if(this.chargerAt) {
		
		// 본인 등록
		if(authAt == "1" || this.DS_MNGD_RCEPT_PROCESS.getRowType(nRow) == "2") {
			flag = false;
		}else{
			flag = true;
		}
		
		var flagNumber = (flag == true ? 0 : 1);
		
		//첨부파일 버튼 visible 여부
		this.div_Cont.btn_delFiles.set_visible(!flag);
		this.div_Cont.btn_uploadFiles.set_visible(!flag);
		
		//소속/성명 expand 버튼 visible 여부
		this.div_Cont.div_sear.search.set_visible(!flag);
		
		//접수일자 readonly 여부
		this.div_Cont.cal_RCEPT_DE.set_readonly(flag);
		this.div_Cont.cal_RCEPT_DE.set_cssclass("cal_WF_"+cssflag[flagNumber]);
		
		//금품내역 readonly 여부
		this.div_Cont.txt_MNGD_DTLS.set_readonly(flag);
		this.div_Cont.txt_MNGD_DTLS.set_cssclass("txt_WF_"+cssflag[flagNumber]);
		
		//소속/성명 readonly 여부
		this.div_Cont.edt_deptNm.set_readonly(flag);
		this.div_Cont.div_sear.edt_empno.set_readonly(flag);
		this.div_Cont.div_sear.edt_emplnm.set_readonly(flag);
		this.div_Cont.edt_deptNm.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		this.div_Cont.div_sear.edt_empno.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		this.div_Cont.div_sear.edt_emplnm.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		
		//수수일자 readonly 여부
		this.div_Cont.cal_MNGD_RCEPT_DE.set_readonly(flag);
		this.div_Cont.cal_MNGD_RCEPT_DE.set_cssclass("cal_WF_"+cssflag[flagNumber]);
		
		//제공자 readonly 여부
		this.div_Cont.edt_OFFER.set_readonly(flag);
		this.div_Cont.edt_OFFER.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		
		//연락처 readonly 여부
		this.div_Cont.edt_CTTPC.set_readonly(flag);
		this.div_Cont.edt_CTTPC.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		
		//처리내용 readonly 여부
		this.div_Cont.txt_PROCESS_CN.set_readonly(false);
		this.div_Cont.txt_PROCESS_CN.set_cssclass("");
		
		
	// 권한 없음	
	}else {
		
		// 승인안됨
		if(this.comUtils.isNull(confmAt) ||  confmAt == "0") {
			flag = false;
		}else {
			flag = true;
		}
		
		var flagNumber = (flag == true ? 0 : 1);
		
		//첨부파일 버튼 visible 여부
		this.div_Cont.btn_delFiles.set_visible(!flag);
		this.div_Cont.btn_uploadFiles.set_visible(!flag);
		
		//접수일자 readonly 여부
		this.div_Cont.cal_RCEPT_DE.set_readonly(flag);
		this.div_Cont.cal_RCEPT_DE.set_cssclass("cal_WF_"+cssflag[flagNumber]);
		
		//금품내역 readonly 여부
		this.div_Cont.txt_MNGD_DTLS.set_readonly(flag);
		this.div_Cont.txt_MNGD_DTLS.set_cssclass("txt_WF_"+cssflag[flagNumber]);
		
		//소속/성명 readonly 여부
		this.div_Cont.edt_deptNm.set_readonly(flag);
		this.div_Cont.div_sear.edt_empno.set_readonly(flag);
		this.div_Cont.div_sear.edt_emplnm.set_readonly(flag);
		this.div_Cont.edt_deptNm.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		this.div_Cont.div_sear.edt_empno.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		this.div_Cont.div_sear.edt_emplnm.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		
		//수수일자 readonly 여부
		this.div_Cont.cal_MNGD_RCEPT_DE.set_readonly(flag);
		this.div_Cont.cal_MNGD_RCEPT_DE.set_cssclass("cal_WF_"+cssflag[flagNumber]);
		
		//제공자 readonly 여부
		this.div_Cont.edt_OFFER.set_readonly(flag);
		this.div_Cont.edt_OFFER.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		
		//연락처 readonly 여부
		this.div_Cont.edt_CTTPC.set_readonly(flag);
		this.div_Cont.edt_CTTPC.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		
		//소속/성명 readonly 여부
		this.div_Cont.edt_deptNm.set_readonly(true);
		this.div_Cont.div_sear.edt_empno.set_readonly(true);
		this.div_Cont.div_sear.edt_emplnm.set_readonly(true);
		this.div_Cont.edt_deptNm.set_cssclass("edt_WF_Readonly");
		this.div_Cont.div_sear.edt_empno.set_cssclass("edt_WF_Readonly");
		this.div_Cont.div_sear.edt_emplnm.set_cssclass("edt_WF_Readonly");
				
		//소속/성명 expand 버튼 visible 여부
		this.div_Cont.div_sear.search.set_visible(false);
	}
}


this.DS_MNGD_RCEPT_PROCESS_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 처리내용
	if(e.columnid == "PROCESS_CN") {
		
		// 처리내용에 따라 처리여부(승인여부) 결정
		if(!this.comUtils.isNull(e.newvalue)) {
			obj.setColumn(e.row, "CONFM_AT", "1");
		}else {
			obj.setColumn(e.row, "CONFM_AT", "0");
		}
	}
}
