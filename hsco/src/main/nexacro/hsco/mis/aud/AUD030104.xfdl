/*
 * 함수명  	: fn_sanction
 * 설명 		: '결재' 버튼 클릭 후
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_sanction();
 */
this.fn_sanction = function()
{
	if(Ex.util.isUpdated(this.DS_INNER_STTEMNT)){
		this.gfn_message("comm.정보.저장.요청","먼저 수정한 정보를");
	}else{
		switch(this.DS_INNER_STTEMNT.getColumn(this.DS_INNER_STTEMNT.rowposition, "SANCTN_STTUS")){
		case "T05":
		case "T07":
		case "T11":
		case "T14":
		case "T15":
			if(this.gfn_message("comm.행위여부","접수 및 처리내용을 수정하고 내부결재 재상신을 진행") ){
				this.DS_INNER_STTEMNT.setColumn(this.DS_INNER_STTEMNT.rowposition, "INNER_SANCTN_NO", "");
				//this.fn_setreadonly(false);
				break;
			}else{
				break;
			}
		default:
			var innerSanctnMdlId = "AUD-100066"; // 내부결재모형ID
			var sysSeCode = "SYS";
			var jobSe = "MIS";
			var svcId = "GetInnerSanctnId";
			if(this.ds_recomInfo.getRowCount() ==0) this.ds_recomInfo.addRow();
			

			var key = "AUD100066" + "^" + this.DS_INNER_STTEMNT.getColumn(this.DS_INNER_STTEMNT.rowposition, "RCEPT_SN");
			this.ds_recomInfo.setColumn(0, "JOB_KEY", key);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_MDL_ID", innerSanctnMdlId);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_ID", this.DS_INNER_STTEMNT.getColumn(this.DS_INNER_STTEMNT.rowposition, "INNER_SANCTN_NO"));
			
			this.ds_recomInfo.setColumn(0, "SYS_SE_CODE", sysSeCode);
			this.ds_recomInfo.setColumn(0, "JOB_SE", jobSe);
			this.fn_ManageInnerSanction(innerSanctnMdlId, sysSeCode, jobSe, svcId);
		break;
		}
	}
}
