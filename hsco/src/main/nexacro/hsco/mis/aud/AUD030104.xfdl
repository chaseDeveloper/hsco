***********************************************************************************/
this.DS_INNER_STTEMNT_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow>=0){
		// 승인 후
		if(this.DS_INNER_STTEMNT.getColumn(this.DS_INNER_STTEMNT.rowposition, "CONFM_AT") == "1")
		{
			this.div_Cont.cal_RCEPT_DE.set_readonly(true);
			this.div_Cont.cal_RCEPT_DE.set_cssclass("cal_WF_Readonly");
			this.div_Cont.edt_CNSLT_MAN.set_readonly(true);
			this.div_Cont.edt_CNSLT_MAN.set_cssclass("edt_WF_Readonly");
			this.div_Cont.txt_CNSLT_CN.set_readonly(true);
			this.div_Cont.txt_CNSLT_CN.set_cssclass("txt_WF_Readonly");
			this.div_Cont.cal_RICFLD_REGIST_DE.set_readonly(true);
			this.div_Cont.cal_RICFLD_REGIST_DE.set_cssclass("cal_WF_Readonly");
			this.div_Cont.edt_RICFLD_REGISTER.set_readonly(true);
			this.div_Cont.edt_RICFLD_REGISTER.set_cssclass("edt_WF_Readonly");
			this.div_Cont.txt_RICFLD_CN.set_readonly(true);
			this.div_Cont.txt_RICFLD_CN.set_cssclass("txt_WF_Readonly");
		}else{
		//승인 전
			this.div_Cont.cal_RCEPT_DE.set_readonly(false);
			this.div_Cont.cal_RCEPT_DE.set_cssclass("cal_WF_Essential");
			this.div_Cont.edt_CNSLT_MAN.set_readonly(false);
			this.div_Cont.edt_CNSLT_MAN.set_cssclass("edt_WF_Essential");
			this.div_Cont.txt_CNSLT_CN.set_readonly(false);
			this.div_Cont.txt_CNSLT_CN.set_cssclass("txt_WF_Essential");
			this.div_Cont.cal_RICFLD_REGIST_DE.set_readonly(false);
			this.div_Cont.cal_RICFLD_REGIST_DE.set_cssclass("");
			this.div_Cont.edt_RICFLD_REGISTER.set_readonly(false);
			this.div_Cont.edt_RICFLD_REGISTER.set_cssclass("");
			this.div_Cont.txt_RICFLD_CN.set_readonly(false);
			this.div_Cont.txt_RICFLD_CN.set_cssclass("");
		}
		this.fn_transaction("selectAtchFile");
	}
}

