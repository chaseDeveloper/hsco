/*
 * 함수명  	: fn_setreadonly
 * 설명 		: 진행중인 결재건인 경우, 수정할 수 없도록 한다.
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_setreadonly();
 */ 
this.fn_setreadonly = function(flag){
	//flag == true ? Readonly : Essential
	var cssflag = ["Readonly", "Essential"];
	var flagNumber=(flag==true?0:1);
	
	
	if(this.comUtils.isNull(this.DS_INNER_STTEMNT.getColumn(this.DS_INNER_STTEMNT.rowposition, "RICFLD_REGIST_DE")))
	{
		this.div_Cont.cal_RCEPT_DE.set_readonly(flag);
		this.div_Cont.cal_RCEPT_DE.set_cssclass("cal_WF_"+cssflag[flagNumber]);
		this.div_Cont.edt_CNSLT_MAN.set_readonly(flag);
		this.div_Cont.edt_CNSLT_MAN.set_cssclass("edt_WF_"+cssflag[flagNumber]);
		this.div_Cont.txt_CNSLT_CN.set_readonly(flag);
		this.div_Cont.txt_CNSLT_CN.set_cssclass("txt_WF_"+cssflag[flagNumber]);
		this.div_Cont.cal_RICFLD_REGIST_DE.set_readonly(flag);
		
		this.div_Cont.edt_RICFLD_REGISTER.set_readonly(flag);
		this.div_Cont.edt_RICFLD_REGISTER.set_cssclass("");
		this.div_Cont.txt_RICFLD_CN.set_readonly(flag);
		this.div_Cont.txt_RICFLD_CN.set_cssclass("");
	}else{
	//답변등록일시가 있는경우
		this.div_Cont.cal_RCEPT_DE.set_readonly(true);
		this.div_Cont.cal_RCEPT_DE.set_cssclass("cal_WF_Readonly");
		this.div_Cont.edt_CNSLT_MAN.set_readonly(true);
		this.div_Cont.edt_CNSLT_MAN.set_cssclass("edt_WF_Readonly");
		this.div_Cont.txt_CNSLT_CN.set_readonly(true);
		this.div_Cont.txt_CNSLT_CN.set_cssclass("txt_WF_Readonly");
		this.div_Cont.cal_RICFLD_REGIST_DE.set_readonly(true);
		this.div_Cont.cal_RICFLD_REGIST_DE.set_cssclass("cal_WF_Readonly");
		this.div_Cont.edt_RICFLD_REGISTER.set_readonly(true);
		this.div_Cont.edt_RICFLD_REGISTER.set_cssclass("edt_WF_Readonly");
		this.div_Cont.txt_RICFLD_CN.set_readonly(true);
		this.div_Cont.txt_RICFLD_CN.set_cssclass("txt_WF_Readonly");
	}
	
	
	//첨부파일 버튼 visible 여부
	this.div_Cont.btn_delFiles.set_visible(!flag);
	this.div_Cont.btn_uploadFiles.set_visible(!flag);
	//행버튼 visible 여부
	this.div_Cont.btn_Add_Row.set_visible(!flag);
	this.div_Cont.btn_Del_Row.set_visible(!flag);
	this.div_Cont.btn_Cancel_Row.set_visible(!flag);
	//접수일자 readonly 여부
	this.div_Cont.cal_RCEPT_DE.set_readonly(flag);
	//상담자 readonly 여부
	this.div_Cont.edt_CNSLT_MAN.set_readonly(flag);
	//상담내용 readonly 여부
	this.div_Cont.txt_CNSLT_CN.set_readonly(flag);
	//답변등록일자 readonly 여부
	this.div_Cont.cal_RICFLD_REGIST_DE.set_readonly(flag);
	//답변등록자 readonly 여부
	this.div_Cont.edt_RICFLD_REGISTER.set_readonly(flag);
	//답변내용 readonly 여부
	this.div_Cont.txt_RICFLD_CN.set_readonly(flag);
}


/*
 * 함수명  	: div_Cont_grd_INNER_STTEMNT_oncellclick
 * 설명 		: 결재여부는 감사결재자만 수정가능
 * params  		: 
 * return Type 	: None
 * 사용법		: n/a
 */
this.div_Cont_grd_INNER_STTEMNT_oncellclick = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(e.cell == obj.getCellCount("Body")-1){
		if(this.gfn_authcode("064") < -1){		
			//부여된 권한이 없습니다. => 메시지 알림 2016.07.01
			this.gfn_message("info.권한.허가목록.없음");
			return false;
		}else if(Number(this.DS_INNER_STTEMNT.getColumn(e.row, "RCEPT_SN"))>0){
				this.ds_confmAt.clearData();
				this.ds_confmAt.addRow();
				this.ds_confmAt.copyRow(0, this.DS_INNER_STTEMNT , e.row);
				this.fn_transaction("CONFMAT");
		}
	}
}

