/**************************************************************************
 *문자전송 버튼 클릭시
**************************************************************************/
this.div_Cont_btn_sendMssage_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rCnt = this.ds_rqestEdayAuditList.rowcount;
	if(rCnt < 1) {
		this.gfn_message("comm.항목.선택.요청","일상감사요청목록");
