/*
 * 함수명  	: fn_setreadonly
 * 설명 		: 진행중인 결재건인 경우, 수정할 수 없도록 한다.
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_setreadonly();
 */ 
this.fn_setreadonly = function(flag){
	//flag == true ? Readonly : Essential
	var cssflag = ["Readonly", "Essential"];
	var cssflag2 = ["Readonly", ""];
	var flagNumber=(flag==true?0:1);
	
	
	//등록일자 readonly 여부
	this.div_Cont.cal_ASK_REGIST_DE.set_readonly(flag);
	this.div_Cont.cal_ASK_REGIST_DE.set_cssclass("cal_WF_"+cssflag[flagNumber]);
	//등록내용 readonly 여부
	this.div_Cont.txt_REGIST_CN.set_readonly(flag);
	this.div_Cont.txt_REGIST_CN.set_cssclass("txt_WF_"+cssflag[flagNumber]);
	//조치일자 readonly 여부
	this.div_Cont.cal_MANAGT_DE.set_readonly(flag);
	this.div_Cont.cal_MANAGT_DE.set_cssclass("cal_WF_"+cssflag2[flagNumber]);
	//조치내용 readonly 여부
	this.div_Cont.txt_MANAGT_CN.set_readonly(flag);
	this.div_Cont.txt_MANAGT_CN.set_cssclass("txt_WF_"+cssflag2[flagNumber]);
	//비고 readonly 여부
	this.div_Cont.txt_RM.set_readonly(flag);
	this.div_Cont.txt_RM.set_cssclass("txt_WF_"+cssflag2[flagNumber]);
}

/*
 * 함수명  	: div_Cont_grd_MNGD_ASK_PORCESS_oncellclick
 * 설명 		: 승인여부는 감사결재자만 수정가능
 * params  		: 
 * return Type 	: None
 * 사용법		: n/a
 */ 
this.div_Cont_grd_MNGD_ASK_PORCESS_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.cell == obj.getCellCount("Body")-1){
		if(this.gfn_authcode("064") < -1){
			//부여된 권한이 없습니다. => 메시지 알림 2016.07.01
			this.gfn_message("info.권한.허가목록.없음");
			return false;
		}else{
			if(Number(this.DS_REQUEST_REGIST.getColumn(e.row, "ASK_SN"))>0){
				this.ds_confmAt.clearData();
				this.ds_confmAt.addRow();
				this.ds_confmAt.copyRow(0, this.DS_REQUEST_REGIST , e.row);
				this.fn_transaction("CONFMAT");
			}
		}
	}
}
