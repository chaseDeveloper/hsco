/*
 * 함수명  	: fn_sanction
 * 설명 		: '결재' 버튼 클릭 후
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_sanction();
 */
this.fn_sanction = function()
{
	if(Ex.util.isUpdated(this.DS_REQUEST_REGIST)){
		this.gfn_message("comm.정보.저장.요청","먼저 수정한 정보를");
	}else{
		switch(this.DS_REQUEST_REGIST.getColumn(this.DS_REQUEST_REGIST.rowposition, "SANCTN_STTUS", "")){
		case "T05":
		case "T07":
		case "T11":
		case "T14":
		case "T15":
			if(this.gfn_message("comm.행위여부","접수 및 처리내용을 수정하고 내부결재 재상신을 진행") ){
				this.DS_REQUEST_REGIST.setColumn(this.DS_REQUEST_REGIST.rowposition, "INNER_SANCTN_NO", "");
				this.fn_setreadonly(false);
				break;
			}else{
				break;
			}
		default:
			var innerSanctnMdlId = "AUD-100068"; // 내부결재모형ID
			var sysSeCode = "SYS";
			var jobSe = "MIS";
			var svcId = "GetInnerSanctnId";
			if(this.ds_recomInfo.getRowCount() ==0) this.ds_recomInfo.addRow();
			

			var key = "ACC100068" + "^" + this.DS_REQUEST_REGIST.getColumn(0, "ASK_SN");
			this.ds_recomInfo.setColumn(0, "JOB_KEY", key);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_MDL_ID", innerSanctnMdlId);
			this.ds_recomInfo.setColumn(0, "INNER_SANCTN_ID", this.DS_REQUEST_REGIST.getColumn(this.DS_REQUEST_REGIST.rowposition, "INNER_SANCTN_NO"));
			
			this.ds_recomInfo.setColumn(0, "SYS_SE_CODE", sysSeCode);
			this.ds_recomInfo.setColumn(0, "JOB_SE", jobSe);
			this.fn_ManageInnerSanction(innerSanctnMdlId, sysSeCode, jobSe, svcId);
		break;
		}
	}
}
/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}else{
		if(strSvcId =="audAdmin")
		{
			this.fn_search();
		}
		if(strSvcId =="RequestlistCUD")
		{ 
			this.fn_transaction("selectRequestlist");
		}
		if(strSvcId == "selectRequestlist")
		{
			//저장트랜잭션 직전 rowposition값으로 이동
			var save_row = this.DS_REQUEST_REGIST.findRow("ASK_SN",save_pk);
			if(save_row != -1)
			{
				this.DS_REQUEST_REGIST.set_rowposition(save_row);
				save_pk = -1;
			}
		}
		//내부결재 트랜잭션 콜백
		if(strSvcId == "GetInnerSanctnId"){
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0, "ASK_SN", this.DS_REQUEST_REGIST.getColumn(this.DS_REQUEST_REGIST.rowposition, "ASK_SN"));
			this.ds_cond02.setColumn(0, "INNER_SANCTN_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
			this.fn_popup_InnerSanctionDrft();
		}
		if(strSvcId == "selectInnerSanctnMasterCheck"){
			if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
			{
				this.fn_transaction("saveInnerSanc");
			}
		}
		if(strSvcId == "saveInnerSanc"){
			this.fn_transaction("incExpElseCashMastrList");
		}
		if (strSvcId == "CONFMAT")
		{
			this.DS_REQUEST_REGIST.set_updatecontrol(false);
			var nConfm = Number(this.DS_REQUEST_REGIST.getColumn(this.DS_REQUEST_REGIST.rowposition, "CONFM_AT"));
			//0 -> 1, 1-> 0
			this.DS_REQUEST_REGIST.setColumn(this.DS_REQUEST_REGIST.rowposition, "CONFM_AT", -(nConfm-1));
			this.DS_REQUEST_REGIST.set_updatecontrol(true);
			this.fn_setreadonly(this.DS_REQUEST_REGIST.getColumn(this.DS_REQUEST_REGIST.rowposition, "CONFM_AT")==1);
		
			//2016.07.01
			this.gfn_message("success.처리.성공");
		}
	}
}

/***********************************************************************
* 조회기간 변경 시
************************************************************************/
this.DS_SEARCH_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	//시작일 변경시
	if(e.columnid == "BGN_ASK_REGIST_DE")
	{
		if(e.newvalue > this.div_SearchArea.cal_END_ASK_DE.value) 
		{
			this.gfn_message("errors.시작일자.종료일자.이하","|");
			return false;
		}
		return true;
	}
	//종료일 변경시
	if(e.columnid == "END_ASK_REGIST_DE")
	{
		if(e.newvalue < this.div_SearchArea.cal_BGN_ASK_DE.value) 
		{
			this.gfn_message("errors.종료일자.시작일자.이상","|");
			return false;
		}
		return true;
	}
}


