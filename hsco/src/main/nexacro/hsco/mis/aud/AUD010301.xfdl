/***********************************************************************************
* 설명	: 상단저장버튼 클릭 시
***********************************************************************************/
this.fn_save = function()
{
	//validation check
	if(!this.gfn_checkValidation(this.ds_requstCtrList, this.ds_validation)){
		return;
	} 
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.ds_requstCtrList.rowposition;
		this.fn_transaction("save");
	}
}

/***********************************************************************************
* 설명	: 상단삭제버튼 클릭 시
***********************************************************************************/
this.fn_delete = function()
{
	if(!this.chargerAt) {
		this.gfn_message("info.처리불가", "삭제 권한이 없습니다. 감사팀에 문의해주십시오.");
		return false;
	}
	if(!this.gfn_message("confirm.삭제여부")) {
		return false;	// 아니오
	}
	
	this.ds_fileList.clearData();
	this.ds_fileListResult.clearData();
	this.ds_requstCtrList.deleteRow(this.ds_requstCtrList.rowposition);
}

/***********************************************************************************
* 설명	: 상단취소버튼 클릭 시
***********************************************************************************/
this.fn_cancel = function()
{
	if(Ex.util.isUpdated(this.ds_requstCtrList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	this.ds_requstCtrList.reset();
	this.ds_requstCtrList.applyChange();
}

