this.ds_requstCtrList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow>-1){
		var empno     = obj.getColumn(e.newrow, "RQESTER_EMPL_NO");
