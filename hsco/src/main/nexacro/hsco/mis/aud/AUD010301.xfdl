this.div_sear_search_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var cRow = this.ds_requstCtrList.rowposition;
	
	 switch(obj.name){
		case "search":			// 요청자
			var parm = {
				 arg_0      : this
			};
			this.gfn_popup("searchEmpno", 0, 0, "", parm, "com::empno_P01.xfdl", "fn_popupCallBack", true);
			break;
		case "searchBsns":		// 사업
			var parm = {
				 arg_0      : this
			};
			this.gfn_popup("popBsns", 0, 0, "", parm, "pms_prj::popBsns.xfdl", "fn_popupCallBack", true);
