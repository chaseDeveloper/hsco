    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 감사관리
 * 02. 화면명   : AUD010301.xfdl
 * 03. 화면설명 : 계약심사를 관리한다.
 * 04. 작성일   : 2015.11.04
 * 05. 작성자   : 이지영
 * 06. 수정이력 : 2015.11.04 최초생성
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */
 /***********************************************************************
 * Script Include 
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
this.chargerAt = false;

/***********************************************************************************
* 설명	: oninit 영역
***********************************************************************************/
this.AUD010301_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * 설명: 폼로딩시 처리
 ************************************************************************/
this.AUD010301_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();   // 조회 데이터셋 초기화	
	this.ds_cond.addRow();      // 조회 데이터셋 행추가
	this.ds_cond.setColumn(0, "REQST_BGNDE", this.gfn_today().substring(0,4) + '0101');
	this.ds_cond.setColumn(0, "REQST_ENDDE", this.gfn_today());
	
	this.div_search.div_dept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	var comboParams = [  ["ds_insttAcctoCl",  "AUD015", 1, ""]
						,["ds_AUD015",  "AUD015", 1, "전체"]
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		// 조회영역의 계약분류
		this.ds_AUD015.filter("CODE > 20 && CODE < 30 || CODE == ''");
		
		// 기관별분류에서 계약관련만 나오도록 필터링
		this.ds_insttAcctoCl.filter("CODE > 20 && CODE < 30");
		
		// 권한그룹 체크
		if(this.gfn_authGrpId("AUD_ADMIN") != -1) {			// 감사담당자
			this.chargerAt = true;
		}
		
		if(this.chargerAt) {
			this.div_search.div_dept.set_enable(true);
		}
		else {
			this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
			this.ds_cond.setColumn(0, "DEPT_NM",   this.gfn_getDeptName());
		}
		
	 	this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
  var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "AUD");
   this.File_UpDownManager.init(fileConfig);	
}

/***********************************************************************
 * 설명: 조회버튼 클릭시 처리
 ************************************************************************/
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_requstCtrList.clearData();
	this.ds_fileList.clearData();
	this.ds_fileListResult.clearData();
	
	this.fn_transaction("selectRequstCtrList");
}

/***********************************************************************
 * 설명: 신규버튼 클릭시 처리
 ************************************************************************/
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_requstCtrList.addRow();
	this.ds_fileList.clearData();
	this.ds_fileListResult.clearData();
	this.ds_requstCtrList.setColumn(nRow, "REQUST_DEPT_CODE", this.gfn_getDeptId());
	this.ds_requstCtrList.setColumn(nRow, "REQUST_DEPT_NM"  , this.gfn_getDeptName());
	this.ds_requstCtrList.setColumn(nRow, "RQESTER_EMPL_NO" , this.fn_getEmpno());
	this.ds_requstCtrList.setColumn(nRow, "RQESTER_EMPL_NM" , this.fn_getEmpnm());
	this.ds_requstCtrList.setColumn(nRow, "REQUST_DE"       , this.gfn_today());
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		case "selectRequstCtrList" :		// 조회(계약심사관리)
			var sController   	= "hsco/mis/aud/AUD010301/selectRequstCtrList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_requstCtrList=output01";
			break;
		case "save" :						// 저장
			var sController   	= "hsco/mis/aud/AUD010301/save.do";
			var sInDatasets   	= "input01=ds_uc input02=ds_requstCtrList:U";
			var sOutDatasets  	= "";
			break;
		case "selectAtchFile":				// 첨부파일 조회
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_requstCtrList.getColumn(this.ds_requstCtrList.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
			break;
		case "selectAtchFileResult":		// 심사결과서 첨부파일 조회
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileListResult";
			var sOutDatasets = "ds_fileListResult=output1";
			break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

/***********************************************************************************
* 설명	: 트랜잭션 콜백함수를 처리한다.
***********************************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}else{
		if (strSvcId == "save")		// 저장
		{
			this.gfn_message("success.처리.성공");
			this.fn_search();
		}
		if (strSvcId == "selectRequstCtrList")		// 조회(계약심사관리)
		{
			//저장트랜잭션 직전 rowposition값으로 이동
			if(save_row != -1)
			{
				this.ds_requstCtrList.set_rowposition(save_row);
				save_row = -1;
			}
			
			var rCnt = this.ds_requstCtrList.rowcount;
			if(rCnt == 0) {
				this.div_work.search.set_visible(false);
			}
		}
		if (strSvcId == "selectAtchFile")
		{
			this.div_work.grd_cmmnAtchmnflResult.setCellProperty("head", 0, "text", "0");
			this.ds_fileListResult.clearData();
			this.ds_fileListResult.addRow();
			this.ds_fileListResult.setColumn(0, "FILE_SN", this.ds_requstCtrList.getColumn(this.ds_requstCtrList.rowposition, "RESULT_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			this.fn_transaction("selectAtchFileResult");
		}
	}
}

