	}
}

/***********************************************************************************
* 설명	: 팝업 콜백
***********************************************************************************/
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	var cRow = this.ds_requstCtrList.rowposition;
	
	if(sPopupId == "searchEmpno")		// 요청자
	{
        var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var empno    = dsTemp.getColumn(0, "EMPNO");
			var empnm    = dsTemp.getColumn(0, "EMPNM");
			var deptCode = dsTemp.getColumn(0, "DEPT_CODE");
			var deptNm   = dsTemp.getColumn(0, "DEPT_NM");
			var telno1   = dsTemp.getColumn(0, "TELNO_1");
			var telno2   = dsTemp.getColumn(0, "TELNO_2");
			var telno    = (!this.comUtils.isNull(telno1)) ? telno1 : telno2;
			
			this.ds_requstCtrList.setColumn(cRow, "RQESTER_EMPL_NO" , empno);		// 요청자사번
			this.ds_requstCtrList.setColumn(cRow, "RQESTER_EMPL_NM" , empnm);		// 요청자명
			this.ds_requstCtrList.setColumn(cRow, "REQUST_DEPT_CODE", deptCode);	// 요청자부서코드
			this.ds_requstCtrList.setColumn(cRow, "REQUST_DEPT_NM"  , deptNm);		// 요청자부서명
			this.ds_requstCtrList.setColumn(cRow, "TELNO"           , telno);		// 요청자전화번호
		}
	}
	else if(sPopupId == "popBsns") {	// 사업
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var bsnsCode = dsTemp.getColumn(0, "BSNS_CODE");
			var bsnsNm   = dsTemp.getColumn(0, "BSNS_NM");
			this.ds_requstCtrList.setColumn(cRow, "BSNS_CODE", bsnsCode);	// 사업코드
			this.ds_requstCtrList.setColumn(cRow, "BSNS_NM",   bsnsNm  );	// 사업명
		}
	}
}

