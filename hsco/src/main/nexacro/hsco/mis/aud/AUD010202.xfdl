this.fn_uploadCallback = function()
{
	if(this.div_Cont.AUD010202.T02.SUB_TAB.tabindex == 0)
	{
		if(this.comUtils.isNull(this.DS_EDAY_AUDIT.getColumn(this.DS_EDAY_AUDIT.rowposition, "ATCH_FILE_3_1")))
		{
			this.DS_EDAY_AUDIT.setColumn(this.DS_EDAY_AUDIT.rowposition, "ATCH_FILE_3_1" ,this.ds_tmp.getColumn(0, "ATCH_FILE"));
		}else{
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_EDAY_AUDIT.getColumn(this.DS_EDAY_AUDIT.rowposition, "ATCH_FILE_3_1"));
			this.fn_transaction("selectAtchFile");
		}
	}else{	
