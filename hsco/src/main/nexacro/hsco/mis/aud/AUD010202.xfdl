this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_tmp.clearData();
	this.ds_tmp.addRow();
	this.ds_tmp.copyRow(0, this.DS_EDAY_AUDIT, this.DS_EDAY_AUDIT.rowposition, "SPLEMNT_SN=SPLEMNT_SN", "EDAY_AUDIT_NO=EDAY_AUDIT_NO","RCEPT_YEAR=RCEPT_YEAR","ATCH_FILE=ATCH_FILE_3_1","ATCH_FILE_2=ATCH_FILE_3_2");
	if(this.div_Cont.AUD010202.T02.SUB_TAB.tabindex == 0)
	{
		//보완통보
		this.fn_autoFileSn(
			 "MIS"
			,"AUD"
			, this.File_UpDownManager
			, this.ds_fileList
			, this.ds_tmp
			, "TBAUD_EDAY_AUDIT_SPLEMNT"
			, "ATCH_FILE"
			, ["SPLEMNT_SN", "EDAY_AUDIT_NO", "RCEPT_YEAR"]
		);
	}else{
		//보완 회신 
		this.fn_autoFileSn(
			 "MIS"
			,"AUD"
			, this.File_UpDownManager
			, this.ds_fileList
			, this.ds_tmp
			, "TBAUD_EDAY_AUDIT_SPLEMNT"
			, "ATCH_FILE_2"
			, ["SPLEMNT_SN", "EDAY_AUDIT_NO", "RCEPT_YEAR"]
		);
	}
}


