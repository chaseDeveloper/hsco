		}else{
			//보완회신 첨부파일 검색
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.DS_EDAY_AUDIT.getColumn(this.DS_EDAY_AUDIT.rowposition, "ATCH_FILE_3_2"));
			this.fn_transaction("selectAtchFile");
		}
	}
}

