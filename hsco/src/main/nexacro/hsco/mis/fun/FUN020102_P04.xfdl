this.btn_Search_onclick = function(obj:Button,  e:nexacro.ClickEventInfo) 
{
	if(Ex.util.isUpdated(this.ds_rewardCditMastr) || Ex.util.isUpdated(this.ds_bndeRegist) || Ex.util.isUpdated(this.ds_scrcmpnWrhousng) || Ex.util.isUpdated(this.ds_indvdlzRegist) || Ex.util.isUpdated(this.DS_OBJT_INFO)) {              //데이터셋의 수정여부 체크
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	
	// 데이터셋 클리어
	this.ds_rewardCditMastr.clearData(); 
	this.ds_bndeRegist.clearData();
	this.ds_scrcmpnWrhousng.clearData();
	this.ds_indvdlzRegist.clearData();
	this.DS_OBJT_INFO.clearData();
	
	
	//변경되지 않았을 경우.
	this.fn_transaction("selectM");
}

this.btn_Insert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var newRow = this.ds_rewardCditMastr.addRow();
	this.ds_rewardCditMastr.setColumn(newRow, "OBJT_AT", "0");
}

this.btn_Delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.gfn_message("confirm.삭제여부")) {
		var nRow = this.ds_rewardCditMastr.rowposition;		// 삭제 전 row 위치
		this.ds_rewardCditMastr.deleteRow(nRow);
		
		var nRow1 = this.ds_rewardCditMastr.rowposition;	// 삭제 후 row 위치
		if(nRow == nRow1){
			this.fn_search_sub();
		}
	}
}

this.btn_Save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_rewardCditMastr) || Ex.util.isUpdated(this.ds_bndeRegist) || Ex.util.isUpdated(this.ds_scrcmpnWrhousng) || Ex.util.isUpdated(this.ds_indvdlzRegist) || Ex.util.isUpdated(this.DS_OBJT_INFO)) {              //데이터셋의 수정여부 체크
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_rewardCditMastr, this.ds_validation)) {
			return false;
		}
		
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_bndeRegist, this.ds_validation)) {
			return false;
		}
		
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_scrcmpnWrhousng, this.ds_validation)) {
			return false;
		}
		
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_indvdlzRegist, this.ds_validation)) {
			return false;
		}
		
		var chk1 = "";
		var chk2 = "";
		var chk3 = "";
		
		for(var i=0; i<this.DS_OBJT_INFO.rowcount; i++) {
			if("1" == this.DS_OBJT_INFO.getColumn(i, "CHK")) {
			
				chk1 = this.comUtils.trimString(this.DS_OBJT_INFO.getColumn(i, "REGIST_CNVRS_DE"));
				chk2 = this.comUtils.trimString(this.DS_OBJT_INFO.getColumn(i, "SCRCMPN_NO"));
				chk3 = this.comUtils.trimString(this.DS_OBJT_INFO.getColumn(i, "REGIST_CNVRS_SE"));
				
				if("" == chk3) {
					this.gfn_message("","전환여부 체크시 전환구분은 필수 입력 항목 입니다.");
					this.div_detail.tab_cdit.tpg2.tab_tpg2.tpg4.grd_locplcInfoList.selectRow(i);
					return;
				}


				if("" == chk1) {
					this.gfn_message("", "전환여부 체크시 전환일자는 필수 입력 항목 입니다.");
					this.div_detail.tab_cdit.tpg2.tab_tpg2.tpg4.grd_locplcInfoList.selectRow(i);
					return;
				}
				
				/*
				if("" == chk2) {
					this.gfn_message("","전환여부 체크시 증권사번호는 필수 입력 항목 입니다.");
					this.div_detail.tab_cdit.tpg2.tab_tpg2.tpg4.grd_locplcInfoList.selectRow(i);
					return;
				}
				*/
				
			}
		}
		
		var payAmount = 0;
		
		if (this.ds_rewardCditMastr.getColumn(this.ds_rewardCditMastr.rowposition, "OBJT_AT") == "1") {
			this.DS_OBJT_INFO.filter("CHK == 0");
			payAmount += this.DS_OBJT_INFO.getSum("UNDTAKE_AMOUNT");
			this.DS_OBJT_INFO.filter("");
		} else {
			payAmount += this.ds_bndeRegist.getSum("UNDTAKE_AMOUNT");
			payAmount += this.ds_scrcmpnWrhousng.getSum("UNDTAKE_AMOUNT");
			payAmount += this.ds_indvdlzRegist.getSum("UNDTAKE_AMOUNT");
		}
		
		this.ds_rewardCditMastr.setColumn(this.ds_rewardCditMastr.rowposition, "PAY_AMOUNT", payAmount);
		
		this.saveRow = this.ds_rewardCditMastr.rowposition;
		this.fn_transaction("save");
	} else {	
	    this.gfn_message("comm.데이터.변경.없음");
	}	
}

this.btn_Cancle_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_rewardCditMastr.reset();		                             //DATASET ROLLBACK
		this.ds_rewardCditMastr.applyChange();	                             //DATASET COMMIT
	}
}

this.btn_Confirm_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var ds_Temp = new Dataset();
	ds_Temp.copyData(this.ds_rewardCditMastr);
	ds_Temp.clearData();
	var nRow = ds_Temp.addRow();
	ds_Temp.copyRow(nRow, this.ds_rewardCditMastr, this.ds_rewardCditMastr.rowposition);

	this.close(ds_Temp.saveXML());
}

this.btn_Close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}

/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(인수내역)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	var nRow = this.ds_rewardCditMastr.rowposition;
	var itemCd = this.ds_rewardCditMastr.getColumn(nRow, "ITEM_CODE");
	var itemNm = this.ds_rewardCditMastr.getColumn(nRow, "ITEM_NM");
	var objtAt = this.ds_rewardCditMastr.getColumn(nRow, "OBJT_AT");
	
	this.ds_cond_sub.clearData();
	this.ds_cond_sub.addRow();
	this.ds_cond_sub.setColumn(0, "ITEM_CODE", itemCd);
	this.ds_cond_sub.setColumn(0, "ITEM_NM", itemNm);
	this.ds_cond_sub.setColumn(0, "OBJT_AT", objtAt);
	this.ds_cond_sub.setColumn(0, "PBNCD_NO", this.parent.pbncdNo);
	
	this.fn_transaction("selectS");
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);
}


/*
 * 함수명  	: fnnc_instt_code
 * 설명 		: 금융기관코드조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fnnc_instt_code();
 */
this.fnnc_instt_code = function()
{
	this.fn_transaction("selectFnncInsttCodeList");
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectM":			// 마스터조회
		
			// 데이터셋 클리어
			this.ds_rewardCditMastr.clearData(); 
			this.ds_bndeRegist.clearData();
			this.ds_scrcmpnWrhousng.clearData();
			this.ds_indvdlzRegist.clearData();
			this.DS_OBJT_INFO.clearData();
			
			var sController   	= "hsco/mis/fun/FUN020501/selectM.do";    
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_rewardCditMastr=output1";
			break;
		case "selectS":	   		// 일괄등록, 증권회사입고신청, 개별등록 조회
			var sController   	= "hsco/mis/fun/FUN020501/selectS.do";   
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_bndeRegist=output1 ds_scrcmpnWrhousng=output2 ds_indvdlzRegist=output3 DS_OBJT_INFO=output4";
			break;
		case "save":	        // 저장               
			var sController   	= "hsco/mis/fun/FUN020501/save.do";
			var sInDatasets   	= "input1=ds_rewardCditMastr:U input2=ds_bndeRegist:U input3=ds_scrcmpnWrhousng:U input4=ds_indvdlzRegist:U input5=DS_OBJT_INFO:U";
			var sOutDatasets  	= "";
			break;		
		case "selectFnncInsttCodeList":	   // 금융기관코드 조회
			var sController   	= "hsco/mis/fun/FUN010201/selectFnncInsttCodeList.do";   
			var sInDatasets   	= "";
			var sOutDatasets  	= "DS_FNNC_INSTT_CODE=output1";
			break;
	}

	switch (strSvcId) {
		case "selectFnncInsttCodeList":
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", "", false, false);
			break;
		default :
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
			break;	
	}
}


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{	
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectM":			// 예금계좌 조회
				break;
			case "save":		    // 예금계좌 저장
			    this.gfn_message("success.처리.성공");
			    
				this.fn_transaction("selectS");
				break;
			case "selectS":		// 금융기관코드 조회
				break;
			case "selectFnncInsttCodeList": // 금융기관코드 조회
				this.DS_FNNC_INSTT_CODE.insertRow(0);
				
				this.DS_FNNC_INSTT_CODE.setColumn(0, "CODE", "");
				this.DS_FNNC_INSTT_CODE.setColumn(0, "CODE_NM", "선택");
				break;	
		}
	}
}

