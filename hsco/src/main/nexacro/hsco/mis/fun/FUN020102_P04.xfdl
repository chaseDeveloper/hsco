/*
 * 함수명  	: ds_rewardCditMastr_onrowposchanged
 * 설명 		: 채권등록발행내역 데이터셋에서 row 변경 후 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_rewardCditMastr_onrowposchanged(obj,e);
 */
this.ds_rewardCditMastr_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var amt1 = this.ds_rewardCditMastr.getColumn(e.newrow, "PBLICTE_AMOUNT");
	var amt2 = this.ds_rewardCditMastr.getColumn(e.newrow, "PAY_AMOUNT");
	
	if(!this.comUtils.isNull(amt1) && !this.comUtils.isNull(amt2)){
		var rate = nexacro.round((amt2/amt1)*100, 2);
		this.div_detail.tab_cdit.tpg1.msk_pblicteRate.set_value(rate);
	} else {
		this.div_detail.tab_cdit.tpg1.msk_pblicteRate.set_value(0);
	}
	
	this.fn_objt(this.ds_rewardCditMastr.getColumn(e.newrow, "OBJT_AT"));
	
	if(this.ds_rewardCditMastr.getRowType(e.newrow) == 2) {
		this.div_detail.tab_cdit.tpg1.chk_objtAt.set_readonly(false);
	} else {
		this.div_detail.tab_cdit.tpg1.chk_objtAt.set_readonly(true);
	}
	
	this.fn_search_sub();
}

/*
 * 함수명  	: btn_Add_R_onclick
 * 설명 		: 행추가 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Add_R_onclick();
 */
this.btn_Add_R_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRowM = this.ds_rewardCditMastr.rowposition;
	var rowType = this.ds_rewardCditMastr.getRowType(nRowM);
	
	if(rowType == 2){
		this.gfn_message("info.진행가능", "신규 내용을 저장");
		return;
	}
	
	var itemCd = this.ds_rewardCditMastr.getColumn(nRowM, "ITEM_CODE");
	var objtAt = this.ds_rewardCditMastr.getColumn(nRowM, "OBJT_AT");
		
	switch(obj.name){
		case "btn_Add01" :
			var nRow = this.ds_bndeRegist.addRow();
			
			this.ds_bndeRegist.setColumn(nRow, "ITEM_CODE", itemCd);
			this.ds_bndeRegist.setColumn(nRow, "OBJT_AT", objtAt);
			break;
		case "btn_Add02" :
			var nRow = this.ds_scrcmpnWrhousng.addRow();
			
			this.ds_scrcmpnWrhousng.setColumn(nRow, "ITEM_CODE", itemCd);
			this.ds_scrcmpnWrhousng.setColumn(nRow, "OBJT_AT", objtAt);
			break;
		case "btn_Add03" :
			var nRow = this.ds_indvdlzRegist.addRow();
			
			this.ds_indvdlzRegist.setColumn(nRow, "ITEM_CODE", itemCd);
			this.ds_indvdlzRegist.setColumn(nRow, "OBJT_AT", objtAt);
			break;

		case "btn_Add04" :
			var nRow = this.DS_OBJT_INFO.addRow();

			this.DS_OBJT_INFO.setColumn(nRow, "UNDTAKE_DE", this.dateUtils.today());
			this.DS_OBJT_INFO.setColumn(nRow, "ITEM_CODE",itemCd);
			this.DS_OBJT_INFO.setColumn(nRow, "OBJT_AT", objtAt);			
			this.DS_OBJT_INFO.setColumn(nRow, "SEQ", 0);
			this.DS_OBJT_INFO.setColumn(nRow, "CHK", "0");
			break;			
	}
}

