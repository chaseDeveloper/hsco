/*
 * 함수명  	: DS_PBNCD_oncolumnchanged
 * 설명 		: 공사채 데이터셋에서 column이 변경된 후 실행
 * params  		: None
 * return Type 	: None
 * 사용법		: this.DS_PBNCD_oncolumnchanged(obj,e);
 */
this.DS_PBNCD_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "INRST_SE" || e.columnid == "CMPNINT_SE") {
		var strCmpnint = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "CMPNINT_SE");
		var strInrst   = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "INRST_SE");
		
		if(strCmpnint == "1" && strInrst == "1") {
			//this.tab_pbncd.tabpage1.cmb_fixingInrstSe.set_readonly(false);
			//this.tab_pbncd.tabpage1.cmb_fixingInrstSe.set_cssclass("cmb_WF_Essential");
		} else {
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition, "FIXING_INRST_SE", null);
			//this.tab_pbncd.tabpage1.cmb_fixingInrstSe.set_readonly(true);
			//this.tab_pbncd.tabpage1.cmb_fixingInrstSe.set_cssclass("cmb_WF_Readonly");
		}
	}
	
	if(e.columnid == "BRRW_DE" || e.columnid == "EXPRTN_DE") {
		var calc = this.fn_monthbetween(obj.getColumn(e.row,  "BRRW_DE"), obj.getColumn(e.row,  "EXPRTN_DE"));
		obj.setColumn(e.row, "RPAY_PD", calc);
	}
}

this.fn_monthbetween = function(sFormDt,sEndDt)
{
    sFormDt 	= parseInt(sFormDt.substr(0, 4), 10) * 12  + parseInt(sFormDt.substr(4, 2), 10);
    sEndDt 		= parseInt(sEndDt.substr(0, 4), 10) * 12  + parseInt(sEndDt.substr(4, 2), 10);
    var Term    = Math.abs(sEndDt - sFormDt);
    return Term;
}


