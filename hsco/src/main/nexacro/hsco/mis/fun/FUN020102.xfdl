this.div_detail_tab_rewardBsnsDstrc_tabpage2_btn_save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var chk1 = "";
	var chk2 = "";
	
	for(var i=0; i<this.DS_OBJT_INFO.rowcount; i++) {
		if("1" == this.DS_OBJT_INFO.getColumn(i, "CHK")) {
		
			chk1 = this.comUtils.trimString(this.DS_OBJT_INFO.getColumn(i, "REGIST_CNVRS_DE"));
			chk2 = this.comUtils.trimString(this.DS_OBJT_INFO.getColumn(i, "SCRCMPN_NO"));
			
			if("" == chk1) {
				this.gfn_message("", "전환여부 체크시 전환일자는 필수 입력 항목 입니다.");
				this.tab_pbncd.tabpage2.grd_locplcInfoList.selectRow(i);
				return;
			}

			if("" == chk2) {
				this.gfn_message("","전환여부 체크시 증권사번호는 필수 입력 항목 입니다.");
				this.tab_pbncd.tabpage2.grd_locplcInfoList.selectRow(i);
