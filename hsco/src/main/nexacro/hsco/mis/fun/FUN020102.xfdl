/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
    if(this.DS_PBNCD_canrowposchange())
    {
		var nRow = this.DS_PBNCD.addRow();
		this.DS_PBNCD.setColumn(nRow, "EXPRTN_AT", "0");
		this.DS_PBNCD.setColumn(nRow, "BRRW_DE",   this.dateUtils.today());
		this.DS_PBNCD.setColumn(nRow, "EXPRTN_DE", this.dateUtils.today());
    }
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	var exprtnAt = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "EXPRTN_AT");
	if(exprtnAt == "1"){
		this.gfn_message("fail.삭제.불가", "만기처리된 공사채 관련정보는");
	} else {
		if (this.gfn_message("confirm.삭제여부")) {
			var nRow = this.DS_PBNCD.rowposition;
			this.DS_PBNCD.deleteRow(nRow);
			this.DS_PBNCD_onrowposchanged();
		}
	}	
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if ( this.gfn_message("confirm.취소여부") ) {
		this.DS_PBNCD.reset();		                                 //DATASET ROLLBACK
		this.DS_PBNCD.applyChange();	                             //DATASET COMMIT
		return;
	}
}


