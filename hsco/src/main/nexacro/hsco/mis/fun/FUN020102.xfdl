    <Script type="xscript5.0"><![CDATA[/**********************************************************************
* 화면ID   : FUN020102.xfdl
* 화면명   : 공사채등록
* 화면설명 : 공사채 테이블을 관리 한다.
* 작성일   : 2015.07.14
* 작성자   : 양성모
* 수정이력 : 
***********************************************************************
*     수정일     수정자   내용
***********************************************************************
*
***********************************************************************
*/

/**********************************************************************
* Script Include 
***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comLib.xjs";
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

this.saveRow = -1;
//this.checkDs = [this.DS_PBNCD];
/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.FUN020102_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.FUN020102_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// 공통코드 조회
	var comboParams = [
		["ds_fun013", "FUN013", "Y", "S"]	// 채권이자지급형태
	   ,["ds_fun014", "FUN014", "Y", "S"]	// 채권이자변동여부
	   ,["ds_fun015", "FUN015", "Y", "S"]	// 채권원금상환방법
	   ,["ds_fun032", "FUN032", "Y", "S"]	// 신용평가등급
	];
	var callback = function (svcId, errorCode, errorMsg) {
		//this.fn_search();
		this.fnnc_instt_code(); //금융기관 코드 조회
	};
	this.gfn_comboLoad (comboParams, callback);		
	
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.DS_OBJT_INFO.clearData();
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction)
* 
***********************************************************************/

/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(공사채현황)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.DS_PBNCD.clearData();
	
	//변경되지 않았을 경우.
	this.fn_transaction("selectPbncdList");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	var exprtnAt = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "EXPRTN_AT");
	if(exprtnAt == "1"){
		this.gfn_message("fail.수정.불가", "만기처리된 공사채 관련정보는");
	} else {
		if(Ex.util.isUpdated(this.DS_PBNCD)) {                           //데이터셋의 수정여부 체크
			// validation 체크
			if(!this.gfn_checkValidation_NoUpdChk(this.DS_PBNCD, this.ds_validation)) {
				return false;
			}
			
			if(this.DS_PBNCD_canrowposchange()) {
				this.saveRow = this.DS_PBNCD.rowposition;
				this.fn_transaction("savePbncdList");
			}
		} else {
			this.gfn_message("comm.데이터.변경.없음");	
		}
	}
}

/*
 * 함수명  	: fnnc_instt_code
 * 설명 		: 금융기관코드조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fnnc_instt_code();
 */
this.fnnc_instt_code = function()
{
	this.fn_transaction("selectFnncInsttCodeList");
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectPbncdList":		       // 공사채 조회
			var sController   	= "hsco/mis/fun/FUN020102/selectPbncdList.do";   
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "DS_PBNCD=output1";
			break;
		case "savePbncdList":	           // 공사채 저장               
			var sController   	= "hsco/mis/fun/FUN020102/PbncdCUD.do";				
			var sInDatasets   	= "input1=DS_PBNCD:U";
			var sOutDatasets  	= "";
			break;
		case "selectFnncInsttCodeList":	   // 금융기관코드 조회
			var sController   	= "hsco/mis/fun/FUN000000/selectFnncInsttCodeList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "DS_FNNC_INSTT_CODE=output1";
			break;
		case "selectObjtInfo":
			var sController   	= "hsco/mis/fun/FUN020102/selectObjtInfo.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "DS_OBJT_INFO=output1";
			break;
		case "ObjtInfoCUD":
			var sController   	= "hsco/mis/fun/FUN020102/ObjtInfoCUD.do";
			var sInDatasets   	= "input1=DS_OBJT_INFO:U";
			var sOutDatasets  	= "DS_OBJT_INFO=output1";
			break;
	}
	
	switch (strSvcId) {
		case "selectFnncInsttCodeList":
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", "", false, false);
			break;
		default :
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
			break;	
	}
}


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectPbncdList":			// 공사채 조회
				this.DS_PBNCD.applyChange();
				
				if(this.saveRow > -1){
					this.DS_PBNCD.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "savePbncdList":		    // 공사채 저장
				//this.gfn_message(strErrorMsg);
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectFnncInsttCodeList": // 금융기관코드 조회
				this.fn_search();    // 초기 데이터 조회
				break;	
			case "ObjtInfoCUD":
				//this.gfn_message(strErrorMsg);
				this.gfn_message("success.처리.성공");
				break;
			case "selectObjtInfo":
				break;
		}
	}
}

 /*
 * 함수명  	: fn_PopBsnsCallback
 * 설명 		: 팝업 콜백 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                sReturn  : 팝업에서 넘어오는 데이터
 * return Type 	: None
 * 사용법		: this.fn_PopBsnsCallback();
 */
this.fn_PopCallback = function(sPopupId, sReturn)
{
	if(sPopupId == "btn_CPTAL_KND") {
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(nRowCnt){
			this.ds_cond02.setColumn(0, "CPTAL_KND", dsRtn.getColumn(0,"CODE"));
			this.ds_cond02.setColumn(0, "CPTAL_KND_NM", dsRtn.getColumn(0,"CODE_NM"));
		}
	}
	else if(sPopupId == "popBsns"){ 
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		
		if(loadCnt) {
			var bsnsCode = dsTemp.getColumn(0, "BSNS_CODE");
			var bsnsNm   = dsTemp.getColumn(0, "BSNS_NM");
			
			this.ds_cond02.setColumn(0, "BSNS_CODE", bsnsCode);	// 사업코드
			this.ds_cond02.setColumn(0, "BSNS_NM",   bsnsNm  );	// 사업명
		}
	}
    else if(sPopupId == "fun020101_p01") {		
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(!nRowCnt){
			
		} else {
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition ,"CONFM_NO"      ,dsRtn.getColumn(0,"CONFM_NO"));			// 승인번호
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition ,"BSNS_CODE"     ,dsRtn.getColumn(0,"BSNS_CODE"));		// 사업코드
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition ,"BSNS_NM"       ,dsRtn.getColumn(0,"BSNS_NM"));			// 사업명
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition ,"BOD_CONFM_DE"  ,dsRtn.getColumn(0,"BOD_CONFM_DE"));		// 이사회
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition ,"GVRN_CONFM_DE" ,dsRtn.getColumn(0,"GVRN_CONFM_DE"));	// 정부
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition ,"CITY_CONFM_DE" ,dsRtn.getColumn(0,"CITY_CONFM_DE"));	// 시장
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition ,"CONFM_AMOUNT"  ,dsRtn.getColumn(0,"CONFM_AMOUNT"));		// 승인금액
		}
	}
	else if(sPopupId == "fun020101_p02") {
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(nRowCnt){
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition, "CPTAL_KND", dsRtn.getColumn(0,"CODE"));
			this.DS_PBNCD.setColumn(this.DS_PBNCD.rowposition, "CPTAL_KND_NM"  , dsRtn.getColumn(0,"CODE_NM"));
		}	
	}
	else if(sPopupId == "fun020102_p05") {
		if(null != sReturn) {
			var dsRtn = new Dataset();
			var nRowCnt = dsRtn.loadXML(sReturn);	
			var rowposition = this.DS_PBNCD.rowposition;
			
			if(0 < this.DS_PBNCD.getColumn(rowposition, "CNT")) {
				this.gfn_message("info.처리불가", "전환등록건이 있습니다.\n전환등록건 삭제 후 KR번호를 변경 할 수 있습니다.");
				return true;
			}
			else {
				if(nRowCnt){
					if(this.gfn_message("comm.행위여부", "기본사항 정보를 선택한 채권정보로 변경")) {
						this.DS_PBNCD.setColumn(rowposition, "KR_NO"      , dsRtn.getColumn(0,"ITEM_CODE"));
						this.DS_PBNCD.setColumn(rowposition, "BRWOFC"             , dsRtn.getColumn(0,"MNGT_CMPNY"));  //차입처/주관사 | 기타 > 주관회사
						
						var temp1 = this.DS_FNNC_INSTT_CODE.getColumn(this.DS_FNNC_INSTT_CODE.findRow("CODE", dsRtn.getColumn(0,"PNINT_FNNC_INSTT_SPOT_CODE")), "CODE_NM");	// 원리금 > 원리금지급장소
						if(!this.comUtils.isNull(temp1)){
							temp1 = temp1 + " " + dsRtn.getColumn(0,"PNINT_FNNC_INSTT_SPOT_NM");
						}
