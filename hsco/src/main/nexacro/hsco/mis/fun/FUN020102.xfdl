/*
 * 함수명  	: DS_PBNCD_canrowposchange
 * 설명 		: 공사채 데이터셋에서 row가 변경되기 전 실행
 * params  		: None
 * return Type 	: None
 * 사용법		: this.DS_PBNCD_canrowposchange(obj,e);
 */
this.DS_PBNCD_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var nRow = this.DS_PBNCD.rowposition;
	
    if(nRow > -1)   // 데이터셋의 크기가 1보다 클 때
    {
		var confmNo = this.DS_PBNCD.getColumn(nRow,"CONFM_NO");
		var confmAmount = this.DS_PBNCD.getColumn(nRow,"CONFM_AMOUNT");                       // 승인금액
		var brrwAmountTot = this.DS_PBNCD.getCaseSum("CONFM_NO =="+confmNo,"BRRW_AMOUNT");    // 승인번호별 총 차입금액
		var brrwAmount = this.DS_PBNCD.getColumn(nRow,"BRRW_AMOUNT");						  // 현재 공사채의 차입금액
		var strCmpnint = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "CMPNINT_SE");
		var strInrst   = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "INRST_SE");				
		var intrCycle   = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "INTR_CYCLE");
		
		if(!this.comUtils.isNull(brrwAmount)){
			if(this.comUtils.isNull(confmNo))
			{
				this.gfn_message("info.항목.선택.알림", "차입금액을 입력 하시려면, 승인번호");
				return false;
			}
		}
		
		// 2016.10.17 박세현 수정. 기등록된 건은 체크하지 않음
		if(!this.comUtils.isNull(confmAmount) && confmNo.substr(0, 4) > '2014') {
			if((confmAmount-brrwAmountTot) >= 0)
			{
				
			} else {
				this.gfn_message("errors.항목값.범위.이하", "총 차입금액|승인금액");
				return false;
			}
		}
		
		if(!this.comUtils.isNull(intrCycle)){
			if(intrCycle < "1"){
				this.gfn_message("info.항목.건수.이상", "이자주기는 1개월");
				return false;	
			}
		}
		
		if(strCmpnint == "1" && strInrst == "1") {
			var strFixing   = this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "FIXING_INRST_SE");
			
			if(!strFixing) {
				this.gfn_message("info.항목.선택.알림", "고정금리구분");
				return false;
			} else {
				return true;
			}
		} else {
			return true;
		}
    } else {
		return true;
    }	
}

