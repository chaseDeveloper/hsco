this.tab_pbncd_tabpage2_btn_excelUpload_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_import.clearData();
	
	var arg = {
		targetDs : this.ds_import,
		headSet : false,
		callBack : function () {
			this.setWaitCursor(true,true);
			
			var nRow = 0;

			for(var x=1 ; x <= this.ds_import.rowcount ; x++)
			{
				if(null == this.ds_import.getColumn(x, "Column0")) {
					continue;
				}
				else if("" == this.comUtils.trimString(this.ds_import.getColumn(x, "Column0"))) {
					continue;
				}
				else {
					nRow = this.DS_OBJT_INFO.addRow();
					this.DS_OBJT_INFO.copyRow(nRow, this.ds_import, x, 
					'UNDTAKE_DE=Column0,'
					+'ACPTR=Column1,'
					+'UNDTAKE_AMOUNT=Column2,'
					+'OBJT_RM=Column3'
					);

					this.DS_OBJT_INFO.setColumn(nRow, "PBNCD_NO", this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "PBNCD_NO"));
					this.DS_OBJT_INFO.setColumn(nRow, "ITEM_CODE","");
					this.DS_OBJT_INFO.setColumn(nRow, "SEQ", 0);
					this.DS_OBJT_INFO.setColumn(nRow, "SN", 0);
					this.DS_OBJT_INFO.setColumn(nRow, "CHK", "0");
				}
			}
			
			this.setWaitCursor(false,true);
		}
	};
	
	this.gfn_importExcel(arg);
}

