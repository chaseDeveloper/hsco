	}
}

this.DS_OBJT_INFO_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var nRow = this.DS_OBJT_INFO.rowposition;
	
	//trace(e.columnid+","+e.newvalue+","+this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "KR_NO"));

	if("CHK" == e.columnid) {
		if("1" == e.newvalue) {
			var krNo = this.comUtils.trimString(this.DS_PBNCD.getColumn(this.DS_PBNCD.rowposition, "KR_NO"));
			if("" == krNo) {
				this.gfn_message("","KR/계좌번호 는 필수 항목입니다.\nKR/계좌번호를 등록 후 진행하시기 바랍니다.");
				return false;
			}
			else {
				obj.setColumn(e.row, "CHK", e.newvalue);
				obj.setColumn(e.row, "ITEM_CODE", krNo);
			}
		}
	}
}

