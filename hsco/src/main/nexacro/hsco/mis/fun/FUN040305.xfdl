/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function() {	
	var row = this.ds_mbnmCptal.rowposition;
	
	if (row < 0) {
		this.gfn_message("comm.데이터.선택.없음");
		return;
	} else {
	
		var dscnAt = this.ds_mbnmCptal.getColumn(this.ds_mbnmCptal.rowposition, "DSCN_AT");
		
		if (dscnAt == "1") {
			this.gfn_message("fail.실행.불가", "승인된 내역은 삭제");
		} else {
			// transaction
			if(this.gfn_message("confirm.삭제여부"))	{
				this.ds_mbnmCptal.deleteRow(row);
				this.fn_transaction("MvmnCptalCUD");
			}
		}
	}
}

