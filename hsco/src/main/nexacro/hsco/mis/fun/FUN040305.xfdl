this.fn_initMonth = function(strDate)
{
	var strYear = null;
	var strMon = null;

	if(this.comUtils.isNullEmpty(strDate)){
		var strToday = this.dateUtils.today();
		strYear = strToday.substr(0, 4);
		strMon = strToday.substr(4, 2);
	}else{
		strYear = strDate.toString().substr(0, 4);
		strMon = strDate.toString().substr(4, 2);
	}
	
	this.objCal.sta_year.set_text(strYear);

	switch(strMon) {
		case "01" :
			this.objCal.btn_jan.setFocus(true);
			break;
		case "02" :
			this.objCal.btn_feb.setFocus(true);
			break;
		case "03" :
			this.objCal.btn_mar.setFocus(true);
			break;
		case "04" :
			this.objCal.btn_apr.setFocus(true);
			break;
		case "05" :
			this.objCal.btn_may.setFocus(true);
			break;
		case "06" :
			this.objCal.btn_jun.setFocus(true);
			break;
		case "07" :
			this.objCal.btn_jul.setFocus(true);
			break;
		case "08" :
			this.objCal.btn_aug.setFocus(true);
			break;
		case "09" :
			this.objCal.btn_sep.setFocus(true);
			break;
		case "10" :
			this.objCal.btn_oct.setFocus(true);
			break;
		case "11" :
			this.objCal.btn_nov.setFocus(true);
			break;
		case "12" :
			this.objCal.btn_dec.setFocus(true);
			break;
	}
}
