	// validation 체크(편성예산, 산출기초)
	if(!Ex.util.isUpdated(this.ds_mbnmCptal)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	var chkGbn = true;
	for(var i=0; i<this.ds_mbnmCptal.rowcount; i++) {
		if(this.ds_mbnmCptal.getColumn(i, "DSCN_AT") == "1" && this.ds_mbnmCptal.getColumn(i, "DSCN_AT") != this.ds_mbnmCptal.getColumn(i, "CHK")) {
			chkGbn = false;
			break;
		}
	}
	
	if(chkGbn) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// transaction
	if(this.gfn_message("confirm.승인취소.여부"))	{
		this.fn_transaction("updateDscnAt");
