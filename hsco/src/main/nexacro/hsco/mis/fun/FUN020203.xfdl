{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction)
* 
***********************************************************************/

/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(공사채 기본정보)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}	
	
	// 데이터셋 클리어
	this.ds_pbncd01.clearData();
	this.ds_pbncd02.clearData();
	this.ds_pbncd03.clearData();
	this.ds_pbncd04.clearData();

	
	this.fn_transaction("selectPbncd01");
	
	
	/*
	if(this.div_work.tab_pbncd.tabindex == 0){
		this.fn_transaction("selectPbncd01");
	}
	
	if(this.div_work.tab_pbncd.tabindex == 1){
		this.fn_transaction("selectPbncd02");
	}
	
	if(this.div_work.tab_pbncd.tabindex == 2){
		this.fn_transaction("selectPbncd03");
	}
	
	if(this.div_work.tab_pbncd.tabindex == 3){
		this.fn_transaction("selectPbncd04");
	}
	*/
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectPbncd01":   // 사업별명세 조회
			var sController   	= "hsco/mis/fun/FUN020203/selectPbncdList01.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_pbncd01=output1";
			break;
		case "selectPbncd02":   //
			var sController   	= "hsco/mis/fun/FUN020203/selectPbncdList02.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_pbncd02=output1";
			break;
		case "selectPbncd03":   //
			var year = Number(this.ds_cond.getColumn(0, "SEARCH_DE").substr(0,4));
	
			this.div_work.tab_pbncd.tpg3.grd_kgaap.setCellProperty('Head', 2, 'text', year + "년(당기 재무상태표)");
		
			var sController   	= "hsco/mis/fun/FUN020203/selectPbncdList03.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_pbncd03=output1";
			break;
		case "selectPbncd04":	// 
			var sController   	= "hsco/mis/fun/FUN020203/selectPbncdList04.do";  
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_pbncd04=output1";
			break;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectPbncd01":	
				this.fn_transaction("selectPbncd02");
				break;
			case "selectPbncd02":	
				this.fn_transaction("selectPbncd03");
				break;
			case "selectPbncd03":	
				this.fn_transaction("selectPbncd04");
				break;
			case "selectPbncd04":
				break;
		}
	}
}

