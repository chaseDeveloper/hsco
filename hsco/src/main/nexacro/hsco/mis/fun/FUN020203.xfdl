***********************************************************************/

this.FUN020203_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.FUN020203_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.ds_cond.setColumn(0, "SEARCH_DE", this.dateUtils.today());
	this.ds_cond.setColumn(0, "SEARCH_GBN", "1");
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
