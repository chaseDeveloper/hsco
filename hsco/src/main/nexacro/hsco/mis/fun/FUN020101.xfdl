	} else {
		this.gfn_popup("Search", 500, 465, "", parm, "mis_acc::ACC000000_P04.xfdl", "fn_PopBsnsCallback", true);
	}	
}


this.div_search_edt_BSNS_CODE_onchanged = function(obj:Edit, e:nexacro.ChangeEventInfo)
{
