/*
 * 함수명  	: btn_EXCEL_IMP_onclick
 * 설명 		: 엑셀 업로드 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_EXCEL_IMP_onclick();
 */
this.btn_EXCEL_IMP_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRowM = this.ds_rewardCditMastr.rowposition;
	var rowType = this.ds_rewardCditMastr.getRowType(nRowM);
	
	if(rowType == 2){
		this.gfn_message("info.진행가능", "신규 내용을 저장");
		return;
	}
	
	var itemCd = this.ds_rewardCditMastr.getColumn(nRowM, "ITEM_CODE");
	var itemNm = this.ds_rewardCditMastr.getColumn(nRowM, "ITEM_NM");
	var objtAt = this.ds_rewardCditMastr.getColumn(nRowM, "OBJT_AT");
	
	this.ds_import.clearData();
	var arg = {
		targetDs : this.ds_import,
		headSet : false,
		callBack : function () {
			if(this.ds_import.rowcount > 1){
				var nRow;
				
				switch(obj.name){
					case "btn_excelImpot01" :
						for(i=1; i< this.ds_import.rowcount; i++) {
							nRow = this.ds_bndeRegist.addRow();
							this.ds_bndeRegist.setColumn(nRow, "ITEM_CODE",      itemCd);
							this.ds_bndeRegist.setColumn(nRow, "OBJT_AT", objtAt);
							this.ds_bndeRegist.setColumn(nRow, "ACNUTNO",        this.ds_import.getColumn(i, "Column0"));
							this.ds_bndeRegist.setColumn(nRow, "DPSTG_MAN_NM",   this.ds_import.getColumn(i, "Column1"));
							this.ds_bndeRegist.setColumn(nRow, "UNDTAKE_AMOUNT", this.ds_import.getColumn(i, "Column2"));
							this.ds_bndeRegist.setColumn(nRow, "FUND_CODE",      this.ds_import.getColumn(i, "Column3"));
						}
						break;
					case "btn_excelImpot02" :
						for(i=1; i< this.ds_import.rowcount; i++) {
							nRow = this.ds_scrcmpnWrhousng.addRow();
							this.ds_scrcmpnWrhousng.setColumn(nRow, "ITEM_CODE",      itemCd);
							this.ds_scrcmpnWrhousng.setColumn(nRow, "OBJT_AT", objtAt);
							this.ds_scrcmpnWrhousng.setColumn(nRow, "ACPTR",          this.ds_import.getColumn(i, "Column0"));
							this.ds_scrcmpnWrhousng.setColumn(nRow, "LSFT_NO",        this.ds_import.getColumn(i, "Column1"));
							this.ds_scrcmpnWrhousng.setColumn(nRow, "UNDTAKE_AMOUNT", this.ds_import.getColumn(i, "Column2"));
							this.ds_scrcmpnWrhousng.setColumn(nRow, "SCRCMPN_NO",     this.ds_import.getColumn(i, "Column3"));
							this.ds_scrcmpnWrhousng.setColumn(nRow, "SCRCMPN_NM",     this.ds_import.getColumn(i, "Column4"));
							this.ds_scrcmpnWrhousng.setColumn(nRow, "CNSGN_ACNUTNO",  this.ds_import.getColumn(i, "Column5"));
						}
						break;
					case "btn_excelImpot03" :
						for(i=1; i< this.ds_import.rowcount; i++) {
							nRow = this.ds_indvdlzRegist.addRow();
							this.ds_indvdlzRegist.setColumn(nRow, "ITEM_CODE",      itemCd);
							this.ds_indvdlzRegist.setColumn(nRow, "OBJT_AT", objtAt);
							this.ds_indvdlzRegist.setColumn(nRow, "ACPTR",          this.ds_import.getColumn(i, "Column0"));
							this.ds_indvdlzRegist.setColumn(nRow, "LSFT_NO",        this.ds_import.getColumn(i, "Column1"));
							this.ds_indvdlzRegist.setColumn(nRow, "UNDTAKE_AMOUNT", this.ds_import.getColumn(i, "Column2"));
						}
						break;
				}				
			}
		}
	};
	
	this.gfn_importExcel(arg);
}

