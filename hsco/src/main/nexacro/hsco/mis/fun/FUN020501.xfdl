/*
 * 함수명  	: btn_Cancel_R_onclick
 * 설명 		: 행취소 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Cancel_R_onclick();
 */
this.btn_Cancel_R_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.gfn_message("confirm.취소여부")) {
