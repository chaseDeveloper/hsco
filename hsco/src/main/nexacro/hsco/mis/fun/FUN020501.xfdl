}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다}
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);
}


/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction)
* 
***********************************************************************/

/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/
/*
 * 함수명  	: fnnc_instt_code
 * 설명 		: 금융기관코드조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fnnc_instt_code();
 */
this.fnnc_instt_code = function()
{
	this.fn_transaction("selectFnncInsttCodeList");
}

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 클리어
	this.ds_rewardCditMastr.clearData(); 
	this.ds_bndeRegist.clearData();
	this.ds_scrcmpnWrhousng.clearData();
	this.ds_indvdlzRegist.clearData();
	
	
	//변경되지 않았을 경우.
	this.fn_transaction("selectM");
}

/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(인수내역)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	var nRow = this.ds_rewardCditMastr.rowposition;
	var itemCd = this.ds_rewardCditMastr.getColumn(nRow, "ITEM_CODE");
	var itemNm = this.ds_rewardCditMastr.getColumn(nRow, "ITEM_NM");
	var objtAt = this.ds_rewardCditMastr.getColumn(nRow, "OBJT_AT");
	
	this.ds_cond_sub.clearData();
	this.ds_cond_sub.addRow();
	this.ds_cond_sub.setColumn(0, "ITEM_CODE", itemCd);
	this.ds_cond_sub.setColumn(0, "ITEM_NM", itemNm);
	this.ds_cond_sub.setColumn(0, "OBJT_AT", objtAt);
	
	this.fn_transaction("selectS");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_rewardCditMastr) || Ex.util.isUpdated(this.ds_bndeRegist) || Ex.util.isUpdated(this.ds_scrcmpnWrhousng) || Ex.util.isUpdated(this.ds_indvdlzRegist)) {              //데이터셋의 수정여부 체크
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_rewardCditMastr, this.ds_validation)) {
			return false;
		}
		
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_bndeRegist, this.ds_validation)) {
			return false;
		}
		
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_scrcmpnWrhousng, this.ds_validation)) {
			return false;
		}
		
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_indvdlzRegist, this.ds_validation)) {
			return false;
		}
		
		this.saveRow = this.ds_rewardCditMastr.rowposition;
		this.fn_transaction("save");
	} else {	
	    this.gfn_message("comm.데이터.변경.없음");
	}
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectM":			// 마스터조회
			var sController   	= "hsco/mis/fun/FUN020501/selectM.do";    
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_rewardCditMastr=output1";
			break;
		case "selectS":	   		// 일괄등록, 증권회사입고신청, 개별등록 조회
			var sController   	= "hsco/mis/fun/FUN020501/selectS.do";   
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_bndeRegist=output1 ds_scrcmpnWrhousng=output2 ds_indvdlzRegist=output3 DS_OBJT_INFO=output4";
			break;
		case "save":	        // 저장               
			var sController   	= "hsco/mis/fun/FUN020501/save.do";
			var sInDatasets   	= "input1=ds_rewardCditMastr:U input2=ds_bndeRegist:U input3=ds_scrcmpnWrhousng:U input4=ds_indvdlzRegist:U input5=DS_OBJT_INFO:U";
			var sOutDatasets  	= "";
			break;
		case "selectFnncInsttCodeList":	   // 금융기관코드 조회
			var sController   	= "hsco/mis/fun/FUN000000/selectFnncInsttCodeList.do";   
			var sInDatasets   	= "";
			var sOutDatasets  	= "DS_FNNC_INSTT_CODE=output1";
			break;
	}

	switch (strSvcId) {
		case "selectFnncInsttCodeList":
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", "", false, false);
			break;
		default :
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
			break;	
	}
}


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectM":			// 예금계좌 조회
				this.ds_dpstAcnut.applyChange();
				
				if(this.saveRow > -1){
					this.ds_rewardCditMastr.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "save":		    // 예금계좌 저장
			    this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectS":		// 금융기관코드 조회
				break;
			case "selectFnncInsttCodeList": // 금융기관코드 조회
				this.DS_FNNC_INSTT_CODE.insertRow(0);
				
				this.DS_FNNC_INSTT_CODE.setColumn(0, "CODE", "");
				this.DS_FNNC_INSTT_CODE.setColumn(0, "CODE_NM", "선택");
				break;	
		}
	}
}

/**********************************************************************
* Transaction 영역 끝
***********************************************************************/


/***********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
************************************************************************
*
* 1. DataSet 영역
* 2. Button 영역
* 
***********************************************************************/

/**********************************************************************
* DataSet 영역 
***********************************************************************/
/*
 * 함수명  	: ds_rewardCditMastr_onrowposchanged
 * 설명 		: 채권등록발행내역 데이터셋에서 row 변경 전 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_rewardCditMastr_onrowposchanged(obj,e);
 */
this.ds_rewardCditMastr_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
    if(Ex.util.isUpdated(this.ds_bndeRegist) || Ex.util.isUpdated(this.ds_scrcmpnWrhousng) || Ex.util.isUpdated(this.ds_indvdlzRegist)) {
	    if (this.gfn_message("confirm.변경.취소.여부")) {
	        return true;
	    }else{
			return false;
		}
	}else{
		//변경되지 않았을 경우.
		return true;
	}
}

/*
 * 함수명  	: ds_rewardCditMastr_onrowposchanged
 * 설명 		: 채권등록발행내역 데이터셋에서 row 변경 후 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_rewardCditMastr_onrowposchanged(obj,e);
 */
this.ds_rewardCditMastr_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var amt1 = this.ds_rewardCditMastr.getColumn(e.row, "PBLICTE_AMOUNT");
	var amt2 = this.ds_rewardCditMastr.getColumn(e.row, "PAY_AMOUNT");
	
	if(!this.comUtils.isNull(amt1) && !this.comUtils.isNull(amt2)){
		var rate = nexacro.round((amt1/amt2)*100, 2);
		this.tab_cdit.tpg1.msk_pblicteRate.set_value(rate);
	} else {
		this.tab_cdit.tpg1.msk_pblicteRate.set_value(0);
	}
	this.fn_search_sub();
}


