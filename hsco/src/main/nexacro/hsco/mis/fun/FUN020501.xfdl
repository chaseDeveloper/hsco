 * 설명 		: 마우스 왼쪽 클릭 후 이벤트 처리(양식 다운로드 팝업의 Title Static)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_print_sta_title_onlbuttondown(obj,e);
 */
this.div_print_sta_title_onlbuttondown = function(obj:Static, e:nexacro.MouseEventInfo)
{
  this.fv_cX = e.screenX;
  this.fv_cY = e.screenY;
}


/*
 * 함수명  	: div_print_sta_title_onmousemove
 * 설명 		: 마우스 이동 후 이벤트 처리(양식 다운로드 팝업의 Title Static)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_print_sta_title_onmousemove(obj,e);
 */
this.div_print_sta_title_onmousemove = function(obj:Static, e:nexacro.MouseEventInfo)
{
	if(e.button == "none") {
		return;
	}
	var nX = e.screenX;
	var nY = e.screenY;
	this.div_print.set_left(parseInt(this.div_print.left) - (this.fv_cX - nX));
	this.div_print.set_top(parseInt(this.div_print.top) - (this.fv_cY - nY));
	this.fv_cX = e.screenX;
	this.fv_cY = e.screenY; 
}

/*
