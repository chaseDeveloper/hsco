 */
this.ds_rewardCditMastr_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "PBLICTE_AMOUNT" || e.columnid == "PAY_AMOUNT"){
		var amt1 = this.ds_rewardCditMastr.getColumn(e.row, "PBLICTE_AMOUNT");
		var amt2 = this.ds_rewardCditMastr.getColumn(e.row, "PAY_AMOUNT");
		
		if(!this.comUtils.isNull(amt1) && !this.comUtils.isNull(amt2)){
			var rate = nexacro.round((amt1/amt2)*100, 2);
			this.tab_cdit.tpg1.msk_pblicteRate.set_value(rate);
		}
	}
}
