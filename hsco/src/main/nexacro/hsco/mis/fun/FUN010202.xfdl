		this.ds_cond.setColumn(this.ds_cond.rowposition, "CPTAL_KND_NM", "");
	}
}

this.div_search_edt_BSNS_CODE_onchanged = function(obj:Edit, e:nexacro.ChangeEventInfo)
{
	if(null == obj.text || "" == obj.text) {
