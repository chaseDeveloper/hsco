this.fn_sanctionPopup = function(){

	this.gfn_message("info.처리불가", "준비중입니다.");
    return;
    
	var newRow = this.ds_dpstAcnut.findRowExpr("NEW_CHK == '1'"); //신규
	var endRow = this.ds_dpstAcnut.findRowExpr("END_CHK == '1'"); //해지
	
	var newAcnutNo = this.ds_dpstAcnut.getColumn(newRow, "ACNUTNO");
	var endAcnutNo = this.ds_dpstAcnut.getColumn(endRow, "ACNUTNO");
	
	//alert("newAcnutNo = " + newAcnutNo + "   endAcnutNo = " + endAcnutNo);
    var parm = {
		arg_0 : this,
		arg_1 : endAcnutNo,
		arg_2 : newAcnutNo,
	};
	
	var popUrl = '';

	if(newAcnutNo && endAcnutNo){
		this.sanctnSe = "P01";
		popUrl = 'mis_fun::FUN030101_P01.xfdl';
	} 
	else if(newAcnutNo && !endAcnutNo){ // 신규
		this.sanctnSe = "P03";
		popUrl = 'mis_fun::FUN030101_P03.xfdl';
	} 
	else if(!newAcnutNo && endAcnutNo){ // 해지
		this.sanctnSe = "P04";
		popUrl = 'mis_fun::FUN030101_P04.xfdl';
	} 
	else{
		this.gfn_message("info.항목.선택.알림", "결재 대상계좌");
		return false;
	}
	
	/**
	* 팝업 공통함수
	* @param {string} svcId
	* @param {number} Width
	* @param {number} Height
	* @param {string} Title
	* @param {string} form URL
	* @param {string} Callback Function
	* @return N/A
	* @example
	* @memberOf comLib
	*/
	this.gfn_popup("fun030101_p01", 582, 404, "", parm, popUrl, "fn_popupCallBack", true);
}

this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 사업코드(조회조건)
	if(sPopupId == "popBsns") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
 		if(loadCnt) {
			var nRow = this.ds_dpstAcnut.rowposition;
			
			var bsnsCode = dsTemp.getColumn(0, "BSNS_CODE");
 			var bsnsNm   = dsTemp.getColumn(0, "BSNS_NM");
			this.ds_dpstAcnut.setColumn(nRow, "BSNS_CODE", bsnsCode);
			this.ds_dpstAcnut.setColumn(nRow, "BSNS_NM", bsnsNm);
 		}
	}
	
	if(sPopupId == "fun030101_p01") {
		if(this.sanctnSe == "P01") {	// 예치 & 해지 동시 결재일 경우		
			this.sanctnSe = "";
			
			if(!this.comUtils.isNull(dsRtn.getColumn(0,"TRMNAT_SANCTN_NO"))){
				var findRow   = this.ds_dpstAcnut.findRow("NEW_CHK", "1");		// 신규예치계좌
				var findRow02 = this.ds_dpstAcnut.findRow("END_CHK", "1");		// 만기해지계좌
				
				this.ds_dpstAcnut.setColumn(findRow,   "DPST_SANCTN_NO",   dsRtn.getColumn(0,"TRMNAT_SANCTN_NO"));
				this.ds_dpstAcnut.setColumn(findRow02, "TRMNAT_SANCTN_NO", dsRtn.getColumn(0,"TRMNAT_SANCTN_NO"));
				
				this.fn_save();
			}
		} 
		else if(this.sanctnSe == "P03") {	// 신규예치 결재일 경우
			this.sanctnSe = "";
			
			if(!this.comUtils.isNull(dsRtn.getColumn(0,"DPST_SANCTN_NO"))){
				this.fn_search();
			}
		} 
		else if(this.sanctnSe == "P04") {	// 만기해지 결재일 경우
			this.sanctnSe = "";
			
			if(!this.comUtils.isNull(dsRtn.getColumn(0,"TRMNAT_SANCTN_NO"))){
				this.fn_search();
			}
			
		}
	}
}

/*
 * 함수명  	: ds_dpstIppDtls_oncolumnchanged
 * 설명 		: 입출금내역 데이터셋 컬럼 데이터 변경 후 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_dpstIppDtls_oncolumnchanged();
 */
this.ds_dpstIppDtls_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "RCPMNY" || e.columnid == "DEFRAY"){
		var amt1 = obj.getSum("RCPMNY");
		var amt2 = obj.getSum("DEFRAY");
		
		obj.setColumn(e.row, "BLCE", amt1-amt2);
	}
}

