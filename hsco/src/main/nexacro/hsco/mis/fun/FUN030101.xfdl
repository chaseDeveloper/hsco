this.btn_Add_Row_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_dpstAcnut.getRowType(this.ds_dpstAcnut.rowposition) != Dataset.ROWTYPE_INSERT){
		
		if(this.ds_dpstAcnut.getColumn(this.ds_dpstAcnut.rowposition, "EXPRTN_AT") == "1"){
			this.gfn_message("fail.수정.불가", "만기처리된 계좌는");
		} 
		else if(this.ds_dpstAcnut.rowposition == -1) {
			this.gfn_message("info.항목.선택.알림", "예금계좌");
		}
		else {
			var nRow = this.ds_dpstIppDtls.addRow();
			this.ds_dpstIppDtls.setColumn(nRow, "ACNUTNO",  this.ds_dpstAcnut.getColumn(this.ds_dpstAcnut.rowposition, "ACNUTNO"));
			this.ds_dpstIppDtls.setColumn(nRow, "DELNG_DE", this.dateUtils.today());
		}
	}
	else {
		this.gfn_message("info.처리불가", "신규 계좌정보를 먼저 저장하시기 바랍니다.")
	}
}

this.btn_Del_Row_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_dpstAcnut.getColumn(this.ds_dpstAcnut.rowposition, "EXPRTN_AT") == "1"){
		this.gfn_message("fail.수정.불가", "만기처리된 계좌는");
	} 
	else if(this.ds_dpstIppDtls.rowposition == -1) {
		this.gfn_message("info.항목.선택.알림", "입출금 내역");
	}
	else {
		if(this.gfn_message("confirm.삭제여부")){
			var nRow = this.ds_dpstIppDtls.rowposition;
			this.ds_dpstIppDtls.deleteRow(nRow);
		}
	}		
}

this.btn_Cancel_Row_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_dpstIppDtls.reset();		                                 //DATASET ROLLBACK
		this.ds_dpstIppDtls.applyChange();	                                 //DATASET COMMIT
		return;
	}	
}

this.ds_dpstAcnut_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{	
	// 신규결재 체크박스 이벤트 처리
	if( (e.columnid == 'NEW_CHK' || e.columnid == 'END_CHK') && e.newvalue == '1'){
		if(!Ex.util.isUpdated(this.ds_dpstIppDtls) && !Ex.util.isUpdated(this.ds_dpstAcnut)){
			
			// 계좌개설결재 체크박스
			if(e.columnid == 'NEW_CHK'){
				var DPST_SANCTN_STS = obj.getColumn(e.row, "DPST_SANCTN_STS");
				
				// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
				// 결재상태가 기안전 외는 신규로 다시 진행.
				switch (DPST_SANCTN_STS) {
					case "1": case "4":
					this.gfn_message("comm.결재.진행");
					return false;
					
					case "5": 
					this.gfn_message("comm.결재.완료");
					return false;
					
					default:
				}
				
				var newCnt = obj.getCaseCount("NEW_CHK == '1'");
				
				if( newCnt == 0){
					return true;
				}
				else {
					this.gfn_message("info.처리불가", "예치결재는 1건씩 가능합니다.");
					return false;
				}	
			}
			
			// 계좌해지결재 체크박스
			if(e.columnid == 'END_CHK'){	
				var TRMNAT_SANCTN_STS = obj.getColumn(e.row, "TRMNAT_SANCTN_STS");
				
				// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
				// 결재상태가 기안전 외는 신규로 다시 진행.
				switch (TRMNAT_SANCTN_STS) {
					case "1": case "4":
					this.gfn_message("comm.결재.진행");
					return false;
					
					case "5":
					this.gfn_message("comm.결재.완료");
					return false;
					
					default:
				}				
				
				var newCnt = obj.getCaseCount("END_CHK == '1'");
				if( newCnt == 0){
					return true;
				}
				else {
					this.gfn_message("info.처리불가", "해지결재는 1건씩 가능합니다.");
					return false;
				}
			}
		}
		else {
			this.gfn_message("info.처리불가", "변경된 계좌정보를 먼저 저장하시기 바랍니다.");
			return false;
		}
	}
}

