/*
 * 함수명  	: ds_dpstAcnut_onrowposchanged
 * 설명 		: 예금계좌 데이터셋 행변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_dpstAcnut_onrowposchanged(obj,e);
 */
this.ds_dpstAcnut_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var nRow = this.ds_dpstAcnut.rowposition;
	var nAppSts = this.ds_dpstAcnut.getColumn(nRow, "DPST_SANCTN_STS");
 
 	// 결재상태 COM022 (0: 기안전 1:기안 2:반송 3:취소 4:중간결재 5:결재완료)
	// 결재상태가 기안전 외는 신규로 다시 진행.
	if("1" != nAppSts && "4" != nAppSts && "5" != nAppSts) { // 기안, 중간결재, 결재완료가 아닌 건만 수정 가능
		// 자금종류
		this.tab_DPST_ACNUT.tpg1.cmb_CPTAL_CODE.set_readonly(false);
		this.tab_DPST_ACNUT.tpg1.cmb_CPTAL_CODE.set_cssclass("cmb_WF_Essential");
		// 금융기관
		this.tab_DPST_ACNUT.tpg1.cmb_FNNC_INSTT_CODE.set_readonly(false);
		this.tab_DPST_ACNUT.tpg1.cmb_FNNC_INSTT_CODE.set_cssclass("cmb_WF_Essential");
	}
	else {
		// 자금종류
		this.tab_DPST_ACNUT.tpg1.cmb_CPTAL_CODE.set_readonly(true);
		this.tab_DPST_ACNUT.tpg1.cmb_CPTAL_CODE.set_cssclass("cmb_WF_Readonly");
		// 금융기관
		this.tab_DPST_ACNUT.tpg1.cmb_FNNC_INSTT_CODE.set_readonly(true);
		this.tab_DPST_ACNUT.tpg1.cmb_FNNC_INSTT_CODE.set_cssclass("cmb_WF_Readonly");
	}

	this.fn_search_detail();
}


