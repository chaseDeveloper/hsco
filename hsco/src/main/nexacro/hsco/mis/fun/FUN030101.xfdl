/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectDpstAcnutList":		// 예금계좌 조회
			var sController   	= "hsco/mis/fun/FUN030101/selectDpstAcnutList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_dpstAcnut=output1";
			break;
		case "selectDpstIppDtlsList":	// 예금계좌 입출금 내역 조회
			var sController   	= "hsco/mis/fun/FUN030101/selectDpstIppDtlsList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_dpstIppDtls=output1";
			break;
		case "saveDpstIppDtlsList":		// 예금계좌 만기여부, 입출금 내역 저장
			var sController   	= "hsco/mis/fun/FUN030101/dpstIppDtlsCUD.do";
			var sInDatasets   	= "input1=ds_dpstAcnut:U, input2=ds_dpstIppDtls:U";
			var sOutDatasets  	= "";
			break;
		case "selectFnncInsttCodeList":	   // 금융기관코드 및 예금자금 조회
			var sController   	= "hsco/mis/fun/FUN000000/selectDpstAcnutCodeList.do";   
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fnncInsttCode=output1 ds_cptalKnd=output2";
			break;			
	}

	switch (strSvcId) {
		case "selectFnncInsttCodeList":
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", "", false, false);
			break;
		default :
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
			break;
		
	} 
}


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectDpstAcnutList":		// 예금계좌 조회				
				if(this.saveRow > -1){
					this.ds_dpstAcnut.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "selectDpstIppDtlsList":	// 예금계좌 입출금 조회
				this.ds_dpstIppDtls.applyChange();
				break;
			case "saveDpstIppDtlsList":		// 저장
				this.gfn_message("success.처리.성공");
				
				// 데이터셋 클리어
				this.ds_dpstAcnut.clearData();
				// 예금계좌 리스트 조회
				this.fn_transaction("selectDpstAcnutList");	
				break;
			case "selectFnncInsttCodeList":		// 금융기관코드 조회				
				this.ds_fnncInsttCode.insertRow(0);
				this.ds_fnncInsttCode.setColumn(0, "CODE", "");
				this.ds_fnncInsttCode.setColumn(0, "CODE_NM", "선택");
				
				this.ds_cond.setColumn(0, "FNNC_INSTT_CODE", "");
				
				this.ds_cptalKnd.insertRow(0);
				this.ds_cptalKnd.setColumn(0, "CODE", "");
				this.ds_cptalKnd.setColumn(0, "CODE_NM", "선택");
				
				this.ds_cond.setColumn(0, "FNNC_INSTT_CODE", "");
				this.ds_cond.setColumn(0, "CPTAL_KND", "");	
				
				this.fn_search();    // 초기 데이터 조회
				break;				
		}
	}
}

