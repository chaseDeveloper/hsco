/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_dpstIppDtls) || Ex.util.isUpdated(this.ds_dpstAcnut)) {              //데이터셋의 수정여부 체크
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_dpstAcnut, this.ds_validation)) {
			return false;
		}
		
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_dpstIppDtls, this.ds_validation)) {
			return false;
		}
		
		if(Ex.util.isUpdated(this.ds_dpstIppDtls)){	// 입출금내역 정합성 체크
			var nRcpmny = this.ds_dpstIppDtls.getSum("RCPMNY"); // 입금액 합
			var nDefray = this.ds_dpstIppDtls.getSum("DEFRAY"); // 출금액 합
			if((nRcpmny - nDefray) < 0) {
				this.gfn_message("errors.항목값.범위.이하", "출금총액|입금총액");
				return;
			}
		}
		
		this.saveRow = this.ds_dpstAcnut.rowposition;
		this.fn_transaction("saveDpstIppDtlsList");
	} else {
	    this.gfn_message("comm.데이터.변경.없음");
	}
}

/*
 * 함수명  	: btn_exprtnAt_onclick
 * 설명 		: 예금계좌 만기처리 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_exprtnAt_onclick();
 */
this.btn_exprtnAt_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRcpmny = this.ds_dpstIppDtls.getSum("RCPMNY"); // 입금액 합
	var nDefray = this.ds_dpstIppDtls.getSum("DEFRAY"); // 출금액 합
	if(this.ds_dpstAcnut.rowcount == 0) {
		return;
	} else {
		
		if (this.ds_dpstAcnut.getColumn(this.ds_dpstAcnut.rowposition, "EXPRTN_AT") == "1") {
			if (this.gfn_message("comm.행위여부", "만기처리를 취소")) {
				this.ds_dpstAcnut.setColumn(this.ds_dpstAcnut.rowposition, "EXPRTN_AT", "0");
				this.fn_transaction("saveDpstIppDtlsList");
			}
		} else {
			if (this.gfn_message("comm.행위여부", "만기처리")) {
				if((nRcpmny - nDefray) == 0){
					this.ds_dpstAcnut.setColumn(this.ds_dpstAcnut.rowposition, "EXPRTN_AT", "1");
					this.fn_transaction("saveDpstIppDtlsList");
				} else {
					this.gfn_message("fail.처리.불가", "입금총액|출금총액|만기처리");
				}
			}
		}	
	}
}

