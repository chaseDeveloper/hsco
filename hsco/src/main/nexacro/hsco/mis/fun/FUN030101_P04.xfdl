/*
 * 함수명  	: fn_sanction
 * 설명 		: 결재 버튼 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_sanction();
 */
this.fn_sanction = function()
{		
	if(!this.div_pop.edt_sbjt.text){
		this.gfn_message("comm.항목.입력.을", "결재 제목");
		return;
	}
	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_trmnat, this.ds_validation)) {
		return false;
	}

//	var findRow = this.ds_cptalKnd.findRow("CODE",this.ds_new.getColumn(0, "CPTAL_CODE"));
	
	var filds = new Array();
	
	filds[0] = this.ds_trmnat.getColumn(0, "REAL_ACNUTNO"); 								// 해지계좌번호
	filds[1] = this.comUtils.formatComma(this.ds_trmnat.getColumn(0, "RCPMNY"));    		// 해지금액
	filds[2] = this.ds_trmnat.getColumn(0, "CNTRCT_DE").substr(0,4)+"."+this.ds_trmnat.getColumn(0, "CNTRCT_DE").substr(4,2)+"."+this.ds_trmnat.getColumn(0, "CNTRCT_DE").substr(6,2);        	// 예치일자	
	filds[3] = this.ds_trmnat.getColumn(0, "EXPRTN_DE").substr(0,4)+"."+this.ds_trmnat.getColumn(0, "EXPRTN_DE").substr(4,2)+"."+this.ds_trmnat.getColumn(0, "EXPRTN_DE").substr(6,2);    		// 만기일자	
	filds[4] = this.comUtils.formatComma(this.ds_trmnat.getColumn(0, "INTR"));   			// 이자
	filds[5] = this.comUtils.formatComma(this.ds_trmnat.getColumn(0, "CRRX"));	       		// 법인세
	filds[6] = this.comUtils.formatComma(this.ds_trmnat.getColumn(0, "AFTTX_INTR"));		// 세후이자
	filds[7] = this.ds_trmnat.getColumn(0, "AFTTX_ACNUTNO") + " 계좌입금.  끝.";		// 세후이자 입금계좌
	
	filds[8] = this.div_pop.edt_sbjt.text;	// 결재제목
	
	// 결재 서식 연동 이후 구현 필요
// 	this.fn_callHandySanc2(
// 		 this
// 		,'0000000052'          				 // formId
// 		,this.ds_trmnat        				 // mis데이터셋
// 		,"TBFUN_DPST_ACNUT"     			 // 갱신테이블명
// 		,["ACNUTNO"] 			 			 // 테이블키배열
// 		,filds                 				 // 갑지데이터배열
// 		,null                  				 // 을지데이터배열
// 		,null                  				 // 다중체크대상
// 		,"TRMNAT_SANCTN_NO"   				 // 갱신대상컬럼
// 	);

	this.close(this.ds_trmnat.saveXML());
};

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectDpstAcnutList":		// 예금계좌 조회
			var sController   	= "hsco/mis/fun/FUN030101/selectPopupDpstAcnutList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_trmnat=output1";
			break;
		case "selectFnncInsttCodeList":	   // 금융기관코드 및 예금자금 조회
			var sController   	= "hsco/mis/fun/FUN000000/selectDpstAcnutCodeList.do";   
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fnncInsttCode=output1 ds_cptalKnd=output2";
			break;
	}

	switch (strSvcId) {
		case "selectFnncInsttCodeList":
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", "", false, false);
			break;
		default :
			Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
			break;
		
	} 
}


