* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0) {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectFnncInsttCodeList":		// 금융기관코드 조회
				
				this.ds_fnncInsttCode.insertRow(0);
				this.ds_fnncInsttCode.setColumn(0, "CODE", "");
				this.ds_fnncInsttCode.setColumn(0, "CODE_NM", "선택");
				
				this.ds_cptalKnd.insertRow(0);
				this.ds_cptalKnd.setColumn(0, "CODE", "");
				this.ds_cptalKnd.setColumn(0, "CODE_NM", "선택");
				
				this.ds_cond.setColumn(0, "FNNC_INSTT_CODE", "");
				this.ds_cond.setColumn(0, "CPTAL_KND", "");	
				
				this.fn_search();    // 초기 데이터 조회
				break;
			case "selectDpstAcnutList":
				break;
		}
	}
}


 /*
 * 함수명  	: fn_PopBsnsCallback
 * 설명 		: 팝업 콜백 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                sReturn  : 팝업에서 넘어오는 데이터
 * return Type 	: None
 * 사용법		: this.fn_PopBsnsCallback();
 */
this.fn_PopCallback = function(sPopupId, sReturn)
{
	if(sPopupId == "fun030101_p02")
	{
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(nRowCnt){
			if(this.se == "1"){ 	// 해지계좌일 경우
				this.ds_trmnat.clearData();
				this.ds_trmnat.addRow();
				this.ds_trmnat.setColumn(0, "REAL_ACNUTNO", dsRtn.getColumn(0, "REAL_ACNUTNO"));
				this.ds_trmnat.setColumn(0, "ACNUTNO",   	dsRtn.getColumn(0, "ACNUTNO"));
				this.ds_trmnat.setColumn(0, "CNTRCT_DE", 	dsRtn.getColumn(0, "CNTRCT_DE"));
				this.ds_trmnat.setColumn(0, "EXPRTN_DE", 	dsRtn.getColumn(0, "EXPRTN_DE"));
				this.ds_trmnat.setColumn(0, "RCPMNY",    	dsRtn.getColumn(0, "RCPMNY"));
				this.ds_trmnat.setColumn(0, "INTR",      	dsRtn.getColumn(0, "INTR"));
				this.ds_trmnat.setColumn(0, "CRRX",      	dsRtn.getColumn(0, "CRRX"));
				
				var intr = dsRtn.getColumn(0, "INTR");
				var crrx = dsRtn.getColumn(0, "CRRX");
				
				this.ds_trmnat.setColumn(0, "AFTTX_INTR", intr-crrx);  // 세후이자 = 이자액 - 법인세액
				
			}
		}	
	}
	
	if(sPopupId == "fun040201_P02")
	{
		var dsRtn   = new Dataset();
		
		if(!this.comUtils.isNull(sReturn)) {
			dsRtn.loadXML(sReturn);				
			
			if (dsRtn.getColumn(0,"SE") == "01") {
				se = "(수입원) ";
			} else if (dsRtn.getColumn(0,"SE") == "05") {
				se = "(MMDA) ";
			}
			
			this.ds_trmnat.setColumn(0, "AFTTX_ACNUTNO", se + dsRtn.getColumn(0,"ACNUTNO"));
		}
	}
}


