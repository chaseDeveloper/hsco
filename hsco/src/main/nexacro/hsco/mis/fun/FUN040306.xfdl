this.btn_excelImpot_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	trace("excel Import START");
	
	this.ds_import.clearData();
	
	var arg = {
		targetDs : this.ds_import,
		headSet : false,
		callBack : function () {
			this.setWaitCursor(true,true);
			
			var sCnt = this.ds_weekCptalPlan.rowcount;
			var nCnt = this.ds_import.rowcount;
			var nRow = 0;

			var sKey = "";
			var nKey = "";
			
			for(var i=0; i<sCnt; i++) {
				if("0" == this.ds_weekCptalPlan.getColumn(i, "CONFM_AT")) {
					sKey = this.comUtils.trimString(this.ds_weekCptalPlan.getColumn(i, "BUDGET_ACNT_CODE")) + 
					       this.comUtils.trimString(this.ds_weekCptalPlan.getColumn(i, "DETAIL_BSNS_CODE")) + 
					       this.comUtils.trimString(this.ds_weekCptalPlan.getColumn(i, "EXPNDTR_UNIT_CODE"));
					
					for(var j=0; j<nCnt; j++) {
						nKey = this.comUtils.trimString(this.ds_import.getColumn(j, "Column2")) + 
							   this.comUtils.trimString(this.ds_import.getColumn(j, "Column4")) + 
							   this.comUtils.trimString(this.ds_import.getColumn(j, "Column6"));
						
						if(sKey == nKey) {
							this.ds_weekCptalPlan.setColumn(i, "PLAN_AMOUNT_01", nexacro.toNumber(this.comUtils.trimString(this.ds_import.getColumn(j, "Column11"))));
							this.ds_weekCptalPlan.setColumn(i, "PLAN_AMOUNT_02", nexacro.toNumber(this.comUtils.trimString(this.ds_import.getColumn(j, "Column12"))));
							this.ds_weekCptalPlan.setColumn(i, "PLAN_AMOUNT_03", nexacro.toNumber(this.comUtils.trimString(this.ds_import.getColumn(j, "Column13"))));
							this.ds_weekCptalPlan.setColumn(i, "PLAN_AMOUNT_04", nexacro.toNumber(this.comUtils.trimString(this.ds_import.getColumn(j, "Column14"))));
							if (this.ds_weekCptalPlan.getColumn(i, "MAX_WEEK") == '5') { // 5주차가 있을 경우에만 입력
								this.ds_weekCptalPlan.setColumn(i, "PLAN_AMOUNT_05", nexacro.toNumber(this.comUtils.trimString(this.ds_import.getColumn(j, "Column15"))));
							}
							break;
						}
					}
				}
			}

			this.setWaitCursor(false,true);
		}
	};
	
	this.gfn_importExcel(arg);
	
	trace("excel Import END");	
}

