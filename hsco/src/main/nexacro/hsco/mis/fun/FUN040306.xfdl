this.ds_weekCptalPlan_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var planAmt = obj.getColumn(e.row, "PLAN_AMOUNT");
	var sumAmt  = obj.getColumn(e.row, "PLAN_AMOUNT_01")+obj.getColumn(e.row, "PLAN_AMOUNT_02")+obj.getColumn(e.row, "PLAN_AMOUNT_03")+obj.getColumn(e.row, "PLAN_AMOUNT_04")+obj.getColumn(e.row, "PLAN_AMOUNT_05")
	
	if (planAmt < sumAmt) {
		this.alert("주간계획 합계는 월간계획을 초과할 수 없습니다.");
		obj.setColumn(e.row, e.columnid, e.oldvalue);
	}
}
