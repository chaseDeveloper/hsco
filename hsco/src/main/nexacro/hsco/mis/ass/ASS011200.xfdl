/*
 * 함수명  	: btn_Cancel_Row
 * 설명 		: 행취소 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Cancel_Row();
 */
this.btn_Cancel_Row = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_pptyDetailList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if (this.gfn_message("confirm.취소여부")) {
		
		if(obj.name == "btn_cancel"){
			this.ds_pptyDetailList.reset();		//DATASET ROLLBACK
			this.ds_pptyDetailList.applyChange();	//DATASET COMMIT
			return;
		}
	}	
}

/*
 * 함수명  	: btn_excelDn_onclick
 * 설명 		: 엑셀다운 버튼 클릭
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_excelDn_onclick();
 */
this.btn_excelDn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_pptyDetailList.rowcount == 0){
		this.gfn_message("info.처리불가", "해당계획에 물품조사목록이 없습니다.");
		return false;
	}else{
		if(obj.name == "btn_excelDown"){
			this.gfn_exportExcel(this.grd_pptyDetailList, "exportExl");
		}
	}
}

/*
 * 함수명  	: grd_assetsMastr_onheadclick
 * 설명 		: 그리드 헤드 클릭
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_assetsMastr_onheadclick();
 */
this.grd_assetsMastr_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == "0") {
		this.ds_assetsMastr.set_updatecontrol(false);
		this.gfn_setGridCheckAll(obj, e);
		this.ds_assetsMastr.set_updatecontrol(true);
	}else {
		LGV_COL_SORT = e.col; 
		this._gridSort(obj, e);	
	}
}

/*
 * 함수명  	: ds_cond_assetsMastr_oncolumnchanged
 * 설명 		: 부서별물품 조회조건 변경후 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_assetsMastr_oncolumnchanged();
 */
this.ds_cond_assetsMastr_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "ASSETS_CL_CODE") {
		if(this.comUtils.isNull(e.newvalue)) {
			obj.setColumn(0, "ASSETS_CL_NM", "");
		}
	}
}

this.grd_pptyDetailList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this._gridSort(obj, e);
}
