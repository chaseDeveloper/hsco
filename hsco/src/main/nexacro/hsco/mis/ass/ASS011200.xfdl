}

this.ASS011200_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// 초기 검색조건
	this.ds_cond.setColumn(0, "DE1",   this.dateUtils.today().substr(0,4)+"0101");
	this.ds_cond.setColumn(0, "DE2",   this.dateUtils.today());

	// 데이터셋 동적 바인딩(부서팝업)
	this.div_dept.fn_setBind("ds_cond_assetsMastr","DEPT_CODE","DEPT_NM");
	this.ds_cond_assetsMastr.clearData();
	this.ds_cond_assetsMastr.addRow();
	this.ds_cond_assetsMastr.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
	this.ds_cond_assetsMastr.setColumn(0, "DEPT_NM"  , this.gfn_getDeptName());

	// 공통코드
	var comboParams = [
		["ds_cmp002", "CMP002", "Y", ""]	// 단위
	   ,["ds_ass007", "ASS007", "Y", ""]	// 물품상태
	];
	
	var callback = function (svcId, errorCode, errorMsg) {
		
		// 공통코드와 연계된 초기검색조건 설정
		// 수정가능여부를 판별하기위해 권한그룹 체크
		if(this.gfn_authGrpId("ASS_ADMIN") != -1) {			// 자산담당자
			this.chargerAt = true;
		}

		// 수정가능한 경우 부서변경 가능
		if(this.chargerAt){
			this.div_dept.set_enable(true);
		}else {
			this.div_dept.set_enable(false);
		}
		
		
		// 조회 이벤트
