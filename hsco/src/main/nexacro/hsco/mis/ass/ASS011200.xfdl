}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_pptyDetailList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_pptyPlanList.reset();		//DATASET ROLLBACK
		this.ds_pptyPlanList.applyChange();	//DATASET COMMIT
		return;	
	}
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_pptyPlanList.getColumn(this.ds_pptyPlanList.rowposition, "INVG_AT") == '0'){
		var nRow = this.ds_pptyPlanList.rowposition;
		if(this.gfn_message("confirm.삭제여부")){
			this.ds_pptyPlanList.deleteRow(nRow);
		   }
	}else{
		this.gfn_message("info.처리불가", "조사가 완료되어 수정할 수 없습니다.");
		return;
	}
}

/*
 * 함수명  	: btn_downFiles_onclick
 * 설명 		: '선택다운로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_downFiles_onclick(obj,e);
 */
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}


