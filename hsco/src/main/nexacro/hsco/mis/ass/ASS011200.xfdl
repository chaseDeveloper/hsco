 */
this.tab_plan_tpg2_grd_pptyManage_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.cell == 1){
		var invgAt = this.ds_pptyPlanList.getColumn(this.ds_pptyPlanList.rowposition, "INVG_AT");
		
		if(invgAt == '1'){
			this.ds_pptyPlanList.setColumn(this.ds_pptyPlanList.rowposition , "COMPT_DE",   this.dateUtils.today());
			this.ds_pptyPlanList.setColumn(this.ds_pptyPlanList.rowposition , "CFMT_DT",   this.dateUtils.today());
		}else{
			this.ds_pptyPlanList.setColumn(this.ds_pptyPlanList.rowposition , "COMPT_DE",   "");
			this.ds_pptyPlanList.setColumn(this.ds_pptyPlanList.rowposition , "CFMT_DT",   "");
		}
	}
}

