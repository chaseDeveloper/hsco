/*
 * 함수명  	: btn_Assets_Popup_onclick
 * 설명 		: 재물조사 대상 목록 -> 재물조사 관리 상세 이동 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Assets_Popup_onclick();
 */
this.btn_move_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	if(this.ds_pptyPlanList.getColumn(this.ds_pptyPlanList.rowposition, "INVG_AT") == '0'){
		var rowType = this.ds_pptyPlanList.getRowType(this.ds_pptyPlanList.rowposition);
		
		if(!rowType){
			this.gfn_message("comm.항목.선택.요청", "재물조사 계획");
			return;
		}
		
		var dsRtn = new Dataset(); 
		dsRtn = this.gfn_getCheckData(this.grd_assetsMastr, "CHK");

		if(dsRtn.rowcount == 0){
			this.gfn_message("comm.항목.선택.요청", "부서별 물품");
			return;
		}
		
		for(var i=0; i<dsRtn.getRowCount(); i++){
			var strVal = dsRtn.getColumn(i, "ASSETS_NO");
			var findRow = this.ds_pptyDetailList.findRow("ASSETS_NO", strVal);
			
			// 매줄조사 상세 계획에서 중복되는 자산이 없을때만 추가됨.
			if(findRow == -1){
				var nRow = this.ds_pptyDetailList.addRow();
				this.ds_pptyDetailList.setColumn(nRow, "ASSETS_NO",                 dsRtn.getColumn(i, "ASSETS_NO"));
				this.ds_pptyDetailList.setColumn(nRow, "ASSETS_CL_CODE",            dsRtn.getColumn(i, "ASSETS_CL_CODE"));
				this.ds_pptyDetailList.setColumn(nRow, "ASSETS_CL_NM",              dsRtn.getColumn(i, "ASSETS_CL_NM"));
				this.ds_pptyDetailList.setColumn(nRow, "ASSETS_SE",                 dsRtn.getColumn(i, "ASSETS_SE"));
				this.ds_pptyDetailList.setColumn(nRow, "ASSETS_NM",                 dsRtn.getColumn(i, "ASSETS_NM"));
				this.ds_pptyDetailList.setColumn(nRow, "DEPT_NM",					dsRtn.getColumn(i, "DEPT_NM"));
				this.ds_pptyDetailList.setColumn(nRow, "DEPT_CODE", 				dsRtn.getColumn(i, "DEPT_CODE"));
				this.ds_pptyDetailList.setColumn(nRow, "UNTPC", 				    dsRtn.getColumn(i, "UNTPC"));
				this.ds_pptyDetailList.setColumn(nRow, "QY", 						dsRtn.getColumn(i, "QY"));
				this.ds_pptyDetailList.setColumn(nRow, "UNIT", 						dsRtn.getColumn(i, "UNIT"));
				this.ds_pptyDetailList.setColumn(nRow, "STNDRD", 					dsRtn.getColumn(i, "STNDRD"));
				this.ds_pptyDetailList.setColumn(nRow, "BIZPLC_NM",					dsRtn.getColumn(i, "BIZPLC_NM"));
				this.ds_pptyDetailList.setColumn(nRow, "ODR", 						this.ds_pptyPlanList.getColumn(this.ds_pptyPlanList.rowposition, "ODR"));
			}
			
			// 상세계획으로 이동된 대상은 불용품대상에서 제외
			var assetsNO = dsRtn.getColumn(i, "ASSETS_NO");
			var nRow1 = this.ds_assetsMastr.findRow("ASSETS_NO" ,assetsNO);
			this.ds_assetsMastr.deleteRow(nRow1);
			
			//재물조사계획 수량 count
			var qtyVal = this.ds_pptyPlanList.getColumn(this.ds_pptyPlanList.rowposition, "INVG_QTY");
			
			//디테일 리스트 수량 conut
			var detailCnt = this.ds_pptyDetailList.rowcount;
			
			this.ds_pptyPlanList.set_updatecontrol(false);
			this.ds_pptyPlanList.setColumn(this.ds_pptyPlanList.rowposition, "INVG_QTY", detailCnt)
			this.ds_pptyPlanList.set_updatecontrol(true);
		}
		this.gfn_message("success.처리.성공");
		
	} else {
		this.btn_delete.set_visible(false);
		
		this.gfn_message("info.처리불가", "조사가 완료되어 수정할 수 없습니다.");
		return;
	}
}



