/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(재물조사계획)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
 	this.ds_pptyPlanList.clearData();
 	
 	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
 		return false;
 	}
 	
	this.fn_transaction("pptyPlanList");
	
}

/*
 * 함수명  	: fn_search_sub02
 * 설명 		: 조회 이벤트 처리(부서물품 리스트)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub02();
 */
this.fn_search_sub02 = function()
{	
	this.ds_assetsMastr.clearData();

	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	var nRow = this.ds_pptyPlanList.rowposition;
	var odr = this.ds_pptyPlanList.getColumn(nRow, "ODR");
	this.ds_cond_assetsMastr.setColumn(0, "ODR", odr);
			
	this.fn_transaction("deptAssetsList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None	
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{	
	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_pptyPlanList) && !this.comUtils.isDatasetUpdated(this.ds_pptyDetailList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_pptyPlanList, this.ds_validation)) {
			return false;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_pptyDetailList, this.ds_validation)) {
			return false;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_pptyPlanList.rowposition;
		this.fn_transaction("pptyPlanListCUD");
	}else{
		this.gfn_message("comm.데이터.변경.없음");
	}

}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "pptyPlanList":        // 재물조사 리스트  조회 
			var sController   	= "hsco/mis/ass/ASS011200/pptyPlanList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_pptyPlanList=output1";
			break;
			
		case "pptyDetailList":      // 재물조사 리스트 디테일 정보 조회 
			var sController   	= "hsco/mis/ass/ASS011200/pptyPlanDetail.do";	
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_pptyDetailList=output1 ds_fileList=output2";
			break;
			
		case "pptyPlanListCUD":		// 재물조사 저장
			var sController   	= "hsco/mis/ass/ASS011200/pptyPlanListCUD.do";	
			var sInDatasets   	= "input1=ds_pptyPlanList:U input2=ds_pptyDetailList:U";
			var sOutDatasets  	= "";
			break;
			
		case "deptAssetsList":		// 부서 자산 조회 
			var sController   	= "hsco/mis/ass/ASS011200/deptAssetsList.do";
			var sInDatasets   	= "input1=ds_cond_assetsMastr";
			var sOutDatasets  	= "ds_assetsMastr=output1";
			break;
			
		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/ass/ASS010500/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_fileList=output1";
			break;		
			
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "pptyPlanList" :
				if(this.saveRow > -1){
					this.ds_pptyPlanList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "pptyDetailList" : 
				// 부서별 물품 조회
				this.fn_search_sub02();
				break;
			case "deptAssetsList":
				break;
			case "pptyPlanListCUD":    	// 취득물품 저장
				this.gfn_message("success.처리.성공");
				if(this.saveRow > -1){
					this.ds_pptyPlanList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				this.fn_search();
				break;
			case "selectFileList":      // 첨부파일 조회
				this.ds_fileList.filter("USE_AT == '1'");
				break;
		}
	}
}

/*
 * 함수명  	: fn_callback
 * 설명 		: 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback5();
 */
this.fn_callback = function(sPopupId, sReturn)
{
    var dsRtn = new Dataset();
    nRowCnt = dsRtn.loadXML(sReturn);
	
	if(sPopupId == "Popup")
	{
        if(nRowCnt){
            this.ds_cond_assetsMastr.setColumn(0, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.ds_cond_assetsMastr.setColumn(0, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
        }
	}
}
