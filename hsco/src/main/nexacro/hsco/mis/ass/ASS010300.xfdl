this.div_DETAIL_tab_Ass_Cl_Info_Info_cmb_DPRC_MTH_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	if(this.div_DETAIL.cmb_DPRC_MTH.value=='1'){
