/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
var saveRow = -1;
var authAt = false;
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ASS011600_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{

}

this.ASS011600_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
	
	this.div_search.div_dept.fn_setBind("ds_cond", "DEPT_CODE", "DEPT_NM");
	this.div_EMP.fn_setBind("ds_lossManageList","LOSS_EMPNO","LOSS_EMPNM");
	
	// 초기 검색조건
	// 초기 검색조건
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "DEPT_CODE"  , this.gfn_getDeptId());
	this.ds_cond.setColumn(0, "DEPT_NM"    , this.gfn_getDeptName());;
	this.ds_cond.setColumn(0, "ASSETS_NM"  , "");
	this.ds_cond.setColumn(0, "CONFM_AT"   , "");
		
	// 권한 분리
	if(this.gfn_authGrpId("ASS_ADMIN") > -1) {		// 자산결재자
		authAt = true;
	}

	if(authAt) { 
		this.div_search.div_dept.set_enable(true);
		// 자산현황 - 승인여부
	}else {
		this.div_search.div_dept.set_enable(false);
	}
	
	
	this.fn_search();
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(손망실관리계획)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_lossManageList.clearData();
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	this.fn_transaction("selectLossManageList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None	
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_lossManageList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}

	if(!this.gfn_checkValidation_NoUpdChk(this.ds_lossManageList, this.ds_validation)) {
		return false;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		saveRow = this.ds_lossManageList.rowposition;
		this.fn_transaction("lossManageCUD");
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_lossManageList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_lossManageList.reset();			//DATASET ROLLBACK
		this.ds_lossManageList.applyChange();	//DATASET COMMIT
	}
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var confmAt = this.ds_lossManageList.getColumn(nRow, "CONFM_AT");
	
	if(confmAt == "1") {
		this.gfn_message("info.처리불가", "승인된 건은 삭제할 수 없습니다.");
		return;
	}
		
	var nRow = this.ds_lossManageList.rowposition;
	if(this.gfn_message("confirm.삭제여부")) {
		this.ds_lossManageList.deleteRow(nRow);
	}
}


/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 클릭시 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_lossManageList.addRow();
	
	// 자산대장 팝업
	this.fn_popup("searchAssetsMastr");
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectLossManageList":	// 손망실관리 리스트  조회 
			var sController   	= "/hsco/mis/ass/ASS011600/selectLossManageList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_lossManageList=output1";
			break;
		case "lossManageCUD":			// 손망실관리 저장
			var sController   	= "/hsco/mis/ass/ASS011600/lossManageCUD.do";	
			var sInDatasets   	= "input1=ds_lossManageList:U";
			var sOutDatasets  	= "";
			break;	
		case "lossManageConfm":		    	// 물품관리전환 승인
			var sController   	= "/hsco/mis/ass/ASS011600/lossManageConfm.do";	
			var sInDatasets   	= "input1=ds_confmProcess:A";
			var sOutDatasets  	= "";
			break;	
		case "lossManageConfmCancel":	     // 물품관리전환 승인취소
			var sController   	= "/hsco/mis/ass/ASS011600/lossManageConfmCancel.do";	
			var sInDatasets   	= "input1=ds_confmProcess:A";
			var sOutDatasets  	= "";
			break;	
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "selectLossManageList":	// 손망실관리 리스트  조회 
				if(saveRow > -1) {
					this.ds_lossManageList.set_rowposition(saveRow);
					saveRow = -1;
				}
				break;
			case "lossManageCUD":			// 손망실관리 저장
				this.fn_search();
				break;		
			case "lossManageConfm":		    	// 물품관리전환 승인
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;	
			case "lossManageConfmCancel":		// 물품관리전환 승인취소
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}

