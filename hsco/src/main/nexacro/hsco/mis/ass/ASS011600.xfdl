/*
 * 함수명  	: ds_lossManageList_oncolumnchanged
 * 설명 		: 부모데이터셋 데이터 변경 후 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_lossManageList_oncolumnchanged();
 */

this.ds_lossManageList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var nRow = obj.rowposition;

	if(e.columnid == "LOSS_UNTPC") {	// 단가
		var untpc  = this.comUtils.isNull(e.newvalue) ? 0 : nexacro.toNumber(e.newvalue);
		var qy     = this.comUtils.isNull(obj.getColumn(nRow, "LOSS_QY")) ? 0 : nexacro.toNumber(obj.getColumn(nRow, "LOSS_QY"));
		var lossAm = untpc * qy;
		
		obj.setColumn(nRow, "LOSS_AM", lossAm);
	}

	if(e.columnid == "LOSS_QY") {		// 수량
		var qy     = this.comUtils.isNull(e.newvalue) ? 0 : nexacro.toNumber(e.newvalue);
		var untpc  = this.comUtils.isNull(obj.getColumn(nRow, "LOSS_UNTPC")) ? 0 : nexacro.toNumber(obj.getColumn(nRow, "LOSS_UNTPC"));
		var lossAm = untpc * qy;
		
		obj.setColumn(nRow, "LOSS_AM", lossAm);
	}
	
	if(e.columnid == "CMNUSE_QLY_UNTPC") {	// 공용품 단가
		var untpc  = this.comUtils.isNull(e.newvalue) ? 0 : nexacro.toNumber(e.newvalue);
		var qy     = this.comUtils.isNull(obj.getColumn(nRow, "CMNUSE_QLY_QY")) ? 0 : nexacro.toNumber(obj.getColumn(nRow, "CMNUSE_QLY_QY"));
		var lossAm = untpc * qy;
		
		obj.setColumn(nRow, "CMNUSE_QLY_AM", lossAm);
	}

	if(e.columnid == "CMNUSE_QLY_QY") {		// 공용품 수량
		var qy     = this.comUtils.isNull(e.newvalue) ? 0 : nexacro.toNumber(e.newvalue);
		var untpc  = this.comUtils.isNull(obj.getColumn(nRow, "CMNUSE_QLY_UNTPC")) ? 0 : nexacro.toNumber(obj.getColumn(nRow, "CMNUSE_QLY_UNTPC"));
		var lossAm = untpc * qy;
		
		obj.setColumn(nRow, "CMNUSE_QLY_AM", lossAm);
	}
}


