 */
this.fn_popup = function(sPopupId)
{
	// 자산팝업
	if(sPopupId == "searchAssetsMastr") {
		var parm = {
			  arg_0      : this
			 ,deptCode   : this.ds_cond.getColumn(0, "DEPT_CODE")
			 ,deptNm     : this.ds_cond.getColumn(0, "DEPT_NM")
			 ,authAt     : authAt
		};
		this.gfn_popup(sPopupId, 400, 400, "자산선택", parm, "mis_ass::ASS010400_P02.xfdl", "fn_popupCallback", true);
	}
	
	// 사업소팝업
	if(sPopupId == "bizplcPopup") {
		var parm = {
			 arg_0      : this
		};
		this.gfn_popup(sPopupId, 400, 400, "사업소선택", parm, "mis_hrm::HRM010102_P1.xfdl", "fn_popupCallback", true);
	}
}


 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallback = function(sPopupId, sReturn)
{
	trace("???????????????? sPopupId : "+sPopupId);
	// 자산팝업
	if(sPopupId == "searchAssetsMastr") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var assetsNo   = dsTemp.getColumn(0, "ASSETS_NO"); 		// 자산번호
			var assetsNm   = dsTemp.getColumn(0, "ASSETS_NM"); 		// 자산명
			var deptCode   = dsTemp.getColumn(0, "DEPT_CODE"); 		// 담당부서
			var deptNm     = dsTemp.getColumn(0, "DEPT_NM"); 		// 담당부서코드
			var empno      = dsTemp.getColumn(0, "EMPNO"); 			// 담당자
			var empnm      = dsTemp.getColumn(0, "EMPNM"); 			// 담당자사번
			var lossStndrd = dsTemp.getColumn(0, "STNDRD"); 		// 품질규격
			var lossQy     = dsTemp.getColumn(0, "QY"); 			// 수량
			var lossUntpc  = dsTemp.getColumn(0, "UNTPC"); 			// 단가

			var nRow = this.ds_lossManageList.rowposition;
			this.ds_lossManageList.setColumn(nRow, "ASSETS_NO"  , assetsNo); 		// 자산번호
			this.ds_lossManageList.setColumn(nRow, "ASSETS_NM"  , assetsNm); 		// 자산명
			this.ds_lossManageList.setColumn(nRow, "DEPT_CODE"  , deptCode); 		// 담당부서
			this.ds_lossManageList.setColumn(nRow, "DEPT_NM"    , deptNm); 			// 담당부서코드
			this.ds_lossManageList.setColumn(nRow, "EMPNO"      , empno); 			// 담당자
			this.ds_lossManageList.setColumn(nRow, "EMPNM"      , empnm); 			// 담당자사번
			this.ds_lossManageList.setColumn(nRow, "LOSS_PRDNM" , assetsNm);		// 손망실_품명
			this.ds_lossManageList.setColumn(nRow, "LOSS_STNDRD", lossStndrd); 		// 손망실_품질규격
			this.ds_lossManageList.setColumn(nRow, "LOSS_QY"    , lossQy); 			// 손망실_수량
			this.ds_lossManageList.setColumn(nRow, "LOSS_UNTPC" , lossUntpc); 		// 손망실_단가
		}else {
			var nRow = this.ds_lossManageList.rowposition;
			var rowType = this.ds_lossManageList.getRowType(nRow);
			if(rowType == "2") {
				this.ds_lossManageList.deleteRow(nRow);
			}
		}
	}	
	// 사업장 구분 팝업(검색조건)
	if(sPopupId == "bizplcPopup"){
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);
		
        if(nRowCnt){
			var bizplcCode = dsRtn.getColumn(0, "BIZPLC_CODE");
			var bizplcNm = dsRtn.getColumn(0, "BIZPLC_NM");
			
            this.ds_cond.setColumn(0, "BIZPLC_CODE", bizplcCode);
            this.ds_cond.setColumn(0, "BIZPLC_NM"  , bizplcNm);
        }
	}
	// 자산정보 - 사원
	if(sPopupId == "Popup1"){
		var dsEmp = new Dataset();
		var nRowCnt = dsEmp.loadXML(sReturn);
		if(nRowCnt){
			var nRow = this.ds_assetsMastr.rowposition;
			var bcncNm = dsRtn.getColumn(0, "EMPNO");
			var bcncCode = dsRtn.getColumn(0, "EMPNM");
			
			this.ds_assetsMastr.setColumn(nRow, "EMPNO", bcncNm);
			this.ds_assetsMastr.setColumn(nRow, "EMPNM", bcncCode);
		}
	}
}
