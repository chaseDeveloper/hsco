this.div_search_btn_bizplcCode_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_popup("bizplcPopup");
}

this.ds_lossManageList_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "LOSS_TM") {
		if(!this.comUtils.isNull(e.newvalue) && !this.comUtils.isTime(e.newvalue)) {
			this.gfn_message("comm.값.무효");
			obj.setColumn(e.row, e.columnid, e.oldvalue);
			return false;
		}
	}
}

this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "BIZPLC_CODE") {
		if(this.comUtils.isNull(e.newvalue)) {
			obj.setColumn(0, "BIZPLC_NM", "");
		}
	}
}





this.grd_lossManageList_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{   
	var nRow = this.ds_lossManageList.rowposition;

	if(e.cell == "1") {

		if(!authAt) {
			return false; 
		}
		
		if(Ex.util.isUpdated(this.ds_lossManageList)) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;    // 아니오
			}
		}

		var confmAt  = this.ds_lossManageList.getColumn(nRow, "CONFM_AT");
		
		this.ds_confmProcess.clearData();
		this.ds_confmProcess.addRow();
		this.ds_confmProcess.copyRow(0, this.ds_lossManageList, nRow);

		if(confmAt != "1") {
			if(application.confirm("해당건을 승인처리 하시겠습니까?\n계속 진행하시면 재물조정 대상품 데이터가 자산대장에 반영됩니다.")) {
				this.ds_confmProcess.setColumn(0, "CONFM_AT", "1");
				// 승인처리
				saveRow = this.ds_lossManageList.rowposition;
				this.fn_transaction("lossManageConfm");
			}
		}else {
			if(application.confirm("해당건을 승인취소처리 하시겠습니까?\n계속 진행하시면 최신데이터로 반영됩니다.")) {
				this.ds_confmProcess.setColumn(0, "CONFM_AT", "0");
				// 승인처리
				saveRow = this.ds_lossManageList.rowposition;
				this.fn_transaction("lossManageConfmCancel");
			}		
		}
		
	}
}

