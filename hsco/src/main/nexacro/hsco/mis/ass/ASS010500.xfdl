/*
 * 함수명  	: btn_Cancel_Row
 * 설명 		: 행취소 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Cancel_Row();
 */
this.btn_Cancel_Row = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.gfn_message("confirm.취소여부")) {
		if(obj.name == "btn_Cancel_Tpg1_L"){
			this.ds_thngReciptPlan.reset();		//DATASET ROLLBACK
			this.ds_thngReciptPlan.applyChange();	//DATASET COMMIT
			return;
		}
		
		if(obj.name == "btn_Cancel_Tpg1_R"){
			this.ds_thngReciptPlanDetail.reset();		//DATASET ROLLBACK
			this.ds_thngReciptPlanDetail.applyChange();	//DATASET COMMIT
			return;
		}
		
		if(obj.name == "btn_Cancel_Tpg1_M"){
			this.ds_atldManage.reset();		//DATASET ROLLBACK
			this.ds_atldManage.applyChange();	//DATASET COMMIT
			return;	
		}
		
		if(obj.name == "btn_Cancel_Tpg1_D"){
			this.ds_atldManageDetail.reset();		//DATASET ROLLBACK
			this.ds_atldManageDetail.applyChange();	//DATASET COMMIT
			return;
		}
	}	
}



