/*
 * 함수명  	: btn_Assets_Popup_onclick
 * 설명 		: 불용품대상 목록 -> 불용품관리상세 이동 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Assets_Popup_onclick();
 */
this.btn_move_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	if(this.ds_atldManage.getColumn(this.ds_atldManage.rowposition, "CHARGER_REGIST_AT") == '1'){
		var rowType = this.ds_atldManage.getRowType(this.ds_atldManage.rowposition);
		var confmAt = this.ds_atldManage.getColumn(this.ds_atldManage.rowposition, "DISUSE_CONFM_AT");
		
		if(!rowType){
			this.gfn_message("comm.항목.선택.요청", "불용계획");
			return;
		}
		
		if(rowType == 2){
			this.gfn_message("info.진행가능", "신규 불용계획을 저장");
			return;
		}
		
		if(confmAt != '0'){
			this.gfn_message("fail.수정.불가", "담당자 승인이 완료되어");
			return;
		}
		
		var dsRtn = new Dataset(); 
		dsRtn = this.gfn_getCheckData(this.tab_plan.tpg2.grd_atldTrgetList, "CHK");
	// 	if(this.ds_atldManageDetail.rowcount + dsRtn.rowcount > 3){
	// 		this.gfn_message("info.항목.초과.입력불가", "상세계획|3건");
	// 		return;
	// 	} 
		
		for(var i=0; i<dsRtn.rowcount; i++){
			var strVal = dsRtn.getColumn(i, "ASSETS_NO");
			var findRow = this.ds_atldManageDetail.findRow("ASSETS_NO", strVal);
			
			// 불용상세 계획에서 중복되는 자산이 없을때만 추가됨.
			if(findRow == -1){	
				var nRow = this.ds_atldManageDetail.addRow();
				this.ds_atldManageDetail.setColumn(nRow, "REQST_YEAR",                this.ds_atldManage.getColumn(this.ds_atldManage.rowposition, "REQST_YEAR"));
				this.ds_atldManageDetail.setColumn(nRow, "ODR",                       this.ds_atldManage.getColumn(this.ds_atldManage.rowposition, "ODR"));
				this.ds_atldManageDetail.setColumn(nRow, "ASSETS_NO",                 dsRtn.getColumn(i, "ASSETS_NO"));
				this.ds_atldManageDetail.setColumn(nRow, "ASSETS_CL_CODE",            dsRtn.getColumn(i, "ASSETS_CL_CODE"));
				this.ds_atldManageDetail.setColumn(nRow, "ASSETS_CL_NM",              dsRtn.getColumn(i, "ASSETS_CL_NM"));
				this.ds_atldManageDetail.setColumn(nRow, "ASSETS_SE",                 dsRtn.getColumn(i, "ASSETS_SE"));
				this.ds_atldManageDetail.setColumn(nRow, "ASSETS_NM",                 dsRtn.getColumn(i, "ASSETS_NM"));
				this.ds_atldManageDetail.setColumn(nRow, "UNTPC",                     dsRtn.getColumn(i, "UNTPC"));
				this.ds_atldManageDetail.setColumn(nRow, "QY",                        dsRtn.getColumn(i, "QY"));
				this.ds_atldManageDetail.setColumn(nRow, "UNIT",                      dsRtn.getColumn(i, "UNIT"));
				this.ds_atldManageDetail.setColumn(nRow, "STNDRD",                    dsRtn.getColumn(i, "STNDRD"));
				this.ds_atldManageDetail.setColumn(nRow, "ACQS_AMOUNT",               dsRtn.getColumn(i, "ACQS_AMOUNT"));
				this.ds_atldManageDetail.setColumn(nRow, "DPRC_ACMTL_AMOUNT",         dsRtn.getColumn(i, "DPRC_ACMTL_AMOUNT"));
				this.ds_atldManageDetail.setColumn(nRow, "GOVSUBY_ACQS_AMOUNT",       dsRtn.getColumn(i, "GOVSUBY_ACQS_AMOUNT"));
				this.ds_atldManageDetail.setColumn(nRow, "GOVSUBY_DPRC_ACMTL_AMOUNT", dsRtn.getColumn(i, "GOVSUBY_DPRC_ACMTL_AMOUNT"));
				this.ds_atldManageDetail.setColumn(nRow, "TRMEND_BLCE",               dsRtn.getColumn(i, "TRMEND_BLCE"));
			}
			
			// 상세계획으로 이동된 대상은 불용품대상에서 제외
			var assetsNO = dsRtn.getColumn(i, "ASSETS_NO");
			var nRow1 = this.ds_atldTrgetList.findRow("ASSETS_NO" ,assetsNO);
			this.ds_atldTrgetList.deleteRow(nRow1);
		}
	} else {
		this.gfn_message("info.처리불가", "담당자가 등록한 계획만 신규입력이 가능합니다.");
		return;
	}
}


