/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
		
	if(this.tab_plan.tabindex == 1){
		if(this.ds_atldManage.getColumn(this.ds_atldManage.rowposition, "CHARGER_REGIST_AT") == '1'){
			var confmAtCnt = this.ds_atldManageDetail.getCaseCount("DISUSE_CONFM_AT == '1'");
			
			if(confmAtCnt == 0){
				
					if (this.gfn_message("confirm.삭제여부")) {
							
							var bfRow = this.ds_atldManage.rowposition;
							this.ds_atldManage.deleteRow(bfRow);
							var afRow = this.ds_atldManage.rowposition;
							
							if(bfRow == afRow){
								this.ds_atldManage.set_rowposition(-1);
								this.ds_atldManage.set_rowposition(afRow);
							}
					}
			} else {
				this.gfn_message("info.처리불가", "승인된 상세계획이 존재합니다.");
				return false;
			}
		}else {
			this.gfn_message("info.처리불가", "담당자가 등록한 계획만 삭제가 가능합니다.");
			return;
		}
	}
	
}

/*
 * 함수명  	: btn_downFiles_onclick
 * 설명 		: '선택다운로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_downFiles_onclick(obj,e);
 */
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}


