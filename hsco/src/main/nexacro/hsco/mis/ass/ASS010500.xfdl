/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{	
	if(this.tab_plan.tabindex == 0){
		if(Ex.util.isUpdated(this.ds_thngReciptPlan) || Ex.util.isUpdated(this.ds_thngReciptPlanDetail)){
			
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_thngReciptPlanDetail, this.ds_validation)) {
				return false;
			}
			this.saveRow = this.ds_thngReciptPlan.rowposition;			
			
			this.fn_transaction("thngReciptPlanCUD");
		}else{
			this.gfn_message("comm.데이터.변경.없음");
		}
	}
	if(this.tab_plan.tabindex == 1){
		if(Ex.util.isUpdated(this.ds_atldManage) || Ex.util.isUpdated(this.ds_atldManageDetail)){
			
			// validation 체크(불용품관리)
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_atldManage, this.ds_validation)) {
				return false;
			}
			
			// validation 체크(불용품관리상세)
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_atldManageDetail, this.ds_validation)) {
				return false;
			}
			this.saveRow = this.ds_atldManage.rowposition;
			
			this.fn_transaction("save");
		}else{
			this.gfn_message("comm.데이터.변경.없음");
		}
	}

}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "thngReciptPlanAcList":        // 취득물품(수급계획목록) 조회 
			var sController   	= "hsco/mis/ass/ASS010500/thngReciptPlanAcList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_thngReciptPlan=output1";
			break;
		case "thngReciptPlanDetailList":    // 취득물품(수급계획상세) 조회 
			var sController   	= "hsco/mis/ass/ASS010500/thngReciptPlanDetail.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_thngReciptPlanDetail=output1 ds_fileList=output2";
			break;
		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/ass/ASS010500/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_fileList=output1";
			break;		
		case "thngReciptPlanCUD":   // 취득물품 저장
			var sController   	= "hsco/mis/ass/ASS010500/thngReciptPlanCUD.do";	
			var sInDatasets   	= "input1=ds_thngReciptPlanDetail:U input2=ds_thngReciptPlan:U";
			var sOutDatasets  	= "";
			break;
		case "select":				// 불용품관리 조회 
			var sController   	= "hsco/mis/ass/ASS010500/atldManageList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_atldManage=output1";
			break;
		case "select01":			// 불용품관리상세 조회 
			var sController   	= "hsco/mis/ass/ASS010500/atldManageDetail.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_atldManageDetail=output1";
			break;
		case "select02":			// 불용품대상 조회 
			var sController   	= "hsco/mis/ass/ASS010500/atldTrgetList.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_atldTrgetList=output1";
			break;			
		case "save":				// 불용품관리 저장
			var sController   	= "hsco/mis/ass/ASS010500/atldManageCUD.do";	
			var sInDatasets   	= "input1=ds_atldManage:U input2=ds_atldManageDetail:U";
			var sOutDatasets  	= "";
			break;	
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "thngReciptPlanAcList":    // 취득물품 조회
				break;
			case "thngReciptPlanDetailList":    // 취득물품 조회
				
				if(this.saveRow > -1){
					this.ds_thngReciptPlan.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "thngReciptPlanCUD":    	// 취득물품 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "selectFileList":      // 첨부파일 조회
				this.ds_fileList.filter("USE_AT == '1'");
				break;
			case "select":				// 불용품관리 조회
				break;
			case "select01":			// 불용품관리상세 조회
				
				if(this.saveRow > -1){
					this.ds_atldManage.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "save":		// 불용품관리 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}

/*
 * 함수명  	: fn_callback5
 * 설명 		: 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback5();
 */
