		this.ds_cond02.setColumn(0, "REQST_YEAR", reqstYear);
		this.ds_cond02.setColumn(0, "ODR",        odr);
		
		this.fn_transaction("select01");
	}
}

/*
 * 함수명  	: DS_ASSETS_MASTR_TABLE_onrowposchanged
 * 설명 		: 상세계획 COLUMN 변경 시 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.DS_ASSETS_MASTR_TABLE_onrowposchanged();
 */
this.ds_atldManageDetail_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var nRow = obj.rowposition;
	
	// 승인여부
	if(e.columnid == "DISUSE_CONFM_AT"){
		if(e.newvalue == "1") {
			obj.setColumn(nRow, "DISUSE_CONFM_DE", this.dateUtils.today());
		}else {
			var processAt = obj.getColumn(mRow, "DISUSE_PROCESS_AT");

			if(processAt != "1" ) {
				this.gfn_message("fail.수정.불가", "처분이 완료되어");
				return false;
			}
			
			obj.setColumn(nRow, "DISUSE_CONFM_DE", "");
			obj.setColumn(nRow, "DISUSE_AMOUNT"  , "0");
			obj.setColumn(nRow, "DISUSE_PRVONSH" , "");
			
			// 처분금액 재계산
			this.ds_atldManage.setColumn(this.ds_atldManage.rowposition, "DISUSE_AMOUNT", obj.getSum("DISUSE_AMOUNT"));
		}
	}
	
	if(e.columnid == "DPRC_ACMTL_AMOUNT"){
		this.ds_atldManage.set_updatecontrol(false);
		this.ds_atldManage.setColumn(this.ds_atldManage.rowposition, "DPRC_ACMTL_AMOUNT", obj.getSum("DPRC_ACMTL_AMOUNT"));
		this.ds_atldManage.set_updatecontrol(true);
	}
	
	if(e.columnid == "ACQS_AMOUNT"){
		this.ds_atldManage.set_updatecontrol(false);
		this.ds_atldManage.setColumn(this.ds_atldManage.rowposition, "ACQS_AMOUNT", obj.getSum("ACQS_AMOUNT"));
		this.ds_atldManage.set_updatecontrol(true);
	}
	
	if(e.columnid == "DISUSE_AMOUNT"){
		this.ds_atldManage.set_updatecontrol(false);
		this.ds_atldManage.setColumn(this.ds_atldManage.rowposition, "DISUSE_AMOUNT", obj.getSum("DISUSE_AMOUNT"));
		this.ds_atldManage.set_updatecontrol(true);
	}
}

