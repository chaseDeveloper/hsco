    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ASS010400.xfdl
 * 화면명   : 물품수급계획등록
 * 화면설명 : 물품수급계획등록을 관리한다.
 * 작성일   : 2015.07.06
 * 작성자   : 이혜선
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.currow;
this.saveRow = -1;
var authAt = false;
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ASS010400_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.ASS010400_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// 초기 검색조건
	this.ds_cond.setColumn(0, "RECIPT_YEAR",    this.dateUtils.today().substr(0,4));
	this.ds_cond.setColumn(0, "REQUST_DEPT",    this.gfn_getDeptId());
	this.ds_cond.setColumn(0, "REQUST_DEPT_NM", this.gfn_getDeptName());

	// 데이터셋 동적 바인딩(부서팝업)
	this.div_search.div_dept.fn_setBind("ds_cond", "REQUST_DEPT", "REQUST_DEPT_NM");

	// 불용대상자산목록 검색조건
	this.ds_cond03.clearData();
	this.ds_cond03.addRow();
	
	// 공통코드
	var comboParams = [
		["ds_cmp002", "CMP002", "Y", ""]	// 단위
	];

	// 권한 분리
	if(this.gfn_authGrpId("ASS_ADMIN") > -1) {		// 자산결재자
		authAt = true;
	}
	
	trace("authAt : "+authAt);
	
	if(authAt) {
		// 검색조건 - 부서
		this.div_search.div_dept.fn_setReadonly(false);
		this.div_search.div_dept.set_enable(true);
		this.div_search.div_dept.div_Search.edt_deptCode.set_cssclass("");
		this.div_search.div_dept.div_Search.edt_deptNm.set_cssclass("");
		
		// 자산현황 - 승인여부
		this.grd_atldManageDetail.setCellProperty("body", 0, "edittype", "checkbox");
	
	}else {
	
		// 검색조건 - 부서
		this.div_search.div_dept.fn_setReadonly(true);
		this.div_search.div_dept.set_enable(false);
		this.div_search.div_dept.div_Search.edt_deptCode.set_cssclass("edt_WF_Essential");
		this.div_search.div_dept.div_Search.edt_deptNm.set_cssclass("edt_WF_Essential");
		
		// 자산현황 - 승인여부
		this.grd_atldManageDetail.setCellProperty("body", 0, "edittype", "none");
	}
		
	var callback = function (svcId, errorCode, errorMsg) {
		// 조회 이벤트
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(Ex.util.isUpdated(this.ds_atldManageDetail)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;
		}
	}
	
 	//this.ds_atldManage.clearData();
 	this.ds_atldManageDetail.clearData();
 	this.ds_atldTrgetList.clearData();
 	
 	this.fn_transaction("select");
}

/*
 * 함수명  	: fn_search_sub02
 * 설명 		: 조회 이벤트 처리(불용품대상)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub02();
 */
this.fn_search_sub02 = function()
{	
	var mRow = this.ds_atldManage.rowposition;
	var deptCode = this.ds_atldManage.getColumn(mRow, "REQUST_DEPT");
	
	//this.ds_cond03.clearData();
	//this.ds_cond03.addRow();
	
// 	this.ds_cond03.setColumn(0, "ASSETS_CL_NM"  , deptCode);
// 	this.ds_cond03.setColumn(0, "ASSETS_CL_CODE", deptCode);
// 	this.ds_cond03.setColumn(0, "ASSETS_NM"     , deptCode);
	this.ds_cond03.setColumn(0, "DEPT_CODE"     , deptCode);
	this.fn_transaction("select02");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_atldManage) || Ex.util.isUpdated(this.ds_atldManageDetail)){
		// validation 체크(불용품관리)
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_atldManage, this.ds_validation)) {
			return false;
		}
		
		// validation 체크(불용품관리상세)
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_atldManageDetail, this.ds_validation)) {
			return false;
		}
		this.saveRow = this.ds_atldManage.rowposition;
		
		this.fn_transaction("save");
	}else{
		this.gfn_message("comm.데이터.변경.없음");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "select":				// 불용품관리 조회 
			var sController   	= "hsco/mis/ass/ASS010400/atldManageList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_atldManage=output1";
			break;
		case "select01":			// 불용품관리상세 조회 
			var sController   	= "hsco/mis/ass/ASS010400/atldManageDetail.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_atldManageDetail=output1";
			break;
		case "save":				// 불용품관리 저장
			var sController   	= "hsco/mis/ass/ASS010400/atldManageCUD.do";	
			var sInDatasets   	= "input1=ds_atldManage:U input2=ds_atldManageDetail:U";
			var sOutDatasets  	= "";
			break;	
		case "select02":			// 불용품대상 조회 
			var sController   	= "hsco/mis/ass/ASS010400/atldTrgetList.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_atldTrgetList=output1";
			break;	
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "select":				// 불용품관리 조회
				this.ds_atldManage.applyChange();
				
				break;
			case "select01":			// 불용품관리상세 조회
				this.ds_atldManageDetail.applyChange();
				
				if(this.saveRow > -1){
					this.ds_atldManage.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				this.fn_search_sub02()
				break;
			case "save":				// 불용품관리 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "select02":			// 불용품대상 조회 
				break;
		}
	}
}

/*
 * 함수명  	: fn_callback5
 * 설명 		: 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback5();
 */
this.fn_callback5 = function(sPopupId, sReturn)
{
    var dsRtn = new Dataset();
    nRowCnt = dsRtn.loadXML(sReturn);
    
	if(sPopupId == "Popup")
	{
        if(nRowCnt){
            this.ds_cond.setColumn(0, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.ds_cond.setColumn(0, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
        }
	}
	
	if(sPopupId == "SEARCH")
	{
