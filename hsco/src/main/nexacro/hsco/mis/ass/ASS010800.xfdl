/*
 * 함수명  	: tab_plan_tpg1_btn_insertAssets_onclick
 * 설명 		: 자산대장 등록
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_plan_tpg1_btn_insertAssets_onclick();
 */
this.tab_plan_tpg1_btn_insertAssets_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
    if( this.ds_thngReciptPlanExec.getColumn(this.ds_thngReciptPlanExec.rowposition, "REGIST_AT") == "1") {
        this.gfn_message("info.처리불가", "이미 자산대장에 등록되었습니다.");
        return;
    }

	if (this.ds_thngReciptPlanExec.getColumn(this.ds_thngReciptPlan.rowposition, "REG_CHK") != "1") {
        this.gfn_message("info.처리불가", "지출결의서를 먼저 등록해야 합니다.");
		return false;
	}
	
	if(this.gfn_message("comm.행위여부","자산대장 등록을 진행") ){
		this.ds_assets.clearData();
		var nRow = this.ds_assets.addRow();
		this.ds_assets.copyRow(nRow, this.ds_thngReciptPlanExec, this.ds_thngReciptPlanExec.rowposition);
		this.fn_transaction("insertAssetsMastr");
	}
}

/*
 * 함수명  	: fn_PopupCallback
 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_PopupCallback();
 */
this.fn_popupCallback = function(strID, variant)
{
	switch(strID) {
		case "expDsnPopup":	// 구입과지출결의서 팝업콜백
			if(variant == "I") {
				// 수급계획상세 재조회
				this.fn_transaction("thngReciptPlanExec");
			}
			break;
		default:
			break;
	}
