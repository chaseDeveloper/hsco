this.ds_thngReciptPlanExec_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "UNTPC" || e.columnid == "QY"){
		var UNTPC = this.ds_thngReciptPlanExec.getColumn(this.ds_thngReciptPlanExec.rowposition, "UNTPC");
		var QY = this.ds_thngReciptPlanExec.getColumn(this.ds_thngReciptPlanExec.rowposition, "QY");
		var AMOUNT = 0; 
		
		if( !this.comUtils.isNullEmpty(UNTPC) && !this.comUtils.isNullEmpty(QY) ){			
			AMOUNT = (UNTPC*1) * (QY*1);
			this.ds_thngReciptPlanExec.setColumn(this.ds_thngReciptPlanExec.rowposition, "AMOUNT", AMOUNT);
		}
	}	
}


