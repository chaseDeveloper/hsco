/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.tab_plan.tabindex == 0){
		var confmAt   = this.ds_thngReciptPlan.getColumn(this.ds_thngReciptPlan.rowposition, "CONFM_AT");
		
		if(confmAt != "1"){
		
			// 전자결재 상태 체크 2016.7
			// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
			// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
			// 기안전인것 : 재상신 및 수정/삭제
			// 결재상태가 상신전 외는 신규로 다시 진행.
			var draftKnd = this.ds_thngReciptPlan.getColumn(this.ds_thngReciptPlan.rowposition, "DRFT_STS");
			var bEnable = (draftKnd == "1" || draftKnd == "9" ) ? false : true;
			
			if(bEnable){	// 기안전
				if (this.gfn_message("confirm.삭제여부")) {
					
					var bfRow = this.ds_thngReciptPlan.rowposition;
					this.ds_thngReciptPlan.deleteRow(bfRow);
					var afRow = this.ds_thngReciptPlan.rowposition;
					
					if(bfRow == afRow){
						this.ds_thngReciptPlan.set_rowposition(-1);
						this.ds_thngReciptPlan.set_rowposition(afRow);
					}
				}
			} 
			else {
				this.gfn_message("fail.수정.불가", "결재가 진행되어");
				return false;
			}		// end of if(bEnable)
			
		} 
		else {
			this.gfn_message("fail.수정.불가", "담당자 승인이 완료되어");
			return false;		
		}	// end of if(confmAt
	}

	
	if(this.tab_plan.tabindex == 1){
		var confmAt = this.ds_atldManage.getColumn(this.ds_atldManage.rowposition, "DISUSE_CONFM_AT");
		
		if(confmAt != "1"){
			// 전자결재 상태 체크 2016.7
			// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
			// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
			// 기안전인것 : 재상신 및 수정/삭제
			// 결재상태가 상신전 외는 신규로 다시 진행.
			var draftKnd = this.ds_atldManage.getColumn(this.ds_atldManage.rowposition, "DRFT_STS");
			var bEnable = (draftKnd == "1" || draftKnd == "9") ? false : true;
			
			if(bEnable){	// 기안전
				if (this.gfn_message("confirm.삭제여부")) {
					
					var bfRow = this.ds_atldManage.rowposition;
					this.ds_atldManage.deleteRow(bfRow);
					var afRow = this.ds_atldManage.rowposition;
					
					if(bfRow == afRow){
						this.ds_atldManage.set_rowposition(-1);
						this.ds_atldManage.set_rowposition(afRow);
					}
				}	// end of if (this.gfn_message
			} 
			else {
				this.gfn_message("fail.수정.불가", "결재가 진행되어");
				return false;
			}	// end of if(bEnable)
				
		} 
		else {
			this.gfn_message("fail.수정.불가", "담당자 승인이 완료되어");
			return false;	
		}	
	}
}

