/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.currow;
this.saveRow = -1;

this.checkDs = [this.ds_thngReciptPlan , this.ds_thngReciptPlanDetail, this.ds_atldManage, this.ds_atldManageDetail];
this.File_UpDownManager = null;

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ASS010800_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.ASS010800_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// 초기 검색조건
	this.ds_cond.setColumn(0, "RECIPT_YEAR",    this.dateUtils.today().substr(0,4));
	this.ds_cond.setColumn(0, "REQUST_DEPT",    this.gfn_getDeptId());
	this.ds_cond.setColumn(0, "REQUST_DEPT_NM", this.gfn_getDeptName());

	// 데이터셋 동적 바인딩(부서팝업)
	this.div_search.div_dept.fn_setBind("ds_cond","REQUST_DEPT","REQUST_DEPT_NM");
	
	this.div_search.div_dept.fn_setReadonly(true);
// 	this.div_search.div_dept.div_Search.edt_deptCode.set_cssclass("edt_WF_Essential");
// 	this.div_search.div_dept.div_Search.edt_deptNm.set_cssclass("edt_WF_Essential");
	
	// 공통코드
	var comboParams = [
		["ds_acc001", "ACC001", "Y", ""]	// 사업장구분
	   ,["ds_cmp002", "CMP002", "Y", ""]	// 단위
	   ,["ds_ass004", "ASS004", "Y", ""]	// 처분구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		this.ds_ass004.filter("CODE != '0'");
		
		// 조회 이벤트
		this.fn_search();
		//this.fn_search_sub02();
		
	};
	this.gfn_comboLoad (comboParams, callback);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "ASS");
	this.File_UpDownManager.init(fileConfig);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(this.tab_plan.tabindex == 0){
		
		this.ds_thngReciptPlan.clearData();
		this.ds_thngReciptPlanDetail.clearData();
		this.ds_fileList.clearData();
		this.fn_transaction("thngReciptPlanAcList");
	}
	
	if(this.tab_plan.tabindex == 1){
		
		this.ds_atldManage.clearData();
		this.ds_atldManageDetail.clearData();
		this.ds_atldTrgetList.clearData();
		this.fn_transaction("select");
		this.fn_transaction("select02");
	}
}

/*
 * 함수명  	: fn_search_sub02
 * 설명 		: 조회 이벤트 처리(불용품대상)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub02();
 */
this.fn_search_sub02 = function()
{	
	this.fn_transaction("select02");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(this.tab_plan.tabindex == 0){
		if(Ex.util.isUpdated(this.ds_thngReciptPlanExec)){
			// validation 체크 취득물품(수급계획목록)
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_thngReciptPlanExec, this.ds_validation)) {
				return false;
			}			
			
			var tot = Number(this.ds_thngReciptPlanDetail.getColumn(this.ds_thngReciptPlanDetail.rowposition, "QY"));
			var cnt = 0;
			for (var i = 0; i < this.ds_thngReciptPlanExec.rowcount; i++)
				cnt = cnt + Number(this.ds_thngReciptPlanExec.getColumn(i, "QY"));
			if (tot < cnt) {
			    alert("실행수량은 계획수량을 초과할 수 없습니다.");
			    return;
			}
			
			this.saveRow = this.ds_thngReciptPlanExec.rowposition;						
			this.fn_transaction("thngReciptExecCUD");
		}else{
			this.gfn_message("comm.데이터.변경.없음");
		}
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "thngReciptPlanAcList":        // 취득물품(수급계획목록) 조회 
			var sController   	= "hsco/mis/ass/ASS010800/thngReciptPlanAcList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_thngReciptPlan=output1";
			break;
		case "thngReciptPlanDetailList":    // 취득물품(수급계획상세) 조회
			var sController   	= "hsco/mis/ass/ASS010800/thngReciptPlanDetail.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_thngReciptPlanDetail=output1";
			break;
		case "thngReciptPlanExec":    // 취득물품(수급계획실행) 조회
			var sController   	= "hsco/mis/ass/ASS010800/thngReciptPlanExec.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_thngReciptPlanExec=output1";
			break;
		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/ass/ASS010800/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_fileList=output1";
			break;		
		case "thngReciptExecCUD":   // 취득물품 실행 저장
			var sController   	= "hsco/mis/ass/ASS010800/thngReciptExecCUD.do";	
			var sInDatasets   	= "input1=ds_thngReciptPlanExec:U";
			var sOutDatasets  	= "";
			break;
		case "select":				// 불용품관리 조회 
			var sController   	= "hsco/mis/ass/ASS010800/atldManageList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_atldManage=output1";
			break;
		case "select01":			// 불용품관리상세 조회 
			var sController   	= "hsco/mis/ass/ASS010800/atldManageDetail.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_atldManageDetail=output1";
			break;
		case "save":				// 불용품관리 저장
			var sController   	= "hsco/mis/ass/ASS010800/atldManageCUD.do";	
			var sInDatasets   	= "input1=ds_atldManage:U input2=ds_atldManageDetail:U";
			var sOutDatasets  	= "";
			break;	
		case "select02":			// 불용품대상 조회 
			var sController   	= "hsco/mis/ass/ASS010800/atldTrgetList.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_atldTrgetList=output1";
			break;
		case "approve":				// 전자결재
			var sController   	= "hsco/mis/ass/ASS010800/approve.do";	
			var sInDatasets   	= "input1=ds_approve";
			var sOutDatasets  	= "ds_approve=output1";
			break;
		case "insertAssetsMastr":	// 자산대장 등록
			var sController   	= "hsco/mis/ass/ASS010800/insertAssetsMastr.do";	
			var sInDatasets   	= "input1=ds_assets:U";
			var sOutDatasets  	= "";
			break;						
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "thngReciptPlanAcList":    // 취득물품 조회
				this.ds_thngReciptPlan.applyChange();
				
				break;
			case "thngReciptPlanDetailList":    // 취득물품 조회
				this.ds_thngReciptPlanDetail.applyChange();
				
				if(this.saveRow > -1){
					this.ds_thngReciptPlan.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "thngReciptPlanExec":    // 취득물품 조회
				this.ds_thngReciptPlanExec.applyChange();
				
				if(this.saveRow > -1){
					this.ds_thngReciptPlanExec.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "thngReciptExecCUD":    	// 취득물품 저장	
				this.fn_transaction("thngReciptPlanExec");
				this.gfn_message("success.처리.성공");								
				break;
			case "approve":
				var sanctnNO = this.ds_approve.getColumn(0,"SANCTN_NO");				
				if(this.tab_plan.tabindex == 0){
					this.ds_thngReciptPlan.set_updatecontrol(false);
					this.ds_thngReciptPlan.setColumn(this.ds_thngReciptPlan.rowposition, "SANCTN_NO", sanctnNO);
					this.ds_thngReciptPlan.set_updatecontrol(true);
				}								
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
				break;		
				
            case "insertAssetsMastr":
				this.fn_transaction("thngReciptPlanExec");				
				this.gfn_message("success.처리.성공");				
				break;
		}
	}
}

/*
 * 함수명  	: fn_callback5
 * 설명 		: 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback5();
 */
this.fn_callback5 = function(sPopupId, sReturn)
{
    var dsRtn = new Dataset();
    nRowCnt = dsRtn.loadXML(sReturn);
    
	if(sPopupId == "Popup")
	{
        if(nRowCnt){
            this.ds_cond.setColumn(0, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.ds_cond.setColumn(0, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
        }
	}
	if(sPopupId == "Popup1")
	{
		if(nRowCnt){
            this.ds_thngReciptPlanDetail.setColumn(this.currow,"ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
			this.ds_thngReciptPlanDetail.setColumn(this.currow,"ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
        }
	}
	
	if(sPopupId == "SEARCH")
	{
        if(nRowCnt){
            this.ds_cond03.setColumn(0, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.ds_cond03.setColumn(0, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
        }
	}
}
