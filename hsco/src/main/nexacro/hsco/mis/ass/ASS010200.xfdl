/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.saveRow = -1;
this.checkDs = [this.DS_ASSETS_MASTR_TABLE];
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ASS010200_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.ASS010200_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.ds_cond.setColumn(0, "ASSETS_CL_NM", "소프트웨어")
	this.ds_cond.setColumn(0, "ASSETS_CL_CODE", "B001")

	
	// TODO..
	this.div_work.Tab00.tabpage3.div_dept.fn_setBind("DS_ASSETS_MASTR_TABLE","DEPT_CODE","DEPT_NM");
	this.div_work.Tab00.tabpage3.div_EMP.fn_setBind("DS_ASSETS_MASTR_TABLE","EMPNO","EMPNM");
	
	this.div_work.Tab00.tabpage3.div_dept.div_Search.edt_deptCode.set_cssclass("edt_WF_Essential");
	this.div_work.Tab00.tabpage3.div_dept.div_Search.edt_deptNm.set_cssclass("edt_WF_Essential");
	
	
	// 공통코드
	var comboParams = [
		["ds_acc001", "ACC001", "Y", ""]	// 사업장구분
	   ,["ds_ass006", "ASS006", "Y", ""]	// 자산구분
	   ,["ds_cmp002", "CMP002", "Y", ""]	// 단위
	];
	var callback = function (svcId, errorCode, errorMsg) {
		
		// 조회 이벤트
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
	
	this.ds_cond.setColumn(0, "ACQS_DE1",   this.dateUtils.today().substr(0,4)+"0101");
	this.ds_cond.setColumn(0, "ACQS_DE2",   this.dateUtils.today());
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}

	this.DS_ASSETS_MASTR_DETAIL_TABLE.clearData();
	this.DS_TBASS_ASSETS_MASTR_HIST.clearData();
	
	this.div_work.Tab00.tabpage3.ImageViewer04.set_image("");	
	
	this.ds_fileList.clearData();
	this.ds_cond_file.clearData();
	this.ds_cond_file.addRow();
		

	this.fn_transaction("assetsMastrList");
}

/*
 * 함수명  	: fn_insert
 * 설명 		: 행추가 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var nRow = this.DS_ASSETS_MASTR_TABLE.insertRow(0);
	this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "DPRT_CNT", 0);
	this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "QY", 1);
	this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "ASSETS_CL_CODE", "B001");
	this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "ASSETS_CL_NM", "소프트웨어");
	this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "ACQS_DE", this.dateUtils.today());
	this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId());
	this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "DEPT_NM",   this.gfn_getDeptName());
	
	
	
	this.div_work.Tab00.tabpage3.ImageViewer04.set_image("");	
	this.ds_fileList.clearData();
	this.ds_cond_file.clearData();
	this.ds_cond_file.addRow();
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	//물품수급계획에서 등록한 자료는 삭제 불가 2016.08.09
	var CNT = this.DS_ASSETS_MASTR_TABLE.getColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "CNT");
	if(CNT > 0){
		this.gfn_message("fail.삭제.불가", "물품수급계획에서 등록한 자료는");
		return;
	}
	
	var dprtComptAt = this.DS_ASSETS_MASTR_TABLE.getColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "DPRT_CNT");
	if(dprtComptAt == 0)
	{
		if ( this.gfn_message("confirm.삭제여부") ) {
			var nRow = this.DS_ASSETS_MASTR_TABLE.rowposition;
			this.DS_ASSETS_MASTR_TABLE.deleteRow(nRow);	
		}
	}
	else{		
		this.gfn_message("fail.삭제.불가", "감가상각내역이 존재하여");
	}
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{

	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.DS_ASSETS_MASTR_TABLE) && !this.comUtils.isDatasetUpdated(this.DS_SW_EMP)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(자산)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_ASSETS_MASTR_TABLE, this.ds_validation)) {
		return;
	}
	
	// validation 체크(자산사용자)
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_SW_EMP, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.DS_ASSETS_MASTR_TABLE.rowposition;
		this.fn_transaction("swAssetsMastrCUD");
		this.fn_transaction("assetsMastrCUD");
	}else{
		this.gfn_message("comm.데이터.변경.없음");
	}



}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "assetsMastrList":        // 자산분류 조회 
			var sController   	= "hsco/mis/ass/ASS010200/assetsMastrList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "DS_ASSETS_MASTR_TABLE=output1";
			break;
		case "assetsMastrR":			// 감가상각내역, 변경내역, 자산사용자 조회 
			var sController   	= "hsco/mis/ass/ASS010200/assetsMastrR.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "DS_ASSETS_MASTR_DETAIL_TABLE=output1 DS_TBASS_ASSETS_MASTR_HIST=output2 DS_SW_EMP=output3";
			break;
		case "swAssetsMastrCUD":		// 소프트웨어, 사용자 등록 수정
			var sController   	= "hsco/mis/ass/ASS010200/assetsSwU.do";	
			var sInDatasets   	= "input1=DS_ASSETS_MASTR_TABLE:U input2=DS_SW_EMP:U";
			var sOutDatasets  	= "";
			break;
		case "assetsMastrCUD":			// 자산분류 저장
			var sController   	= "hsco/mis/ass/ASS010100/assetsMastrCUD.do";	
			var sInDatasets   	= "input1=DS_ASSETS_MASTR_TABLE:U input2=DS_SW_EMP:U";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":      	// 첨부파일 조회
			var sController   	= "hsco/mis/ass/ASS000000/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond_file";
			var sOutDatasets  	= "ds_fileList=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode == 0){
		switch (strSvcId) {	
			case "assetsMastrList":		// 자산분류 조회
				this.DS_ASSETS_MASTR_TABLE.applyChange();
				
				if(this.saveRow > -1){
					this.DS_ASSETS_MASTR_TABLE.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "assetsMastrR":		// 감가상각내역 조회
				break;
			case "swAssetsMastrCUD":	// 자산분류 저장
				
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "assetsMastrCUD":		// 자산분류 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
				
			case "selectFileList":      // 첨부파일 조회 (이미지)
				if(this.ds_fileList.rowcount > 0){
					var fileExtsn = this.ds_fileList.getColumn(0, "FILE_EXTSN");
					var fileSn = this.ds_fileList.getColumn(0, "FILE_SN");			// atchFileInfArr[0];	// 파일일련번호(파일경로)
					var fileOrder = this.ds_fileList.getColumn(0, "FILE_ORDR");	// atchFileInfArr.length > 2 ? atchFileInfArr[1] : "";	// 파일 순차번호
					var fileName = this.ds_fileList.getColumn(0, "ATCH_FILE");		// 실제 파일명
					var fileType = this.ds_fileList.getColumn(0, "FILE_TY");  // 파일 타입
					
					if(fileType.indexOf("image/")>-1){
						var param = {};
						param["FILE_SN"] = fileSn;
						param["FILE_ORDR"] = fileOrder;
						
						var strUrl = application.gv_host + "hsco/cmm/download/viewImage.do";
						var params = "?FILE_SN=" + fileSn + "&FILE_ORDR=" + fileOrder;
						this.div_work.Tab00.tabpage3.ImageViewer04.set_image(strUrl + params);
					}
				}
				break;		
				
		}
		
	}
}

/*
 * 함수명  	: fn_callback5
 * 설명 		: 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback5();
 */
this.fn_callback5 = function(sPopupId, sReturn)
{
    var dsRtn = new Dataset();
    nRowCnt = dsRtn.loadXML(sReturn);
    
	if(sPopupId == "SEARCH")
	{
        if(nRowCnt){
            this.ds_cond.setColumn(0, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.ds_cond.setColumn(0, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
        }
	}

	if(sPopupId == "GRID")
	{
        if(nRowCnt){
			var nRow = this.DS_ASSETS_MASTR_TABLE.rowposition;
			
			this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "ASSETS_SE", null);
            this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
            this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "DPRC_MTH",       dsRtn.getColumn(0, "DPRC_MTH"));
            this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "CN_YYCNT",       dsRtn.getColumn(0, "CN_YYCNT"));
            this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "DPRC_RT",        dsRtn.getColumn(0, "DPRC_RT"));
            
            var clCode = dsRtn.getColumn(0, "ASSETS_CL_CODE").substr(0,1);
            if(clCode == "E" || clCode == "F" || clCode == "G"){
				var nRowCnt  = this.ds_acc001.rowcount;
				
				if(nRowCnt > 0){
					var clCode2  = this.ds_acc001.getColumn(0, "CODE");
					
					this.DS_ASSETS_MASTR_TABLE.setColumn(nRow, "ASSETS_SE", clCode2);
				}
            }
        }
	}
	
	//사원선택
	if(sPopupId == "POPUP_EMP"){
		var dsEmp = new Dataset();
		nRowCnt = dsEmp.loadXML(sReturn);
		
		if(nRowCnt){
			this.DS_SW_EMP.setColumn(this.DS_SW_EMP.rowposition, "ASSETS_EMPNO", dsEmp.getColumn(0, "EMPNO"));
			this.DS_SW_EMP.setColumn(this.DS_SW_EMP.rowposition, "ASSETS_EMPNM", dsEmp.getColumn(0, "EMPNM"));
			this.DS_SW_EMP.setColumn(this.DS_SW_EMP.rowposition, "ASSETS_NO", this.DS_ASSETS_MASTR_TABLE.getColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "ASSETS_NO"));
		}
	}
	
	//사원선택
	if(sPopupId == "Popup1"){
		var dsEmp = new Dataset();
		nRowCnt = dsEmp.loadXML(sReturn);
		
		if(nRowCnt){
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "EMPNO", dsEmp.getColumn(0, "EMPNO"));
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "EMPNM", dsEmp.getColumn(0, "EMPNM"));
		}
	}
	
	if(sPopupId == "empno_P01") {
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "DEPT_CODE", dsRtn.getColumn(0, "DEPT_CODE"));
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "DEPT_NM",   dsRtn.getColumn(0, "DEPT_NM"));
		}
	}
	
	if(sPopupId == "CTR010101_P02") {
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "PUCHAS_BCNC", dsRtn.getColumn(0, "BCNC_CODE"));
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "BCNC_NM",     dsRtn.getColumn(0, "BCNC_NM"));
		}
	}
	
	// 자산 이미지 등록
	if(sPopupId == "ASS000000_P01"){
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "FILE_SN", dsRtn.getColumn(0, "FILE_SN"));
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "FILE_CNT", dsRtn.getColumn(0, "FILE_CNT"));
		}
	}
	
		if(sPopupId == "bizplcPopup"){
		var dsbizplc = new Dataset();
		nRowCnt = dsbizplc.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "BIZPLC_CODE", dsRtn.getColumn(0, "BIZPLC_CODE"));
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "BIZPLC_NM", dsRtn.getColumn(0, "BIZPLC_NM"));
			
			
		}
	}
	
	if(sPopupId == "bcncPopup"){
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(nRowCnt){
			this.DS_ASSETS_MASTR_TABLE.setColumn(this.DS_ASSETS_MASTR_TABLE.rowposition, "BCNC_NM", dsRtn.getColumn(0, "BCNC_NM"));
		}
	}
	

	
	
