{
	this.ds_cond2.clearData();
	this.ds_cond2.addRow();
	
	this.ds_assetsDetail.clearData();
	
// 	if(obj.getRowType(e.newrow) == "2" && this.comUtils.isNull(obj.getColumn(e.newrow, "CONFM_AT"))) {
// 		obj.setColumn(e.newrow, "CONFM_AT"     , "0");
