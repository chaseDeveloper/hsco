    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ASS011300.xfdl
 * 화면명   : 재물조사
 * 화면설명 : 재물조사 계획등록을 관리한다.
 * 작성일   : 2015.07.06
 * 작성자   : 이혜선
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.currow;
this.saveRow = -1;

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ASS011300_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.ASS011300_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// 초기 검색조건
	this.ds_cond.setColumn(0, "DE1",   this.dateUtils.today().substr(0,4)+"0101");
	this.ds_cond.setColumn(0, "DE2",   this.dateUtils.today());

	// 공통코드
	var comboParams = [
		["ds_cmp002", "CMP002", "Y", ""]	// 단위
	   ,["ds_ass007", "ASS007", "Y", ""]	// 물품상태
	];
	
	var callback = function (svcId, errorCode, errorMsg) {
		
		// 조회 이벤트
		this.fn_search();
		
	};
	this.gfn_comboLoad (comboParams, callback);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}




/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "ASS");
	this.File_UpDownManager.init(fileConfig);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(재물조사계획)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_pptyPlanList.clearData();
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	this.fn_transaction("pptyPlanList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None	
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_pptyPlanList) &&!this.comUtils.isDatasetUpdated(this.ds_pptyDetailList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}

	if(!this.gfn_checkValidation_NoUpdChk(this.ds_pptyDetailList, this.ds_validation)) {
			return false;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.saveRow = this.ds_pptyPlanList.rowposition;
		this.fn_transaction("pptyPlanListCUD");
	}

}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_pptyPlanList) && !this.comUtils.isDatasetUpdated(this.ds_pptyDetailList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_pptyDetailList.reset();		//DATASET ROLLBACK
		this.ds_pptyDetailList.applyChange();	//DATASET COMMIT
		
		this.ds_pptyPlanList.reset();		//DATASET ROLLBACK
		this.ds_pptyPlanList.applyChange();	//DATASET COMMIT
		return;	
	}
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "pptyPlanList":        // 재물조사 리스트  조회 
			var sController   	= "hsco/mis/ass/ASS011300/pptyPlanList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_pptyPlanList=output1";
			break;
		case "pptyDetailList":      // 재물조사 리스트 디테일 정보 조회 
			var sController   	= "hsco/mis/ass/ASS011300/pptyPlanDetail.do";	
			var sInDatasets   	= "input1=ds_cond_sub";
			var sOutDatasets  	= "ds_pptyDetailList=output1 ds_fileList=output2";
			break;
		case "pptyPlanListCUD":		// 재물조사 저장
			var sController   	= "hsco/mis/ass/ASS011300/pptyPlanListCUD.do";	
			var sInDatasets   	= "input1=ds_pptyPlanList:U input2=ds_pptyDetailList:U";
			var sOutDatasets  	= "";
			break;		
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "pptyPlanList" :
				if(this.saveRow > -1){
					this.ds_pptyPlanList.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "pptyDetailList" : 
				break;
			case "pptyPlanListCUD":    	// 취득물품 저장
				this.fn_search();
				break;
		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
************************************************************************
* 1. Button 영역 
* 2. 데이터셋 영역
* 3. 탭 영역
* 4. 그리드 영역
* 5. 팝업영역
************************************************************************/

/**********************************************************************
* Button 영역 
***********************************************************************/


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_pptyPlanList.rowposition;
	if(this.ds_pptyPlanList.getColumn(nRow, "INVG_AT") == '1') {
		this.gfn_message("info.처리불가", "조사가 완료되어 삭제할 수 없습니다.");
		return;
	}
	
	if(this.gfn_message("confirm.삭제여부")) {
		this.ds_pptyPlanList.deleteRow(nRow);
	}
}

/**********************************************************************
* 데이터셋 영역 
***********************************************************************/

/*
 * 함수명  	: ds_pptyPlanList_canrowposchange
 * 설명 		: 재물조사관리 ROW 변경 전 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_pptyPlanList_canrowposchange();
 */
this.ds_pptyPlanList_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(Ex.util.isUpdated(this.ds_pptyDetailList)){
		if (this.gfn_message("confirm.변경.취소.여부")) {
			return true;
	    } else {
			return false;
	    }
	}
	
	return;
}


/*
 * 함수명  	: ds_pptyPlanList_onrowposchanged
 * 설명 		: 재물조사관리 ROW 변경 후 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_pptyPlanList_onrowposchanged();
 */
this.ds_pptyPlanList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_pptyDetailList.clearData();
	
	if(e.newrow > -1){
		this.ds_cond_sub.clearData();
		this.ds_cond_sub.addRow();
		
		var nRow = this.ds_pptyPlanList.rowposition;
		
		// 조사여부
		if(this.ds_pptyPlanList.getColumn(nRow, "INVG_AT") == '0'){
			this.btn_allDelete.set_visible(true);
			this.btn_delete.set_visible(true);
			this.btn_cancel.set_visible(true);
			this.grd_pptyDetailList.set_readonly(false);
		}else{
			this.btn_allDelete.set_visible(false);
			this.btn_delete.set_visible(false);
			this.btn_cancel.set_visible(false);
			this.grd_pptyDetailList.set_readonly(true);
		}
		
		this.ds_cond_sub.clearData();
		this.ds_cond_sub.addRow();
		
		var odr = this.ds_pptyPlanList.getColumn(nRow, "ODR");
		this.ds_cond_sub.setColumn(0, "ODR", odr);
		
		this.fn_transaction("pptyDetailList");
	}
}


/**********************************************************************
* 팝업영역 
***********************************************************************/


/*
 * 함수명  	: btn_delete
 * 설명 		: 행삭제 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_delete();
 */
this.btn_delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_pptyPlanList.rowposition;
	var dRow = this.ds_pptyDetailList.rowposition;
	
	if(nRow < 0 || dRow < 0) {
		this.gfn_message("info.처리불가", "삭제할 데이터가 없습니다.");
		return;
	}
	
	if(this.ds_pptyPlanList.getColumn(nRow, "INVG_AT") == '1') {
		this.gfn_message("info.처리불가", "조사가 완료되어 삭제할 수 없습니다.");
		return;
	}
				
	if(this.gfn_message("confirm.삭제여부")){
		this.ds_pptyDetailList.deleteRow(dRow);
	}
}


/*
 * 함수명  	: btn_cancel
 * 설명 		: 행취소 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_cancel();
 */
this.btn_cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_pptyDetailList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	
	if (this.gfn_message("confirm.취소여부")) {
		if(obj.name == "btn_cancel"){
			this.ds_pptyDetailList.reset();			//DATASET ROLLBACK
			this.ds_pptyDetailList.applyChange();	//DATASET COMMIT
			return;
		}
	}	
}


/*
 * 함수명  	: btn_excelDn_onclick
 * 설명 		: 엑셀다운  버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_excelDn_onclick();
 */
this.btn_excelDn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_pptyDetailList.rowcount == 0){
		this.gfn_message("info.처리불가", "해당계획에 물품조사목록이 없습니다.");
		return false;
	}else{
		if(obj.name == "btn_excelDown"){
			this.gfn_exportExcel(this.grd_pptyDetailList, "exportExl");
		}
	}
	
}


/*
 * 함수명  	: ds_pptyDetailList_oncolumnchanged
 * 설명 		: 데이터셋 데이터 변경 후 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_pptyDetailList_oncolumnchanged();
 */
this.ds_pptyDetailList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var nRow = obj.rowposition;
	
	if(e.columnid == "ACINS_CO") {
		var qy = !this.comUtils.isNull(obj.getColumn(nRow, "QY")) ? nexacro.toNumber(obj.getColumn(nRow, "QY")) : 0 ;
		var acinsCo = !this.comUtils.isNull(e.newvalue) ? nexacro.toNumber(e.newvalue) : 0 ;
		
		var overs = 0;
		var shorts = 0;
		
		if(qy - acinsCo > 0) {
			shorts = qy - acinsCo;
		}else if(qy - acinsCo < 0) {
			overs = acinsCo - qy;
		}
		
		obj.setColumn(nRow, "OVERS"  , overs);
		obj.setColumn(nRow, "SHORTS" , shorts);
	}
}

/*
 * 함수명  	: btn_allDelete_onclick
 * 설명 		: 전체삭제 버튼 클릭시 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_allDelete_onclick();
 */
this.btn_allDelete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_pptyPlanList.rowposition;
	var dRow = this.ds_pptyDetailList.rowposition;
	
	if(this.ds_pptyPlanList.getColumn(nRow, "INVG_AT") == '1') {
		this.gfn_message("info.처리불가", "조사가 완료되어 삭제할 수 없습니다.");
		return;
	}
		
	if(nRow < 0 || dRow < 0) {
		this.gfn_message("info.처리불가", "삭제할 데이터가 없습니다.");
		return;
	}
	
	if(application.confirm("전체삭제를 하시겠습니까?")) {
		this.ds_pptyDetailList.deleteAll();
	}
}

/*
 * 함수명  	: fn_displayChange
 * 설명 		: 조사여부에 따라 edittype 변경
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_displayChange();
 */
this.fn_displayChange = function(nRow) {
	var nRow = this.ds_pptyPlanList.rowposition;
	var invgAt = this.ds_pptyPlanList.getOrgColumn(nRow, "INVG_AT");
	var result = "";
	
	if(invgAt == "1") {
		result = "edit";
	}else {
		result = "display";
	}
	return result;
}
this.grd_pptyDetailList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this._gridSort(obj, e);
}
