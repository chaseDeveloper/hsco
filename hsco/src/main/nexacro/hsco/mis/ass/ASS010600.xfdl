/*
 * 함수명  	: fn_PopupCallback
 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_PopupCallback();
 */
this.fn_popupCallback = function(strID, variant)
{
	switch(strID) {
		case "expDsnPopup":	// 구입과지출결의서 팝업콜백
			if(variant == "I") {
				// 수급계획상세 재조회
				this.fn_transaction("thngReciptPlanDetailList");
			}
			break;
		default:
			break;
	}
}

/*
 * 함수명  	: ds_sanctnList1_canrowposchange
 * 설명 		: 취득계획 행변경 전처리 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_sanctnList1_canrowposchange();
 */
this.ds_sanctnList1_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{

	// 수급계획상세가 변경된 경우
	if(this.comUtils.isDatasetUpdated(this.ds_thngReciptPlanDetail)) {
		if ( this.gfn_message("confirm.변경.취소.여부") ) {
			this.ds_thngReciptPlanDetail.reset();
			return true;
		} else {
			return false;
		}
	}
	
	// 이전 선택된 행이 신규인 경우
	if(obj.getRowType(e.oldrow) == Dataset.ROWTYPE_INSERT) {
		if ( this.gfn_message("confirm.변경.취소.여부") ) {
			obj.deleteRow(e.oldrow);
			return true;
		} else {
			return false;
		}
	}
	
	return true;
}

/*
 * 함수명  	: tab_plan_tpg1_grd_thngReciptPlanDetail_oncellclick
 * 설명 		: 그리드 셀 클릭 이벤트 (선택 처리)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_plan_tpg1_grd_thngReciptPlanDetail_oncellclick();
 */
this.tab_plan_tpg1_grd_thngReciptPlanDetail_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	
	switch(e.cell) {
		case 1:	// 지출결의여부
			
			//var sanctnKnd = this.ds_sanctnList1.getColumn(this.ds_sanctnList1.rowposition, "SANCTN_KND");			
			//if(sanctnKnd == '5'){
			
			var SANCTN_STS = this.ds_sanctnList1.getColumn(this.ds_sanctnList1.rowposition, "SANCTN_STS");
			if(SANCTN_STS == '256'){//전자결재 : 완료
				
				var nRow = this.ds_thngReciptPlanDetail.rowposition;
				var DECSN_NO = this.ds_thngReciptPlanDetail.getColumn(nRow, "DECSN_NO");
				
				// 지출결의서가 등록되지 않은 경우만
				if(this.comUtils.isNull(DECSN_NO)) {
					this.ds_thngReciptPlanDetail.set_updatecontrol(false);
					this.ds_thngReciptPlanDetail.setColumn(nRow, "CHK", this.ds_thngReciptPlanDetail.getColumn(nRow, "CHK") == "1" ? "0" : "1");
					this.ds_thngReciptPlanDetail.set_updatecontrol(true);
				}
			}
			else{
				this.gfn_message("info.처리불가", "결재 완료 후 진행 가능 합니다.");
			}
			
			break;
		
		case 2:	// 자산대장등록여부
			var nRow = this.ds_thngReciptPlanDetail.rowposition;
			var DECSN_NO = this.ds_thngReciptPlanDetail.getColumn(nRow, "DECSN_NO");
			
			// 지출결의서가 등록되고, 자산대장에 등록되지 않은 경우만
			if( !this.comUtils.isNull(DECSN_NO) ) {
				
				if(this.ds_thngReciptPlanDetail.getColumn(nRow, "REGIST_AT") == "0") {
					
					this.ds_thngReciptPlanDetail.set_updatecontrol(false);
					this.ds_thngReciptPlanDetail.setColumn(nRow, "REG_CHK", this.ds_thngReciptPlanDetail.getColumn(nRow, "REG_CHK") == "1" ? "0" : "1");
					this.ds_thngReciptPlanDetail.set_updatecontrol(true);
				}
				else{
					this.gfn_message("info.처리불가", "이미 자산대장에 등록되었습니다.");
				}
			}
			else{
				this.gfn_message("info.처리불가", "지출결의 등록 후 진행 가능 합니다.");
			}
			break;
		default:
			break;
	}
}

/*
 * 함수명  	: tab_plan_tpg1_btn_insertAssets_onclick
 * 설명 		: 자산대장 등록
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_plan_tpg1_btn_insertAssets_onclick();
 */
this.tab_plan_tpg1_btn_insertAssets_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 자산대장에 등록되지 않았고, 자산대장 등록대상으로 선택된 것
	if(this.ds_thngReciptPlanDetail.getCaseCount("REG_CHK == '1' && REGIST_AT == '0'") <= 0) {
		this.gfn_message("info.처리불가","자산대장에 등록할 자산이 선택되지 않았습니다.");
		return false;
	}
	
	if(this.gfn_message("comm.행위여부","자산대장 등록을 진행") ){
		var nRow = 0;
		this.ds_assets.clearData();
		
		for(var i = 0; i < this.ds_thngReciptPlanDetail.getRowCount(); i++) {
			
			if( this.ds_thngReciptPlanDetail.getColumn(i, "REG_CHK") == "1" 
				&& this.ds_thngReciptPlanDetail.getColumn(i, "REGIST_AT") == "0" ) {
				
				nRow = this.ds_assets.addRow();
				this.ds_assets.copyRow(nRow, this.ds_thngReciptPlanDetail, i);
			}
		}
		
		this.fn_transaction("insertAssetsMastr");
	}
}
