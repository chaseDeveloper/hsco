    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ASS010400.xfdl
 * 화면명   : 물품수급계획등록
 * 화면설명 : 물품수급계획등록을 관리한다.
 * 작성일   : 2015.07.06
 * 작성자   : 이혜선
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.currow;
this.saveRow = -1;

this.checkDs = [this.ds_sanctnList1 , this.ds_sanctnList2];
this.File_UpDownManager = null;
this.insertYn = 'N';

this.chk	= new Array();
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ASS010600_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.ASS010600_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// 초기 검색조건
	this.ds_cond.setColumn(0, "FDATE",    this.dateUtils.today().substr(0,4)+"0101");
	this.ds_cond.setColumn(0, "TDATE",    this.dateUtils.today());
	
	// 공통코드
	var comboParams = [
		["ds_acc001", "ACC001", "Y", ""]	// 사업장구분
	   ,["ds_cmp002", "CMP002", "Y", ""]	// 단위
	   ,["ds_ass004", "ASS004", "Y", ""]	// 처분구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		this.ds_ass004.filter("CODE != '0'");
		
		// 조회 이벤트
		this.fn_search();	
	};
	this.gfn_comboLoad (comboParams, callback);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "ASS");
	this.File_UpDownManager.init(fileConfig);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	if(this.tab_plan.tabindex == 0){
		
		this.ds_thngReciptPlanDetail.clearData();
		this.ds_fileList.clearData();
		this.ds_cond.setColumn(0, "PLAN_SE", "1");
		this.fn_transaction("selectSanctnList1");
	}
	
	if(this.tab_plan.tabindex == 1){
		
		this.ds_atldManageDetail.clearData();
		this.ds_atldTrgetList.clearData();
		this.ds_cond.setColumn(0, "PLAN_SE", "2");
		this.fn_transaction("selectSanctnList2");
	}
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(this.tab_plan.tabindex == 0){
		if(Ex.util.isUpdated(this.ds_sanctnList1) || Ex.util.isUpdated(this.ds_thngReciptPlanDetail)){
			// validation 체크 취득물품(수급계획목록)
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_sanctnList1, this.ds_validation)) {
				return false;
			}
			this.saveRow = this.ds_sanctnList1.rowposition;			
			
			this.fn_transaction("sanctnListCUD1");
		}else{
			this.gfn_message("comm.데이터.변경.없음");
		}
	}
	if(this.tab_plan.tabindex == 1){
		if(Ex.util.isUpdated(this.ds_sanctnList2)){
			// validation 체크(불용품관리)
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_sanctnList2, this.ds_validation)) {
