 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "selectSanctnList1":    			// 취득계획 전자결재 목록 조회 
				this.ds_thngReciptPlan.applyChange();
				break;
			case "selectSanctnList2":    			// 불용계획 전자결재 목록 조회 
				this.ds_thngReciptPlanDetail.applyChange();
				break;
			case "thngReciptPlanDetailList":    	// 취득계획 결재 상세 목록 조회
				break;
			case "ds_atldManageDetail":				// 불용계획 결재 상세 목록 조회		
				break;
			case "sanctnListCUD1":					// 취득계획 전자결재 목록 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();	
				break;
			case "sanctnListCUD2":					// 불용계획 전자결재 목록 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();	
				break;
			case "selectSanctnList3":				// 취득계획 결재대상 조회 
					if(this.ds_thngReciptPlanDetail.rowcount > 0){
						
						this.insertYn = 'Y';
						
						this.ds_temp1.copyData(this.ds_thngReciptPlanDetail);			// 임시 데이터셋어 활동목록내역 임시저장(행 변경 이벤트에서 사용)
						
						var nRow = this.ds_sanctnList1.addRow();
						
						this.ds_sanctnList1.setColumn(nRow, "PLAN_SE" ,      '1');
						this.ds_sanctnList1.setColumn(nRow, "SANCTN_KND" ,   '0');
						
					}else{
						
						if(this.ds_sanctnList1.rowposition > -1){
							this.fn_transaction("thngReciptPlanDetailList");
						}
						this.gfn_message("fail.정보검색.실패","결재대상 목록을");
					}			
				break;
			case "selectSanctnList4":				// 불용계획 결재대상 조회 
					if(this.ds_atldManageDetail.rowcount > 0){
						
						this.insertYn = 'Y';
						
						this.ds_temp2.copyData(this.ds_atldManageDetail);			// 임시 데이터셋어 활동목록내역 임시저장(행 변경 이벤트에서 사용)
						
						var nRow = this.ds_sanctnList2.addRow();
						
						this.ds_sanctnList2.setColumn(nRow, "PLAN_SE" ,      '2');
						this.ds_sanctnList2.setColumn(nRow, "SANCTN_KND" ,   '0');
						
					}else{
						
						if(this.ds_sanctnList2.rowposition > -1){
							this.fn_transaction("atldManageDetailList");
						}
						this.gfn_message("fail.정보검색.실패","결재대상 목록을");
					}				
				break;
			case "approve":
				var sanctnNO = this.ds_approve.getColumn(0,"SANCTN_NO");
				
				if(this.tab_plan.tabindex == 0){
					this.ds_sanctnList1.set_updatecontrol(false);
					this.ds_sanctnList1.setColumn(this.ds_sanctnList1.rowposition, "SANCTN_NO", sanctnNO);
					this.ds_sanctnList1.set_updatecontrol(true);
				}
				
				if(this.tab_plan.tabindex == 1){
					this.ds_sanctnList2.set_updatecontrol(false);
					this.ds_sanctnList2.setColumn(this.ds_sanctnList2.rowposition, "SANCTN_NO", sanctnNO);
					this.ds_sanctnList2.set_updatecontrol(true);
				}
				
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
				break;		
			case "insertAssetsMastr":	// 자산대장등록 콜백
				// 수급계획상세 재조회
				this.fn_transaction("thngReciptPlanDetailList");
				break;
		}
	}
}

/*
 * 함수명  	: fn_callback5
 * 설명 		: 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_callback5();
 */
this.fn_callback5 = function(sPopupId, sReturn)
{
    var dsRtn = new Dataset();
    nRowCnt = dsRtn.loadXML(sReturn);
    
	if(sPopupId == "Popup")
	{
        if(nRowCnt){
            this.ds_cond.setColumn(0, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.ds_cond.setColumn(0, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
        }
	}
	if(sPopupId == "Popup1")
	{
		if(nRowCnt){
            this.ds_thngReciptPlanDetail.setColumn(this.currow,"ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
			this.ds_thngReciptPlanDetail.setColumn(this.currow,"ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
        }
	}
	
	if(sPopupId == "SEARCH")
	{
        if(nRowCnt){
            this.ds_cond03.setColumn(0, "ASSETS_CL_NM",   dsRtn.getColumn(0, "ASSETS_CL_NM"));
            this.ds_cond03.setColumn(0, "ASSETS_CL_CODE", dsRtn.getColumn(0, "ASSETS_CL_CODE"));
        }
	}
}
