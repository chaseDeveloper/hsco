{
	var key = obj.getColumn(e.newrow, "PLAN_SANCTN_SN");
	var fileKey = obj.getColumn(e.newrow, "FILE_SN");
	
	this.ds_cond02.clearData();
	this.ds_cond02.addRow();
	this.ds_cond02.setColumn(0, "PLAN_SANCTN_SN", key);
	this.ds_cond02.setColumn(0, "FILE_SN", fileKey);

	if(this.tab_plan.tabindex == 0){
		if(!key){	// 신규일 경우 상세 조회를 하지 않고 임시 데이터셋을 복사한다.
			this.ds_thngReciptPlanDetail.clearData();
			this.ds_thngReciptPlanDetail.copyData(this.ds_temp1);
		}else {
			this.fn_transaction("thngReciptPlanDetailList");
		}
	}
	
	if(this.tab_plan.tabindex == 1){
		if(!key){	// 신규일 경우 상세 조회를 하지 않고 임시 데이터셋을 복사한다.
			this.ds_atldManageDetail.clearData();
