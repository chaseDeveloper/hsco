/*
 * 함수명  	: ds_prptyMdatManageList_oncolumnchanged
 * 설명 		: 부모데이터셋 데이터 변경 후 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_prptyMdatManageList_oncolumnchanged();
 */

this.ds_prptyMdatManageList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var nRow = obj.rowposition;

	if(e.columnid == "QY") {		// 수량
		var qy     = this.comUtils.isNull(e.newvalue) ? 0 : nexacro.toNumber(e.newvalue);
		var untpc  = this.comUtils.isNull(obj.getColumn(nRow, "ORI_UNTPC")) ? 0 : nexacro.toNumber(obj.getColumn(nRow, "ORI_UNTPC"));
		var am = untpc * qy;
		
		obj.setColumn(nRow, "PRPTY_MDAT_AM", am);
	}
}


