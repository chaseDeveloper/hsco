 /*
 * 함수명  	: fn_popup
 * 설명 		: Popup 호출 이벤트 처리
 * params  		: sPopupId    : 팝업 ID
 * return Type 	: None
 * 사용법		: this.fn_popup(sPopupId)
 */
this.fn_popup = function(sPopupId)
{
	// 자산팝업
	if(sPopupId == "searchAssetsMastr") {
		var parm = {
			  arg_0      : this
			 ,deptCode   : this.ds_cond.getColumn(0, "DEPT_CODE")
			 ,deptNm     : this.ds_cond.getColumn(0, "DEPT_NM")
			 ,authAt     : authAt
		};
		this.gfn_popup(sPopupId, 400, 400, "자산선택", parm, "mis_ass::ASS010400_P02.xfdl", "fn_popupCallBack", true);
	}
	// 사업소팝업
	if(sPopupId == "bizplcPopup") {
		var parm = {
			 arg_0      : this
		};
		this.gfn_popup("sPopupId", 400, 400, "사업소선택", parm, "mis_hrm::HRM010102_P1.xfdl", "fn_popupCallBack", true);
	}
}

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	alert(sPopupId);

	// 자산팝업
	if(sPopupId == "searchAssetsMastr") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var assetsNo   = dsTemp.getColumn(0, "ASSETS_NO"); 			// 자산번호
			var assetsNm   = dsTemp.getColumn(0, "ASSETS_NM"); 			// 자산명
			var deptCode   = dsTemp.getColumn(0, "DEPT_CODE"); 			// 담당부서
			var deptNm     = dsTemp.getColumn(0, "DEPT_NM"); 			// 담당부서코드
			var empno      = dsTemp.getColumn(0, "EMPNO"); 				// 담당자
			var empnm      = dsTemp.getColumn(0, "EMPNM"); 				// 담당자사번
			var lossStndrd = dsTemp.getColumn(0, "STNDRD"); 			// 품질규격
			var qy     = dsTemp.getColumn(0, "QY"); 				// 수량
			var untpc  = dsTemp.getColumn(0, "UNTPC"); 				// 단가
			var sttusSe    = dsTemp.getColumn(0, "STTUS_SE"); 			// 상태
			var resrceAccto   = dsTemp.getColumn(0, "RESRCE_ACCTO"); 	// 자원별
			var gnvnThngClNo  = dsTemp.getColumn(0, "GVRN_THNG_CL_NO"); // 정부물품분류번호
			var unit          = dsTemp.getColumn(0, "UNIT"); 			// 정부물품분류번호
			
			var nRow = this.ds_prptyMdatManageList.rowposition;
			this.ds_prptyMdatManageList.setColumn(nRow, "ASSETS_NO"  , assetsNo); 				// 자산번호
			this.ds_prptyMdatManageList.setColumn(nRow, "ASSETS_NM"  , assetsNm); 				// 자산명
			this.ds_prptyMdatManageList.setColumn(nRow, "DEPT_CODE"  , deptCode); 				// 담당부서
			this.ds_prptyMdatManageList.setColumn(nRow, "DEPT_NM"    , deptNm); 				// 담당부서코드
			this.ds_prptyMdatManageList.setColumn(nRow, "EMPNO"      , empno); 					// 담당자
			this.ds_prptyMdatManageList.setColumn(nRow, "EMPNM"      , empnm); 					// 담당자사번
			this.ds_prptyMdatManageList.setColumn(nRow, "STNDRD"     , lossStndrd); 	        // 재물조정_규격
			this.ds_prptyMdatManageList.setColumn(nRow, "QY"         , qy); 		        // 재물조정_수량
			this.ds_prptyMdatManageList.setColumn(nRow, "UNTPC"      , untpc); 		   		// 재물조정_단가
			this.ds_prptyMdatManageList.setColumn(nRow, "STTUS_SE"        , sttusSe);   		// 재물조정_상태
			this.ds_prptyMdatManageList.setColumn(nRow, "RESRCE_ACCTO"    , resrceAccto); 		// 재물조정_자원별
			this.ds_prptyMdatManageList.setColumn(nRow, "GVRN_THNG_CL_NO" , gnvnThngClNo); 		// 재물조정_정부물품분류번호
			this.ds_prptyMdatManageList.setColumn(nRow, "UNIT"            , unit); 				// 재물조정_단위
		
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_ASSETS_NM"       , assetsNm);		// 자산마스터_품명
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_STNDRD"          , lossStndrd); 	// 자산마스터_품질규격
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_QY"              , qy); 		// 자산마스터_수량
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_UNTPC"           , untpc); 	// 자산마스터_단가
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_STTUS_SE"        , sttusSe);   	// 자산마스터_상태
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_RESRCE_ACCTO"    , resrceAccto); 	// 자산마스터_자원별
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_GVRN_THNG_CL_NO" , gnvnThngClNo);	// 자산마스터_정부물품분류번호
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_UNIT"            , unit); 			// 자산마스터_단위
			
			// 금액
			var qy     = this.comUtils.isNull(qy)    ? 0 : nexacro.toNumber(qy);
			var untpc  = this.comUtils.isNull(untpc) ? 0 : nexacro.toNumber(untpc);
			var am = untpc * qy;
			this.ds_prptyMdatManageList.setColumn(nRow, "ORI_AM", am);
			this.ds_prptyMdatManageList.setColumn(nRow, "PRPTY_MDAT_AM", am);
			
		}else {
			var nRow = this.ds_prptyMdatManageList.rowposition;
			var rowType = this.ds_prptyMdatManageList.getRowType(nRow);
			if(rowType == "2") {
				this.ds_prptyMdatManageList.deleteRow(nRow);
			}
		}
	}	
	// 사업장 구분 팝업(검색조건)
	if(sPopupId == "bizplcPopup"){
		var dsRtn = new Dataset();
		var nRowCnt = dsRtn.loadXML(sReturn);
		
        if(nRowCnt){
			var bizplcCode = dsRtn.getColumn(0, "BIZPLC_CODE");
			var bizplcNm = dsRtn.getColumn(0, "BIZPLC_NM");
			
            this.ds_cond.setColumn(0, "BIZPLC_CODE", bizplcCode);
            this.ds_cond.setColumn(0, "BIZPLC_NM"  , bizplcNm);
        }
	}
}
/**********************************************************************
* 사용자 정의 함수
***********************************************************************/

this.grd_prptyMdatManageList_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{    
	var nRow = this.ds_prptyMdatManageList.rowposition;
	
	// 승인여부
	if(e.cell == "1") {
	
		if(!authAt) {
			return false; 
		}
	
		if(Ex.util.isUpdated(this.ds_prptyMdatManageList)) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;    // 아니오
			}
		}

		var confmAt  = this.ds_prptyMdatManageList.getColumn(nRow, "CONFM_AT");
		
		this.ds_confmProcess.clearData();
		this.ds_confmProcess.addRow();
		this.ds_confmProcess.copyRow(0, this.ds_prptyMdatManageList, nRow);

		if(confmAt != "1") {
			if(application.confirm("해당건을 승인처리 하시겠습니까?\n계속 진행하시면 재물조정 대상품 데이터가 자산대장에 반영됩니다.")) {
				this.ds_confmProcess.setColumn(0, "CONFM_AT", "1");
				this.ds_confmProcess.setColumn(0, "CONFM_DE", this.dateUtils.today());
				// 승인처리
				saveRow = this.ds_prptyMdatManageList.rowposition;
				this.fn_transaction("prptyMdatConfm");
			}
		}else {
			if(application.confirm("해당건을 승인취소처리 하시겠습니까?\n계속 진행하시면 최신데이터로 반영됩니다.")) {
				this.ds_confmProcess.setColumn(0, "CONFM_AT", "0");
				this.ds_confmProcess.setColumn(0, "CONFM_DE", "");
				// 승인처리
				saveRow = this.ds_prptyMdatManageList.rowposition;
				this.fn_transaction("prptyMdatConfmCancel");
			}		
		}
	}
}

/***********************************************************************
 * 첨부파일버튼 관련 이벤트 처리
 ***********************************************************************/

/*
 * 함수명  	: btn_delFiles_onclick
 * 설명 		: '선택삭제' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_delFiles_onclick(obj,e);
 */
this.btn_delFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
		return false;
	}
	
	for(var fRow = this.ds_fileList.getRowCount()-1; fRow >= 0; fRow--) {
		var chkVal = this.ds_fileList.getColumn(fRow, "CHK");
		if(chkVal == '1') {
			this.ds_fileList.deleteRow(fRow);
		}
	}
	
	var strSvcId   	 = "deleteFile";
	var sController  = "hsco/cmm/file/deleteFile.do";
	var sInDatasets  = "input1=ds_fileList:U";
	var sOutDatasets = "ds_fileList=output1";
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/*
 * 함수명  	: btn_uploadFiles_onclick
 * 설명 		: '파일업로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_uploadFiles_onclick(obj,e);
 */
this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	var nRow     = this.ds_prptyMdatManageList.rowposition;	
	var nRowType = this.ds_prptyMdatManageList.getRowType(nRow);
	
	if(nRow < 0){
		this.gfn_message("comm.항목.선택.요청","대상 정보");
		return;
	}
	
	if(nRowType == 2){
		this.gfn_message("info.진행가능","신규 정보를 저장");
		return;
	}
	
	// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
	this.mCallback = function() {
		this.fn_autoFileSn(
			 "MIS"									// 대분류   (SYS_SE)
			,"ASS"									// 업무구분 (JOB_SE)
			, this.File_UpDownManager	            // fileManager 오브젝트
			, this.ds_fileList		                // fileObject
			, this.ds_prptyMdatManageList			// fileSn을 불러오거나 반영해야 하는 컬럼
			, "TBASS_PRPTY_MDAT_MANAGE"				// fileSn을 저장할 테이블
			, "FILE_SN"								// fileSn 컬럼명이 다를 경우를 대비
			, ["SN"]								// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
		);
	}
	
	this.mCallback();
}



/*
 * 함수명  	: btn_downFiles_onclick
 * 설명 		: '선택다운로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_downFiles_onclick(obj,e);
 */
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}


 /*
 * 함수명  	: fn_onFileSuccess
 * 설명 		: 파일 업로드 성공시 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_onFileSuccess();
 */
this.fn_onFileSuccess = function(){
	this.gfn_message("success.처리.성공");
	
	var nRow = this.ds_prptyMdatManageList.rowposition;
	this.ds_condFile.setColumn(0, "FILE_SN", this.ds_prptyMdatManageList.getColumn(nRow, "FILE_SN"));
	
	this.fn_transaction("selectFileList");
}

 /*
 * 함수명  	: fn_onFileError
 * 설명 		: 파일 업로드 실패시 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_onFileError();
 */
this.fn_onFileError = function(){
	
	//this.gfn_message("info.저장.초과", "이미지 파일만");	
}

/*
 * 함수명  	: file_upload_popup
 * 설명 		: 첨부파일 그리드 전체파일선택 ON/OFF 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.file_upload_popup(obj,e);
 */
this.grd_cmmnAtchmnfl_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
	}
}

/*
 * 함수명  	: file_upload_popup
 * 설명 		: 첨부파일 그리드 Body의 체크 버튼이 눌러졌을 때, Head 부분의 체크 버튼을 꺼 줌.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.file_upload_popup(obj,e);
 */
this.grd_cmmnAtchmnfl_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.cell == 0) {
		obj.setCellProperty("Head", e.cell, "text", "0");
	}
}
this.ds_prptyMdatManageList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{	
	this.ds_fileList.clearData();
	
	if(e.newrow > -1) {
		var confmAt = obj.getColumn(e.newrow, "CONFM_AT");
		if(confmAt == "1") {
			this.grd_cmmnAtchmnfl.set_readonly(true);
			this.btn_delFiles.set_enable(false);
			this.btn_downFiles.set_enable(false);
			this.btn_uploadFiles.set_enable(false);
		}else {
			this.grd_cmmnAtchmnfl.set_readonly(false);
			this.btn_delFiles.set_enable(true);
			this.btn_downFiles.set_enable(true);
			this.btn_uploadFiles.set_enable(true);		
		}
	
		var fileSn  = obj.getColumn(e.newrow, "FILE_SN");
		this.ds_condFile.clearData();
		this.ds_condFile.addRow();
		this.ds_condFile.setColumn(0, "FILE_SN", fileSn);
		this.fn_transaction("selectFileList");
	}
}
