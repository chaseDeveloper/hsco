/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(재물조정관리계획)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_prptyMdatManageList.clearData();
	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	this.fn_transaction("selectPrptyMdatManageList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None	
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_prptyMdatManageList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}

	if(!this.gfn_checkValidation_NoUpdChk(this.ds_prptyMdatManageList, this.ds_validation)) {
		return false;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		saveRow = this.ds_prptyMdatManageList.rowposition;
		this.fn_transaction("prptyMdatManageCUD");
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_prptyMdatManageList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_prptyMdatManageList.reset();			//DATASET ROLLBACK
		this.ds_prptyMdatManageList.applyChange();	//DATASET COMMIT
	}
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트(마스터)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var confmAt = this.ds_prptyMdatManageList.getColumn(nRow, "CONFM_AT");
	
	if(confmAt == "1") {
		this.gfn_message("info.처리불가", "승인된 건은 삭제할 수 없습니다.");
		return;
	}
	
	var nRow = this.ds_prptyMdatManageList.rowposition;
	if(this.gfn_message("confirm.삭제여부")) {
		this.ds_prptyMdatManageList.deleteRow(nRow);
	}
}


/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 클릭시 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_prptyMdatManageList.addRow();
	this.ds_prptyMdatManageList.setColumn(nRow, "INVNV_DE", this.dateUtils.today());
	
	// 자산대장 팝업
	this.fn_popup("searchAssetsMastr");
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectPrptyMdatManageList":	// 재물조정관리 리스트  조회 
			var sController   	= "/hsco/mis/ass/ASS011700/selectPrptyMdatManageList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_prptyMdatManageList=output1";
			break;
		case "prptyMdatManageCUD":			// 재물조정관리 저장
			var sController   	= "/hsco/mis/ass/ASS011700/prptyMdatManageCUD.do";	
			var sInDatasets   	= "input1=ds_prptyMdatManageList:U";
			var sOutDatasets  	= "";
			break;
		case "prptyMdatConfm":		    	// 재물조정관리 승인
			var sController   	= "/hsco/mis/ass/ASS011700/prptyMdatConfm.do";	
			var sInDatasets   	= "input1=ds_confmProcess:A";
			var sOutDatasets  	= "";
			break;	
		case "prptyMdatConfmCancel":	     // 재물조정관리 승인취소
			var sController   	= "/hsco/mis/ass/ASS011700/prptyMdatConfmCancel.do";	
			var sInDatasets   	= "input1=ds_confmProcess:A";
			var sOutDatasets  	= "";
			break;	
		case "selectFileList":     			 // 첨부파일 조회
			var sController   	= "hsco/mis/ass/ASS011100/selectFileList.do";
			var sInDatasets   	= "input1=ds_condFile";
			var sOutDatasets  	= "ds_fileList=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "selectPrptyMdatManageList":	// 재물조정관리 리스트  조회 
				if(saveRow > -1) {
					this.ds_prptyMdatManageList.set_rowposition(saveRow);
					saveRow = -1;
				}
				break;
			case "prptyMdatManageCUD":			// 재물조정관리 저장
				this.fn_search();
				break;		
			case "prptyMdatConfm":		    	// 재물조정관리 승인
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;	
			case "prptyMdatConfmCancel":		// 재물조정관리 승인취소
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;	
			case "selectFileList":      // 첨부파일 조회
				this.ds_fileList.filter("USE_AT == '1'");
				break;
		}
	}
}

