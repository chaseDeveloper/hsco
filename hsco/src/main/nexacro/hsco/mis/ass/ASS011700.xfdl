// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
var saveRow = -1;
var authAt = false;
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ASS011700_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{

}

this.ASS011700_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();

	this.div_search.div_dept.fn_setBind("ds_cond", "DEPT_CODE", "DEPT_NM");

	// 공통코드
	var comboParams = [
			 ["ds_ass009", "ASS009", "Y", ""]	// 상태구분
			,["ds_cmp002", "CMP002", "Y", ""]	// 단위

	];
	
	// 검색조건 셋팅
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	
	// 초기 검색조건
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "DEPT_CODE"  , this.gfn_getDeptId());
	this.ds_cond.setColumn(0, "DEPT_NM"    , this.gfn_getDeptName());
	this.ds_cond.setColumn(0, "ASSETS_NM"  , "");
	this.ds_cond.setColumn(0, "CONFM_AT"   , "");

	// 권한 분리
	if(this.gfn_authGrpId("ASS_ADMIN") > -1) {		// 자산결재자
		authAt = true;
	}
	
	if(authAt) { 
		this.div_search.div_dept.set_enable(true);
	}else {
		this.div_search.div_dept.set_enable(false);
	}
	
	var callback = function (svcId, errorCode, errorMsg) {
		
		// 조회 이벤트
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

