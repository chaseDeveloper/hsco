/***********************************************************************************
* 설명	: 지출결의서 팝업 콜백
***********************************************************************************/
this.fn_PopUpCallback3 = function(sPopupId, sReturn)
{
	switch(sPopupId)
	{
		case "expDsnPopup":
			// 신규 결의서 저장시 재조회
			this.fn_transaction("selectLawyerCtList");
		break;
		case "pymntPopup":
			var dsTemp = new Dataset();
			var loadCnt = dsTemp.loadXML(sReturn);
			if(loadCnt) {
				var nRow = this.ds_lawyerCtList.addRow();
				this.ds_lawyerCtList.setColumn(nRow, "PYMNT_SE", dsTemp.getColumn(0, "PYMNT_SE"));
				this.ds_lawyerCtList.setColumn(nRow, "PYMNT_AMOUNT", dsTemp.getColumn(0, "AMOUNT"));
				this.ds_lawyerCtList.setColumn(nRow, "PYMNT_PLAN_DE", dsTemp.getColumn(0, "DE"));
				this.ds_lawyerCtList.setColumn(nRow, "PYMNT_DTLS", dsTemp.getColumn(0, "RM"));
				this.ds_lawyerCtList.setColumn(nRow, "INCDNT_NO",      this.ds_lwstList.getColumn(this.ds_lwstList.rowposition,"INCDNT_NO"));
				this.ds_lawyerCtList.setColumn(nRow, "LWST_MANAGE_NO", this.ds_lwstList.getColumn(this.ds_lwstList.rowposition,"LWST_MANAGE_NO"));
			}
		break;
	}
}
