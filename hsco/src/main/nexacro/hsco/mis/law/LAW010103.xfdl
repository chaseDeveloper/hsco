this.div_work_btn_drft2_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 전자결재 상태 체크 2016.7
	// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
	// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
	// 기안전인것 : 재상신 및 수정/삭제
	// 결재상태가 상신전 외는 신규로 다시 진행.
	var nRow = this.ds_lawyerCtList.rowposition;
	var sanctnKnd = this.ds_lawyerCtList.getColumn(nRow, "SANCTN_STS");

	switch (sanctnKnd) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");
		return false;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return false;
		
		default:
	}
	
	if(Ex.util.isUpdated(this.ds_lawyerCtList)){
		this.gfn_message("comm.정보.저장.요청", "수정된");
		return false;
	}
	
	var incdntNo = this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "INCDNT_NO");						//사건번호
	var lwstNo   = this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "LWST_MANAGE_NO");					//소송관리번호
	var pymntSe  = this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "PYMNT_SE");
	var seq		= this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "SEQ");
	var pymntplanDe  = this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "PYMNT_PLAN_DE");		//지급계획일
	var pymntAmount  = this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "PYMNT_AMOUNT");		//지급금액
	var sanctnNo     = this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "SANCTN_NO");
	var exUnitCd     = this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "EXPNDTR_UNIT_CODE");
	var exUnitNm     = this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "EXPNDTR_UNIT_NM");
	var offmNm     = this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "OFFM_NM");
	
	var findPymntSe = this.ds_pymntSe.findRow("CODE", pymntSe);
	var pymntSeNm = this.ds_pymntSe.getColumn(findPymntSe, "CODE_NM");
	
	pymntplanDe = pymntplanDe.substr(0,4)+"-"+pymntplanDe.substr(4,2)+"-"+pymntplanDe.substr(6,2);

	var filds = new Array(5);
	filds[0] = "금"+this.comUtils.formatComma(pymntAmount)+"원(금"+this.fn_numToHangul(pymntAmount)+"원정,부가세 포함)"; 	//incdntNo;	//지출금액
	filds[1] = "";	//산출기초 
	filds[2] = offmNm;	//지출처
	filds[3] = exUnitNm+" "+exUnitCd; 	//지출과목
	
	var dataValue = filds.join("|");
	
	var dataTitle = "소송비용지출내역";
	var title     = "소송비용지출내역";
	
	this.ds_approve.clearData();	
	this.ds_approve.addRow();

	this.ds_approve.setColumn(0, "TITLE",           title           );
	this.ds_approve.setColumn(0, "DATA_TITLE",      dataTitle       );
	this.ds_approve.setColumn(0, "DATA_VALUE",      dataValue       );
	this.ds_approve.setColumn(0, "INCDNT_NO",       incdntNo		);
	this.ds_approve.setColumn(0, "LWST_MANAGE_NO",  lwstNo			);
	this.ds_approve.setColumn(0, "PYMNT_SE",        pymntSe			);
	this.ds_approve.setColumn(0, "SEQ",        		seq			);
 	this.ds_approve.setColumn(0, "DEPT_CD",    this.gfn_getDeptId() );
 	this.ds_approve.setColumn(0, "EMP_CD",     this.fn_getEmpno()   );
 	this.ds_approve.setColumn(0, "SANCTN_NO",     sanctnNo   );
 	
 	
	var sSvcID        	= "approve";                    
	var sController   	= "hsco/mis/law/LAW010103/approve.do";	
	var sInDatasets   	= "input1=ds_approve";
	var sOutDatasets  	= "ds_approve=output1";
	var sArgs = "";	
	Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs);
}

