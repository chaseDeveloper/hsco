/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBsnsAreaList":
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();
			var sController   	= "hsco/mis/mis/MIS000000/bsnsAreaList.do";
			var sInDatasets   	= "input01=ds_cond2";
			var sOutDatasets  	= "ds_bsnsAreaCode=output01";
		break;
		//소송목록 조회
		case "selectLwstList": 
			this.ds_lawyerCtList.clearData();
			this.ds_lwstList.clearData();
			this.ds_fileList.clearData();
			this.ds_cond1.setColumn(0, "LWST_MANAGE_NO", this.ds_incdntList.getColumn(this.ds_incdntList.rowposition, "LWST_MANAGE_NO"));
			var sController   	= "hsco/mis/law/LAW010103/selectLwstList.do";
			var sInDatasets   	= "input01=ds_cond1";
			var sOutDatasets  	= "ds_lwstList=output01";
		break;	
		
		//사건목록 조회
		case "selectIncdntList": 	
			this.ds_incdntList.clearData();
			this.ds_lawyerCtList.clearData();
			this.ds_lwstList.clearData();
			this.ds_fileList.clearData();
			var sController   	= "hsco/mis/law/LAW010300/selectIncdntList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_incdntList=output01";
		break;	
		
		//소송지출목록 조회
		case "selectLawyerCtList": 
		    //조회조건
		    this.ds_fileList.clearData();
		    this.ds_lawyerCtList.clearData();
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();	 
			
			this.ds_cond2.setColumn(0, "LWST_MANAGE_NO", this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "LWST_MANAGE_NO"));
			this.ds_cond2.setColumn(0, "INCDNT_NO",      this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "INCDNT_NO"));
			var sController   	= "hsco/mis/law/LAW010103/selectLawyerCtList.do";
			var sInDatasets   	= "input02=ds_cond2";
			var sOutDatasets  	= "ds_lawyerCtList=output02";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
		//소송지출목록 저장,수정,삭제
		case "lawyerCtListCUD":
			save_row = this.ds_incdntList.rowposition;
			var sController   	= "hsco/mis/law/LAW010103/lawyerCtListCUD.do";
			var sInDatasets   	= "input03=ds_lawyerCtList:U";
			var sOutDatasets  	= "";
		break;
		
		case "GetInnerSanctnId": // 내부결재id 조회   
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
		break;
		
		case "innerSanctnUbiDataset": // 시스템연계 내부결재보고서 정보 조회
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0, "INCDNT_NO",      this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "INCDNT_NO"));
			this.ds_cond02.setColumn(0, "LWST_MANAGE_NO", this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "LWST_MANAGE_NO"));
			this.ds_cond02.setColumn(0, "PYMNT_SE",       this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "PYMNT_SE"));
			var sController   	= "hsco/mis/law/LAW010103/innerSanctnUbiDataset.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_ubiDataset=output1";
		break; 
		
		//최초 내부결재 팝업 호출 시 저장버튼을 눌러 시작되었는지, 닫기버튼을 눌러 취소를 했는지 여부check
		case "selectInnerSanctnMasterCheck"	:
			var sController   	= "hsco/mis/law/LAW010103/selectInnerSanctnMasterCheck.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_innerSanctnChek=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 

}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {	
		var comboOption = null;
		switch(strSvcId)
		{
			case "selectIncdntList" :
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row > -1) {
					this.ds_incdntList.set_rowposition(save_row);
					save_row = -1;
				}
				break;
			//사업지구조회 콜백
			case "selectBsnsAreaList" :
				this.ds_bsnsAreaCode.insertRow(0);
				this.ds_bsnsAreaCode.setColumn(0, "CODE","");
				this.ds_bsnsAreaCode.setColumn(0, "ALL_NM","전체");
				this.div_search.cmb_landCodeSh.set_index(0);
				this.fn_search();
			break;			
			//비용지출 목록 CU 콜백
			case "lawyerCtListCUD" :
				this.fn_transaction("selectLawyerCtList");
				this.ds_lawyerCtList.applyChange();
			break;
			
			//소송지출목록 조회 콜백
			case "selectLawyerCtList":
// 				if(this.ds_lawyerCtList.getRowCount()>0){
// 					var sanctnKnd = this.ds_lawyerCtList.getColumn(0, "SANCTN_KND");
// 					if(sanctnKnd=='0'){
// 						this.div_work.btn_drft.set_enable(true);
// 					}else{
// 						this.div_work.btn_drft.set_enable(false);
// 					}
// 				}else{
// 					this.div_work.btn_drft.set_enable(false);
// 				}
			break;
			
			// 내부결재id 조회
			case "GetInnerSanctnId":
			    this.fn_popup_InnerSanctionDrft();
			break;
			
			//최초 내부결재 진행여부체크
			case "selectInnerSanctnMasterCheck":
				if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
				{
					this.ds_lawyerCtList.setColumn(this.ds_lawyerCtList.rowposition, "DRFT_JOB_CONFM_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
					this.ds_lawyerCtList.setColumn(this.ds_lawyerCtList.rowposition, "DRFT_DE",           this.gfn_today());
					this.fn_transaction("lawyerCtListCUD");
				}else{
					
				}
			break;
			
			//전자결재 콜백
			case "approve":
// 				var sanctnNO = this.ds_approve.getColumn(this.ds_approve.rowposition,"SANCTN_NO");
// 				trace("1 ::: sanctnNO - "+sanctnNO);
// 				this.ds_lawyerCtList.set_updatecontrol(false);
// 				this.ds_lawyerCtList.setColumn(this.ds_lawyerCtList.rowposition, "SANCTN_NO", sanctnNO);
// 				this.ds_lawyerCtList.set_updatecontrol(true);
// 				
 				var oUrl = this.ds_approve.getColumn(0,"O_URL");
 				system.execBrowser(oUrl);
				
				this.fn_transaction("selectLawyerCtList");
			break;
		}
	}
}

