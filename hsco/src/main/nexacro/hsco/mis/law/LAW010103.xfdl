***********************************************************************/ 
this.fn_insert = function()
{
	if(this.ds_lawyerCtList.getDeletedRowCount()>0){
		this.gfn_message("comm.정보.저장.요청","삭제된 지출내역");
		return;
	}
	if(this.ds_lwstList.rowcount < 1)
	{
		this.gfn_message("comm.항목.선택.요청","소송목록");
	}else{
		this.ds_lawyerCtList.addRow();
		this.ds_lawyerCtList.setColumn(this.ds_lawyerCtList.rowposition, "INCDNT_NO",      this.ds_lwstList.getColumn(this.ds_lwstList.rowposition,"INCDNT_NO"));
		this.ds_lawyerCtList.setColumn(this.ds_lawyerCtList.rowposition, "LWST_MANAGE_NO", this.ds_lwstList.getColumn(this.ds_lwstList.rowposition,"LWST_MANAGE_NO"));	
		this.ds_lawyerCtList.setColumn(this.ds_lawyerCtList.rowposition, "PYMNT_SE",       '01');	
		this.ds_lawyerCtList.setColumn(this.ds_lawyerCtList.rowposition, "PYMNT_PLAN_DE",  this.gfn_today());	
	}
}
/**********************************************************************
* 상단 삭제 
***********************************************************************/ 
this.fn_delete = function()
{
	var nRow = this.ds_lawyerCtList.rowposition;
	var sanctnKnd = this.ds_lawyerCtList.getColumn(nRow, "SANCTN_STS");

	switch (sanctnKnd) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");
		return false;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return false;
		
		default:
	}
	
	if(!this.comUtils.isNull(this.ds_lawyerCtList.getColumn(this.ds_lawyerCtList.rowposition, "CPTAL_ASIGN_REQST_DE")))
	{
		this.gfn_message("fail.삭제.불가","자금배정이 등록되어 있는 건");
	}else{
		if(this.gfn_message("confirm.삭제여부")){
			this.ds_lawyerCtList.deleteRow(this.ds_lawyerCtList.rowposition);
		}
	}
}
/*********************************************************************
* 상단 취소
***********************************************************************/ 
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_lawyerCtList))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(Ex.util.isUpdated(this.ds_lawyerCtList)){
			if(this.gfn_message("confirm.취소여부")){
				//모든 내용을 복원 하시겠습니까?
				this.ds_lawyerCtList.reset();		//DATASET ROLLBACK
				this.ds_lawyerCtList.applyChange();	//DATASET COMMIT
			return;
