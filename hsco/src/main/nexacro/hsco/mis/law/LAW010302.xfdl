/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "pms_lib::pmsUtil.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
  this.ds_lwstList
 ,this.ds_lwstDtaList
];

//저장시 rowposition값 저장할 변수
var save_row1 = -1;
this.File_UpDownManager = null;
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.LAW010302_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.LAW010302_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_search.cmb_incdntSe.setFocus(true);
	this.fn_transaction("selectBsnsAreaList");
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	// 공통코드
	var comboParams = [
	                   ["ds_incdntSe",      "LAW002", 1, "전체"],
	                   ["ds_lwstTy"  ,      "LAW003", 1, "전체"],
	                   ["ds_dtaSe",         "LAW013", 1, ""],
	                   ["ds_cmptncCourtSe", "LAW016", 1, ""],
	                   ["ds_cmptncCourt",   "LAW015", 1, ""],
	                   ["ds_refreeeSe",     "LAW004", 1, ""],
	                   ["ds_edAt",          "LAW008", 1, "전체"],
	                   ["ds_law018",		"LAW018", 1, ""]	//공사지위
	                  ];
	                  
	var callback = function (svcId, errorCode, errorMsg) {
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "LAW");
   this.File_UpDownManager.init(fileConfig);
}
/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	this.fn_transaction("selectIncdntList");
	
}

/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{	
	//validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_lwstDtaList, this.ds_validation)){
		return;
	}
	if(this.gfn_message("confirm.저장.여부")){	
		//현재 rowpostion값을 저장
		save_row1 = this.ds_incdntList.rowposition;
		this.fn_transaction("lwstDtaListCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//사업코드조회
		case "selectBsnsAreaList":
			this.ds_cond1.clearData();
			this.ds_cond1.addRow();
			var sController   	= "hsco/mis/mis/MIS000000/bsnsAreaList.do";
			var sInDatasets   	= "input01=ds_cond1";
			var sOutDatasets  	= "ds_bsnsAreaCode=output01";
		break;
		
		//부서콤보 조회
		case "selectDeptcodeList":
			this.ds_cond03.clearData();
			var nRow = this.ds_cond03.addRow();
			var sController   	= "hsco/mis/sal/SAL010108/selectDeptCodeList.do";
			var sInDatasets   	= "input5=ds_cond03";
			var sOutDatasets  	= "ds_dept=output5";
		break;	
		
		//사건목록 조회
		case "selectIncdntList": 
			this.ds_incdntList.clearData();
			this.ds_fileList.clearData();
			this.ds_lwstDtaList.clearData();
			this.ds_lwstList.clearData();
			var sController   	= "hsco/mis/law/LAW010300/selectIncdntList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_incdntList=output01";
		break;	
		
		//소송현황목록 조회
		case "selectLwstList": 
			this.ds_fileList.clearData();
			this.ds_lwstDtaList.clearData();
			this.ds_lwstList.clearData();
			this.ds_cond1.clearData();
			this.ds_cond1.addRow();
			this.ds_cond1.setColumn(0, "LWST_MANAGE_NO",this.ds_incdntList.getColumn(this.ds_incdntList.rowposition,"LWST_MANAGE_NO"));
			var sController   	= "hsco/mis/law/LAW010301/selectLwstList.do";
			var sInDatasets   	= "input01=ds_cond1";
			var sOutDatasets  	= "ds_lwstList=output01";
		break;	
		
		//자료목록 조회
		case "selectLwstDtaList": 
			this.ds_fileList.clearData();
		    //조회조건
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();	 
			this.ds_cond2.setColumn(0, "LWST_MANAGE_NO", this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "LWST_MANAGE_NO"));
			this.ds_cond2.setColumn(0, "INCDNT_NO",      this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "INCDNT_NO"));

			var sController   	= "hsco/mis/law/LAW010302/selectLwstDtaList.do";
			var sInDatasets   	= "input02=ds_cond2";
			var sOutDatasets  	= "ds_lwstDtaList=output02";
		break;	
		
		//자료목록 저장 CUD
		case "lwstDtaListCUD":
			var sController   	= "hsco/mis/law/LAW010302/lwstDtaListCUD.do";
			var sInDatasets   	= "input03=ds_lwstDtaList:U";
			var sOutDatasets  	= "";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_lwstDtaList.getColumn(this.ds_lwstDtaList.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {	
		var comboOption = null;
		switch(strSvcId)
		{		
			//사업지구조회 콜백 조회용 전체추가
			case "selectBsnsAreaList" :
				this.fn_search();
			break;	
			
			//자료목록 CUD 콜백
			case "lwstDtaListCUD":
				this.fn_transaction("selectLwstList");
			break;
			
			//사건목록 조회 콜백
			case "selectIncdntList":
				if(save_row1 != -1)
				{
					this.ds_incdntList.set_rowposition(save_row1);
					save_row1 = -1;
				}
			
			break;
		}
	}
}

/***********************************************************************
* 헤드 정렬
************************************************************************/
this.div_work_grd_lwstList_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	  this.gfn_gridSort(obj, e); // Grid 데이터 정렬
}
/***********************************************************************
* 자료목록 행 추가 버튼 클릭시
************************************************************************/
this.fn_insert = function()
{
	if(this.ds_lwstList.rowcount == 0){
		this.gfn_message("comm.항목.선택.요청","소송목록");
	}else{
		var nRow = this.ds_lwstDtaList.addRow();
		this.ds_lwstDtaList.setColumn(nRow, "LWST_MANAGE_NO" , this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "LWST_MANAGE_NO"));    
		this.ds_lwstDtaList.setColumn(nRow, "INCDNT_NO"      , this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "INCDNT_NO"));    
		this.ds_lwstDtaList.setColumn(nRow, "REGIST_DE"      , this.gfn_today());    	
	}
}
/***********************************************************************
* 자료목록 행 삭제 버튼 클릭시
************************************************************************/
this.fn_delete = function()
{
	if(this.gfn_message("confirm.삭제여부")){
		this.ds_lwstDtaList.deleteRow(this.ds_lwstDtaList.rowposition);
	}
}
/***********************************************************************
* 자료목록 행 취소 버튼 클릭시
************************************************************************/
this.fn_cancel = function()
{
	//dataset update check
	if(!this.comUtils.isDatasetUpdated(this.ds_lwstDtaList))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_lwstDtaList.reset();		//DATASET ROLLBACK
			this.ds_lwstDtaList.applyChange();	//DATASET COMMIT
			return;
		}
	}
}
/***********************************************************************
* 소송목록 행 변경 시 
************************************************************************/
this.ds_lwstList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.fn_transaction("selectLwstDtaList");
}

/***********************************************************************************
* 설명	: 자료목록 행 변경 시 첨부파일 조회
***********************************************************************************/
this.ds_lwstDtaList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.fn_transaction("selectAtchFile");
}

/***********************************************************************************
* 설명	: 사건목록 행 변경시 소송 목록 조회 
***********************************************************************************/
this.ds_incdntList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.fn_transaction("selectLwstList");
}


/***********************************************************************
 * 설명 : 첨부파일 선택 삭제 버튼클릭 시
 ***********************************************************************/
this.btn_delFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
		return false;
	}
	
	for(var fRow = this.ds_fileList.getRowCount()-1; fRow >= 0; fRow--) {
		var chkVal = this.ds_fileList.getColumn(fRow, "CHK");
		if(chkVal == '1') {
			this.ds_fileList.deleteRow(fRow);
		}
	}
	var strSvcId   	 = "deleteFile";
	var sController  = "hsco/cmm/file/deleteFile.do";
	var sInDatasets  = "input1=ds_fileList:U";
	var sOutDatasets = "ds_fileList=output1";
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
/***********************************************************************
 * 설명 : 첨부파일 업로드 버튼클릭 시
 ***********************************************************************/
this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
	this.fn_autoFileSn(
		 "MIS"							 // 대분류   (SYS_SE)
		,"LAW"							 // 업무구분 (JOB_SE)
		, this.File_UpDownManager	     // fileManager 오브젝트
		, this.ds_fileList		         // fileObject
		, this.ds_lwstDtaList	         // fileSn을 불러오거나 반영해야 하는 컬럼
		, "TBLAW_LWST_DTA"				 // fileSn을 저장할 테이블
		, "ATCH_FILE"					 // fileSn 컬럼명이 다를 경우를 대비
		, ["DTA_SN", "LWST_MANAGE_NO", "INCDNT_NO"] // fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
	);
}
/***********************************************************************
 * 설명 : 법률자문회신 첨부파일 선택다운로드 버튼클릭 시
 ***********************************************************************/
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
		return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}

/***********************************************************************
 * 설명 : 첨부파일 그리드 헤드 클릭 시 
 ***********************************************************************/
this.grd_cmmnAtchmnfl_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0)
	{	
		//모든 체크박스 선택
		this.gfn_setGridCheckAll(obj, e);
	}else {
		//선택한 헤드에 따른 정렬
		this.gfn_gridSort(obj, e);
	}
}

/***********************************************************************
 * 설명 : 그리드1 사건목록 행 변경전 
 ***********************************************************************/
this.ds_incdntList_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(!this.fn_check()){
		return false;
	}else{
		return true;
	}
}

/***********************************************************************
 * 설명 : 그리드2 소송목록 행 변경전 
 ***********************************************************************/
this.ds_lwstList_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(!this.fn_check()){
		return false;
	}else{
		return true;
	}
}

/***********************************************************************
 * 설명 : 변경여부 체크 함수
 ***********************************************************************/
this.fn_check = function()
{
	if(Ex.util.isUpdated(this.ds_lwstDtaList))
	{
		if(!this.gfn_message("confirm.변경.취소.여부")){  	
			return false; //유지
		}else{
			this.ds_lwstDtaList.reset();
			return true;
		}
	}else{
		return true;
	}
}]]></Script>
