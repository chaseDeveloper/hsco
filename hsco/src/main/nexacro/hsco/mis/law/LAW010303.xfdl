* excel download button click
***********************************************************************/ 
this.div_work_btn_excelDn01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_exportExcel(this.div_work.grd_Calendar, "Calendar");
}
