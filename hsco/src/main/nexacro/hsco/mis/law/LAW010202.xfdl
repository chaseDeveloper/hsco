{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.LAW010202_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_search.cmb_landCodeSh.setFocus(true);
	this.fn_transaction("selectBsnsAreaList");
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	
	//법무 담당권한이 없을 때
	if(this.gfn_authcode("115") == -1){
		this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
		this.ds_cond.setColumn(0, "EMPNO", this.gfn_getUserId());
		this.ds_cond.setColumn(0, "DEPT_CODE",    this.gfn_getDeptId());
		this.ds_cond.setColumn(0, "DEPT_NM", this.gfn_getDeptName());
	}
	
// 	if(!this.fn_getMisAuth (this, "LAW_ADMIN")&&!this.fn_getMisAuth (this, "LAW_USER")&&!this.fn_getMisAuth (this, "DEV_GRP")){
// 		this.div_search.div_srchDept.fn_setReadonly(true);
// 	}
	
	// 공통코드
	var comboParams = [["ds_cnsutSe",  "LAW007", 1, "전체"],
				       ["ds_rceptAt",  "LAW010", 1, ""],
	                   ["ds_rplyAt",   "LAW012", 1, ""],
		              ];
		              
	var callback = function (svcId, errorCode, errorMsg) {
	
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "LAW");
   this.File_UpDownManager.init(fileConfig);
}
/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	this.fn_transaction("selectLawCnsutReqst");
}
/***********************************************************************
* 법률자문신청 목록 행 변경시 법률자문 결과 조회
************************************************************************/
this.ds_lawCnsutReqst_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.fn_transaction("selectLawCnsutResult");
}
/***********************************************************************
* 법률자문신청 목록 접수버튼 클릭 시
************************************************************************/
this.div_work_btn__onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_lawCnsutReqst.getColumn(this.ds_lawCnsutReqst.rowposition, "RCEPT_AT") != '01')
	{
		if(this.gfn_message("confirm.접수.여부"))	{
			//접수여부가 01(접수완료) 인 경우
			this.ds_lawCnsutReqst.setColumn(this.ds_lawCnsutReqst.rowposition, "RCEPT_AT", "01");
			this.fn_transaction("lawCnsutReqstU");
		}
	}
}
/***********************************************************************
* 상단저장버튼 클릭 시
************************************************************************/
this.fn_save = function()
{
	//validation check		
	if(!this.gfn_checkValidation(this.ds_lawCnsutResult, this.ds_validation)){
		return;
	} 
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.ds_lawCnsutReqst.rowposition;
		this.fn_transaction("lawCnsutResultCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	
		//사업코드조회
		case "selectBsnsAreaList":
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();
			var sController   	= "hsco/mis/mis/MIS000000/bsnsAreaList.do";
			var sInDatasets   	= "input01=ds_cond2";
			var sOutDatasets  	= "ds_bsnsAreaCode=output01";
		break;

		//변호사(법무사)사무실 목록 조회
		case "selectJcffrOffmList":
			this.ds_cond03.clearData();
			var nRow = this.ds_cond03.addRow();		
			var sController   	= "hsco/mis/law/LAW010301/selectJcffrOffmList.do";
			var sInDatasets   	= "input6=ds_cond03";
			var sOutDatasets  	= "ds_jcffrOffm=output6";
		break;
		
		//변호사(법무사)목록 조회
		case "selectLawyerList" :
			this.ds_cond03.clearData();
			var nRow = this.ds_cond03.addRow();
			var sController   	= "hsco/mis/law/LAW010301/selectLawyerList.do";
			var sInDatasets   	= "input7=ds_cond03";
			var sOutDatasets  	= "ds_lawyer=output7";
		break;
		
		//법률자문신청목록 조회
		case "selectLawCnsutReqst": 
		    //초기화
		    this.ds_lawCnsutReqst.clearData();
		    this.ds_lawCnsutResult.clearData();
		    this.ds_fileList.clearData();
		    this.ds_fileList2.clearData();
			var sController   	= "hsco/mis/law/LAW010202/selectLawCnsutReqst.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_lawCnsutReqst=output01";
		break;	
		
		//법률자문신청목록 접수 
		case "lawCnsutReqstU":
			var sController   	= "hsco/mis/law/LAW010202/lawCnsutReqstU.do";
			var sInDatasets   	= "input02=ds_lawCnsutReqst:U";
			var sOutDatasets  	= "";
		break;
		
		//법률자문결과목록 조회
		case "selectLawCnsutResult":
			this.ds_lawCnsutResult.clearData();
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();		 
			this.ds_cond2.setColumn(0, "REQST_NO", this.ds_lawCnsutReqst.getColumn(this.ds_lawCnsutReqst.rowposition,"REQST_NO"));
			var sController   	= "hsco/mis/law/LAW010202/selectLawCnsutResult.do";
			var sInDatasets   	= "input03=ds_cond2";
			var sOutDatasets  	= "ds_lawCnsutResult=output03";
		break;
		
		//법률자문결과 CUD
		case "lawCnsutResultCUD":
			var sController   	= "hsco/mis/law/LAW010202/lawCnsutResultCUD.do";
			var sInDatasets   	= "input04=ds_lawCnsutResult:U";
			var sOutDatasets  	= "";
		break;
		
		//법률자문회신 첨부파일조회
		case "selectAtchFile":
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_lawCnsutResult.getColumn(this.ds_lawCnsutResult.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
		//법률자문신청 첨부파일조회
		case "selectAtchFile2":
			this.div_work.grd_cmmnAtchmnfl00.setCellProperty("head", 0, "text", "0");
			this.ds_fileList2.clearData();
			this.ds_fileList2.addRow();
			this.ds_fileList2.setColumn(0, "FILE_SN", this.ds_lawCnsutReqst.getColumn(this.ds_lawCnsutReqst.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList2";
			var sOutDatasets = "ds_fileList2=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {	
		var comboOption = null;
		switch(strSvcId)
		{		
			//사업지구조회 콜백
			case "selectBsnsAreaList" :
				this.ds_bsnsAreaCode.insertRow(0);
				this.ds_bsnsAreaCode.setColumn(0, "CODE","");
				this.ds_bsnsAreaCode.setColumn(0, "ALL_NM","전체");
				this.div_search.cmb_landCodeSh.set_index(0);
				this.fn_transaction("selectJcffrOffmList");
			break;		

			//변호사사무실 조회 콜백
			case "selectJcffrOffmList":
				this.fn_transaction("selectLawyerList");
			break;
			
			//변호사목록 조회 콜백
			case "selectLawyerList":
				this.fn_search();
			break;
			//접수 콜백
			case "lawCnsutReqstU" :
				this.fn_search();
			break;
			
			//법률자문결과 CUD 콜백
			case "lawCnsutResultCUD":
				this.fn_search();
			break;
			
			//법률자문신청목록 조회 콜백
			case "selectLawCnsutReqst":
			if(this.ds_lawCnsutReqst.rowcount > 0){
				if(this.ds_lawCnsutReqst.getColumn(this.ds_lawCnsutReqst.rowposition, "RCEPT_AT") == '01')
				{
					this.div_work.cal_MNGD_RCEPT_DE.set_enable(true);
					this.div_work.Combo00.set_enable(true);
					this.div_work.Combo01.set_enable(true);
					this.div_work.edt_RCEPTER_EMPNM.set_enable(true);
					this.div_work.txt_PROCESS_CN.set_enable(true);
				}
			}else{
				this.div_work.cal_MNGD_RCEPT_DE.set_enable(false);
				this.div_work.Combo00.set_enable(false);
				this.div_work.Combo01.set_enable(false);
				this.div_work.edt_RCEPTER_EMPNM.set_enable(false);
				this.div_work.txt_PROCESS_CN.set_enable(false);
			}
			break;
			//법률자문결과 조회 콜백
			case "selectLawCnsutResult":
				if(this.ds_lawCnsutReqst.rowcount > 0){
					
					if(this.ds_lawCnsutResult.rowcount  <= 0)
					{
						this.ds_lawCnsutResult.set_updatecontrol(false);
						this.ds_lawCnsutResult.addRow();
						this.ds_lawCnsutResult.setColumn(0, "REQST_NO", this.ds_lawCnsutReqst.getColumn(this.ds_lawCnsutReqst.rowposition, "REQST_NO"));
						this.ds_lawCnsutResult.set_updatecontrol(true);
					} else {
						this.ds_lawyer.filter("OFFM_SN == '" + this.ds_lawCnsutResult.getColumn(0,"LAWYER_OFFM") + "'");
						trace("filter : OFFM_SN == '" + this.ds_lawCnsutResult.getColumn(0,"LAWYER_OFFM") + "'");
					}
				}
				this.fn_transaction("selectAtchFile2");
			break;
			
			//법률자문 신청 첨부파일 조회 콜백
			case "selectAtchFile2":
				this.fn_transaction("selectAtchFile");
			break;
			
			case "selectAtchFile":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_lawCnsutReqst.set_rowposition(save_row);
					save_row = -1;
				}
			break;
		}
	}
}
