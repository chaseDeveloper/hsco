/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "pms_lib::pmsUtil.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 
var saveDs = [
  this.ds_hdchRegistList		//소장
 ,this.ds_smallLdggReqestList	//소제기의뢰
 ,this.ds_lwstDtaList			//소송자료
 ,this.ds_lwstAgentList			//소송대리인
 ,this.ds_lwstListResult		//소송판결관리
 ,this.ds_ldgmntAmountList		//공탁금
 ,this.ds_judmnAmountList		//판결금
 ,this.ds_lwstCtList			//소송비용
];

//저장시 rowposition값 저장할 변수
var save_row = -1;
var save_row_lwst = -1;
var save_index_tab = -1;
var save_index_row = -1;
this.File_UpDownManager = null;
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.LAW010301_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.LAW010301_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	this.div_search.cal_bgnDe.setFocus(true);
