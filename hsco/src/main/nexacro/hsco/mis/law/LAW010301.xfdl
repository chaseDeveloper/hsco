/***********************************************************************
* 소송현황 목록 ROW 변경 후 
************************************************************************/
this.ds_lwstList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{				
	if(this.div_work.LAW010301.tabindex == 4) {
		this.div_work.LAW010301.T05.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
		if(this.comUtils.isNotNull(obj.getColumn(e.newrow, "ATCH_FILE"))){
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", obj.getColumn(e.newrow, "ATCH_FILE"));
			this.fn_transaction("selectAtchFile");	
		}else{
			this.ds_fileList.clearData();
		}
	}
	
	if(e.newrow > -1){
		
		if(save_row_lwst > -1){
			obj.set_enableevent(false);
			//onrowposchanged 발생방지
			obj.set_rowposition(save_row_lwst);
			obj.set_enableevent(true);
			save_row_lwst = -1;
		}		
		
		this.fn_transaction("selectSubTab");
	}
}
