}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	this.div_search.cal_bgnDe.set_value(this.gfn_today().substring(0,4) + '0101');
	this.div_search.cal_endDe.set_value(this.gfn_today());
	// 공통코드
	var comboParams = [["ds_incdntSe",      "LAW002", 1, ""],
	                   ["ds_incdntSeSearch","LAW002", 1, "전체"],
	                   ["ds_lwstTyS"  ,     "LAW003", 1, "전체"],
	                   ["ds_lwstTy"  ,      "LAW003", 1, ""],
	                   ["ds_refreeeSe",     "LAW004", 1, ""],
	                   ["ds_edAt",          "LAW008", 1, "전체"],
	                   ["ds_dtaSe",         "LAW013", 1, ""],
	                   ["ds_dudtKndList",   "LAW014", 1, ""],
	                   ["ds_cmptncCourtSe", "LAW016", 1, ""],
	                   ["ds_cmptncCourt",   "LAW015", 1, ""],
	                   ["ds_lwstResult",    "LAW009", 1, "선택"],	//판결결과
	                   ["ds_law018",		"LAW018", 1, ""]	//공사지위
		              ];
		              
	var callback = function (svcId, errorCode, errorMsg) {
		
	};
