***********************************************************************************/
this.ds_ldgmntAmountList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
 	if(e.col == 5)
 	{
		this.ds_ldgmntAmountList.set_updatecontrol(false);
		//공탁금액
		var ldgmt_amo = this.ds_ldgmntAmountList.getColumn(this.ds_ldgmntAmountList.rowposition, "LDGMNT_AMOUNT");
		//회수금액
		var rtrvl_amo = e.newvalue;
		if(nexacro.toNumber(ldgmt_amo) < nexacro.toNumber(rtrvl_amo)){
			this.gfn_message("errors.항목값.범위.이하","회수금액|공탁금액");
			this.ds_ldgmntAmountList.setColumn(e.row, "RTRVL_AMOUNT", e.oldvalue);
			return false;
		}
		this.ds_ldgmntAmountList.set_updatecontrol(true);
		
		
	}
	
	//잔액 자동계산 2016.06.27
	if(e.col == 4 || e.col == 5){		
		//공탁금액
		var ldgmt_amo = this.ds_ldgmntAmountList.getColumn(this.ds_ldgmntAmountList.rowposition, "LDGMNT_AMOUNT");
		//회수금액
		var rtrvl_amo = this.ds_ldgmntAmountList.getColumn(this.ds_ldgmntAmountList.rowposition, "RTRVL_AMOUNT");
		var blce = ldgmt_amo - rtrvl_amo;
		this.ds_ldgmntAmountList.setColumn(this.ds_ldgmntAmountList.rowposition, "BLCE", blce);
	}
}

