/***********************************************************************
* 소송 행버튼
************************************************************************/
this.btn_lwstList_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.comUtils.isNull(this.ds_incdntList.getColumn(this.ds_incdntList.rowposition, "REGIST_DT"))){
		this.gfn_message("comm.정보.저장.요청","먼저 사건목록");
	}else{
		switch(obj.name) {
			case "btn_add":
				//사건의 종결여부가 종결일경우
				if(this.ds_incdntList.getColumn(this.ds_incdntList.rowposition, "ED_AT") == '01')
				{
					this.gfn_message("comm.종결","사건");
				}else{
				
					if(this.ds_lwstList.rowcount == 3)
					{
						this.gfn_message("info.저장.초과", "3심까지");
					}else{	
						var nRow = this.ds_lwstList.addRow();
						var refreeeNo = nRow + 1;
						this.ds_lwstList.setColumn(nRow, "LWST_MANAGE_NO", this.ds_incdntList.getColumn(this.ds_incdntList.rowposition, "LWST_MANAGE_NO"));    
						this.ds_lwstList.setColumn(nRow, "LWST_TY",   this.ds_incdntList.getColumn(this.ds_incdntList.rowposition, "LWST_TY"));    
						this.ds_lwstList.setColumn(nRow, "REFREEE",   '0' + refreeeNo);
						
					}
				}
				break;
			case "btn_del":
				if(this.gfn_message("confirm.삭제여부")){
					this.fn_transaction("selectLwstListChild");
				}
				break;
			case "btn_reset":
				//dataset update check
				if(!this.comUtils.isDatasetUpdated(this.ds_lwstList))
				{
					this.gfn_message("comm.내역.변경.없음");
				}else{
					if(this.gfn_message("confirm.취소여부")){
						this.ds_lwstList.reset();		//DATASET ROLLBACK
						this.ds_lwstList.applyChange();	//DATASET COMMIT
						return;
					}
				}
			break;
		}
	}
}

