/**********************************************************************
* dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "LAW");
   this.File_UpDownManager.init(fileConfig);
}
/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	this.ds_incdntList.clearData();
	this.ds_lwstList.clearData();
	this.fn_transaction("selectIncdntList");
}

/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_lwstList) && !this.comUtils.isDatasetUpdated(this.ds_hdchRegistList)
	&& !this.comUtils.isDatasetUpdated(this.ds_smallLdggReqestList) && !this.comUtils.isDatasetUpdated(this.ds_lwstDtaList)
	&& !this.comUtils.isDatasetUpdated(this.ds_lwstAgentList)  && !this.comUtils.isDatasetUpdated(this.ds_lwstListResult)
	&& !this.comUtils.isDatasetUpdated(this.ds_ldgmntAmountList) && !this.comUtils.isDatasetUpdated(this.ds_judmnAmountList)
	&& !this.comUtils.isDatasetUpdated(this.ds_lwstCtList) && !this.comUtils.isDatasetUpdated(this.ds_incdntList)) {
		this.gfn_message("comm.내역.변경.없음");
		return false;
	}
	
	//사건현황 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_incdntList, this.ds_validation)){
		return;
	} 
	
	//소송현황 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_lwstList, this.ds_validation)){
		return;
	}
	//소장등록 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_hdchRegistList, this.ds_validation)){
		return;
	}
	//소제기의뢰 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_smallLdggReqestList, this.ds_validation)){
		return;
	}
	
	//소송자료 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_lwstDtaList, this.ds_validation)){
		return;
	}
	
	//소송대리인 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_lwstAgentList, this.ds_validation)){
		return;
	}
	
	//소송판결관리 진행상태를 업데이트함
	/*
	if(this.comUtils.isDatasetUpdated(this.ds_lwstListResult)){
		return;
	}
	*/

	//공탁금 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_ldgmntAmountList, this.ds_validation)){
		return;
	}

	//판결금 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_judmnAmountList, this.ds_validation)){
		return;
	}
	
	//소송비용 validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_lwstCtList, this.ds_validation)){
		return;
	}

	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.ds_incdntList.rowposition;
		save_row_lwst = this.ds_lwstList.rowposition;
		save_index_tab = this.div_work.LAW010301.tabindex;
		save_index_row = saveDs[save_index_tab].rowposition;
		/*
		if(this.comUtils.isDatasetUpdated(this.ds_incdntList)){
			this.fn_transaction("incdntListCUD");
			if(this.comUtils.isDatasetUpdated(this.ds_lwstListResult)){
				this.fn_transaction("lwstListCUD");
			}
		}else{
			this.fn_transaction("lwstListCUD");
		}
		*/
		
		this.ds_incdntList.set_enableevent(false);
		saveDs[save_index_tab].set_enableevent(false);
		this.fn_transaction("lwstListCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		//사업코드조회
		case "selectBsnsAreaList":
			this.ds_cond03.clearData();
			this.ds_cond03.addRow();
			var sController   	= "hsco/mis/mis/MIS000000/bsnsAreaList.do";
			var sInDatasets   	= "input01=ds_cond03";
			var sOutDatasets  	= "ds_bsnsAreaCode=output01";
		break;
		
		//변호사(법무사)사무실 목록 조회
		case "selectJcffrOffmList":
			this.ds_cond03.clearData();
			var nRow = this.ds_cond03.addRow();
			
			var sController   	= "hsco/mis/law/LAW010301/selectJcffrOffmList.do";
			var sInDatasets   	= "input6=ds_cond03";
			var sOutDatasets  	= "ds_jcffrOffm=output6";
		break;
		
		//변호사(법무사)목록 조회
		case "selectLawyerList" :
			this.ds_cond03.clearData();
			var nRow = this.ds_cond03.addRow();
			
			var sController   	= "hsco/mis/law/LAW010301/selectLawyerList.do";
			var sInDatasets   	= "input7=ds_cond03";
			var sOutDatasets  	= "ds_lawyer=output7";
		break;
		
		
		//사건목록 조회
		case "selectIncdntList": 
			var sController   	= "hsco/mis/law/LAW010300/selectIncdntList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_incdntList=output01";
		break;	
				
				
		//소송현황목록 조회
		case "selectLwstList": 
			//초기화
			this.ds_fileList.clearData();
			this.ds_smallLdggReqestList.clearData();
			this.ds_hdchRegistList.clearData();
			this.ds_lwstDtaList.clearData();
			this.ds_lwstAgentList.clearData();
			this.ds_judmnAmountList.clearData();
			this.ds_ldgmntAmountList.clearData();
			this.ds_lwstCtList.clearData();
			this.ds_cond1.clearData();
			this.ds_cond1.addRow();
			this.ds_cond1.setColumn(0, "LWST_MANAGE_NO",this.ds_incdntList.getColumn(this.ds_incdntList.rowposition,"LWST_MANAGE_NO"));
			var sController   	= "hsco/mis/law/LAW010301/selectLwstList.do";
			var sInDatasets   	= "input01=ds_cond1";
			var sOutDatasets  	= "ds_lwstList=output01";
		break;	
		
		// 하위 탭 8개 조회
		case "selectSubTab":
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();
			this.ds_cond2.setColumn(0, "LWST_MANAGE_NO", this.ds_lwstList.getColumn(this.ds_lwstList.rowposition,"LWST_MANAGE_NO"));
			this.ds_cond2.setColumn(0, "INCDNT_NO",      this.ds_lwstList.getColumn(this.ds_lwstList.rowposition,"INCDNT_NO"));
			var sController   	= "hsco/mis/law/LAW010301/selectSubTab.do";
			var sInDatasets   	= "input03=ds_cond2";
			var sOutDatasets  	= "ds_hdchRegistList=output03 ds_smallLdggReqestList=output05 ds_lwstDtaList=output07 ds_lwstAgentList=output09 ds_ldgmntAmountList=output13 ds_judmnAmountList=output15 ds_lwstCtList=output17" ;
		break;
		
		//사건 목록 CUD (미사용)
		case "incdntListCUD" :
			var sController   	= "hsco/mis/law/LAW010300/incdntListCUD.do";
			var sInDatasets   	= "input02=ds_incdntList:U";
			var sOutDatasets  	= "";
		break;		
		
		//소송현황목록 CUD
		case "lwstListCUD":
			var sController   	= "hsco/mis/law/LAW010301/lwstListCUD.do";
			var sInDatasets   	= "input01=ds_incdntList:U input02=ds_lwstList:U input04=ds_hdchRegistList:U input06=ds_smallLdggReqestList:U input08=ds_lwstDtaList:U input10=ds_lwstAgentList:U input12=ds_lwstListResult:U input14=ds_ldgmntAmountList:U input16=ds_judmnAmountList:U input18=ds_lwstCtList:U" ;
			var sOutDatasets  	= "";
		break;
				
		//상단 삭제 트랜잭션
		case "selectLwstListChild" :
			this.ds_delete.clearData();
			this.ds_delete.addRow();
			this.ds_delete.setColumn(0, "LWST_MANAGE_NO", this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "LWST_MANAGE_NO"));
			this.ds_delete.setColumn(0, "INCDNT_NO",      this.ds_lwstList.getColumn(this.ds_lwstList.rowposition, "INCDNT_NO"));
			
			var sController   	= "hsco/mis/law/LAW010301/selectLwstListChild.do";
			var sInDatasets   	= "input19=ds_delete";
			var sOutDatasets  	= "ds_deleteAt=output19";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			if(save_row_lwst< 0 && save_index_row > -1){
				var fileSn_Id = (save_index_tab!=3? "ATCH_FILE" : "ATCH_FILE_1");
				var fileSn = (saveDs[save_index_tab].getColumn(save_index_row, fileSn_Id));
				if(this.comUtils.isNull(fileSn)){
					break;
				}else{
					this.ds_fileList.setColumn(0, "FILE_SN", fileSn);
				}
			}
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {	
		var comboOption = null;
		switch(strSvcId)
		{		
			//사업지구조회 콜백 조회용 전체추가
			case "selectBsnsAreaList" :
				this.ds_bsnsAreaCode.insertRow(0);
				this.ds_bsnsAreaCode.setColumn(0, "CODE","");
				this.ds_bsnsAreaCode.setColumn(0, "ALL_NM","전체");
				this.div_search.cmb_landCodeSh.set_index(0);
				this.fn_transaction("selectJcffrOffmList");
			break;		
			
			//사건목록 조회 콜백
			case "selectIncdntList":
				/*최초조회시 onrowposchanged로 가지않게 하고 직접 소송현황을 조회함*/
				this.ds_incdntList.enableevent ? '': this.ds_incdntList.set_enableevent(true);
				if(save_row > 0){
					this.ds_incdntList.set_rowposition(save_row);
					save_row = -1;
				}else{
					this.fn_transaction("selectLwstList");
				}
			break;
			
			case "selectLwstList":
			//소송현황
			saveDs[save_index_tab].enableevent ? '': saveDs[save_index_tab].set_enableevent(true);
			break;
	
			//변호사(법무사)사무실 목록 조회 콜백
			case "selectJcffrOffmList":
				this.fn_transaction("selectLawyerList");
			break;
			
			//변호사(법무사)목록 조회 콜백
			case "selectLawyerList":
				this.fn_search();
			break;
			
			//사건현황목록 CUD 콜백
			case "incdntListCUD":
				this.fn_search();
			break;	

			//소송현황목록 CUD 콜백
			case "lwstListCUD":
				this.fn_search();
			break;		
				
			//상단삭제 트랜잭션 콜백
			case "selectLwstListChild":
				//자식 레코드가 있을경우
				if(this.ds_deleteAt.getColumn(0, "SUM_1" ) > 0 )
				{
					var alertArr = new Array();
					if(this.ds_deleteAt.getColumn(0, "CNT_1" ) > 0 )
					{
						alertArr.push("소장등록");
					}
					if(this.ds_deleteAt.getColumn(0, "CNT_2" ) > 0 )
					{
						alertArr.push("소제기의뢰");
					}
					if(this.ds_deleteAt.getColumn(0, "CNT_3" ) > 0 )
					{
						alertArr.push("소송자료");
					}
					if(this.ds_deleteAt.getColumn(0, "CNT_4" ) > 0 )
					{
						alertArr.push("소송대리인");
					}
					if(this.ds_deleteAt.getColumn(0, "CNT_6" ) > 0 )
					{
						alertArr.push("공탁금");
					}
					if(this.ds_deleteAt.getColumn(0, "CNT_7" ) > 0 )
					{
						alertArr.push("판결금");
					}
					if(this.ds_deleteAt.getColumn(0, "CNT_8" ) > 0 )
					{
						alertArr.push("소송비용");
					}
					//존재하는 자식레코드 명 alert
					var alertStr = (alertArr.join(","));
					alertStr += "목록을 먼저 삭제하여 주십시오."
					this.alert(alertStr);
				}else{
					//자식 레코드가없는 경우 삭제
					this.ds_lwstList.deleteRow(this.ds_lwstList.rowposition);	
				}
			break;		
			//하단 탭 조회 콜백 
			case "selectSubTab":
				//소장등록 탭
				//onrowposchanged 발생방지
				if(save_index_row > -1 && saveDs[save_index_tab].rowcount > -1 ){
					saveDs[save_index_tab].set_rowposition(save_index_row);
					save_index_tab = -1;
					save_index_row = -1;
				}
				
				//소송판결관리탭
				if(this.ds_lwstList.rowposition > -1){
					//소송판결관리에 현재 소송의 판결정보를 복사한다
					this.ds_lwstListResult.clearData();
					this.ds_lwstListResult.addRow();
					
					this.ds_lwstListResult.copyRow(this.ds_lwstListResult.rowposition, this.ds_lwstList, this.ds_lwstList.rowposition);
					
					//종결일
					this.ds_lwstListResult.setColumn(this.ds_lwstListResult.rowposition, "END_DE", this.ds_incdntList.getColumn(this.ds_incdntList.rowposition, "END_DE"));//사건목록의 종결일
					
					this.ds_lwstListResult.applyChange();
				}
				else{
					this.ds_lwstListResult.clearData();
					this.ds_lwstListResult.applyChange();
				}
				
				if(this.div_work.LAW010301.tabindex == 0)
				{
					//소장정보가 존재할경우
					if(this.ds_hdchRegistList.rowcount > 0)
					{	
						//신규버튼 -> 삭제버튼으로
						//this.div_work.LAW010301.T01.btn_insert.set_enable(false);
						//this.div_work.LAW010301.T01.btn_delete.set_enable(true);
						this.div_work.LAW010301.T01.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
						
						if(this.comUtils.isNotNull(this.ds_hdchRegistList.getColumn(this.ds_hdchRegistList.rowposition, "ATCH_FILE"))){
							this.ds_fileList.clearData();
							this.ds_fileList.addRow();
							this.ds_fileList.setColumn(0, "FILE_SN", this.ds_hdchRegistList.getColumn(this.ds_hdchRegistList.rowposition, "ATCH_FILE"));
							this.fn_transaction("selectAtchFile");
						}
					}else{ //소장정보가 없는경우
						//삭제버튼 -> 신규버튼
						//this.div_work.LAW010301.T01.btn_insert.set_enable(true);
