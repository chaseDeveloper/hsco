 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {	
		var comboOption = null;
		switch(strSvcId)
		{		
		
			//사업지구조회 콜백 조회용 전체추가
			case "selectBsnsAreaList" :
				this.ds_bsnsAreaCode.insertRow(0);
				this.ds_bsnsAreaCode.setColumn(0, "CODE","");
				this.ds_bsnsAreaCode.setColumn(0, "ALL_NM","전체");
				this.div_search.cmb_landCodeSh.set_index(0);
				this.fn_transaction("selectBsnsAreaList2");
			break;		
			
			//사업지구조회 콜백 등록용
			case "selectBsnsAreaList2":
				this.fn_transaction("selectJcffrOffmList");
			break;
			
			//변호사사무실 조회 콜백
			case "selectJcffrOffmList":
				this.fn_transaction("selectLawyerList");
			break;
			
			//변호사목록 조회 콜백
			case "selectLawyerList":
				this.fn_search();
			break;
			
			//상단저장 콜백
			case "lawCnsutReqstCUD":
				this.fn_search();
			break;
			
			//법률자문 신청조회 콜백
			case "selectLawCnsutReqst":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_lawCnsutReqst.set_rowposition(save_row);
					save_row = -1;
				}
			break;
			
			//법률자문 결과조회 콜백
			case "selectLawCnsutResult":
				this.fn_transaction("selectAtchFile");
			break;
			
			//법률자문 신청 첨부파일 조회 콜백
			case "selectAtchFile":
				if(this.ds_lawCnsutResult.rowcount > 0){
					this.fn_transaction("selectAtchFile2");
				}else{
					this.ds_fileList2.clearData();
				}
			break;
			
			//소송지출목록 조회 콜백
			case "selectLawyerCtList":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_indpndQestn.set_rowposition(save_row);
					save_row = -1;
				}
				this.fn_transaction("selectAtchFile");
			break;
			
			case "innerSanctU":
				this.fn_search();
			break;
			
// 			// 내부결재id 조회
// 			case "GetInnerSanctnId":
// 			    this.fn_popup_InnerSanctionDrft();
// 			break;
			
// 			//최초 내부결재 진행여부체크
// 			case "selectInnerSanctnMasterCheck":
// 				if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
// 				{
// 					this.ds_lawCnsutReqst.setColumn(this.ds_lawCnsutReqst.rowposition, "DRFT_JOB_CONFM_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
// 					this.ds_lawCnsutReqst.setColumn(this.ds_lawCnsutReqst.rowposition, "DRFT_DE",           this.gfn_today());
// 					this.fn_transaction("lawCnsutReqstCUD");
// 				}else{
// 					
// 				}
// 			break;
		}
	}
}


/***********************************************************************
* 상단 신규
************************************************************************/
this.fn_insert = function()
{
	var nRow = this.ds_lawCnsutReqst.addRow();
	this.ds_lawCnsutReqst.setColumn(nRow, "REQST_DE", this.gfn_today());
	
	//법무 담당권한이 없을 때
	if(this.gfn_authcode("115") == -1){
		this.ds_lawCnsutReqst.setColumn(nRow, "REQST_DEPT", this.gfn_getDeptId());
		this.ds_lawCnsutReqst.setColumn(nRow, "REQST_DEPT_NM", this.gfn_getDeptName());
		this.ds_lawCnsutReqst.setColumn(nRow, "APPLCNT", this.gfn_getUserId());
		this.ds_lawCnsutReqst.setColumn(nRow, "APPLCNT_NM", this.gfn_getUserName());
		this.div_work.edt_emplnm.set_cssclass("edt_WF_Readonly");
		this.div_work.edt_emplnm.set_readonly(true);
		this.div_work.edt_deptNm.set_cssclass("edt_WF_Readonly");
		this.div_work.edt_deptNm.set_readonly(true);
		this.div_work.search.set_enable(false);
	}else{
		this.div_work.edt_emplnm.set_cssclass("edt_WF_Essential");
		this.div_work.edt_emplnm.set_readonly(false);
		this.div_work.edt_deptNm.set_cssclass("edt_WF_Essential");
		this.div_work.edt_deptNm.set_readonly(false);
		this.div_work.search.set_enable(true); 
	}
}

/***********************************************************************
* 상단삭제
************************************************************************/
this.fn_delete = function()
{
	var nRow = this.ds_lawCnsutReqst.rowposition;
	if(this.ds_lawCnsutReqst.getColumn(nRow, "RPLY_AT") == '01')
	{
		this.gfn_message("fail.삭제.불가","법률자문결과가 존재하여");
	}else{
		if(this.gfn_message("confirm.삭제여부")){
			this.ds_lawCnsutReqst.deleteRow(nRow);	
		}
	}
}

/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_lawCnsutReqst))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_lawCnsutReqst.reset();		//DATASET ROLLBACK
			this.ds_lawCnsutReqst.applyChange();	//DATASET COMMIT
			return;
		}
	}
}
