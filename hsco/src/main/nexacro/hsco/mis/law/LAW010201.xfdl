/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	var ds_bsnsCode = new Dataset();
	var loadCnt = ds_bsnsCode.loadXML(sReturn);
	if(loadCnt) {
		var bsnsCode = ds_bsnsCode.getColumn(0, "BSNS_CODE");
		var bsnsNm   = ds_bsnsCode.getColumn(0, "BSNS_NM");
		var euRow    = this.ds_lawCnsutReqst.rowposition;
		this.ds_lawCnsutReqst.setColumn(euRow, "BSNS_CODE", bsnsCode);
		this.ds_lawCnsutReqst.setColumn(euRow, "BSNS_NM",   bsnsNm  );
	}
