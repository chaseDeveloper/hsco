this.fn_callback5 = function(sPopupId, sReturn)
{
	var ds_temp = new Dataset;
	nRowCnt = ds_temp.loadXML(sReturn);
	if(!nRowCnt){
		this.ds_temp.addRow();
	} else {
		this.ds_lawCnsutReqst.setColumn(this.ds_lawCnsutReqst.rowposition, "APPLCNT", ds_temp.getColumn(0, "EMPNO"));
		this.ds_lawCnsutReqst.setColumn(this.ds_lawCnsutReqst.rowposition, "APPLCNT_NM", ds_temp.getColumn(0, "EMPNM"));
		this.ds_lawCnsutReqst.setColumn(this.ds_lawCnsutReqst.rowposition, "REQST_DEPT", ds_temp.getColumn(0, "DEPT_CODE"));
		this.ds_lawCnsutReqst.setColumn(this.ds_lawCnsutReqst.rowposition, "REQST_DEPT_NM", ds_temp.getColumn(0, "DEPT_NM"));
	}
}

