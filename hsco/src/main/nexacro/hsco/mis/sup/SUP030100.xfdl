    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : SUP030100
 * 화면명   : 대외협력관리
 * 화면설명 : 대외협력관리를 관리한다.
 * 작성일   : 2015.10.12
 * 작성자   : 양성모
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
//this.checkDs = [this.ds_extrnlCorpr];
this.saveRow = -1;
this.checkDs = [this.ds_extrnlCorpr];
this.File_UpDownManager = null;

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SUP030100_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.SUP030100_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{

	// 초기 검색조건
	this.ds_cond.setColumn(0, "YEAR",   this.dateUtils.today().substr(0,4));
	
	
	// 공통코드
	var comboParams = [
		["ds_sup009", "SUP009", "Y", "A"],	    // 대외협력기관구분(검색콤보)
		["ds_sup009_01", "SUP009", "Y", "D"]	// 대외협력기관구분
		,["ds_sacnctnSttusSe", "COM023", 1, ""]	// 결재상태
	];
	var callback = function (svcId, errorCode, errorMsg) {
		
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);	
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "SUP");
	this.File_UpDownManager.init(fileConfig);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	this.tab_extrnlCorpr.set_tabindex(0);
	
	// 데이터셋 클리어
	this.ds_extrnlCorpr.clearData();
	
	this.fn_transaction("select");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_extrnlCorpr)) {              //데이터셋의 수정여부 체크
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_extrnlCorpr, this.ds_validation)) {
			return false;
		}
		
		this.saveRow = this.ds_extrnlCorpr.rowposition;
		this.fn_transaction("save");
	} else {
		this.gfn_message("comm.데이터.변경.없음");	
	}
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "select":    			// 대외협력관리 조회
			var sController   	= "hsco/mis/sup/SUP030100/selectExtrnlCorpr.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_extrnlCorpr=output1";
			break;
		case "save":    			// 대외협력관리 저장
			var sController   	= "hsco/mis/sup/SUP030100/saveExtrnlCorpr.do";
			var sInDatasets   	= "input1=ds_extrnlCorpr:U";
			var sOutDatasets  	= "";
			break;
		case "approve":				// 전자결재
			var sController   	= "hsco/mis/ass/SUP030100/approve.do";	
			var sInDatasets   	= "input1=ds_approve";
			var sOutDatasets  	= "ds_approve=output1";
			break;
		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/sup/SUP030100/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_fileList=output1";
			break;					
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "select":
				this.ds_extrnlCorpr.applyChange();
				
				if(this.saveRow > -1){
					var tempRow = this.saveRow;
					this.saveRow = -1;
					this.ds_extrnlCorpr.set_rowposition(tempRow);
					
				}
				break;
			case "save":
			    this.gfn_message("success.처리.성공");
			    this.fn_search();
				break;
			case "approve":		// 전자결재
				var sanctnNO = this.ds_approve.getColumn(0,"SANCTN_NO");
				
				this.ds_extrnlCorpr.set_updatecontrol(false);
				if(this.tab_extrnlCorpr.tabindex == 0){
					this.ds_extrnlCorpr.setColumn(this.ds_extrnlCorpr.rowposition, "REQUST_SANCTN_NO", sanctnNO);
				}
				
				if(this.tab_extrnlCorpr.tabindex == 1){
					this.ds_extrnlCorpr.setColumn(this.ds_extrnlCorpr.rowposition, "PRESENTN_SANCTN_NO", sanctnNO);
				}
				
				this.ds_extrnlCorpr.set_updatecontrol(true);
					
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
				break;	
			case "selectFileList":      // 첨부파일 조회
				if(this.tab_extrnlCorpr.tabindex == 0){
					this.tab_extrnlCorpr.tpg1.grd_cmmnAtchmnfl.setCellProperty("Head", 0, "text", "0");
				}
				
				if(this.tab_extrnlCorpr.tabindex == 1){
					this.tab_extrnlCorpr.tpg2.grd_cmmnAtchmnfl.setCellProperty("Head", 0, "text", "0");
				}
				
				break;
		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
************************************************************************
* 1. 탭 이벤트 영역
* 2. Button 영역 
************************************************************************/

/***********************************************************************
* 탭 이벤트 영역
************************************************************************/
/*
 * 함수명  	: tap_extrnlCorpr_canchange
 * 설명 		: 탭 페이지 변경 전 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tap_extrnlCorpr_canchange();
 */
this.tap_extrnlCorpr_canchange = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	if(this.tab_extrnlCorpr.tabindex == 0) {
	    var nRow = this.ds_extrnlCorpr.rowposition;
		var strConfmAt = this.ds_extrnlCorpr.getColumn(nRow, "REQUST_CONFM_AT");
		var strPresentnDe = this.ds_extrnlCorpr.getColumn(nRow, "PRESENTN_DE");
		var strPresentnInsttSe = this.ds_extrnlCorpr.getColumn(nRow, "PRESENTN_INSTT_SE");
		
// 		if(strConfmAt == "0") {
// 			this.gfn_message("info.진행가능", "대외요청 자료 승인");
// 			return false;
// 		}
		
		if(!strPresentnDe) {
			this.ds_extrnlCorpr.setColumn(nRow, "PRESENTN_DE", this.dateUtils.today());
		}
		
		if(!strPresentnInsttSe) {
			this.ds_extrnlCorpr.setColumn(nRow, "PRESENTN_INSTT_SE", "1");
		}
	}
}

/*
 * 함수명  	: tap_extrnlCorpr_canchange
 * 설명 		: 탭 페이지 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tap_extrnlCorpr_canchange();
 */
this.tap_extrnlCorpr_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	var nRow = this.ds_extrnlCorpr.rowposition;
	var extrnlManageNo = this.ds_extrnlCorpr.getColumn(nRow, "EXTRNL_MANAGE_NO");
	var fileSn;
	
	if(this.tab_extrnlCorpr.tabindex == 0) {
		fileSn = this.ds_extrnlCorpr.getColumn(nRow, "REQUST_FILE_SN");
	}
	
	if(this.tab_extrnlCorpr.tabindex == 1) {
		fileSn = this.ds_extrnlCorpr.getColumn(nRow, "PRESENTN_FILE_SN");
	}
	this.ds_fileList.clearData();
	
	this.ds_cond02.clearData();
	this.ds_cond02.addRow();
	
	this.ds_cond02.setColumn(0, "EXTRNL_MANAGE_NO", extrnlManageNo);
	this.ds_cond02.setColumn(0, "FILE_SN", fileSn);
	
	this.fn_transaction("selectFileList");  // 첨부파일 조회
}

/**********************************************************************
* Button 영역 
***********************************************************************/
/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert= function()
{
	this.ds_extrnlCorpr.set_enableevent(false);
		var nRow = this.ds_extrnlCorpr.addRow();
		this.ds_extrnlCorpr.setColumn(nRow, "REQUST_CONFM_AT",   "0");
		this.ds_extrnlCorpr.setColumn(nRow, "PRESENTN_CONFM_AT", "0");
		this.ds_extrnlCorpr.setColumn(nRow, "REQUST_INSTT_SE", "1");
		this.ds_extrnlCorpr.setColumn(nRow, "REQUST_DE", this.dateUtils.today());
		this.ds_extrnlCorpr.setColumn(nRow, "REQUST_SANCTN_KND", "0"); //
		this.ds_extrnlCorpr.setColumn(nRow, "PRESENTN_SANCTN_KND", "0"); //
	this.ds_extrnlCorpr.set_enableevent(true);
	
	this.ds_extrnlCorpr_onrowposchanged();

}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete= function()
{
	/*
	var sanctnKnd = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "PRESENTN_SANCTN_KND");
	
	if(sanctnKnd != '0' && sanctnKnd != '2' && sanctnKnd != '3'){
		if(sanctnKnd == '1' || sanctnKnd == '4'){		// 기안, 중간결재
			this.gfn_message("comm.결재.진행");
			return false;
		} else if(sanctnKnd == '5') {					// 결재완료
			this.gfn_message("comm.결재.완료");
			return false;
		}
	}
	*/
	
	//2016.08.10
	var SANCTN_STS = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "SANCTN_STS");
	switch (SANCTN_STS) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");					
		return;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return;
		
		default:
	}	
	
	if ( this.gfn_message("confirm.삭제여부") ) {
		var nRow = this.ds_extrnlCorpr.rowposition;
		this.ds_extrnlCorpr.deleteRow(nRow);
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if ( this.gfn_message("confirm.취소여부") ) {
		this.ds_extrnlCorpr.reset();
		this.ds_extrnlCorpr.applyChange();
	}
}

/*
 * 함수명  	: fn_saveDtaCreatCUD
 * 설명 		: 결재 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_saveDtaCreatCUD();
 */
this.fn_sanction= function()
{	
    // 대외요청
	if(this.tab_extrnlCorpr.tabindex == 0){
// 		var sanctnKnd = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "REQUST_SANCTN_KND");
// 		var subject   = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "REQUST_SJ");
// 		var se        = "REQUST";
// 		return;
		this.gfn_message("info.처리불가" , "대외제출시에\n결재가 가능합니다.");
		return false;
	}
	
	// 대외제출
	if(this.tab_extrnlCorpr.tabindex == 1){
		var sanctnKnd = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "PRESENTN_SANCTN_KND");
		var subject   = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "PRESENTN_SJ");
		var contentCn = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "PRESENTN_CN");
		var se        = "PRESENTN";
	}
	
	if(!subject){
		this.gfn_message("info.처리불가" , "대외제출 제목을 입력해\n주시기 바랍니다.");
		return false;		
	}
	
	if(!contentCn){
		this.gfn_message("info.처리불가" , "대외제출 내용을 입력해\n주시기 바랍니다.");
		return false;		
	}
	
	if(this.ds_extrnlCorpr.getRowType(this.ds_extrnlCorpr.rowposition) != Dataset.ROWTYPE_NORMAL){
		this.gfn_message("comm.정보.저장.요청", "변경된");
		return false;
	}
	
	// 전자결재 상태 체크 2016.7
	// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
	// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
	// 기안전인것 : 재상신 및 수정/삭제
	// 결재상태가 상신전 외는 신규로 다시 진행.
	var sanctnKnd = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "SANCTN_STS");

	switch (sanctnKnd) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");
		return false;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return false;
		
		default:
	}
	
	
	var nRow = this.ds_extrnlCorpr.rowposition;	
	
	var contentCn = this.ds_extrnlCorpr.getColumn(nRow, "PRESENTN_CN");
	
	var rtnStr = "";
	for(var fRow = 0; fRow < contentCn.length; fRow++) {
		var strCode = contentCn.charCodeAt(fRow);	// code
		var str = String.fromCharCode(strCode);
		if(strCode == '10') {
			str = String.fromCharCode(13) + String.fromCharCode(10);
		} else {
			str = String.fromCharCode(strCode);
		}
		
		rtnStr += str;
	}
	
	var filds = new Array(3);
	filds[0] = rtnStr;     		// 내용
	filds[1] = null; 			// 붙임
	
	var dataValue = filds.join("|");
	
	var dataTitle = subject;
	var title     = subject;
	
	this.ds_approve.clearData();
	this.ds_approve.addRow();
	
	this.ds_approve.setColumn(0, "TITLE",      title                );
	this.ds_approve.setColumn(0, "DATA_TITLE", dataTitle            );
	this.ds_approve.setColumn(0, "DATA_VALUE", dataValue            );
 	this.ds_approve.setColumn(0, "DEPT_CD",    this.gfn_getDeptId() );
 	this.ds_approve.setColumn(0, "EMP_CD",     this.fn_getEmpno() );
    
    // 대외협력관리 테이블에 전자결재번호를 업데이트 하기위한 설정
    var extrnlManageNo      = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "EXTRNL_MANAGE_NO");
	this.ds_approve.setColumn(0, "EXTRNL_MANAGE_NO", extrnlManageNo);
	this.ds_approve.setColumn(0, "SE", se);
	
	//2016.7 전자결재
	var sanctn_no = this.ds_extrnlCorpr.getColumn(this.ds_extrnlCorpr.rowposition, "SANCTN_NO") ;
	this.ds_approve.setColumn(0, "SANCTN_NO", sanctn_no);

	this.fn_transaction("approve");	
}

