 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 해당 Form에 필요한 초기화 부분 기술
	// =======================================
	this.fn_transaction("cntrwkList", false);
	for(i = 0; i < this.ds_cntrwk.getRowCount(); i++) {
		this.grd.setCellProperty("Head", i + 6, "text", this.ds_cntrwk.getColumn(i, "ETC"));
		this.grd.setCellProperty("Head", i + 6, "tooltiptext", this.ds_cntrwk.getColumn(i, "CODE_NM"));
		this.grd.setCellProperty("Body", i + 6, "text", "bind:"+this.ds_cntrwk.getColumn(i, "COL_NM"));
		this.grd.setCellProperty("Body", i + 6, "edittype", "expr:CL_STEP == '0' ?  'none' :  'masknumber'");
		this.grd.setCellProperty("Body", i + 6, "editdisplay", "expr:CL_STEP == '0' ?  'none' :  'display'");
		this.grd.setCellProperty("Body", i + 6, "mask", "expr:dataset.getColumn(currow, \"IX_CODE\")==\"R\" ? '#' : (!"+this.ds_cntrwk.getColumn(i, "COL_NM")+" ?  '!###.##' : '!##0.00')");
	}
}

