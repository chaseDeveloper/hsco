/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{		
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}

	this.fn_transaction("cntrwkCmprList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_cntrwkCmpr)) {	
		
		this.saveRow = this.ds_cntrwkCmpr.rowposition;
	    this.fn_transaction("mngmtEvlCntrwkCmprU");
	} else {
		this.gfn_message("comm.데이터.변경.없음");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "cntrwkList":			// 지방공기업 목록
			var sController   	= "hsco/mis/sup/SUP010204/cntrwkList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_cntrwk=output1";
			break;
		case "cntrwkCmprList":		// 경영평가결과 조회
			this.ds_cntrwkCmpr.clearData();
			var sController   	= "hsco/mis/sup/SUP010204/cntrwkCmprList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cntrwkCmpr=output1";
			break;
		case "mngmtEvlCntrwkCmprU":		// 경영평가결과 저장
			var sController   	= "hsco/mis/sup/SUP010204/mngmtEvlCntrwkCmprU.do";
			var sInDatasets   	= "input1=ds_cntrwkCmpr:U";
			var sOutDatasets  	= "";
			break;
	}

	if(arguments.length > 1) {
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", "", false, arguments[1]); 
	} else {
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
	}
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0) {
		// 오류 및 성공 메시지를 화면에 보여준다.
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);

		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "cntrwkList":
				break;
			case "cntrwkCmprList":
				break;
			case "mngmtEvlStdrU":
				break;
			default:
				break;
		}
	} else {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch(strSvcId) {
			case "cntrwkList":
				break;
			case "cntrwkCmprList":
// 				for(i = this.ds_cntrwkCmpr.getRowCount() - 1;i >= 0; i--) {
// 					if(this.ds_cntrwkCmpr.getColumn(i, "CL_STEP") == "1") {
// 						this.grd.setTreeStatus(i, false);
// 					}
// 				}
				
				if(this.saveRow > -1){
					this.ds_cntrwkCmpr.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "mngmtEvlStdrU":
				this.gfn_message("success.처리.성공");
				
				// 재조회
				this.fn_transaction("cntrwkCmprList");
				break;
			default:
				break;
		}
	}
	
	
}

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: btn_onclick
 * 설명 		: 버튼 클릭 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_onclick();
 */
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_cntrwkCmpr.reset();		                                 //DATASET ROLLBACK
		this.ds_cntrwkCmpr.applyChange();	                             //DATASET COMMIT
		return;
	}
}



this.ds_cntrwkCmpr_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	return (e.newvalue <= Number(obj.getColumn(e.row,"ALLOT")));
}


/*
 * 함수명  	: Excel_down_onclick
 * 설명 		: 엑셀다운로드
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_onclick();
 */
this.Excel_down_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.grd_excel.set_enableevent(false);
	this.grd_excel.set_formats(this.grd.getCurFormatString());
	for(var i=0; i < this.grd_excel.getCellCount("Body"); i++){
		this.grd_excel.setCellProperty("Body", i, "mask", "");
	}
	this.gfn_exportExcel(this.grd_excel, "exportExl");
	this.grd_excel.set_enableevent(true);
}

/*
 * 함수명  	: div_WORK_btn_excelUpload_onclick
 * 설명 		: 엑셀 업로드
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.Excel_up_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var arg = {
		targetDs : this.ds_excel,
		headSet : false,
		callBack : function () {	
		
		this.ds_excel.getColID
			if(this.ds_excel.rowcount > 3){
				this.ds_cntrwkCmpr.set_enableevent(false);
				for(j=0; j< this.ds_excel.rowcount; j++) {	//값 세팅 위한 반복문
					for(i=0; i<this.ds_cntrwk.rowcount+2; i++){		//공사 컬럼 셋팅을 위한 반복문
						var colNm = this.ds_cntrwk.getColumn(i-2, "COL_NM")
						var eVal  = this.ds_excel.getColumn(j, "Column"+i);
						var ixNm = this.ds_excel.getColumn(j, "Column0");
						var findRow = this.ds_cntrwkCmpr.findRow("IX_NM", ixNm);
						
						if(colNm){
							this.ds_cntrwkCmpr.setColumn(findRow, colNm, eVal);
						}
					}
				}
				this.ds_cntrwkCmpr.set_enableevent(true);
			}
		}
	};
	this.gfn_importExcel(arg);
}

/*
 * 함수명  	: btn_excelDn_onclick
 * 설명 		: 그리드 엑셀 다운로드(업로드양식)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_excelDn_onclick();
 */
this.btn_excelDn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	for(i = 0; i < this.ds_cntrwk.getRowCount(); i++) {
		this.grd_EXCEL_SAMPLE.setCellProperty("Head", i + 2, "text", this.ds_cntrwk.getColumn(i, "ETC"));
		this.grd_EXCEL_SAMPLE.setCellProperty("Head", i + 2, "tooltiptext", this.ds_cntrwk.getColumn(i, "CODE_NM"));
	}
	this.gfn_exportExcel(this.grd_EXCEL_SAMPLE, "EXCEL_UPLOAD_SAMPLE");
}
