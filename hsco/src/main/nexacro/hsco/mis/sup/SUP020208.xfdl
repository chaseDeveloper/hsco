/*
 * 함수명  	: btn_Del_Row_onclick
 * 설명 		: 검토부서 행삭제 버튼 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Del_Row_onclick();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//2016.08.10
	var SANCTN_STS = this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "SANCTN_STS");
	switch (SANCTN_STS) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");					
		return;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return;
		
		default:
	}
	
	if (this.gfn_message("confirm.삭제여부")) {
		var nRow = this.ds_propseExmntDept.rowposition;
		this.ds_propseExmntDept.deleteRow(nRow);
	}		
}

