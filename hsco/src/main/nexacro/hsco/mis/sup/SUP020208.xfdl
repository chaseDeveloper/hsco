	}
	
    if(sPopupId == "sup020208_p01")
    {		
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(!nRowCnt){
			
		} else {			
			if(this.ds_propseJdgmnMfcmm.rowcount > 1) {
				var ds_propseJdgmnMfcmmCopy = new Dataset();
