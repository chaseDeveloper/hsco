/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{

	switch (strSvcId) {
		case "select":			// 제안목록 조회
			var sController   	= "hsco/mis/sup/SUP020208/selectPropseMastrList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_propseMastr=output1";
			break;
		case "selectSub":		// 제안자, 검토부서, 심사위원 조회
			var sController   	= "hsco/mis/sup/SUP020208/selectPropseSubList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_propsePropsr=output1 ds_propseExmntDept=output2 ds_propseJdgmnMfcmm=output3";
			break;			
		case "save":    		// 검토부서, 심사위원 저장
			var sController   	= "hsco/mis/sup/SUP020208/savePropseExmntDept.do";
			var sInDatasets   	= "input1=ds_propseExmntDept:U input2=ds_propseJdgmnMfcmm:U";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":  // 첨부파일 조회
			var sController   	= "hsco/mis/sup/SUP020208/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_files=output1";
			break;
		case "approve":			// 전자결재
			var sController   	= "hsco/mis/sup/SUP020208/approve.do";	
			var sInDatasets   	= "input1=ds_approve";
			var sOutDatasets  	= "ds_approve=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "select":		// 제안자료 조회
				this.ds_propseExmntDept.applyChange();
				this.ds_propseJdgmnMfcmm.applyChange();
				
				if(this.saveRow > -1){
					this.ds_propseMastr.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "selectSub":	// 제안자, 검토부서 조회
				this.ds_propseExmntDept.applyChange();
				this.ds_propseJdgmnMfcmm.applyChange();
				break;	
			case "save":		// 검토부서 저장
			    this.gfn_message("success.처리.성공");
			    this.fn_search_sub();
				break;
			case "selectFileList":      // 첨부파일 조회
				this.ds_files.filter("USE_AT == '1'");
				break;
			case "approve":				// 전자결재					
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
				break;
		}
	}
}

