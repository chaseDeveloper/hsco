/*
 * 함수명  	: btn_fileUpload_onclick
 * 설명 		: '첨부파일 다운로드' 버튼 클릭 후 이벤트 처리(첨부파일 Grid)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_fileUpload_onclick(obj,e);
 */
this.btn_fileUpload_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_files.rowposition < 0){
		return;
	}	
	
	for(var i = 0 ; i < this.ds_files.getRowCount() ; i++)
	{
		if(this.ds_files.getColumn( i , "CHK") == 1){
			var fileId = this.ds_files.getColumn(i, "FILE_ID");
			var fileName = this.ds_files.getColumn(i, "ORGINL_FILE_NM");
			var fileKey = this.ds_files.getColumn(i, "STRE_FILE_NM");
			var fileSize = this.ds_files.getColumn(i, "FILE_MG");
			var fileInfo = {"id": fileId, "name": fileName, "size": fileSize, "row": 0};
				
			var downloadUrl = this.fileConfig.host + this.fileConfig.downloadUrl;
			
			var encodeFileName = encodeURIComponent(fileName);
			
			this.extDown.download(downloadUrl +"?fileKey=" + fileKey + "&filename="+ encodeFileName, fileName);	
		}
	}
}


