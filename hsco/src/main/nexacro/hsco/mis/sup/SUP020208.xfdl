					for(r=0; r<ds_propseJdgmnMfcmmCopy.rowcount; r++) {
						var strEmpNo02 = ds_propseJdgmnMfcmmCopy.getColumn(r, "JUDGE_SN");
						if(strEmpNo01 == strEmpNo02) {
							r = ds_propseJdgmnMfcmmCopy.rowcount;
							strDplctYn = "Y"
						}
					}
					
					if(strDplctYn == "N") {
						var nRow = this.ds_propseJdgmnMfcmm.addRow();
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "PROPSE_NO",   this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "PROPSE_NO"));
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "JUDGE_SN",        dsRtn.getColumn(i,"JUDGE_SN"));
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "EMPNM",       dsRtn.getColumn(i,"EMPNM"));
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "DEPT_CODE",   dsRtn.getColumn(i,"DEPT_CODE"));
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "DEPT_NM",     dsRtn.getColumn(i,"DEPT_NM"));
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "OFCPS",       dsRtn.getColumn(i,"OFCPS"));
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "OFCPS_NM",    dsRtn.getColumn(i,"OFCPS_NM"));
						this.ds_propseJdgmnMfcmm.setColumn(nRow, "EXTRL_HNF",    dsRtn.getColumn(i,"EXTRL_HNF"));
					}
				}								
			} else {
				for(i=0; i<nRowCnt; i++){
					var nRow = this.ds_propseJdgmnMfcmm.addRow();
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "PROPSE_NO",   this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "PROPSE_NO"));
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "JUDGE_SN",        dsRtn.getColumn(i,"JUDGE_SN"));
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "EMPNM",       dsRtn.getColumn(i,"EMPNM"));
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "DEPT_CODE",   dsRtn.getColumn(i,"DEPT_CODE"));
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "DEPT_NM",     dsRtn.getColumn(i,"DEPT_NM"));
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "OFCPS",       dsRtn.getColumn(i,"OFCPS"));
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "OFCPS_NM",    dsRtn.getColumn(i,"OFCPS_NM"));
					this.ds_propseJdgmnMfcmm.setColumn(nRow, "EXTRL_HNF",    dsRtn.getColumn(i,"EXTRL_HNF"));
				}
			}
		}
	}
		
	if(sPopupId == "sup020200_p01") {
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			this.ds_cond.setColumn(0, "EMPNO",   dsRtn.getColumn(0, "EMPNO"));
			this.ds_cond.setColumn(0, "EMPNM",   dsRtn.getColumn(0, "EMPNM"));
		}
	}
} 

