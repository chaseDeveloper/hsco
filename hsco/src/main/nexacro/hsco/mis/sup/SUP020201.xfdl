 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(월간 행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	// 변경여부 체크
	if(Ex.util.isUpdated(this.ds_propseMastr) || Ex.util.isUpdated(this.ds_propsePropsr) || Ex.util.isUpdated(this.ds_propseOprtnRslt)) {
		if (this.gfn_message("confirm.변경.취소.여부")) {
			
			this.ds_propseMastr.set_enableevent(false);
			this.ds_propseMastr.reset();
			this.ds_propseMastr.applyChange();
			this.ds_propseMastr.set_rowposition(this.ds_propseMastr.rowcount -1);
			this.ds_propseMastr.set_enableevent(true);
			
		} else {
			return false;
			
		}
	}

	this.ds_propsePropsr.clearData();
	this.ds_propseOprtnRslt.clearData();
	
	// 제안 마스터 목록 추가
	var nRow = this.ds_propseMastr.addRow();
	this.ds_propseMastr.setColumn(nRow, "PROPSE_YEAR", this.ds_cond.getColumn(this.ds_cond.rowposition, "PROPSE_YEAR"));
	this.ds_propseMastr.setColumn(nRow, "PROPSE_ODR_SE", this.ds_cond.getColumn(this.ds_cond.rowposition, "PROPSE_ODR_SE"));
	this.ds_propseMastr.setColumn(nRow, "PROPSE_MTH", "01");
	
	if(this.tab_propseMastr.tabindex == 2){
		this.ds_propseMastr.setColumn(nRow, "OPRTN_DEPT", this.gfn_getDeptId());
		this.ds_propseMastr.setColumn(nRow, "OPRTN_DEPT_NM", this.gfn_getDeptName());
	}
}

