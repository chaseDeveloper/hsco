 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_propseMastr_canrowposchange();
 */
this.ds_propseMastr_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var rowType = this.ds_propseMastr.getRowType(e.oldrow);
	
	if(rowType != Dataset.ROWTYPE_NORMAL && rowType != Dataset.ROWTYPE_EMPTY){
		if(Ex.util.isUpdated(this.ds_propsePropsr)|| Ex.util.isUpdated(this.ds_propseOprtnRslt)) {
			
			if ( this.gfn_message("confirm.변경.취소.여부") ) {
				
				this.ds_propseMastr.set_enableevent(false);
				this.ds_propseMastr.reset();
				this.ds_propseMastr.applyChange();
				this.ds_propseMastr.set_enableevent(true);
				
				return true;
			} else {
				return false;
			}
		}
	}
}

/*
 * 함수명  	: ds_propseMastr_onrowposchanged
 * 설명 		: 제안목록 row 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_propseMastr_onrowposchanged();
 */
this.ds_propseMastr_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var nRow = this.ds_propseMastr.rowposition;
	
	if(this.ds_propseMastr.getRowType(nRow) == Dataset.ROWTYPE_INSERT) {
		if(this.tab_propseMastr.tabindex == 0){
			this.ds_propseMastr.setColumn(nRow, "PROPSE_SE", "01");
			this.ds_propseMastr.setColumn(nRow, "PROPSE_DE", this.dateUtils.today());
			this.ds_propseMastr.setColumn(nRow, "CNFIRM_AT", "0");
			this.ds_propseMastr.setColumn(nRow, "REGISTER_ID", this.fn_getEmpno());
		}
		
		if(this.tab_propseMastr.tabindex == 1){
			this.ds_propseMastr.setColumn(nRow, "PROPSE_SE", "03");
			this.ds_propseMastr.setColumn(nRow, "PROPSE_DE", this.dateUtils.today());
			this.ds_propseMastr.setColumn(nRow, "CNFIRM_AT", "0");
			this.ds_propseMastr.setColumn(nRow, "REGISTER_ID", this.fn_getEmpno());
		}
		
		if(this.tab_propseMastr.tabindex == 2){
			this.ds_propseMastr.setColumn(nRow, "PROPSE_SE", "02");
			this.ds_propseMastr.setColumn(nRow, "PROPSE_DE", this.dateUtils.today());
			this.ds_propseMastr.setColumn(nRow, "CNFIRM_AT", "0");
			this.ds_propseMastr.setColumn(nRow, "REGISTER_ID", this.fn_getEmpno());
		}
	}
	
	if(this.tab_propseMastr.tabindex == 0){
		this.tab_propseMastr.tpg1.grd_cmmnAtchmnfl.setCellProperty("Head", 0, "text", "0");
	}
	
	if(this.tab_propseMastr.tabindex == 1){
		this.tab_propseMastr.tpg2.grd_cmmnAtchmnfl.setCellProperty("Head", 0, "text", "0");
	}
	
	if(this.tab_propseMastr.tabindex == 2){
		this.tab_propseMastr.tpg3.grd_cmmnAtchmnfl.setCellProperty("Head", 0, "text", "0");
	}
	
	this.fn_search_sub();
}

