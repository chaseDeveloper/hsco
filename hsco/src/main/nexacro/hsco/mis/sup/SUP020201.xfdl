    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : SUP020201.xfdl
 * 화면명   : 제안등록
 * 화면설명 : 제안등록을 관리한다.
 * 작성일   : 2015.10.22
 * 작성자   : 양성모
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.saveRow = -1;
this.checkDs = [this.ds_propseMastr, this.ds_propsePropsr, this.ds_propseOprtnRslt];
this.File_UpDownManager = null;

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SUP020201_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.SUP020201_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// 초기 검색조건
	var se;
	if(this.dateUtils.today().substr(4,2) > 6){
		se = "2";
	} else {
		se = "1";
	}
	
	this.ds_cond.setColumn(0, "PROPSE_ODR_SE", se);
	this.ds_cond.setColumn(0, "PROPSE_YEAR", this.dateUtils.today().substr(0,4));
	this.ds_cond.setColumn(0, "EMPNO", this.fn_getEmpno());
	this.ds_cond.setColumn(0, "EMPNM", this.fn_getEmpnm());
	
	// 데이터셋 동적 바인딩(부서팝업, 사원팝업)
	this.tab_propseMastr.tpg3.tab_oprtn.tpg1.div_dept.fn_setBind("ds_propseMastr", "OPRTN_DEPT", "OPRTN_DEPT_NM");
	
	// 공통코드
	var comboParams = [
		["ds_sup004", "SUP004", "Y", "D"]	// 제안구분
	   ,["ds_sup005", "SUP005", "Y", "D"]	// 제안종류
	   ,["ds_sup006", "SUP006", "Y", "D"]	// 제안방법
	];
	var callback = function (svcId, errorCode, errorMsg) {
		
		this.ds_cond.setColumn(0, "PROPSE_SE", "01");
		
		// 조회 이벤트 / 함수 호출
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);	
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "SUP");
	this.File_UpDownManager.init(fileConfig);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(제안목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(orderId)
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	var strPropseSe = this.ds_cond.getColumn(0, "PROPSE_SE");
	
	if(strPropseSe == "01"){
		this.tab_propseMastr.set_enableevent(false);
		this.tab_propseMastr.set_tabindex(0);
		this.tab_propseMastr.set_enableevent(true);
		
		this.sta_mainTitle.set_text("업무제안 목록");
	}
	
	if(strPropseSe == "02"){
		this.tab_propseMastr.set_enableevent(false);
		this.tab_propseMastr.set_tabindex(2);
		this.tab_propseMastr.set_enableevent(true);
		
		this.sta_mainTitle.set_text("실시제안 목록");
	}
	
	if(strPropseSe == "03"){
		this.tab_propseMastr.set_enableevent(false);
		this.tab_propseMastr.set_tabindex(1);
		this.tab_propseMastr.set_enableevent(true);
		
		this.sta_mainTitle.set_text("아이디어제안 목록");
	}
	
	//this.ds_propseMastr.clearData();
	this.ds_propsePropsr.clearData();
	this.ds_propseOprtnRslt.clearData();
	this.ds_fileList.clearData();
	this.fn_transaction("select");
}

/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(제안자목록, 첨부파일목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function()
{
	this.ds_cond01.clearData();
	this.ds_cond02.clearData();
	this.ds_cond01.addRow();
	this.ds_cond02.addRow();

	this.ds_cond01.setColumn(0, "PROPSE_NO", this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "PROPSE_NO"));
	this.ds_cond02.setColumn(0, "PROPSE_NO", this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "PROPSE_NO"));
	this.ds_cond02.setColumn(0, "FILE_SN", this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition,   "FILE_SN"));

	var propseSe = this.ds_cond.getColumn(0, "PROPSE_SE");
	
	if(propseSe == "02"){	// 실시제안일 경우
		this.fn_transaction("selectSub01");
	} else {
		this.fn_transaction("selectSub");
	}
	
	
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	if(Ex.util.isUpdated(this.ds_propseMastr) || Ex.util.isUpdated(this.ds_propsePropsr) || Ex.util.isUpdated(this.ds_propseOprtnRslt)) {
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_propseMastr, this.ds_validation)) {
			if(this.tab_propseMastr.tabindex == 0){
				this.ds_cond.setColumn(0, "PROPSE_SE", "01");
			}

			if(this.tab_propseMastr.tabindex == 1){
				this.ds_cond.setColumn(0, "PROPSE_SE", "03");
			}

			if(this.tab_propseMastr.tabindex == 2){
				this.ds_cond.setColumn(0, "PROPSE_SE", "02");
			}			
			return;
		} else if(this.ds_propseMastr.rowposition > -1 && this.ds_propsePropsr.rowcount < 1){
			this.gfn_message("comm.입력.요청.를", "제안자");
			return;
		} else if (!this.gfn_checkValidation_NoUpdChk(this.ds_propsePropsr, this.ds_validation)) {
			return;
		} else {
			if(this.tab_propseMastr.tabindex == 0){
				this.ds_cond.setColumn(0, "PROPSE_SE", "01");
			}

			if(this.tab_propseMastr.tabindex == 1){
				this.ds_cond.setColumn(0, "PROPSE_SE", "03");
			}

			if(this.tab_propseMastr.tabindex == 2){
				this.ds_cond.setColumn(0, "PROPSE_SE", "02");
			}
			
			this.saveRow = this.ds_propseMastr.rowposition;
			this.fn_transaction("save");
		}
	} else {	
	    this.gfn_message("comm.데이터.변경.없음");	
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{

	switch (strSvcId) {
		case "select":			// 제안목록 조회
			var sController   	= "hsco/mis/sup/SUP020201/selectPropseMastrList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_propseMastr=output1";
			break;
		case "selectSub":		// 제안자 조회
			var sController   	= "hsco/mis/sup/SUP020201/selectPropsePropsrList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_propsePropsr=output1";
			break;
		case "selectSub01":		// 제안자, 제안실시성과 조회
			var sController   	= "hsco/mis/sup/SUP020201/selectPropseOprtnList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_propsePropsr=output1 ds_propseOprtnRslt=output2";
			break;
		case "save":    		// 제안 저장
			var sController   	= "hsco/mis/sup/SUP020201/savePropseMastr.do";
			var sInDatasets   	= "input1=ds_propseMastr:U input2=ds_propsePropsr:U input3=ds_propseOprtnRslt:U";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/sup/SUP020201/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_fileList=output1";
			break;			
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "select":		// 제안자료 조회
				this.ds_propseMastr.applyChange();
				
				if(this.saveRow > -1){
					this.ds_propseMastr.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "selectSub":	// 제안자 조회
				this.ds_propsePropsr.applyChange();
				
				if(this.ds_propseMastr.getRowType(this.ds_propseMastr.rowposition) == 2){
					this.ds_propsePropsr.clearData();
					this.ds_propsePropsr.addRow();
					this.ds_propsePropsr.setColumn(0, "EMPNO", this.fn_getEmpno());
					this.ds_propsePropsr.setColumn(0, "EMPNM", this.fn_getEmpnm());
					this.ds_propsePropsr.setColumn(0, "MAIN_PROPSR_AT", '1');
				}
				
				this.fn_ds_propseMastr_rowChange_event();
				
				this.fn_transaction("selectFileList");
				break;
			case "selectSub01":	// 제안자, 제안 실시성과 조회
				this.ds_propsePropsr.applyChange();
				this.ds_propseOprtnRslt.applyChange();
				
				if(this.ds_propseMastr.getRowType(this.ds_propseMastr.rowposition) == 2){
					this.ds_propsePropsr.clearData();
					this.ds_propsePropsr.addRow();
					this.ds_propsePropsr.setColumn(0, "EMPNO", this.fn_getEmpno());
					this.ds_propsePropsr.setColumn(0, "EMPNM", this.fn_getEmpnm());
					this.ds_propsePropsr.setColumn(0, "MAIN_PROPSR_AT", '1');
				}
				
				this.fn_ds_propseMastr_rowChange_event();
				
				this.fn_transaction("selectFileList");
				break;	
			case "save":		// 제안자료 저장
			    this.gfn_message("success.처리.성공");
			    this.fn_search();
				break;
			case "selectFileList":      // 첨부파일 조회
				this.ds_fileList.filter("USE_AT == '1'");
				break;	
		}
	}
}

