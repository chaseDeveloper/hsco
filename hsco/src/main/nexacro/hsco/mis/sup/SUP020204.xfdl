    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : SUP020205.xfdl
 * 화면명   : 심사결과등록
 * 화면설명 : 제안 심사결과를 관리한다.
 * 작성일   : 2015.10.28
 * 작성자   : 양성모
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.strPropseSe = "01";
this.tabpageIndex = 0;
this.title;
this.code;
this.File_UpDownManager = null;
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SUP020204_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.SUP020204_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{		
	// 초기 검색조건
	var se;
	if(this.dateUtils.today().substr(4,2) > 6){
		se = "2";
	} else {
		se = "1";
	}
	
	this.ds_cond.setColumn(0, "PROPSE_ODR_SE", se);
	this.ds_cond.setColumn(0, "PROPSE_YEAR", this.dateUtils.today().substr(0,4));
	this.ds_cond.setColumn(0, "DEPT_CODE", "000000");
	this.ds_cond.setColumn(0, "DEPT_NM", "화성도시공사");
	
	// 데이터셋 동적 바인딩(부서팝업)
	this.div_search.div_dept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	// 공통코드
	var comboParams = [
		["ds_sup004", "SUP004", "Y", "D"]	// 제안구분
	   ,["ds_sup005", "SUP005", "Y", "D"]	// 제안종류
	   ,["ds_sup006", "SUP006", "Y", "D"]	// 제안방법
	];
	var callback = function (svcId, errorCode, errorMsg) {
		
		this.ds_cond.setColumn(0, "PROPSE_SE", "01");
		
		// 조회 이벤트 / 함수 호출
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);	
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
		//responseGrid : this.div_multiFile.grd_fileList,
		//responseArea : this.div_multiFile.sta_helpMessage
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "SUP");
	this.File_UpDownManager.init(fileConfig);
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(제안목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(orderId)
{
	this.strPropseSe = this.ds_cond.getColumn(0, "PROPSE_SE");
	
	if(this.strPropseSe == "01"){
		this.tab_propseMastr.set_enableevent(false);
		this.tab_propseMastr.set_tabindex(0);
		this.tab_propseMastr.set_enableevent(true);
		
		this.sta_mainTitle.set_text("업무제안 목록");
	}
	
	if(this.strPropseSe == "02"){
		this.tab_propseMastr.set_enableevent(false);
		this.tab_propseMastr.set_tabindex(2);
		this.tab_propseMastr.set_enableevent(true);
		
		this.sta_mainTitle.set_text("실시제안 목록");
	}
	
	if(this.strPropseSe == "03"){
		this.tab_propseMastr.set_enableevent(false);
		this.tab_propseMastr.set_tabindex(1);
		this.tab_propseMastr.set_enableevent(true);
		
		this.sta_mainTitle.set_text("아이디어제안 목록");
	}
	
	this.ds_propseMastr.clearData();
	this.ds_propsePropsr.clearData();
	this.ds_propseExmntDept.clearData();
	this.ds_PropseDeptExmntOpinio.clearData();
	this.ds_propseOprtnRslt.clearData();
	this.ds_fileList.clearData();

	// 제안목록 head 초기화
    this.grd_propseMastr.setCellProperty("Head", 0, "text", "0");	
    
    // 보고서 출력 제목 설정
	var nRow = this.ds_sup004.findRow("CODE",this.ds_cond.getColumn(this.ds_cond.rowposition, "PROPSE_SE"));
	this.code = this.ds_sup004.getColumn(nRow, "CODE");
	this.title = this.ds_sup004.getColumn(nRow, "CODE_NM");
	
	this.fn_transaction("select");
}

/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(제안자목록, 검토부서목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function()
{	
	this.ds_cond01.clearData();
	this.ds_cond04.clearData();
	this.ds_cond01.addRow();
	this.ds_cond04.addRow();
	this.ds_PropseDeptExmntOpinio.clearData();

	this.ds_cond01.setColumn(0, "PROPSE_NO", this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "PROPSE_NO"));
	this.ds_cond01.setColumn(0, "DEPT_CODE", this.ds_cond.getColumn(this.ds_cond.rowposition, "DEPT_CODE"));
	this.ds_cond04.setColumn(0, "PROPSE_NO", this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "PROPSE_NO"));
	this.ds_cond04.setColumn(0, "FILE_SN", this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition,   "FILE_SN"));

	var propseSe = this.ds_cond.getColumn(0, "PROPSE_SE");
	
	if(propseSe == "02"){	// 실시제안일 경우
		this.fn_transaction("selectSub01");
	} else {
		this.fn_transaction("selectSub");
	}
}

