/*
 * 함수명  	: fn_print
 * 설명 		: 제안심사자료 출력
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
 */
this.fn_print1 = function()
{
	var path;
	var saveFileNm;
	
	if(this.code == "01"){
		path = "/mis/sup/SUP020204_R1.jrf";
		saveFileNm = "업무제안_심사자료";
	} else if(this.code == "03") {
		path = "/mis/sup/SUP020204_R2.jrf";
		saveFileNm = "아이디어제안_심사자료";
	} else if(this.code == "02") {
		path = "/mis/sup/SUP020204_R3.jrf";
		saveFileNm = "실시제안_심사자료";
	}
	
	var vOpts = new this.cf_Opts();
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setReportPath(path);
	vOpts.setSaveFilename(saveFileNm);
	vOpts.setLocalDs(["ds_propseMastr_R",           this.ds_propseMastr_R]);
	vOpts.setLocalDs(["ds_PropseDeptExmntOpinio_R", this.ds_PropseDeptExmntOpinio_R]);
	if(this.code == "02") {
		vOpts.setLocalDs(["ds_propseOprtnRslt_R", this.ds_propseOprtnRslt_R]);
	}
	vOpts.setArgs("TITLE",this.title);
	
	var parm = {
		arg_0 : this,
		opts : vOpts
	};
	this.gfn_popup("Popup_Report", 884, 735, "제안심사자료", parm, "common::frmReportPopup.xfdl", "report_callback");
}

