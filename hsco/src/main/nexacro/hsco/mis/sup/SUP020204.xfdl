* Tab 영역 
***********************************************************************/
/*
 * 함수명  	: tab_propseMastr_onchanged
 * 설명 		: 탭 페이지 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_propseMastr_onchanged();
 */
this.tab_propseMastr_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{ 	
	if(this.tab_propseMastr.tabindex == 3){
		this.tabpageIndex = e.preindex;
		
		this.tab_propseMastr.tpg4.set_url("mis_sup::SUP020205_T4_1.xfdl");
	} else {
		if(this.tab_propseMastr.tabindex == 0){
			this.ds_cond.setColumn(0, "PROPSE_SE", "01");
			this.strPropseSe = "01";
			this.sta_mainTitle.set_text("업무제안 목록");
			
		} else if(this.tab_propseMastr.tabindex == 1){
			this.ds_cond.setColumn(0, "PROPSE_SE", "03");
			this.strPropseSe = "03";
			this.sta_mainTitle.set_text("아이디어제안 목록");
			
		} else if(this.tab_propseMastr.tabindex == 2){
			this.ds_cond.setColumn(0, "PROPSE_SE", "02");
			this.strPropseSe = "02";
			this.sta_mainTitle.set_text("실시제안 목록");
		} 
		
		if((e.preindex != 3) || this.tabpageIndex != e.postindex) {
			this.ds_propseMastr.clearData();
			this.ds_propsePropsr.clearData();
			this.ds_PropseDeptExmntOpinio.clearData();
			
			var nRow = this.ds_sup004.findRow("CODE",this.ds_cond.getColumn(0, "PROPSE_SE"));
			this.code = this.ds_sup004.getColumn(nRow, "CODE");
			this.title = this.ds_sup004.getColumn(nRow, "CODE_NM");
			
			this.fn_transaction("select");
		}
	}
}

