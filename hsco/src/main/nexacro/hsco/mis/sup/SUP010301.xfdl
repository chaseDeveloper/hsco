 * return Type 	: None
 * 사용법		: n/a
 */
this.fn_cancel = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.div_Dept.set_visible(false);
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_mngmtEvlImprvmTask.reset();
		this.ds_mngmtEvlImprvmTask.applyChange();
