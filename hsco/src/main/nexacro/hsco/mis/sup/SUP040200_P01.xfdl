		{
		//	alert("정상적으로 조회되었습니다");	
		}	
	}
}


/***********************************************************************************
* 설명	: 헤드클릭 정렬
***********************************************************************************/
this.div_work_grd_bodMaster_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	LGV_COL_SORT = e.col;
    this._gridSort(obj, e);
}


/***********************************************************************************
* 설명	: 닫기
***********************************************************************************/
this.btn_Close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}]]></Script>
