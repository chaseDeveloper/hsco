/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{

	switch (strSvcId) {
		case "selectJudge":			// 심사위원 정보 조회
			var sController   	= "hsco/mis/sup/SUP020205/selectPropseMastrList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_propseMastr=output1 ds_propseIemStdr=output2";
			break;
		case "select":				// 제안목록, 채점표기준 조회
			var sController   	= "hsco/mis/sup/SUP020205/selectPropseMastrList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_propseMastr=output1 ds_propseIemStdr=output2";
			break;
		case "selectSub":			// 제안자 목록, 검토부서,  심사결과 조회
			var sController   	= "hsco/mis/sup/SUP020205/selectPropseSubList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_propsePropsr=output1 ds_propseExmntDept=output2 ds_propseJdgmnResult=output3";
			break;
		case "selectSub01":			// 제안자, 검토부서, 심사결과, 제안실시성과 조회
			var sController   	= "hsco/mis/sup/SUP020205/selectPropseOprtnList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_propsePropsr=output1 ds_propseExmntDept=output2 ds_propseJdgmnResult=output3 ds_propseOprtnRslt=output4";
			break;
		case "selectExmntOpinio":	// 검토의견 조회
			var sController   	= "hsco/mis/sup/SUP020205/selectExmntOpinio.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_PropseDeptExmntOpinio=output1";
			break;									
		case "save":    			// 심사결과 저장
			var sController   	= "hsco/mis/sup/SUP020205/savePropseJdgmnResult.do";
			var sInDatasets   	= "input1=ds_propseJdgmnResult:U";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/sup/SUP020205/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_fileList=output1";
			break;	
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		// 실패 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		switch (strSvcId) {	
			case "select":		// 제안자료 및 검토의견 조회
				if(this.saveRow > -1){
					this.ds_propseMastr.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "selectSub":	// 제안자, 심사결과, 검토부서 조회
				this.ds_propseJdgmnResult.applyChange();
				
				// 신규버튼 권할 설정(자신 외 수정 불가)
				var loginEmpno = this.fn_getEmpno();	// 현재 로그인된 사원번호(외부인의 경우 사용자ID) 정보
				var judgeEmpno = this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "EMPNO");	// 검색조건 심사위원의 사원번호(외부인의 경우 사용자ID)
				
				if(loginEmpno == judgeEmpno || this.fn_getMisAuth (this, "SUP2_USER")){
					if(this.ds_propseJdgmnResult.rowcount > 0){
						this.tab_propseMastr.tpg5.btn_commit.set_enable(true);
						if(this.ds_propseJdgmnResult.getColumn(0, "DCSN_AT") == "0"){
							this.tab_propseMastr.tpg5.btn_commit.set_text("확 정");
						}else {
							this.tab_propseMastr.tpg5.btn_commit.set_text("확정취소");
						}
					} else {
						this.tab_propseMastr.tpg5.btn_commit.set_text("확 정");
						this.tab_propseMastr.tpg5.btn_commit.set_enable(false);
					}
				}
				this.fn_transaction("selectFileList");
				break;
			case "selectSub01":	// 제안자, 심사결과, 검토부서, 제안실시성과 조회
				this.ds_propseJdgmnResult.applyChange();
				
				// 신규버튼 권할 설정(자신 외 수정 불가)
				var loginEmpno = this.fn_getEmpno();	// 현재 로그인된 사원번호(외부인의 경우 사용자ID) 정보
				var judgeEmpno = this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "EMPNO");	// 검색조건 심사위원의 사원번호(외부인의 경우 사용자ID)
				
				if(loginEmpno == judgeEmpno || this.fn_getMisAuth (this, "SUP2_USER")){
					if(this.ds_propseJdgmnResult.rowcount > 0){
						this.tab_propseMastr.tpg5.btn_commit.set_enable(true);
						if(this.ds_propseJdgmnResult.getColumn(0, "DCSN_AT") == "0"){
							this.tab_propseMastr.tpg5.btn_commit.set_text("확 정");
						}else {
							this.tab_propseMastr.tpg5.btn_commit.set_text("확정취소");
						}
					} else {
						this.tab_propseMastr.tpg5.btn_commit.set_text("확 정");
						this.tab_propseMastr.tpg5.btn_commit.set_enable(false);
					}	
				}
				this.fn_transaction("selectFileList");
				break;
			case "save":		// 검토의견 저장
			    this.gfn_message("success.처리.성공");
			    this.fn_search();
				break;
			case "selectFileList":      // 첨부파일 조회
				this.ds_fileList.filter("USE_AT == '1'");
				break;	
		}
	}
}

