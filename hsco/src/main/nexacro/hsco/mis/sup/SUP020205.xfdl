 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_propseMastr_tpg5_btn_insert_onclick();
 */
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 신규버튼 권할 설정(자신 외 수정 불가)
 	var loginEmpno = this.fn_getEmpno();														// 현재 로그인된 사원번호(외부인의 경우 사용자ID) 정보
 	var judgeEmpno = this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "EMPNO");	// 검색조건 심사위원의 사원번호(외부인의 경우 사용자ID)
	
	if(this.ds_propseMastr.rowposition < 0){
		this.gfn_message("comm.항목.선택.요청","제안");
	} 
	else {
		if(loginEmpno == judgeEmpno || this.fn_getMisAuth (this, "SUP2_USER")){
			
			if(this.ds_propseJdgmnResult.rowcount > 0){
				
				this.gfn_message("info.진행가능", "평가결과를 삭제");
				return;
			}
			
			//this.tab_propseMastr.tpg5.btn_insert.set_enable(false);
			this.tab_propseMastr.tpg5.btn_commit.set_enable(true);
			//this.tab_propseMastr.tpg5.btn_delete.set_enable(true);
			
			if(this.ds_propseIemStdr.rowcount == 0){
				
				this.gfn_message("fail.정보검색.실패", "등록된 평가 기준 정보를");
			}
			
			for(var i=0; i<this.ds_propseIemStdr.rowcount; i++){
				
				var strPropseSe       = this.ds_propseIemStdr.getColumn(i, "PROPSE_SE");
				var strEvlIemCode     = this.ds_propseIemStdr.getColumn(i, "EVL_IEM_CODE");
				var strEvlIemNm       = this.ds_propseIemStdr.getColumn(i, "EVL_IEM_NM");
				var strPropseEvlScore = this.ds_propseIemStdr.getColumn(i, "PROPSE_EVL_SCORE");
				var stdrYear		  = this.ds_propseIemStdr.getColumn(i, "STDR_YEAR");
				var odrSe			  = this.ds_propseIemStdr.getColumn(i, "ODR_SE");
				
				var nRow = this.ds_propseJdgmnResult.addRow();
				
				this.ds_propseJdgmnResult.setColumn(nRow, "DCSN_AT",          "0");
				this.ds_propseJdgmnResult.setColumn(nRow, "SN",               this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "SN"));
				this.ds_propseJdgmnResult.setColumn(nRow, "PROPSE_NO",        this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "PROPSE_NO"));
				this.ds_propseJdgmnResult.setColumn(nRow, "PROPSE_SE",        strPropseSe);        // 제안구분
				this.ds_propseJdgmnResult.setColumn(nRow, "EVL_IEM_CODE",     strEvlIemCode);      // 평가항목코드
				this.ds_propseJdgmnResult.setColumn(nRow, "EVL_IEM_NM",       strEvlIemNm);        // 평가항목명
				this.ds_propseJdgmnResult.setColumn(nRow, "PROPSE_EVL_SCORE", strPropseEvlScore);  // 제안평가점수(기준)
				this.ds_propseJdgmnResult.setColumn(nRow, "STDR_YEAR", 		  stdrYear);  		   // 평가 기준년도
				this.ds_propseJdgmnResult.setColumn(nRow, "ODR_SE", 		  odrSe);  			   // 평가 차수
			}
		}
		else {
			this.gfn_message("fail.수정.불가", "현재 사용자와 심사위원 정보가\n동일하지 않아");
		}
	}
}

/*
 * 함수명  	: tab_propseMastr_tpg5_btn_delete_onclick
 * 설명 		: 심사결과 삭제 버튼 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_propseMastr_tpg5_btn_delete_onclick();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 신규버튼 권할 설정(자신 외 수정 불가)
 	var loginEmpno = this.fn_getEmpno();	// 현재 로그인된 사원번호(외부인의 경우 사용자ID) 정보
 	var judgeEmpno = this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "EMPNO");	// 검색조건 심사위원의 사원번호(외부인의 경우 사용자ID)
 	
 	if(loginEmpno == judgeEmpno || this.fn_getMisAuth (this, "SUP2_USER")){
		if(this.ds_propseJdgmnResult.getColumn(0, "DCSN_AT") == "1"){
			this.gfn_message("info.진행가능", "확정여부 취소");
			return;
		}
		
		if ( this.gfn_message("confirm.삭제여부") ) {
			this.ds_propseJdgmnResult.deleteAll();
			
			//this.tab_propseMastr.tpg5.btn_insert.set_enable(true);
			this.tab_propseMastr.tpg5.btn_commit.set_enable(false);
			//this.tab_propseMastr.tpg5.btn_delete.set_enable(false);
		}
	}else {
		this.gfn_message("fail.수정.불가", "현재 사용자와 심사위원 정보가\n동일하지 않아");
	}
}

/*
 * 함수명  	: tab_propseMastr_tpg5_btn_commit_onclick
 * 설명 		: 심사결과 확정 버튼 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.tab_propseMastr_tpg5_btn_commit_onclick();
 */
this.tab_propseMastr_tpg5_btn_commit_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_propseJdgmnResult.getColumn(0, "DCSN_AT") == "0") {
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_propseJdgmnResult, this.ds_validation)) {
			return;
		}
		
		if (this.gfn_message("comm.행위여부", "확정")) {			
			for(var i=0; i<this.ds_propseJdgmnResult.rowcount; i++){
				this.ds_propseJdgmnResult.setColumn(i, "DCSN_AT", "1");
			}
			
			this.fn_save();
			//this.tab_propseMastr.tpg5.btn_delete.set_enable(false);
		}	
	} else {
		var sanctnKnd = this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "SANCTN_KND");
		
		if(sanctnKnd == '1' || sanctnKnd == '4' || sanctnKnd == '5'){
			this.gfn_message("errors.없음.요청", "확정 취소를 할 수|결재상태를");
		} else {
			if (this.gfn_message("comm.행위여부", "확정 취소")) {
				for(var i=0; i<this.ds_propseJdgmnResult.rowcount; i++){
					this.ds_propseJdgmnResult.setColumn(i, "DCSN_AT", "0");
				}
				
				this.fn_save();
				//this.tab_propseMastr.tpg5.btn_delete.set_enable(true);
			}		
		}
	}	
}

/*
 * 함수명  	: btn_downFiles_onclick
 * 설명 		: '선택다운로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_downFiles_onclick(obj,e);
 */
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}

