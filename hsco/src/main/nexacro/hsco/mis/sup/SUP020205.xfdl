this.tab_propseMastr_canchange = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	if(e.postindex == this.tabpageIndex || e.postindex == 3 || e.postindex == 4) {
		return true;
	} else {
		if(Ex.util.isUpdated(this.ds_propseJdgmnResult)) {		// 업데이트 여부 체크
			
			if (this.gfn_message("confirm.변경.취소.여부")) {
