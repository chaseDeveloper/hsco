/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save= function()
{
	// 신규버튼 권할 설정(자신 외 수정 불가)
 	var loginEmpno = this.fn_getEmpno();	// 현재 로그인된 사원번호(외부인의 경우 사용자ID) 정보
 	var judgeEmpno = this.ds_propseMastr.getColumn(this.ds_propseMastr.rowposition, "EMPNO");	// 검색조건 심사위원의 사원번호(외부인의 경우 사용자ID)
 	
 	if(loginEmpno == judgeEmpno || this.fn_getMisAuth (this, "SUP2_USER")){
		if(this.tab_propseMastr.tabindex == 0){
			this.ds_cond.setColumn(0, "PROPSE_SE", "01");
		}

		if(this.tab_propseMastr.tabindex == 1){
			this.ds_cond.setColumn(0, "PROPSE_SE", "03");
		}

		if(this.tab_propseMastr.tabindex == 2){
			this.ds_cond.setColumn(0, "PROPSE_SE", "02");
		}
		
		if(this.tab_propseMastr.tabindex == 3){
			this.ds_cond.setColumn(0, "PROPSE_SE", this.strPropseSe);
		}
		
		if(Ex.util.isUpdated(this.ds_propseJdgmnResult)) {
			// validation 체크
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_propseJdgmnResult, this.ds_validation)) {
				return;
			}
			this.saveRow = this.ds_propseMastr.rowposition;
			this.fn_transaction("save");
		} else {	
			this.gfn_message("comm.데이터.변경.없음");	
		}
	}else {
		this.gfn_message("fail.수정.불가", "현재 사용자와 심사위원 정보가\n동일하지 않아");
	}	
}

