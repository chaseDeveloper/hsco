				this.tab_propseMastr.tpg3.cmb_exclAt.set_readonly(false);
				this.tab_propseMastr.tpg3.edt_exclBasis.set_readonly(false);
				this.tab_propseMastr.tpg3.btn_propseMastr.set_visible(true);
				
