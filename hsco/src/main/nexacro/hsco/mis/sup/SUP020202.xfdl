				this.tab_propseMastr.tpg3.edt_exclBasis.set_cssclass("edt_WF_Readonly");
			}		
		}
		
	}
	
	if(e.columnid == "EXCL_AT"){
		if(obj.getColumn(obj.rowposition, "EXCL_AT") == "0"){
			this.ds_propseMastr.setColumn(this.ds_propseMastr.rowposition, "EXCL_BASIS", "");
		}		
	}
	
	if(e.columnid == "PROPSE_ODR_SE"){
		var oldValue = obj.getColumn(e.row, "OLD_PROPSE_ODR_SE");
		var newValue = e.newvalue;
		
		if(newValue == '2'){
			if(oldValue == '2'){	// 년도감소
				var year = obj.getColumn(e.row, "PROPSE_YEAR");
				var newYear = nexacro.toNumber(year) - 1;
				obj.setColumn(e.row, "PROPSE_YEAR", newYear);
			}
		} else {
			if(oldValue == '2'){	// 년도증가
				var year = obj.getColumn(e.row, "PROPSE_YEAR");
				var newYear = nexacro.toNumber(year) + 1;
				obj.setColumn(e.row, "PROPSE_YEAR", newYear);
			}
		}
	}
}

