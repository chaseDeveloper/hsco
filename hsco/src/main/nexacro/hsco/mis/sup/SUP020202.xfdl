				this.tab_propseMastr.tpg2.edt_exclBasis.set_readonly(false);
				this.tab_propseMastr.tpg2.btn_propseMastr.set_visible(true);
				
				this.tab_propseMastr.tpg2.cmb_exclAt.set_cssclass("cmb_WF_Essential");
				this.tab_propseMastr.tpg2.cmb_propseSe.set_cssclass("cmb_WF_Essential");
				this.tab_propseMastr.tpg2.edt_exclBasis.set_cssclass("");
