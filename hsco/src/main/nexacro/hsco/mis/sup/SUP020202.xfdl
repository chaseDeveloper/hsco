/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SUP020202_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.SUP020202_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{		
	// 초기 검색조건
	var se;
	if(this.dateUtils.today().substr(4,2) > 6){
		se = "2";
	} else {
		se = "1";
	}
	
	this.ds_cond.setColumn(0, "PROPSE_ODR_SE", se);
	this.ds_cond.setColumn(0, "PROPSE_YEAR", this.dateUtils.today().substr(0,4));
	
	// 공통코드
	var comboParams = [
		["ds_sup004", "SUP004", "Y", "D"]	// 검색 제안구분
	   ,["ds_sup005", "SUP005", "Y", "D"]	// 제안종류
	   ,["ds_sup006", "SUP006", "Y", "D"]	// 제안방법
	   ,["ds_sup004_1", "SUP004", "Y", "D"]	// 제안구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		
		this.ds_cond.setColumn(0, "PROPSE_SE", "01");
		this.ds_sup004_1.filter("CODE != '02'");
		
		// 조회 이벤트 / 함수 호출
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);	
}

