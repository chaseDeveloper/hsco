						var nRow = this.ds_mmList.addRow();
						
						this.ds_mmList.setColumn(nRow, "MM_CODE", "12");
						this.ds_mmList.setColumn(nRow, "MM_NM", "12월");
					}
				}
				
				if(evlCycle == '12'){
					if(odrSe == '6'){
						var nRow = this.ds_mmList.addRow();
						
						this.ds_mmList.setColumn(nRow, "MM_CODE", "6");
						this.ds_mmList.setColumn(nRow, "MM_NM", "6월");
					}
					
					if(odrSe == '12'){
						var nRow = this.ds_mmList.addRow();
						
						this.ds_mmList.setColumn(nRow, "MM_CODE", "12");
						this.ds_mmList.setColumn(nRow, "MM_NM", "12월");
					}
				}
				break;

			case "006": // 년
				if(evlCycle == '1'){
					var nRow = this.ds_mmList.addRow();
					this.ds_mmList.setColumn(nRow, "MM_CODE", "12");
					this.ds_mmList.setColumn(nRow, "MM_NM", "12월");
				}
				
				if(evlCycle == '2'){				
					if(odrSe == '2'){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", "12");
						this.ds_mmList.setColumn(nRow, "MM_NM", "12월");
					}
				}
				
				if(evlCycle == '4'){				
					if(odrSe == '4'){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", "12");
						this.ds_mmList.setColumn(nRow, "MM_NM", "12월");
					}
				}
				
				if(evlCycle == '12'){				
					if(odrSe == '12'){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", "12");
						this.ds_mmList.setColumn(nRow, "MM_NM", "12월");
					}
				}
				break;
		}	

		this.ds_mmList.set_rowposition(-1);
		this.ds_mmList.set_enableevent(true);
		
		if(this.ds_mmList.rowcount > 0){
			if(this.nRowPos2 > -1){
				this.ds_mmList.set_rowposition(this.nRowPos2);
				this.nRowPos2 = -1;
			} else {
				this.ds_mmList.set_rowposition(0);	  //temp
			}
		}
	}
}

//------------------------------------
// ListDtl Item 변경시
//------------------------------------
this.ds_evlListDtl_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 평가등급 Value 변경시
	if(e.columnid == "EVL_GRAD" || e.columnid == "EVL_GRAD2" || e.columnid == "EVL_GRAD3"){
		
		// 평가구분 설정
		var se = "";
		if(e.columnid == "EVL_GRAD"){
			se = "001";
		} else if(e.columnid == "EVL_GRAD2"){
			se = "002";
		} else if(e.columnid == "EVL_GRAD3"){
			se = "003";
		}
		var sModGrad = e.newvalue;
		var nRow = e.row;
		var nFindRow = this.ds_evlScoreList.findRowExpr("EVL_SE == '"+se+"' && EVL_GRAD == '"+sModGrad+"'");

		if (nFindRow == -1 ){
		    this.gfn_message("errors.없음.요청","해당 등급의 등록된 평가기준점수가|등급 설정 정보를");
		    obj.set_updatecontrol(false);
			obj.setColumn(e.row, e.columnid, e.oldvalue);
			obj.set_updatecontrol(true);
			return false;

		}else{
			var nEvlScore 	= 0;
			var nEvlWghval 	= 0;
			var nTempCalc 	= 0;
			var oldScore    = 0;
			var oldScore    = 0;
			var nFindOldRow = this.ds_evlScoreList.findRowExpr("EVL_SE == '"+se+"' && EVL_GRAD == '"+e.oldvalue+"'");
			
			if(nFindOldRow > -1){
				oldScore = this.ds_evlScoreList.getColumn(nFindOldRow,"EVL_SCORE");
			}
			newScore = this.ds_evlScoreList.getColumn(nFindRow,"EVL_SCORE");
			
			nEvlScore 	= this.ds_evlListDtl.getColumn(nRow,"GRAD_SCORE"); // 기존 평가점수 취득
			
			//alert(nEvlScore +" / "+ oldScore +" / "+ newScore)
			
			nEvlScore = nEvlScore - oldScore + newScore;
			
			this.ds_evlListDtl.setColumn(nRow, "GRAD_SCORE", nEvlScore);	// 변경된 등급점수 반영
			
			//alert(this.ds_ACH026.findRow("CODE", nEvlScore));
			var evlScore = this.ds_ACH026.getColumn(this.ds_ACH026.findRow("CODE", nEvlScore), "CODE_NM");	// 적용폄점 취득
			
			this.ds_evlListDtl.setColumn(nRow, "EVL_SCORE", evlScore);	// 변경된 적용평점 반영
			
			nEvlWghval 	= this.ds_evlListDtl.getColumn(nRow, "EVL_WGHTVAL");

			var nTempCalc = evlScore * nEvlWghval / 100 ;

			this.ds_evlListDtl.setColumn(nRow, "RSLT_SCORE_C", nTempCalc );
		}
	}
}

