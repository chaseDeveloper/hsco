					}
					if(odrSe == '3'){
						for (var i = 7; i <= 9; i++){
							var nRow = this.ds_mmList.addRow();
							this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
							this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
