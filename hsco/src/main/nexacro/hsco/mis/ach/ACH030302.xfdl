this.ds_qKpiListMst_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var nCurRow = e.newrow;
	//초기화
	this.ds_condDtl.clearData();
	
	if(nCurRow > -1){
		//Key Set
		this.ds_condDtl.addRow();
		this.ds_condDtl.setColumn(0, "EVL_YEAR",    this.ds_qKpiListMst.getColumn(nCurRow, "EVL_YEAR"));
		this.ds_condDtl.setColumn(0, "ODR_SE",      this.ds_qKpiListMst.getColumn(nCurRow, "ODR_SE"));
		this.ds_condDtl.setColumn(0, "IX_CODE",     this.ds_qKpiListMst.getColumn(nCurRow, "IX_CODE"));
		
		this.ds_condDtl.setColumn(0, "EVL_MAN_NO",  this.ds_cond.getColumn(0, "EVL_MAN_NO"));
		
		this.ds_condDtl.setColumn(0, "ORGNZT_CODE", this.ds_cond.getColumn(0, "ORGNZT_CODE"));
		
		var sMCycle = this.ds_qKpiListMst.getColumn(nCurRow,"MESURE_CYCLE");		// 지표측정주기
		var evlCycle = this.ds_evlCycle.getColumn(0, "EVL_CYCLE");					// 성과평가주기
		var odrSe = this.ds_cond.getColumn(0,"ODR_SE");								// 검색조건 구분항목
		
		this.ds_mmList.set_enableevent(false);
		this.ds_mmList.clearData();
		switch (sMCycle) {

			case "003": // 월
				if(evlCycle == '1'){
					for (var i = 1; i <= 12; i++){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
						this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
					}
				}
				
				if(evlCycle == '2'){
					if(odrSe == '1'){
						for (var i = 1; i <= 6; i++){
							var nRow = this.ds_mmList.addRow();
							this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
							this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
						}
					}
					if(odrSe == '2'){
						for (var i = 7; i <= 12; i++){
							var nRow = this.ds_mmList.addRow();
							this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
							this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
						}
					}
				}
				
				if(evlCycle == '4'){
					if(odrSe == '1'){
						for (var i = 1; i <= 3; i++){
							var nRow = this.ds_mmList.addRow();
							this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
							this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
						}
					}
					if(odrSe == '2'){
						for (var i = 4; i <= 6; i++){
							var nRow = this.ds_mmList.addRow();
							this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
							this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
