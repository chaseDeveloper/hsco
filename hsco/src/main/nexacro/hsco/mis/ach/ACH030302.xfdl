/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectListMst": // ListMst 불러오기(지표)
			var sController   	= "hsco/mis/ach/ACH030302/selectListMst.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_qKpiListMst=output1";
			break;

		case "selectListDtl": // ListDtl 불러오기
			var sController   	= "hsco/mis/ach/ACH030302/selectListDtl.do";
			var sInDatasets   	= "input1=ds_condDtl";  //ds_condDtl
			var sOutDatasets  	= "ds_evlListDtl=output1";
			break;

		case "dtlCUD": 		  // 저장하기( Dtl )
			var sController   	= "hsco/mis/ach/ACH030302/dtlCUD.do";
			var sInDatasets   	= "input1=ds_evlListDtl:U";
			var sOutDatasets  	= "";
			break;

		case "selectEvlScoreList": // GET 평가점수목록
			var sController   	= "hsco/mis/ach/ACHCommon/selectEvlScoreList.do";  //ACHCommon
			var sInDatasets   	= "input1=ds_cond";  //ds_cond
			var sOutDatasets  	= "ds_evlScoreList=output1";
			break;

		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/ach/ACH030302/selectFileList.do";
			var sInDatasets   	= "input1=ds_condFile";
			var sOutDatasets  	= "ds_fileList=output1";
			break;

		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;
		
		case "getQleEvlManList":	//등록 평가자 콤보박스 리스트(조회조건)			
			var sController   	= "hsco/mis/ach/ACH030302/getQleEvlManList.do";
			var sInDatasets   	= "input1=ds_condMan";
			var sOutDatasets  	= "ds_qleEvlMan=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectListMst": // ListMst 불러오기
				
				if(this.ds_qKpiListMst.rowcount > 0 ){
					this.ds_qKpiListMst.set_rowposition(this.nRowPos);
					this.nRowPos = 0;
				}
				
				this.fn_getEvlScoreList();
				break;

			case "selectListDtl": // List Dtl 불러오기
				break;

			case "dtlCUD": // 저장하기( Dtl )
				this.gfn_message("success.처리.성공");
			    this.fn_search();
				break;

			case "selectEvlScoreList":	// GET 평가점수목록
				break;

			case "selectFileList":      // 첨부파일 조회
				break;

			case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				this.fn_loadComboNonCodeTbl();
				break;
			case "getQleEvlManList":
				//alert(this.ds_qleEvlMan.rowcount);
				
				if(this.ds_qleEvlMan.rowcount > 0){
					this.ds_cond.setColumn(0, "EVL_MAN_NO", this.ds_qleEvlMan.getColumn(0, "EVL_MAN_NO"));
					
					this.fn_search_mst();
				} else {
					this.ds_cond.setColumn(0, "EVL_MAN_NO", null);
					this.gfn_message("info.처리불가", "등록된 평가자가 없습니다.");
				}
				//this.fn_transaction("selectListMst");
				break;
		}
	}
}

