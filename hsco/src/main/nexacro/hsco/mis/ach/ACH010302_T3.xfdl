//------------------------------------
 // 지표코드 선택변경 전
//------------------------------------
this.ds_kpiInfo_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 지표 Value 변경시
	if (e.columnid == "IX_CODE"){
		var sIxCode = e.newvalue;
		var nRow = e.row;
		var nFindRow1 = this.ds_kpiInfo.findRow("IX_CODE", sIxCode, 0, e.row);	// 시작위치, 마지막행 +1(현재Row 이전Row 까지)
		var nFindRow2 = this.ds_kpiInfo.findRow("IX_CODE", sIxCode, e.row+1, obj.rowcount);

		if(nFindRow1 > -1 || nFindRow2 > -1) {
			this.gfn_message("info.항목.중복.재입력.요청", "지표코드");
			this.ds_kpiInfo.set_updatecontrol(false);
			this.ds_kpiInfo.setColumn(e.row, e.columnid, e.oldvalue);
			this.ds_kpiInfo.set_updatecontrol(true);
			return false;			
		}
	}
}

//------------------------------------
 // 지표코드 선택변경 후
//------------------------------------
this.ds_kpiInfo_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
// 	if(e.columnid == "IX_CODE"){
// 		var nCnt = this.ds_refrnList.rowcount;
// 		
// 		for(var i = 0; i< nCnt; i++){
// 			this.ds_refrnList.setColumn(i, "IX_CODE", e.newvalue);
// 		}		
// 	}
		
	if(e.columnid == "IX_CODE"){
		var row1 = this.ds_ixNmSe.findRow("CODE", e.newvalue);
		
		var charct = this.ds_ixNmSe.getColumn(row1, "IX_CHARCT");
		var scopeSe = this.ds_ixNmSe.getColumn(row1, "APPLC_SCOPE_SE");
		var cycle = this.ds_ixNmSe.getColumn(row1, "MESURE_CYCLE");
		var unit = this.ds_ixNmSe.getColumn(row1, "MESURE_UNIT");
		var mthd = this.ds_ixNmSe.getColumn(row1, "ACMSLT_VALUE_MANAGE_MTHD");
		var frmla = this.ds_ixNmSe.getColumn(row1, "ACMSLT_CALC_FRMLA");
		
		obj.setColumn(e.row, "IX_CHARCT", charct);
		obj.setColumn(e.row, "APPLC_SCOPE_SE", scopeSe);
		obj.setColumn(e.row, "MESURE_CYCLE", cycle);
		obj.setColumn(e.row, "MESURE_UNIT", unit);
		obj.setColumn(e.row, "ACMSLT_VALUE_MANAGE_MTHD", mthd);
		obj.setColumn(e.row, "ACMSLT_CALC_FRMLA", frmla);
	}
}

