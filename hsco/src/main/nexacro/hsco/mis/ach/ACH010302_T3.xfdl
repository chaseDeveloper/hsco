}

this.ACH010302_T3_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();
	this.parent.parent.parent.checkDs.push(this.ds_kpiInfo);
	this.parent.parent.parent.checkDs.push(this.ds_refrnList);
}

//----------------------------------
// Form/Dataset 오브젝트 초기화
//----------------------------------
