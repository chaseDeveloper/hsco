//-------
// 조회
//-------
this.fn_search = function()
{
    this.ds_kpiInfo.clearData();
    this.ds_refrnList.clearData();

	this.fn_transaction("selectListMst");
}

//-------------
// Dtl Search
//-------------
this.fn_search_dtl = function()
{
	//초기화
	this.ds_refrnList.clearData();
	this.fn_transaction("selectListDtl");
}

//-------
// 저장
//-------
this.fn_save = function()
{
    // Dtl 기준
	if(Ex.util.isUpdated(this.ds_kpiInfo)){
		if(this.gfn_message("confirm.저장.여부"))	{
		    
			// validation 체크
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_kpiInfo, this.ds_validation)) {
				return false;
			}
			
// 			for(var i=0; i<this.ds_refrnList.rowcount; i++){
// 			
// 				if(Number(this.ds_refrnList.getColumn(i, "A1")) < Number(this.ds_refrnList.getColumn(i, "A2"))){
// 					this.gfn_message("errors.항목.입력값.이상", "달성율 최대값|달성율 최소값");
// 					return;
// 				}
// 				if(Number(this.ds_refrnList.getColumn(i, "A3")) < Number(this.ds_refrnList.getColumn(i, "A4"))){
// 					this.gfn_message("errors.항목.입력값.이상", "실적 최대값|실적 최소값");
