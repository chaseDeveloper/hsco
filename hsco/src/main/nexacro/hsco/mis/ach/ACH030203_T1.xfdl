this.fn_setMonth = function()
{
	var sMCycle = this.ds_kpiInfo.getColumn(0,"MESURE_CYCLE");
	var evlCycle = this.ds_evlCycle.getColumn(0, "EVL_CYCLE");
	var odrSe = this.ds_cond.getColumn(0,"ODR_SE");
	
	this.ds_mmList.set_enableevent(false);
	this.ds_mmList.clearData();
	switch (sMCycle) {

		case "003": // 월
			if(evlCycle == '1'){
				for (var i = 1; i <= 12; i++){
					var nRow = this.ds_mmList.addRow();
					this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
					this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
				}
			}
			
			if(evlCycle == '2'){
				if(odrSe == '1'){
					for (var i = 1; i <= 6; i++){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
						this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
					}
				}
				if(odrSe == '2'){
					for (var i = 7; i <= 12; i++){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
						this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
					}
				}
			}
			
			if(evlCycle == '4'){
				if(odrSe == '1'){
					for (var i = 1; i <= 3; i++){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
						this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
					}
				}
				if(odrSe == '2'){
					for (var i = 4; i <= 6; i++){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
						this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
					}
				}
				if(odrSe == '3'){
					for (var i = 7; i <= 9; i++){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
						this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
					}
				}
				if(odrSe == '4'){
					for (var i = 10; i <= 12; i++){
						var nRow = this.ds_mmList.addRow();
						this.ds_mmList.setColumn(nRow, "MM_CODE", i.toString());
						this.ds_mmList.setColumn(nRow, "MM_NM", i.toString() + "월");
					}
				}				
			}
			
			if(evlCycle == '12'){
				var nRow = this.ds_mmList.addRow();
				this.ds_mmList.setColumn(0, "MM_CODE", odrSe);
				this.ds_mmList.setColumn(0, "MM_NM", odrSe + "월");
			}
			break;

		case "004": // 분기
			if(evlCycle == '1'){
				for (var i = 1; i <= 4; i++){
					var nRow = this.ds_mmList.addRow();
					if (i == 1){
						var sMMcode = "3";
					}else if (i == 2){
						var sMMcode = "6";
					}else if (i == 3){
						var sMMcode = "9";
					}else if (i == 4){
						var sMMcode = "12";
					}
					this.ds_mmList.setColumn(nRow, "MM_CODE", sMMcode);
					this.ds_mmList.setColumn(nRow, "MM_NM", sMMcode + "월");
				}
			}
			
			if(evlCycle == '2'){
				if(odrSe == '1'){
					for (var i = 1; i <= 2; i++){
						var nRow = this.ds_mmList.addRow();
						if (i == 1){
							var sMMcode = "3";
						}else if (i == 2){
							var sMMcode = "6";
						}else if (i == 3){
							var sMMcode = "9";
						}else if (i == 4){
							var sMMcode = "12";
						}
						this.ds_mmList.setColumn(nRow, "MM_CODE", sMMcode);
						this.ds_mmList.setColumn(nRow, "MM_NM", sMMcode + "월");
					}
				}
				
				if(odrSe == '2'){
					for (var i = 3; i <= 4; i++){
						var nRow = this.ds_mmList.addRow();
						if (i == 1){
							var sMMcode = "3";
						}else if (i == 2){
							var sMMcode = "6";
						}else if (i == 3){
							var sMMcode = "9";
						}else if (i == 4){
							var sMMcode = "12";
						}
						this.ds_mmList.setColumn(nRow, "MM_CODE", sMMcode);
						this.ds_mmList.setColumn(nRow, "MM_NM", sMMcode + "월");
					}
