//------------------------------------
// ListDtl Item 변경시
//------------------------------------
this.ds_refrnList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 부서별인원/업무제안활동횟수/CS제안활동횟수 Value 변경시
	if(e.columnid == "ORGNZT_NMPR" || e.columnid == "ACMSLT_A"  || e.columnid == "ACMSLT_B" ){

		var nRow = e.row;
		var nTempCalc 	= 0;
		var nORGNZT_NMPR = 0;
		var nACMSLT_A = 0;
		var nACMSLT_B = 0;

		nORGNZT_NMPR = this.ds_refrnList.getColumn(nRow,"ORGNZT_NMPR");
		nACMSLT_A 	= this.ds_refrnList.getColumn(nRow,"ACMSLT_A");
		nACMSLT_B 	= this.ds_refrnList.getColumn(nRow,"ACMSLT_B");
		//Check Null
		if( this.comUtils.isNullEmpty(nACMSLT_A) ){
			nACMSLT_A = 0;
		}else if( this.comUtils.isNullEmpty(nACMSLT_B) ){
			nACMSLT_B = 0;
		}else if( this.comUtils.isNullEmpty(nORGNZT_NMPR) ){
			nORGNZT_NMPR = 0;
		}
		//Calc [ 1인당 참여횟수 = 활동실적의합 / 조직인원수 ]
		nTempCalc = nexacro.round(( nACMSLT_A + nACMSLT_B ) / nORGNZT_NMPR, 2) ;

		this.ds_refrnList.setColumn(nRow, "ACMSLT_C", nTempCalc );
	}
	
	if(e.columnid == "ACMSLT_RANK"){
		var a1 = this.ds_kpiInfo01.getColumn(0, "A1");
		var a2 = this.ds_kpiInfo01.getColumn(0, "A2");
		var a3 = this.ds_kpiInfo01.getColumn(0, "A3");
		var a4 = this.ds_kpiInfo01.getColumn(0, "A4");
		
		var score = a2 + (((a1-a2)/(a4-a3))*(a4- e.newvalue));
		
		obj.setColumn(e.row, "ACMSLT_SCORE", score);
		
		this.fn_calc();		// 월별 실적 계산
	}
}

/*
 * 함수명  	: ds_acmsltList_oncolumnchanged
 * 설명 		: 월별실적값 변경시 달성율 및 월성과값 자동계산
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_acmsltList_oncolumnchanged(obj,e);
 */
this.fn_calc = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var score = this.ds_refrnList.getColumn(this.ds_refrnList.rowposition,"ACMSLT_SCORE");		// 실적(점수)
	
	var nRow = 0;
	var frmla = this.ds_kpiInfo.getColumn(nRow, "ACHIV_RT_CALC_FRMLA");			// 현재 지표의 달성율 계산식
	var inputTy = this.ds_kpiInfo.getColumn(nRow, "ACMSLT_INPUT_TY");			// 현재 지표의 실적 등록방식
	var mthd = this.ds_kpiInfo.getColumn(nRow, "ACMSLT_VALUE_MANAGE_MTHD");     // 현재 지푱의 실적값 관리방식
	var mxmmLmt = this.ds_kpiInfo.getColumn(nRow, "MXMM_EVL_LMT");		    	// 현재 지표의 최대평가한도
	var excpFrmla = this.ds_kpiInfo.getColumn(nRow, "EXCP_RSLT_CALC_FRMLA");	// 현재 지표의 예외성과계산식	
	
	var deptCode = this.ds_refrnList.getColumn(this.ds_refrnList.rowposition, "ORGNZT_CODE");
	var nRow = this.ds_acmsltList.findRowExpr("ORGNZT_CODE == '"+deptCode+"'");
	
	var evlWghtval = this.ds_acmsltList.getColumn(nRow, "EVL_WGHTVAL"); //평가가중치
	
	// Calc. 월별 실적에 따른 월별 달성율 계산
	var goal = this.ds_acmsltList.getColumn(nRow, "MT_GOAL");	// 월별 목표치
	var result = 0;
	var scoreResult = 0;
	
	if(frmla == "001"){		// 실적치/목표치
			
		if(goal > 0 && score > 0){			
			result = nexacro.round((score/goal) * 100,2);				
			if(mxmmLmt > 0 && result > mxmmLmt ){	// 최대평가한도 적용			
				result = mxmmLmt;
			}			
		}else if(mthd == 'C' && goal == 0 && score == 0){ // 마일리지(가감점)타입인 경우
			result = 100;
		}else if(excpFrmla == '3' && goal == 0 && score == 0){ // 예외성과계산식 존재
			result = 100;					
		}else{
			result = 0;
		}				
	}		
	if(frmla == "002"){		// 목표치/실적치
		
		if(goal > 0 && score > 0){			
			result = nexacro.round((goal/score) * 100,2);				
			if(mxmmLmt > 0 && result > mxmmLmt ){	// 최대평가한도 적용			
				result = mxmmLmt;
			}			
		}else if(excpFrmla == '4' && score == 0){ // 예외성과계산식 존재
			result = 100;					
		}else{
			result = 0;
		}
	}
	// Calc. 월별 실적에 따른 월별 성과점수 계산
	if (mthd == 'C' && evlWghtval == 0 && score != 0 ){ // 마일리지(가감점)타입인 경우
		scoreResult = score ;
	
	}else{
		scoreResult = nexacro.round(evlWghtval * result / 100, 2);
	}		
	
	this.ds_acmsltList.setColumn(nRow, "MT_ACHIV_RT", result);  			// 월별달성율
	this.ds_acmsltList.setColumn(nRow, "MT_ACMSLT_VAL", score);			// 월별 실적
	this.ds_acmsltList.setColumn(nRow, "MT_RESULT_SCORE", scoreResult);	// 월별 성과점수
	
	// Calc. 월별 실적에 따른 최종실적 계산
	var tot_val = this.ds_acmsltList.getColumn(nRow, "TOT_ACMSLT_VAL");	// 현재월 까지의 누계 실적치
	result = 0;
	
	if(inputTy == "001"){			// 누계합산
		result = (tot_val - this.mtOldVal + score);
	}else if(inputTy == "002"){		// 누계평균
		result = nexacro.round((tot_val - this.mtOldVal + score)/(this.ds_mmList.rowposition+1),2);
	}else {							// 누계실적치
		result = score;
	}
	this.ds_acmsltList.setColumn(nRow, "FINAL_ACMSLT", result);
}


//------------------------------------
// 조직코드 선택변경시
//------------------------------------
this.ds_refrnList_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 조직코드 Value 변경시
	if (e.columnid == "ORGNZT_CODE"){
		var sCode = e.newvalue;
		
		if(sCode){
			var nRow = e.row;
			var nFindRow = this.ds_refrnList.findRow("ORGNZT_CODE", sCode);

			if (nFindRow > -1){
				this.gfn_message("comm.데이터.기등록.존재", this.ds_orgnztSe.getColumn(nRow, "ALL_CODE_NM")+"(는)");
				return false;
			}
		}
	}
	
	
	if (e.columnid == "ACMSLT_RANK"){
		if(e.oldvalue){
			this.mtOldVal = e.oldvalue;
		} else {
			this.mtOldVal = 0;
		}
	}

}

