    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACH030201
 * 화면명   : 계량지표 목표치 등록
 * 화면설명 : 계량 성과지표에 대한 조직별/측정월 별 목표치를 등록한다.
 * 작성일   : 2015.09.02
 * 작성자   : 박병일
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.sClosAt = "";
this.nRowPos = 0;
this.nRowPos2 = -1;
this.nSetInit = 0;

 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH030201_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_misFormInit(this);
	this.fn_init_form();
	this.fn_init_dataset();
}

this.ACH030201_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();
	this.fn_init_current();
}

//----------------------------------
// Form/Dataset 오브젝트 초기화
//----------------------------------
this.fn_init_form = function()
{
	this.gfn_initForm(this);
}

//----------------------------------
// Get Current 평가차수
//----------------------------------
this.fn_init_current = function()
{
	this.ds_curInfo.clearData();
	this.fn_transaction("selectCurEvlOdrInfo");
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

//-----------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (공통코드)
//-----------------------------------------------------
 this.fn_loadCombo = function ()
{
    // ----------------------------------------- Condition Area -----------------------------------------------
	// 적용범위구분
//	this.gfn_loadCodeCombo(this, this.div_search.cmb_appSe, "ds_gubun02", "ACH015", "", "Y", "전체", "N", "");
	var comboParams = [
		["ds_gubun03", "ACH015", "Y", "A"]	// 적용범위구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		
	};
	this.gfn_comboLoad (comboParams, callback);
	// ----------------------------------------- Dataset to Grid Area -----------------------------------------
	// 적용범위
	this.gfn_loadCodeComboWithDs(this, "", this.ds_applcSe, "ACH015", "", "", " ", "N", "");
	// 측정단위
	this.gfn_loadCodeComboWithDs(this, "", this.ds_mesure_unit_se, "ACH011", "", "", " ", "N", "");
	// 실적등록방식
	this.gfn_loadCodeComboWithDs(this, "", this.ds_acmslt_it_se, "ACH008", "", "", " ", "N", "");
	// 관점구분
	this.gfn_loadCodeComboWithDs(this, "", this.ds_viewPointSe, "ACH002", "", "", " ", "N", "");
	// 측정주기
	this.gfn_loadCodeComboWithDs(this, "", this.ds_me_cycle_se, "ACH005", "", "", " ", "N", "");
}


//--------------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (Non-공통코드)
//--------------------------------------------------------
this.fn_loadComboNonCodeTbl = function (strDs, obj)
{
	var comboParams = [
			["ds_odrSe", 	"ds_cond", "", "", "hsco/mis/ach/ACHCommon/selectOdrList.do", 0]
		   ,["ds_evlCycle", "ds_cond", "", "", "hsco/mis/ach/ACHCommon/selectEvlCycleSingle.do", 0]	
		];

	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
}

this.fn_loadComboNonCodeTbl2 = function (strDs, obj)
{
	var comboParams = [
		["ds_orgnztSe", "ds_cond", "", "전체", "hsco/mis/ach/ACHCommon/selectOrgnztList.do", 0]
	];

	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: combo에 대한 callback 함수 이후에 처리되어야 할 코딩 기술
 * params  		: svcId - Transaction Service ID.
 *				  ※ 반드시, 데이터를 담아 올 dataset명(예 : "ds_comCode03")을 사용해야 한다.
 * return Type 	: None
 * 사용법		: this.fn_init_dataset();
 * ※ 함수의 특성상, comboParams의 개수만큼 호출되므로, svcId로 분기하여 처리해야 한다.
 */
this.fn_callbackAfter = function (svcId)
{
	switch (svcId) {
		case "ds_odrSe" :
			if (nSetInit == 1){ // Load시 1회만 적용
				this.fn_setCurInfo("odrSe");
				nSetInit = 0;
			}
			this.fn_loadComboNonCodeTbl2();
			break;
		case "ds_orgnztSe" :
			this.fn_search();
			break;
	}
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/

//-------
// 조회
//-------
this.fn_search = function()
{
    this.ds_qKpiListMst.clearData();
    this.ds_goalList.clearData();
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}    
	this.fn_transaction("selectListMst");
}

//-------------
// Dtl Search
//-------------
this.fn_search_dtl = function()
{
	this.ds_goalList.clearData();
	this.fn_transaction("selectListDtl");
}

//-------
// 저장
//-------
this.fn_save = function()
{

	
	if(Ex.util.isUpdated(this.ds_goalList)){
		if(this.gfn_message("confirm.저장.여부"))	{
			if(!this.gfn_checkValidation(this.ds_goalList, this.ds_validation)) {
				return;
			}
			
		    nRowPos = this.ds_qKpiListMst.rowposition;
		    this.nRowPos2 = this.ds_mmList.rowposition;
			this.fn_transaction("dtlCUD");
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectListMst": // ListMst 불러오기
			var sController   	= "hsco/mis/ach/ACH030201/selectListMst.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_qKpiListMst=output1";
			break;

		case "selectListDtl": // ListDtl 불러오기
			var sController   	= "hsco/mis/ach/ACH030201/selectListDtl.do";
			var sInDatasets   	= "input1=ds_condDtl";  //ds_condDtl
			var sOutDatasets  	= "ds_goalList=output1";
			break;

		case "dtlCUD": // 저장하기( Dtl )
			var sController   	= "hsco/mis/ach/ACH030201/dtlCUD.do";
			var sInDatasets   	= "input1=ds_condDtl";
				sInDatasets    += " " + "input2=ds_goalList:U";
			var sOutDatasets  	= "";
			break;

		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;
			
		case "updateAllU": 		// 월별목표 일괄등록 처리
			var sController   	= "hsco/mis/ach/ACH030201/dtlAllCU.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectListMst": // ListMst 불러오기
				if(this.ds_qKpiListMst.rowcount > 0 ){
					this.ds_qKpiListMst.set_rowposition(nRowPos);
					nRowPos = 0;
				}
				break;

			case "selectListDtl": // List Dtl 불러오기
				break;

			case "dtlCUD": // 저장하기( Dtl )
			    this.fn_search_dtl();
				break;

			case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				this.fn_loadComboNonCodeTbl();
				break;
				
			case "updateAllU": // 월별목표 일괄등록 처리
			    this.gfn_message("success.처리.성공");
			    this.fn_search();
				break;

		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/

//---------
// 취소
//---------
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.취소여부")) {
	    // DTL
		this.ds_goalList.reset();			//DATASET ROLLBACK
		this.ds_goalList.applyChange();		//DATASET COMMIT
		return;
	}
}


