/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 사원 팝업
	if(sPopupId == "empPopup") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
 		if(loadCnt) {

			var empno     = dsTemp.getColumn(0, "EMPNO");
			var empnm     = dsTemp.getColumn(0, "EMPNM");
			var jssfcNm   = dsTemp.getColumn(0, "JSSFC_NM");
			var hrClsfNm  = dsTemp.getColumn(0, "HR_CLSF_NM");

			// 존재하지 않는 사원에 대해서만 행 추가
			if(this.ds_evlEmpList.findRow("EMPNO", empno) == -1) {
				var nRow = this.ds_evlEmpList.addRow();
				
				this.ds_evlEmpList.setColumn(nRow, "EVL_YEAR"    , this.ds_cond3.getColumn(0, "EVL_YEAR"));
				this.ds_evlEmpList.setColumn(nRow, "ODR_SE"      , this.ds_cond3.getColumn(0, "ODR_SE"));
				this.ds_evlEmpList.setColumn(nRow, "ORGNZT_CODE" , this.ds_cond3.getColumn(0, "ORGNZT_CODE"));
				this.ds_evlEmpList.setColumn(nRow, "SEQ"         , this.ds_cond3.getColumn(0, "SEQ"));
				this.ds_evlEmpList.setColumn(nRow, "IX_CODE"     , this.ds_KPIInfo.getColumn(0, "IX_CODE"));
						
				this.ds_evlEmpList.setColumn(nRow, "EMPNO"      , empno);
				this.ds_evlEmpList.setColumn(nRow, "EMPNM"      , empnm);
				this.ds_evlEmpList.setColumn(nRow, "JSSFC_NM"   , jssfcNm);
				this.ds_evlEmpList.setColumn(nRow, "HR_CLSF_NM" , hrClsfNm);
				this.ds_evlEmpList.setColumn(nRow, "ACMSLT" , 0);
			}
 		}
	}
}
