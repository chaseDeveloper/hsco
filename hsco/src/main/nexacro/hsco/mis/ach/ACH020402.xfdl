/*
 * 함수명  	: ds_refrnList_oncolumnchanged
 * 설명 		: 월별실적값 변경시 달성율 및 월성과값 자동계산
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_refrnList_oncolumnchanged(obj,e);
 */
this.ds_refrnList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
    // Case. 월별실적 등록/변경시
	if(e.columnid == "MT_ACMSLT_VAL"){//scoreResult  
		if(!obj.getColumn(e.row, "MT_GOAL")){
			return false;
		}
 		var nRow      = e.row
 		var frmla     = obj.getColumn(nRow, "ACHIV_RT_CALC_FRMLA");		 // 현재 지표의 달성율 계산식
 		var inputTy   = obj.getColumn(nRow, "ACMSLT_INPUT_TY");			 // 현재 지표의 실적 등록방식
 		var mthd      = obj.getColumn(nRow, "ACMSLT_VALUE_MANAGE_MTHD"); // 현재 지푱의 실적값 관리방식
 		var mxmmLmt   = obj.getColumn(nRow, "MXMM_EVL_LMT");		     // 현재 지표의 최대평가한도
 		var excpFrmla = obj.getColumn(nRow, "EXCP_RSLT_CALC_FRMLA");	 // 현재 지표의 예외성과계산식	
			
		var evlWghtval = obj.getColumn(e.row, "EVL_WGHTVAL"); //평가가중치
		
		// Calc. 월별 실적에 따른 월별 달성율 계산
		var goal = obj.getColumn(e.row, "MT_GOAL");	// 월별 목표치
		var result = 0;
		
		var scoreResult = 0;
		
		if(frmla == "001"){		// 실적치/목표치
				
			if(goal > 0 && e.newvalue > 0){			
				result = (e.newvalue/goal) * 100;				
				if(mxmmLmt > 0 && result > mxmmLmt ){	// 최대평가한도 적용			
					result = mxmmLmt;
				}			
			}else if(mthd == 'C' && goal == 0 && e.newvalue == 0){ // 마일리지(가감점)타입인 경우
				result = 100;
			}else if(excpFrmla == '3' && goal == 0 && e.newvalue == 0){ // 예외성과계산식 존재
				result = 100;					
			}else{
				result = 0;
			}				
		}		
		if(frmla == "002"){		// 목표치/실적치
			
			if(goal > 0 && e.newvalue > 0){			
				result = (goal/e.newvalue) * 100;				
				if(mxmmLmt > 0 && result > mxmmLmt ){	// 최대평가한도 적용			
					result = mxmmLmt;
				}			
			}else if(excpFrmla == '4' && e.newvalue == 0){ // 예외성과계산식 존재
				result = 100;					
			}else{
				result = 0;
			}
		}
		// Calc. 월별 실적에 따른 월별 성과점수 계산
		if (mthd == 'C' && evlWghtval == 0 && e.newvalue != 0 ){ // 마일리지(가감점)타입인 경우
			scoreResult = e.newvalue ;
		
		}else{
			//scoreResult = evlWghtval * result / 100;
			//scoreResult = goal / e.newvalue * evlWghtval;
			scoreResult = nexacro.round(e.newvalue * evlWghtval / goal , 2);	// 실적 / 목표 * 가중치 (반올림하여 소수점 2자리까지 표현)
		}		
		
		obj.set_updatecontrol(false);
		obj.setColumn(e.row, "MT_ACHIV_RT", result);  			// 월별달성율
		obj.setColumn(e.row, "MT_RESULT_SCORE", scoreResult);	// 월별 성과점수	
		obj.set_updatecontrol(true);		
		
		// Calc. 월별 실적에 따른 최종실적 계산 -> Calc는 실적저장이후 집계처리로 변경
// 		var tot_val = obj.getColumn(e.row, "TOT_ACMSLT_VAL");	// 현재월 까지의 누계 실적치
// 		result = 0;
// 		
// 		if(inputTy == "001"){			// 누계합산
// 			result = (tot_val - e.oldvalue + e.newvalue);
// 		}else if(inputTy == "002"){		// 누계평균
// 			result = nexacro.round((tot_val - e.oldvalue + e.newvalue)/(this.ds_mmList.rowposition+1),2);
// 		}else {							// 누계실적치
// 			result = e.newvalue;
// 		}
// 		obj.set_updatecontrol(false);
// 		obj.setColumn(e.row, "FINAL_ACMSLT", result);
// 		obj.set_updatecontrol(true);	
	}
}


