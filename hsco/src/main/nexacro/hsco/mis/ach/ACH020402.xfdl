    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACH020402
 * 화면명   : 수사회공헌이관리
 * 화면설명 : 수사회공헌이를 관리한다.
 * 작성일   : 2017.07.26
 * 작성자   : 정 민
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ************************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";


/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.sClosAt = "";
this.nRowPos = 0;
this.nSetInit = 0;
this.nRowPos2 = -1;

// rowposition 유지
var saveRow = -1;

 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH020402_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_misFormInit(this);
}

this.ACH020402_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
	this.fn_init_current();
}

//----------------------------------
// Form/Dataset 오브젝트 초기화
//----------------------------------
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	
	// 검색조건 데이터셋
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	
	// 사회공헌이수관리 코드번호 셋팅 (012)
	this.ds_cond.setColumn(0, "REFRN_SE", "012");
		
	var comboParams = [
		["ds_mesure_unit_se", 	"ACH011", "1", ""], 	// 측정단위
		["ds_me_cycle_se", 		"ACH005", "1", ""], 	// 측정주기
		["ds_acmsltValueMSe", 	"ACH014", "1", ""] 		// 실적값관리방식
	];

	this.gfn_comboLoad(comboParams, this.fn_callbackAfterLoad);
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

//----------------------------------
// Get Current 평가차수
//----------------------------------
this.fn_init_current = function()
{	
	this.ds_curInfo.clearData();
	this.fn_transaction("selectCurEvlOdrInfo");
}

//--------------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (Non-공통코드)
//--------------------------------------------------------
this.fn_loadComboNonCodeTbl = function (strDs, obj)
{
	var comboParams = [
							 ["ds_odrSe"       , "ds_cond", "", ""      , "hsco/mis/ach/ACHCommon/selectOdrList.do"       , 0]	  		// 구분    		 
							,["ds_evlCycle"    , "ds_cond", "", ""      , "hsco/mis/ach/ACHCommon/selectEvlCycleSingle.do", 0]		    // 평가주기
				      ];

	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
}

// 검색조건 구분 코드값을 가져오기 위해 분리
this.fn_loadComboNonCodeTbl2 = function (strDs, obj)
{
	var comboParams = [
							["ds_orgnztSeCond", "ds_cond", "", "전체"	, "hsco/mis/ach/ACHCommon/selectOrgnztList.do"    , 0]			// 조직
					  ];

	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
}


/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: combo에 대한 callback 함수 이후에 처리되어야 할 코딩 기술
 * params  		: svcId - Transaction Service ID.
 *				  ※ 반드시, 데이터를 담아 올 dataset명(예 : "ds_comCode03")을 사용해야 한다.
 * return Type 	: None
 * 사용법		: this.fn_init_dataset();
 * ※ 함수의 특성상, comboParams의 개수만큼 호출되므로, svcId로 분기하여 처리해야 한다.
 */
this.fn_callbackAfter = function (svcId)
{
	switch (svcId) {
		case "ds_odrSe" :
			if (nSetInit == 1){ // Load시 1회만 적용
				this.fn_setCurInfo("odrSe");
				nSetInit = 0;
			}
			this.fn_loadComboNonCodeTbl2();
			break;
  		case "ds_orgnztSeCond" :
			this.fn_search();
    		break;

	}
}



/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/
//-------
// 조회
//-------
this.fn_search = function()
{	
	this.ds_evlEmpList.set_enableevent(false);
	if(Ex.util.isUpdated(this.ds_evlEmpList)) {
		if(!application.confirm("저장하지 않은 데이터가 존재합니다.\n현재 작업을 취소하시겠습니까?")) {
			return false;	// 아니오
		}else {
			this.ds_evlEmpList.reset();
			this.ds_evlEmpList.applyChange();
			
			this.ds_refrnList.reset();
			this.ds_refrnList.applyChange();
		}
	}
	this.ds_evlEmpList.set_enableevent(true);
	
	// 데이터셋 초기화
	this.ds_refrnList.clearData();
	this.ds_evlEmpList.clearData();
	
	this.ds_KPIInfo.clearData();
	this.ds_mmList.clearData();
	
	this.fn_transaction("selectKPIInfo");
}

//-------
// 저장
//-------
this.fn_save = function()
{
	// 데이터셋 미변경 체크
     if (!Ex.util.isUpdated(this.ds_refrnList) && !Ex.util.isUpdated(this.ds_evlEmpList)) {
         this.gfn_message("comm.데이터.변경.없음")
         return false;
     }

    // validation 체크
    if(!this.gfn_checkValidation_NoUpdChk(this.ds_evlEmpList, this.ds_validation)) {
        return;
    }

    if(this.gfn_message("confirm.저장.여부")) {
        saveRow  = this.ds_refrnList.rowposition;
        //this.ds_refrnList.set_enableevent(false);
        this.fn_transaction("saveEmpTlphonKdDgree");
    }
	
}

this.fn_cancel = function() {
	 
	 // 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_evlEmpList)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }

    if (this.gfn_message("confirm.취소여부")) {
        this.ds_evlEmpList.reset();
        this.ds_evlEmpList.applyChange();
    }

}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// GET 현재 평가차수(평가연도&차수)
		case "selectCurEvlOdrInfo": 
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;
 	    // 대상 계량지표 정보 조회
 		case "selectKPIInfo":
 			var sController   	= "hsco/mis/ach/ACH020402/selectKPIInfo.do";
 			var sInDatasets   	= "input1=ds_cond";
 			var sOutDatasets  	= "ds_KPIInfo=output1";
			break;
 		// 실적집계내역 조회
 		case "selectRefrnList":
 			var sController   	= "hsco/mis/ach/ACH020402/selectRefrnList.do";
 			var sInDatasets   	= "input1=ds_cond2";
 			var sOutDatasets  	= "ds_refrnList=output1";
 			break;
 		// 기초자료생성
 		case "bsisDtaCreat":
			var sController   	= "/hsco/com/sym/cdm/empNo/selectEmpnoList.do";    
			var sInDatasets   	= "input1=ds_bsisDtaCreatCond";
			var sOutDatasets  	= "ds_evlEmpList=output1";	
			break;
		// 평가사원 목록 조회
		case "selectEvlEmpList":
			var sController   	= "hsco/mis/ach/ACH020402/selectEvlEmpList.do";
			var sInDatasets   	= "input1=ds_cond3";
			var sOutDatasets  	= "ds_evlEmpList=output1";	
			break;
		// 실적집계내역 저장 & 평가사원 목록 저장
		case "saveEmpTlphonKdDgree": 
			var sController   	= "hsco/mis/ach/ACH020402/saveEmpTlphonKdDgree.do";
			var sInDatasets   	= "input1=ds_refrnList:U input2=ds_evlEmpList:U";	
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			// GET 현재 평가차수(평가연도&차수)
			case "selectCurEvlOdrInfo": 
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				this.fn_loadComboNonCodeTbl();
				break;
			// 대상 계량지표 정보 조회
			case "selectKPIInfo":
				this.fn_setMonth();
				break;
			// 실적집계내역 조회
			case "selectRefrnList":
				if(saveRow != -1) {
					//this.ds_refrnList.set_enableevent(true);
					this.ds_refrnList.set_rowposition(saveRow);
					saveRow = -1;
				}
				break;
			// 기초자료생성
			case "bsisDtaCreat":
				for(var i = 0; i < this.ds_evlEmpList.getRowCount(); i++) {
					this.ds_evlEmpList.set_updatecontrol(false);
					this.ds_evlEmpList.setRowType(i, "I");
					this.ds_evlEmpList.set_updatecontrol(true);
					
					this.ds_evlEmpList.setColumn(i, "EVL_YEAR"    , this.ds_cond3.getColumn(0, "EVL_YEAR"));
					this.ds_evlEmpList.setColumn(i, "ODR_SE"      , this.ds_cond3.getColumn(0, "ODR_SE"));
					this.ds_evlEmpList.setColumn(i, "ORGNZT_CODE" , this.ds_cond3.getColumn(0, "ORGNZT_CODE"));
					this.ds_evlEmpList.setColumn(i, "SEQ"         , this.ds_cond3.getColumn(0, "SEQ"));
					this.ds_evlEmpList.setColumn(i, "IX_CODE"     , this.ds_KPIInfo.getColumn(0, "IX_CODE"));
					this.ds_evlEmpList.setColumn(i, "ACMSLT" , 0);
				}
				break;
			// 평가사원 목록 조회
			case "selectEvlEmpList":
				break;
			// 실적집계내역 저장 & 평가사원 목록 저장
			case "saveEmpTlphonKdDgree":
				this.fn_search();
				break;
		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
 
/**********************************************************************
* Button 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명		: btn_delete_onclick
 * 설명			: 행삭제 버튼 클릭시 발생
 * params		: None
 * return Type	: None
 * 사용법		: this.btn_delete_onclick();
 */
this.btn_delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(!this.gfn_message("confirm.삭제여부")) {
		return false;	// 아니오
	}
	
	var cRow = this.ds_evlEmpList.rowposition;
	this.ds_evlEmpList.deleteRow(cRow);
	
	this.fn_getMtAcmsltVal();
}


/*
 * 함수명		: btn_cancel_onclick
 * 설명			: 행취소 버튼 클릭시 발생
 * params		: None
 * return Type	: None
 * 사용법		: this.btn_cancel_onclick();
 */
this.btn_cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_evlEmpList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	
	this.ds_evlEmpList.reset();
	this.ds_evlEmpList.applyChange();
}


/*
 * 함수명		: btn_bsisDtaCreat_onclick
 * 설명			: 기초자료생성 버튼 클릭
 * params		: None
 * return Type	: None
 * 사용법		: this.btn_bsisDtaCreat_onclick();
 */
this.btn_bsisDtaCreat_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var cRow = this.ds_refrnList.rowposition;

	if(!this.comUtils.isNull(this.ds_refrnList.getColumn(cRow, "MT_GOAL"))) {
		if(this.ds_evlEmpList.getRowCount() > 0) {
			if(!application.confirm("기존 사원의 실적점수, 환산점수 데이터는 삭제됩니다.\n계속 진행하시겠습니까?")) {
				return false;
			}
		}
	
		this.ds_evlEmpList.clearData();
		var orgnztCode = this.ds_refrnList.getColumn(cRow, "ORGNZT_CODE");
		
		this.ds_bsisDtaCreatCond.clearData();
		this.ds_bsisDtaCreatCond.addRow();
		this.ds_bsisDtaCreatCond.setColumn(0, "DEPT_CODE", orgnztCode);
		
		this.fn_transaction("bsisDtaCreat");	
	}else {
		alert("월별목표를 먼저 등록해주세요.");
	}
	
}


/*
 * 함수명		: btn_add_onclick
 * 설명			: 행추가 버튼 클릭
 * params		: None
 * return Type	: None
 * 사용법		: this.btn_add_onclick();
 */
this.btn_add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	var cRow = this.ds_refrnList.rowposition;
	
	if(!this.comUtils.isNull(this.ds_refrnList.getColumn(cRow, "MT_GOAL"))) {
		var cRow = this.ds_refrnList.rowposition;
		var orgnztCode = this.ds_refrnList.getColumn(cRow, "ORGNZT_CODE");
		this.empPopup(orgnztCode);
	}else {
