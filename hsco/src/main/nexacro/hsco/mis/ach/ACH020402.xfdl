/*
 * 함수명  	: fn_getMtAcmsltVal
 * 설명 		: 평가사원목록 수정시 실적집계내역(평가인원, 총득점, 환산득점) 반영 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_getMtAcmsltVal();
 */
this.fn_getMtAcmsltVal = function() {
	var empCnt = this.ds_evlEmpList.rowcount;
	var cnvrsnScoreSum = this.ds_evlEmpList.getSum("ACMSLT");
	var mtAcmsltVal = nexacro.round((cnvrsnScoreSum/(empCnt*4))*100, 2);
	if(mtAcmsltVal > 100){
		mtAcmsltVal = 100;
	}
	var cRow = this.ds_refrnList.rowposition;
	this.ds_refrnList.setColumn(cRow, "EMP_CNT"         , empCnt);
	this.ds_refrnList.setColumn(cRow, "CNVRSN_SCORE_SUM", cnvrsnScoreSum);
	this.ds_refrnList.setColumn(cRow, "MT_ACMSLT_VAL"   , mtAcmsltVal);
}

