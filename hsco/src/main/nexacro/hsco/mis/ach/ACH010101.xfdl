    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACH010101
 * 화면명   : 평가기본정보 설정
 * 화면설명 : 성과평가 기본정보(평가주기,차수,기간)를 설정한다.
 * 작성일   : 2015.09.02
 * 작성자   : 박병일
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.sClosAt = "";
this.sBisInsChk = "";
this.saveRow = -1;
this.checkDs = [this.ds_evlCycle, this.ds_evlListDtl];
 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH010101_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	
	this.fn_init_form();
	
	this.fn_init_dataset();
}

this.ACH010101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Combo에 사용되는 Dataset Load
	this.fn_loadCombo();

	// 검색조건 컴포넌트 초기값 부여
	this.div_search.spn_evalYear.set_value( this.comUtils.getClientDate("YYYY") );

	this.fn_search();
}

//----------------------------------
// Form/Dataset 오브젝트 초기화
//----------------------------------
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

//-----------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (공통코드)
//-----------------------------------------------------
 this.fn_loadCombo = function ()
{
    // 평가주기구분
	this.gfn_loadCodeCombo(this, this.div_work.cmb_evlCycle, "ds_gubun01", "ACH019", "", "Y", "", "N", "");
	// 기준월
	this.gfn_loadCodeComboWithDs(this, "", this.ds_seqSe,   "ACH024", "", "Y", "", "N", "");

}

//-----------------------------------------
// 공통 Callback 함수에서 호출할 함수
//-----------------------------------------
this.fn_callBackAfter01 = function ()
{
}



/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/

//-------
// 조회
//-------
this.fn_search = function()
{
    this.ds_evlCycle.clearData();
    this.ds_evlListDtl.clearData();
    
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.fn_transaction("selectListMst");
}

//-------------
// Dtl Search
//-------------
this.fn_search_dtl = function()
{
	//초기화
	this.ds_evlListDtl.clearData();
	this.fn_transaction("selectListDtl");
}

//---------------
// Basis Search
//---------------
this.fn_bsisSearch = function()
{
	//초기화
	this.ds_evlListTemp.clearData();
	this.fn_transaction("selectTempOdr");
}


//-------
// 저장
//-------
this.fn_save = function()
{

	if(Ex.util.isUpdated(this.ds_evlCycle)){
		if(this.gfn_message("confirm.삭제.진행", "선택한 평가년도에 대한 평가차수 및 관련 정보가 삭제됩니다.")){
			// validation 체크
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_evlCycle, this.ds_validation)) {
				return false;
			}
			
			// 기존 평가차수 일괄삭제
			this.fn_delOdr();
			// 평가주기 선행등록
			//this.fn_addOdr();
		}else{
			return;
		}
	}else if (Ex.util.isUpdated(this.ds_evlListDtl)){
	
		if(!this.gfn_checkValidation(this.ds_evlListDtl, this.ds_validation)) {
			return;
		}
			
		if(this.gfn_message("confirm.저장.여부"))	{			
			this.saveRow = this.ds_evlListDtl.rowposition;
			this.fn_transaction("dtlCUD");
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectListMst": // ListMst 불러오기
			var sController   	= "hsco/mis/ach/ACHCommon/selectEvlCycleSingle.do";  //Common
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_evlCycle=output1";
			break;

		case "selectListDtl": // ListDtl 불러오기
			var sController   	= "hsco/mis/ach/ACH010101/selectListDtl.do";
			var sInDatasets   	= "input1=ds_condDtl";  //ds_evlCycle
			var sOutDatasets  	= "ds_evlListDtl=output1";
			break;

		case "selectTempOdr": // 임시조회
			var sController   	= "hsco/mis/ach/ACH010101/selectTempOdr.do";
			var sInDatasets   	= "input1=ds_condDtl";  //ds_evlCycle
			var sOutDatasets  	= "ds_evlListTemp=output1";
			break;

		case "beforeD": // 삭제하기( Dtl : 기존 평가차수)
			var sController   	= "hsco/mis/ach/ACH010101/beforeD.do";
			var sInDatasets   	= "input1=ds_condDtl";
			var sOutDatasets  	= "";
			break;

		case "mstCUD": // 저장하기( Mst : 평가주기)
			var sController   	= "hsco/mis/ach/ACH010101/mstCUD.do";
			var sInDatasets   	= "input1=ds_evlCycle:U"; //ds_evlCycle:U
			var sOutDatasets  	= "";
			break;

		case "dtlCUD": // 저장하기( Dtl )
			var sController   	= "hsco/mis/ach/ACH010101/dtlCUD.do";
			var sInDatasets   	= "input1=ds_evlListDtl:U";
			var sOutDatasets  	= "";
			break;

		case "mstDtlD": // 삭제
			var sController   	= "hsco/mis/ach/ACH010101/mstDtlD.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectListMst": // ListMst 불러오기
				this.fn_condset();
				this.div_work.cmb_evlCycle.set_readonly(true);
				this.div_work.cmb_evlCycle.set_cssclass("cmb_WF_Readonly");
 				break;

			case "selectListDtl": // List Dtl 불러오기
				if(this.saveRow > -1){
					this.ds_evlListDtl.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}			
				break;

			case "selectTempOdr": // List Dtl 불러오기
				this.fn_bsisCopy();
				break;

			case "beforeD":	// 삭제하기( Mst : 기존 평가차수)
				// 평가주기 선행등록
				this.fn_addOdr();
				break;

			case "mstCUD": // 저장하기( Mst : 평가주기)
			    if (sBisInsChk == "Y") {
					this.fn_bsisSearch();
			    }else{
					this.fn_search();
			    }
				break;

			case "dtlCUD": // 저장하기( Dtl )
			    sBisInsChk = "";
			    this.fn_search();
				break;

			case "mstDtlD": // 삭제
			    sBisInsChk = "";
			    this.fn_search();
				break;

		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/

//---------
// 신규
//---------
this.fn_insert = function()
{
	this.ds_evlCycle.clearData();
	this.ds_evlCycle.addRow();
	var sEvlYear = this.div_search.spn_evalYear.value;
	this.ds_evlCycle.setColumn(0, "EVL_YEAR", sEvlYear);

	this.ds_evlListDtl.clearData();

	this.div_work.cmb_evlCycle.set_readonly(false);
	this.div_work.cmb_evlCycle.set_cssclass("cmb_WF_Essential");
	this.div_work.cmb_evlCycle.setFocus();
}

//---------
// 삭제
//---------
this.fn_delete = function()
{
	if(this.gfn_message("confirm.삭제.진행", "선택한 평가년도에 대한 모든 관련 정보가 삭제됩니다.")){
		this.fn_transaction("mstDtlD");
	}
}


//---------
// 취소
//---------
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.취소여부")) {
		this.ds_evlListDtl.reset();				//DATASET ROLLBACK
		this.ds_evlListDtl.applyChange();		//DATASET COMMIT
		return;		
	}
}

//----------------------------------------------------
// Grid Head 클릭 이벤트 처리(Grid 데이터 정렬)
//----------------------------------------------------
this.div_work_grd_evlListDtl_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
    this.gfn_gridSort(obj, e); // Grid 데이터 정렬
}

//-----------------------------------------------------------
// ListMst EnterKey down시
//-----------------------------------------------------------
this.div_work_grd_evlListDtl_onkeydown = function(obj:Grid, e:nexacro.KeyEventInfo)
{
	if (e.keycode == 13) {
		var brlst = obj.moveToNextCell();
	}
}

//---------------------------------------
// 평가주기 ITEM 변경시
//---------------------------------------
this.div_work_cmb_evlCycle_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.fn_condset();
	//Dtl Search
	this.fn_search_dtl();
}

//---------------------------------------
// 평가차수생성 Button Clicked CallBack
//---------------------------------------
this.div_work_btn_odeC_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_evlCycle, this.ds_validation)) {
		return false;
	}
    

    //confirm
	if(this.gfn_message("confirm.삭제.진행", "선택한 평가년도에 대한 평가차수 및 관련 정보가 삭제됩니다.")){
	    // 기존 평가차수 일괄삭제
	    this.fn_delOdr();
	    // 평가주기 선행등록
		//this.fn_addOdr();

	}else{
		return;
	}
}

/***********************************************************************
* 사용자 정의 함수
************************************************************************/

//------------------------------------------------------
//  평가차수 기초자료 생성 - 기존차수 일괄삭제
//------------------------------------------------------
this.fn_delOdr = function()
{
	this.fn_transaction("beforeD");
}
//------------------------------------------------------
//  평가차수 기초자료 생성 - 평가주기선행등록
//------------------------------------------------------
this.fn_addOdr = function()
{
	sBisInsChk = "Y";
	this.fn_transaction("mstCUD");

}
//---------------------------------------
// Condition Set
//---------------------------------------
this.fn_condset = function()
{
	//초기화
	this.ds_condDtl.clearData();
	//Key Set
	this.ds_condDtl.addRow();
	this.ds_condDtl.setColumn(0, "EVL_YEAR", this.ds_evlCycle.getColumn(0, "EVL_YEAR"));
	this.ds_condDtl.setColumn(0, "EVL_CYCLE", this.ds_evlCycle.getColumn(0, "EVL_CYCLE"));

	this.fn_search_dtl();
}

//------------------------------------------------
//  기초자료 -> 평가차수 DataSet ( RowType Mod )
//-------------------------------------------------
this.fn_bsisCopy = function()
{
    var nRowCnt = this.ds_evlListTemp.rowcount;
	this.ds_evlListDtl.clearData();

	for(var i = 0 ; i < nRowCnt ; i++){
	       var nRow = this.ds_evlListDtl.addRow();
		   this.ds_evlListDtl.copyRow(nRow, this.ds_evlListTemp, i);
	}
	this.fn_transaction("dtlCUD");
}

//----------------------------------------------
// 평가연도 text 변경시 - ds_cond 에 강제set
//----------------------------------------------
this.div_search_spn_evalYear_ontextchanged = function(obj:Spin, e:nexacro.TextChangedEventInfo)
{
	this.ds_cond.setColumn(0, "EVL_YEAR", e.posttext);
	this.fn_search();
}
