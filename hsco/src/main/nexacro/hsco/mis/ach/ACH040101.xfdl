 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.sClosAt = "";
this.nRowPos = 0;
this.nSetInit = 0;

 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH040101_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
}

this.ACH040101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Combo에 사용되는 Dataset Load
	this.fn_loadCombo();
	// 검색조건 컴포넌트 초기값 부여
	this.fn_init_current();
	
	// Set 관점구분(고정)
// 	this.div_work.cmb_viewPointSeC.set_value("C");
// 	this.div_work.cmb_viewPointSeF.set_value("F");
// 	this.div_work.cmb_viewPointSeL.set_value("L");
// 	this.div_work.cmb_viewPointSeP.set_value("P");
	// 조회 이벤트 / 함수 호출
//    this.fn_search();
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);
    //this.fn_get_comn_cd();
}
//----------------------------------
// Get Current 평가차수
//----------------------------------
this.fn_init_current = function()
{
	this.ds_curInfo.clearData();
	this.fn_transaction("selectCurEvlOdrInfo");
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

//-----------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (공통코드)
//-----------------------------------------------------
 this.fn_loadCombo = function ()
{
	var comboParams = [
			["ds_viewPointSe", 		"ACH002", "1", ""]		// 관점구분
		];

	this.gfn_comboLoad(comboParams, this.fn_callbackAfterLoad);
}

//-----------------------------------------
// 공통 Callback 함수에서 호출할 함수
//-----------------------------------------
this.fn_callbackAfterLoad = function ()
{
}

//--------------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (Non-공통코드)
//--------------------------------------------------------
this.fn_loadComboNonCodeTbl = function (strDs, obj)
{
	var comboParams = [
		// 평가차수
		["ds_odrSe", 	"ds_cond", "", "", "hsco/mis/ach/ACHCommon/selectOdrList.do", 0]
	];
	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
}


//-----------------------------------------
// ( Non-공통코드) Callback 함수에서 호출할 함수
//-----------------------------------------
this.fn_callbackAfter = function (svcId)
{
	switch (svcId) {
		case "ds_odrSe" :
			this.div_search.sta_closeInfo.set_text("집계 기준일 : ");
			this.div_work.cmb_viewPointSeC.set_value("");
			this.div_work.cmb_viewPointSeF.set_value("");
			this.div_work.cmb_viewPointSeL.set_value("");
			this.div_work.cmb_viewPointSeP.set_value("");
			
			for(var i=1; i<=4; i++){
				var objC = eval("this.div_work.sta_C" + i);
				var objCR = eval("this.div_work.sta_C" + i + "R");
				var objF = eval("this.div_work.sta_F" + i);
				var objFR = eval("this.div_work.sta_F" + i + "R");
				var objL = eval("this.div_work.sta_L" + i);
				var objLR = eval("this.div_work.sta_L" + i + "R");
				var objP = eval("this.div_work.sta_P" + i);
				var objPR = eval("this.div_work.sta_P" + i + "R");
				
				// 초기화
				objC.set_text("");
				objCR.set_text("");
				
				objC.set_visible(false);
				objCR.set_visible(false);
				
				objF.set_text("");
				objFR.set_text("");
				
				objF.set_visible(false);
				objFR.set_visible(false);
				
				objL.set_text("");
				objLR.set_text("");
				
				objL.set_visible(false);
				objLR.set_visible(false);
				
				objP.set_text("");
				objPR.set_text("");
				
				objP.set_visible(false);
				objPR.set_visible(false);
				
			}
			
			if(this.ds_odrSe.rowcount > 0){
				// 집계기준일
				var curDate = this.ds_odrSe.getColumn(0, "CODE_CN");
				this.div_search.sta_closeInfo.set_text("집계 기준일 : "+curDate);
				
				if (nSetInit == 1){ // Load시 1회만 적용
					this.fn_setCurInfo("odrSe");
					nSetInit = 0;
				}
				
				this.fn_search();
			} else {
				this.gfn_message("info.처리불가", "해당년도의 등록된 평가차수가 없습니다.");
			}
			break;
	}
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/
//-------
// 조회
//-------
this.fn_search = function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}
	
	this.ds_refrnList.clearData();
	this.ds_colorList.clearData();
	this.fn_transaction("selectList");
	this.fn_transaction("selectAchivRtSctnList");	
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectList": // LIST 불러오기
			var sController   	= "hsco/mis/ach/ACH040101/selectList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_refrnList=output1";
			break;
			
		case "selectAchivRtSctnList": // 달성율구간Color LIST 불러오기
			var sController   	= "hsco/mis/ach/ACHCommon/selectAchivRtSctnList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_colorList=output1";
			break;			

		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "selectList": // LIST 불러오기
				this.fn_setViewPoint();
				this.fn_setIxContents();
				break;
				
			case "selectAchivRtSctnList": // 달성율구간Color LIST 불러오기
				break;				

			case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				//this.fn_loadComboNonCodeTbl();
				break;
		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
//-----------------------------------------------------------------------------
// Grid Head 클릭 이벤트 처리(Grid 데이터 정렬)
// Grid의 Head의 체크 버튼을 클릭하였을 때, 실 데이터의 체크버튼 처리
//-----------------------------------------------------------------------------
this.div_work_grd_list_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
	}else{
	    this.gfn_gridSort(obj, e); // Grid 데이터 정렬
	}
}

//------------------------------------------------------------------------
// Body의 체크 버튼이 눌러졌을 때, Head 부분의 체크 버튼을 꺼 줌.
//------------------------------------------------------------------------
this.div_work_grd_list_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.cell == 0) {
		obj.setCellProperty("Head", e.cell, "text", "0");
	}
}

//-----------------------------------------------------------
// ListMst EnterKey downUp 시
//-----------------------------------------------------------
this.div_work_grd_list_onkeyup = function(obj:Grid, e:nexacro.KeyEventInfo)
{
	if (e.keycode == 13) {
		var brlst = obj.moveToNextCell();
	}
}


//----------------------------------------------
// 평가연도 text 변경시 - ds_cond 에 강제set
//----------------------------------------------
this.div_search_spn_evalYear_ontextchanged = function(obj:Spin, e:nexacro.TextChangedEventInfo)
{
	
	this.ds_cond.setColumn(0, "EVL_YEAR", e.posttext);
	this.fn_loadComboNonCodeTbl();

}

//----------------------------------------------
// 평가차수 item 변경시 - ds_cond 에 강제set
//----------------------------------------------
this.div_search_cmb_odrSe_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.ds_cond.setColumn(0, "ODR_SE", e.postvalue);
	
	// 집계처리일자
	var curDate = this.ds_odrSe.getColumn(e.postindex, "CODE_CN");
	this.div_search.sta_closeInfo.set_text("집계 기준일 : "+curDate);
}

/***********************************************************************
* 사용자 정의 함수
************************************************************************/

//-------------------------------------------------------
// KEY 검색조건 컴포넌트 초기값(현채차수) 부여
//-------------------------------------------------------
this.fn_setCurInfo = function(strComp)
{
	if (this.ds_curInfo.rowcount ==  1 ){

		if (strComp == "evalYear"){
			var sCurEvlYear = this.ds_curInfo.getColumn(0,"CUR_EVL_YEAR");
			this.ds_cond.setColumn(0, "EVL_YEAR", sCurEvlYear);
			this.fn_loadComboNonCodeTbl();
			//this.div_search.spn_evalYear.set_value(sCurEvlYear);
			
		}else if (strComp == "odrSe"){
			var sCurOdrSe 	= this.ds_curInfo.getColumn(0,"CUR_ODR_SE");
			this.div_search.cmb_odrSe.set_value(sCurOdrSe);
			
			// 집계처리일자
			var fRow = this.ds_odrSe.findRow("CODE", sCurOdrSe);
			var curDate = this.ds_odrSe.getColumn(fRow, "CODE_CN");
			this.div_search.sta_closeInfo.set_text("집계 기준일 : "+curDate);
		}
	}
}

//-------------------------------------------------------
// Contents 동적SET
//-------------------------------------------------------
this.fn_setIxContents = function()
{
	for (var i = 0; i < this.ds_refrnList.rowcount; i++){

		//Get
		var sGrpCode = this.ds_refrnList.getColumn(i,"GRP_CODE");

		if( this.comUtils.isNotNull(sGrpCode) ){
			if(sGrpCode.substr(1,1) <= 4){		// 관점별 최대 4개의 전략목표를 표시할 수 있다. 4개이상 필요시 수정필요
				var sIxnm = this.ds_refrnList.getColumn(i,"IX_NM");
				var sAchivRT = this.ds_refrnList.getColumn(i,"ACHIV_RT");
				
				var obj = eval("this.div_work.sta_" + sGrpCode);
				var objR = eval("this.div_work.sta_" + sGrpCode + "R");
				
				this.fn_setCss(objR, sAchivRT);
				
				//Set
				obj.set_text(sIxnm);
				objR.set_text(sAchivRT + '%');
				
				obj.set_visible(true);
				objR.set_visible(true);
			}
		}
	}
}
//-------------------------------------------------------
// css 변경
//-------------------------------------------------------
this.fn_setCss = function(objR, sAchivRT)
{
	var nAchivRT = nexacro.toNumber(sAchivRT);	
// 	var fRow = this.ds_colorList.findRowExpr("BEGIN_SCTN <= '" + sAchivRT + "' && END_SCTN >= '" + sAchivRT  + "'", 0,  this.ds_colorList.getRowCount());
// 	if(fRow > -1){
// 		var sColor = this.ds_colorList.getColumn(fRow,"APPN_COLOR");
// 		objR.set_cssclass(sColor);	
// 	}

	// 달성율에 맞는 구간검색
	var nRow = -1;
	for (var i = 0; i < this.ds_colorList.rowcount; i++){	
	    
		var nBegin = nexacro.toNumber( this.ds_colorList.getColumn(i,"BEGIN_SCTN") );
		var nEnd   = nexacro.toNumber( this.ds_colorList.getColumn(i,"END_SCTN") );	
		if ( nBegin <= nAchivRT && nEnd >= nAchivRT) {		
			var nRow = i;			
		}	
	}
	
	// 달성율에 맞는 구간에 해당하는 설정된 컬러 적용
	if ( nRow > -1 ){
		var sColor = this.ds_colorList.getColumn(nRow,"APPN_COLOR");
		trace(objR);
		//objR.set_cssclass(sColor);
		var cssCode=null;
		
		switch(sColor){
			case 'color1':
				cssCode = "#e1f5fe";
			break;
			case 'color2':
				cssCode = "#b3e5fc";
			break;
			case 'color3':
				cssCode = "#81d4fa";
			break;
			case 'color4':
				cssCode = "#4fc3f7";
			break;
			case 'color5':
				cssCode = "#29b6f6";
			break;
			case 'color6':
				cssCode = "#03a9f4";
			break;
			case 'color7':
				cssCode = "#039be5";
			break;
			case 'color8':
				cssCode = "#0288d1";
			break;
			case 'color9':
				cssCode = "#0277bd";
			break;
			case 'color10':
				cssCode = "#01579b";
			break;
		}
		
		objR.style.set_align("right middle");
		objR.style.set_background(cssCode.toString());
		objR.style.set_border("1 solid #cdcdcd");
		objR.style.set_bordertype("round 4 4 righttop rightbottom");
		objR.style.set_color("#000000");
		objR.style.set_font("bold 11 Gulim");
		objR.style.set_padding("0 0 0 0");

	}
}

//-------------------------------------------------------
// 관점명 셋팅
//-------------------------------------------------------
this.fn_setViewPoint = function()
{
    var nRow = this.ds_refrnList.findRow("VIEWPOINT_SE", "C" );
    if(nRow > -1){
		this.div_work.cmb_viewPointSeC.set_value("C");
    }
    
    nRow = this.ds_refrnList.findRow("VIEWPOINT_SE", "F" );
    if(nRow > -1){
		this.div_work.cmb_viewPointSeF.set_value("F");
    }
    
    nRow = this.ds_refrnList.findRow("VIEWPOINT_SE", "L" );  
    if(nRow > -1){
		this.div_work.cmb_viewPointSeL.set_value("L");
    }
  
    nRow = this.ds_refrnList.findRow("VIEWPOINT_SE", "P" );
    if(nRow > -1){
		this.div_work.cmb_viewPointSeP.set_value("P");
    }
}]]></Script>
