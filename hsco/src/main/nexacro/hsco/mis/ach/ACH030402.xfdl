    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACH030402
 * 화면명   : 마감처리
 * 화면설명 : 현재평가차수의 마감여부를 설정등록한다.
 * 작성일   : 2015.09.16
 * 작성자   : 박병일
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ***********************************************************************/
include "lib::comInclude.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.sClosAt = "";
this.nRowPos = 0;

 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH030402_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
}

this.ACH030402_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();
	this.fn_search();
}

//----------------------------------
// Form/Dataset 오브젝트 초기화
//----------------------------------
this.fn_init_form = function()
{
	this.gfn_initForm(this);
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

//-----------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (공통코드)
//-----------------------------------------------------
 this.fn_loadCombo = function ()
{
    // 평가주기구분
	this.gfn_loadCodeCombo(this, this.div_work.cmb_evlCycle, "ds_gubun01", "ACH019", "", "Y", "", "N", "");
    this.gfn_loadCodeComboWithDs(this, "", this.ds_evlCycle_se, "ACH019", "", "", " ", "N", "");
    // 평가차수구분
	this.gfn_loadCodeComboWithDs(this, "", this.ds_odrSe, "ACH010", "", "Y", "", "N", "");
    // 기준월
	this.gfn_loadCodeComboWithDs(this, "", this.ds_seqSe, "ACH024", "", "Y", "", "N", "");
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/
//-------
// 조회
//-------
this.fn_search = function()
{
	this.ds_curOdrInfo.clearData();
	this.fn_transaction("selectCurOdr");
}

//--------------------------
// 조회 - 평가차수 목록
//--------------------------
this.fn_searchOdrList = function()
{
    this.ds_evlOdrList.clearData();
	this.fn_transaction("selectOdrList");
}

//--------------------------
// 조회 - 집계목록
//--------------------------
this.fn_searchSmList = function()
{
    this.ds_smScoreList.clearData();
	this.fn_transaction("selectSmScoreList");
}

//--------------------------
// 조회 - 집계상세내역
//--------------------------
this.fn_searchSmDtlList = function()
{
    this.ds_smScoreDtlList.clearData();
	this.fn_transaction("selectSmScoreDtlList");
}

//-------
// 저장
//-------
this.fn_save = function()
{
	if(Ex.util.isUpdated(this.ds_evlOdrList)){
		if(this.gfn_message("confirm.저장.여부"))	{
		    nRowPos = this.ds_evlOdrList.rowposition;
			this.fn_transaction("mainCUD");
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
}

//------------------
// 저장- 적용여부
//------------------
this.fn_save_app = function()
{
	if(Ex.util.isUpdated(this.ds_smScoreList)){
		nRowPos = this.ds_smScoreList.rowposition;	
		this.fn_transaction("dtlCUD");

	}else{
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectCurOdr": // 현재차수 Info 불러오기
			var sController   	= "hsco/mis/ach/ACH030402/selectCurOdr.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curOdrInfo=output1";
			break;

		case "selectOdrList": // 평가차수 List불러오기
			var sController   	= "hsco/mis/ach/ACH030402/selectOdrList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_evlOdrList=output1";
			break;

		case "mainCUD": // 저장하기( main : 마감)
			var sController   	= "hsco/mis/ach/ACH030402/mainCUD.do";
			var sInDatasets   	= "input1=ds_evlOdrList:U";
			var sOutDatasets  	= "";
			break;
			
		case "selectSmScoreList": // 집계목록 불러오기
			var sController   	= "hsco/mis/ach/ACH030402/selectSmScoreList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_smScoreList=output1";
			break;	
					
		case "selectSmScoreDtlList": // 집계상세내역 List불러오기
			var sController   	= "hsco/mis/ach/ACH030402/selectSmScoreDtlList.do";
			var sInDatasets   	= "input1=ds_condDtl";
			var sOutDatasets  	= "ds_smScoreDtlList=output1";
			break;	
								
		case "insertOrgnztLastRankSm": // 집계목록및내역생성
			var sController   	= "hsco/mis/ach/ACH030402/insertOrgnztLastRankSm.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;	
										
		case "dtlCUD": // 저장하기 (집계목록)
			var sController   	= "hsco/mis/ach/ACH030402/dtlCUD.do";
			var sInDatasets   	= "input1=ds_smScoreList:U";
			var sOutDatasets  	= "";
			break;										
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectCurOdr": // 현재차수 Info 불러오기
			    this.fn_searchOdrList();
				break;

			case "selectOdrList": // 평가 차수 List 불러오기
				if(this.ds_evlOdrList.rowcount > 0 ){
					this.ds_evlOdrList.set_rowposition(nRowPos);
					nRowPos = 0
				}			
				break;

			case "mainCUD": // 저장하기( main : 마감)
				this.gfn_message("success.처리.성공");			
				this.fn_search();
				break;
				
			case "selectSmScoreList": // 집계목록 불러오기
				if(this.ds_smScoreList.rowcount > 0 ){
					this.ds_smScoreList.set_rowposition(nRowPos);
					nRowPos = 0
				}	
				break;
							
			case "selectSmScoreDtlList": // 집계상세내역 List 불러오기
				break;
												
			case "insertOrgnztLastRankSm": // 집계목록및내역생성
				this.gfn_message("success.처리.성공");
				this.fn_searchSmList();				
				break;	
															
			case "dtlCUD": // 저장하기( 집계목록)
				this.fn_searchSmList();
				break;															

		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

