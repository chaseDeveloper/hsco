/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/

//-------
// 조회
//-------
this.fn_search = function()
{
    this.ds_qKpiListMst.clearData();
    this.ds_fileList.clearData();
    this.ds_evlListSub.clearData();
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}    
	this.fn_transaction("selectListMst");
}

//-------------
// Dtl File Search
//-------------
this.fn_search_file = function()
{
	this.ds_fileList.clearData();
	this.fn_transaction("selectFileList");
}

//-------------
// Dtl Search
//-------------
this.fn_search_dtl = function()
{
	var nCurRow = this.ds_evlList.rowposition;
	//초기화
	this.ds_condDtl.clearData();
	//Key Set
	this.ds_condDtl.addRow();
	this.ds_condDtl.setColumn(0, "EVL_YEAR", this.ds_evlList.getColumn(nCurRow, "EVL_YEAR"));
	this.ds_condDtl.setColumn(0, "ODR_SE", this.ds_evlList.getColumn(nCurRow, "ODR_SE"));
	this.ds_condDtl.setColumn(0, "IX_CODE", this.ds_evlList.getColumn(nCurRow, "IX_CODE"));
	this.ds_condDtl.setColumn(0, "ORGNZT_CODE", this.ds_evlList.getColumn(nCurRow, "ORGNZT_CODE"));
	this.ds_condDtl.setColumn(0, "SEQ", this.ds_cond.getColumn(0, "SEQ")); //ADD
	
	this.ds_evlListSub.clearData();
	this.fn_transaction("selectListDtl");
}



/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectListMst": // ListMst 불러오기
			var sController   	= "hsco/mis/ach/ACH030305/selectListMst.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_evlList=output1";
			break;

		case "selectListDtl": // ListDtl 불러오기
			var sController   	= "hsco/mis/ach/ACH030305/selectListDtl.do";
			var sInDatasets   	= "input1=ds_condDtl";  //ds_condDtl
			var sOutDatasets  	= "ds_evlListSub=output1";
			break;

		case "selectEvlScoreList": // GET 평가점수목록
			var sController   	= "hsco/mis/ach/ACHCommon/selectEvlScoreList.do";  //ACHCommon
			var sInDatasets   	= "input1=ds_cond";  //ds_cond
			var sOutDatasets  	= "ds_evlScoreList=output1";
			break;

		case "selectFileList":		// 첨부파일 조회
			var sController   	= "hsco/mis/ach/ACH030305/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond_file";
			var sOutDatasets  	= "ds_fileList=output1";
			break;

		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectListMst": 		// ListMst 불러오기
				if(this.ds_qKpiListMst.rowcount > 0 ){
					this.ds_qKpiListMst.set_rowposition(nRowPos);
					nRowPos = 0;
				}
				break;

			case "selectListDtl": 		// List Dtl 불러오기
				break;

			case "selectEvlScoreList":	// GET 평가점수목록
				break;

			case "selectFileList":		// 조회(파일정보)
				this.fn_search_dtl();
				break;

			case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				this.fn_loadComboNonCodeTbl();
				break;

		}
	}
}

