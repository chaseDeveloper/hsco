 **********************************************************************/

//-------
// 조회
//-------
this.fn_search = function()
{
	this.ds_kpiInfo.clearData();
	this.fn_transaction("mainSearchList");
}

//-------
// 저장
//-------
this.fn_save = function()
{
	// Dataset 수정여부 체크
	if ( !Ex.util.isUpdated(this.ds_kpiInfo) ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;

	}else{

		if(this.gfn_message("confirm.저장.여부"))	{
			//Validation 체크
			if(!this.gfn_checkValidation_NoUpdChk(this.ds_kpiInfo, this.ds_validation)){
				return;
			}
			//Validation 체크 (미사용시)
			if (this.div_detail01.chk_useAt.value == "0" ) {
				if(!this.gfn_checkValidation_NoUpdChk(this.ds_kpiInfo, this.ds_validationUn)){
					return;
				}
			}
			
			//Get KeyCode
			this.sKeyCode = this.ds_kpiInfo.getColumn(0, "IX_CODE");
			
			if(this.ds_ixCode.findRow("CODE", this.ds_kpiInfo.getColumn(0, "IX_CODE"))>=0){
				this.gfn_message("info.처리불가","지표코드가 중복되었습니다.");
				return;
			}else{
				this.fn_transaction("mainCUD");
			}
		}else{
			return;
		}
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "mainSearchList":  // Info 조회
			var sController   	= "hsco/mis/ach/ACH020102P1/mainSearchList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_kpiInfo=output1";
			break;

		case "mainCUD": // info 저장
			var sController   	= "hsco/mis/ach/ACH020102P1/mainCUD.do";
			var sInDatasets   	= "input1=ds_kpiInfo:U"
			var sOutDatasets  	= "";
			break;
			
		case "ixCodeList": // info 저장
			var sController   	= "hsco/mis/ach/ACHCommon/selectIxcodeAllList.do";
			var sInDatasets   	= "input1=ds_cond"
			var sOutDatasets  	= "ds_ixCode=output1"
			break;

		case "firstInsert": // 초기값 셋팅
			var sController   	= "hsco/mis/ach/ACH020102P1/firstInsert.do";
			var sInDatasets   	= "input1=ds_cond"
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {
			case "mainSearchList":
				var sIxSe = this.ds_kpiInfo.getColumn(0, "IX_SE");
				
				if(sMode == "R") {
					this.fn_readOnlyOption();
				}else{
					var sIxSe = this.ds_kpiInfo.getColumn(0, "IX_SE");
					
					//지표구분이 "전략목표"일 경우에만 관점등록 가능
					if (sIxSe == "002"){
						this.div_detail01.cmb_viewPointSe.set_cssclass('cmb_WF_Essential');
						this.div_detail01.cmb_upIxCode.set_cssclass('cmb_WF_Readonly');
						this.div_detail01.cmb_upIxCode.set_readonly(true);
						this.div_detail01.cmb_viewPointSe.set_enable(true);
					}else{
						this.div_detail01.cmb_viewPointSe.set_cssclass('');		
						this.div_detail01.cmb_upIxCode.set_cssclass('cmb_WF_Essential');
						this.div_detail01.cmb_upIxCode.set_readonly(false);
						this.div_detail01.cmb_viewPointSe.set_enable(false);
					}
				}
				this.ds_cond.setColumn(0, "IX_SE", sIxSe);
				this.fn_loadComboNonCodeTbl();
				break;
			case "mainCUD":
				if(this.ds_cond.rowcount == 0 ){
					var nRow = this.ds_cond.addRow();
					this.ds_cond.setColumn(nRow, "IX_CODE", this.sKeyCode);
				}
			    this.close();
				break;
				
			case "ixCodeList":
				if(this.ds_ixCode.rowcount == 0 ){
					this.fn_transaction("firstInsert");
				}
				break;
			case "firstInsert":
				this.fn_loadComboNonCodeTbl();
				break;
		}
	}
}

