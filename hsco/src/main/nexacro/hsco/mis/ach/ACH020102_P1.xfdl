    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACH020102_P1
 * 화면명   : 성과지표 관리[상세]
 * 화면설명 : 성과관리에서 사용되는 성과지표의 세부속성을 조회 및 등록한다.
 * 작성일   : 2015.07.23
 * 작성자   : 박병일
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */

/***********************************************************************
 * Script Include
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 this.sMode = "";
 this.sKeyCode = "";

 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
 this.ACH020102_P1_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
}

this.ACH020102_P1_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
    sMode = this.parent.param1;
	// Combo에 사용되는 Dataset Load
	this.fn_loadCombo();
    // 신규모드일때
	if (sMode == "N"){		
		
		this.btn_Del.set_visible(false);	//신규시 삭제버튼 unvisible 처리
		this.div_detail.btn_print_sub.set_visible(false); //신규시 정의서출력버튼 unvisible 처리
		this.ds_kpiInfo.clearData();
	    //var nRow = this.ds_cond.addRow();
		
		var sEvlYear 	= this.parent.param2;
		var sOdrSe 		= this.parent.param3;
		this.ds_cond.setColumn(0, "EVL_YEAR", sEvlYear);
		this.ds_cond.setColumn(0, "ODR_SE", sOdrSe);
		
		//After ds_cond Setting
		
		this.div_detail.edt_ixCode.set_readonly(false);	//key 지표코드
		this.fn_insert();
		
		
	//상세모드일때
	}else if(sMode == "D") {
		
		var sEvlYear 	= this.parent.param2;
		var sOdrSe 		= this.parent.param3;
		var sIxCode 	= this.parent.param4;
		var sIxSe 		= this.parent.param5;
		
	    //var nRow = this.ds_cond.addRow();
		this.ds_cond.setColumn(0, "EVL_YEAR", sEvlYear);
		this.ds_cond.setColumn(0, "ODR_SE", sOdrSe);
		this.ds_cond.setColumn(0, "IX_CODE", sIxCode);
		this.ds_cond.setColumn(0, "IX_SE", sIxSe);
		
		//After ds_cond Setting
		this.fn_loadComboNonCodeTbl();
		
		this.div_detail.edt_ixCode.set_readonly(true);	//key 지표코드
		this.fn_search();
	//조회모드일때
	}else if(sMode == "R") {
		
		this.btn_Del.set_visible(false);	//조회모드시 삭제버튼 unvisible 처리
		this.btn_Save.set_visible(false);	//조회모드시 저장버튼 unvisible 처리
		
		var sEvlYear 	= this.parent.param2;
		var sOdrSe 		= this.parent.param3;
		var sIxCode 	= this.parent.param4;
		var sIxSe 		= this.parent.param5;
		
	    //var nRow = this.ds_cond.addRow();
		this.ds_cond.setColumn(0, "EVL_YEAR", sEvlYear);
		this.ds_cond.setColumn(0, "ODR_SE", sOdrSe);
		this.ds_cond.setColumn(0, "IX_CODE", sIxCode);
		this.ds_cond.setColumn(0, "IX_SE", sIxSe);
		
		//After ds_cond Setting
		this.fn_loadComboNonCodeTbl();
		
		//this.div_detail.edt_ixCode.set_readonly(true);	//key 지표코드
		
