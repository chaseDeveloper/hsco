// 신규
this.fn_insert = function()
{
	var nRow = this.ds_kpiInfo.addRow();
	// Set Key
	var sEvlYear 	= this.parent.param2;
	var sOdrSe 		= this.parent.param3;

	this.ds_kpiInfo.setColumn(nRow, "EVL_YEAR", sEvlYear);
	this.ds_kpiInfo.setColumn(nRow, "ODR_SE", sOdrSe);

	this.div_detail01.chk_useAt.set_value('1'); //add
	this.div_detail.edt_ixCode.setFocus(true);
	
	this.fn_transaction("ixCodeList");
}

// 삭제
this.fn_delete = function()
{
	if (this.gfn_message("confirm.삭제여부") ) {
		var nCurRow  = this.ds_kpiInfo.rowposition;
		this.ds_kpiInfo.deleteRow(nCurRow);
		this.fn_transaction("mainCUD");
	}
}


// 취소
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.변경.취소.여부")) {
		this.ds_kpiInfo.reset();			//DATASET ROLLBACK
		this.ds_kpiInfo.applyChange();		//DATASET COMMIT
		return;
	}
}


