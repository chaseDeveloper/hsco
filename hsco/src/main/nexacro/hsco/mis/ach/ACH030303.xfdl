    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACH030303
 * 화면명   : 비계량지표 평가등록(조직별)
 * 화면설명 : 비계량 성과지표에 대한 조직별 평가결과를 평가자별로 등록한다.
 * 작성일   : 2015.09.02
 * 작성자   : 박병일
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.sClosAt = "";
this.nRowPos = 0;
this.nSetInit = 0;

this.checkDs = [this.ds_evlList];
this.File_UpDownManager = null;
 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH030303_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	
	this.fn_init_form();
	
	this.fn_init_dataset();
}

this.ACH030303_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Combo에 사용되는 Dataset Load
	this.fn_loadCombo();
	// 검색조건 컴포넌트 초기값 부여
	this.fn_init_current();
    this.div_search.cmb_evlManNo.set_value('1'); //temp
}

//----------------------------------
// Form/Dataset 오브젝트 초기화
//----------------------------------
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);
	
	// 파일 메니저 설정
	var fileConfig = {
		//useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "ACH");
	this.File_UpDownManager.init(fileConfig);
}

//----------------------------------
// Get Current 평가차수
//----------------------------------
this.fn_init_current = function()
{
	this.ds_curInfo.clearData();
	this.fn_transaction("selectCurEvlOdrInfo");
}

//-----------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (공통코드)
//-----------------------------------------------------
 this.fn_loadCombo = function ()
{
    // ----------------------------------------- Condition Area -----------------------------------------------
	// 기준월
	this.gfn_loadCodeComboWithDs(this, this.div_search.cmb_seq, this.ds_seqSe,   "ACH024", "", "Y", "", "N", "");
	
	// ----------------------------------------- Dataset to Grid Area -----------------------------------------
	// 평가그룹구분
	this.gfn_loadCodeComboWithDs(this, "", this.ds_evlGrpSe, "ACH013", "", "Y", " ", "N", "");
	// 평가등급구분
	this.gfn_loadCodeComboWithDs(this, "", this.ds_evlGradSe, "ACH016", "", "Y", " ", "N", "");
}


//--------------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (Non-공통코드)
//--------------------------------------------------------
this.fn_loadComboNonCodeTbl = function (strDs, obj)
{
	var comboParams = [
			["ds_odrSe", 	"ds_cond", "", "", "hsco/mis/ach/ACHCommon/selectOdrList.do", 0]
			,["ds_evlCycle", "ds_cond", "", "", "hsco/mis/ach/ACHCommon/selectEvlCycleSingle.do", 0]
		];

	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
	
}

this.fn_loadComboNonCodeTbl2 = function (strDs, obj)
{
	var comboParams = [
			["ds_orgnztSe", "ds_cond", "1", "전체", "hsco/mis/ach/ACHCommon/selectOrgnztList.do", 0]
		];

	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
	this.fn_transaction("getQleEvlManList");
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: combo에 대한 callback 함수 이후에 처리되어야 할 코딩 기술
 * params  		: svcId - Transaction Service ID.
 *				  ※ 반드시, 데이터를 담아 올 dataset명(예 : "ds_comCode03")을 사용해야 한다.
 * return Type 	: None
 * 사용법		: this.fn_init_dataset();
 * ※ 함수의 특성상, comboParams의 개수만큼 호출되므로, svcId로 분기하여 처리해야 한다.
 */
this.fn_callbackAfter = function (svcId)
{
	switch (svcId) {
		case "ds_odrSe" :
			if (nSetInit == 1){ // Load시 1회만 적용
				this.fn_setCurInfo("odrSe");
				nSetInit = 0;
			}
			this.fn_loadComboNonCodeTbl2();
			break;

		case "ds_orgnztSe" :
			this.div_search_cmb_orgnztSe_onitemchanged();
			//this.fn_search();
			break;
	}
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/

//-------
// 조회
//-------
this.fn_search = function()
{
    this.ds_evlList.clearData();
    this.ds_fileList.clearData();
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}    
	this.fn_transaction("selectList");
}

//-------------
// Dtl File Search
//-------------
this.fn_search_file = function()
{
	this.ds_fileList.clearData();
	this.fn_transaction("selectFileList");
}

//-------
// 저장
//-------
this.fn_save = function()
{
	if(Ex.util.isUpdated(this.ds_evlList)){
		if(this.gfn_message("confirm.저장.여부"))	{
		    nRowPos = this.ds_evlList.rowposition;
			this.fn_transaction("dtlCUD");
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
}

//----------------------------
// GET 평가점수목록  조회
//----------------------------
this.fn_getEvlScoreList = function()
{
	this.fn_transaction("selectEvlScoreList");
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectList": 			// List 불러오기
			var sController   	= "hsco/mis/ach/ACH030303/selectList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_evlList=output1";
			break;

		case "dtlCUD": 				// 저장하기( Dtl )
			var sController   	= "hsco/mis/ach/ACH030303/dtlCUD.do";
			var sInDatasets   	= "input1=ds_evlList:U";
			var sOutDatasets  	= "";
			break;

		case "selectEvlScoreList": // GET 평가점수목록
			var sController   	= "hsco/mis/ach/ACHCommon/selectEvlScoreList.do";  //ACHCommon
			var sInDatasets   	= "input1=ds_cond";  //ds_cond
			var sOutDatasets  	= "ds_evlScoreList=output1";
			break;

		case "selectFileList":      // 첨부파일 조회
			var sController   	= "hsco/mis/ach/ACH030303/selectFileList.do";
			var sInDatasets   	= "input1=ds_condFile";
			var sOutDatasets  	= "ds_fileList=output1";
			break;

		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;
		
		case "getQleEvlManList":
			this.ds_condMan.clearData();
			var nRow = this.ds_condMan.addRow();
			
			this.ds_condMan.setColumn(nRow, "EVL_YEAR", this.ds_cond.getColumn(0, "EVL_YEAR"));
			this.ds_condMan.setColumn(nRow, "ODR_SE", this.ds_cond.getColumn(0, "ODR_SE"));

			var sController   	= "hsco/mis/ach/ACH030303/getQleEvlManList.do";
			var sInDatasets   	= "input1=ds_condMan";
			var sOutDatasets  	= "ds_qleEvlMan=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectList": // List 불러오기
				this.fn_getEvlScoreList();

				if(this.ds_evlList.rowcount > 0 ){
					this.ds_evlList.set_rowposition(nRowPos);
					nRowPos = 0;
				}
				break;

			case "dtlCUD": // 저장하기( Dtl )
				this.gfn_message("success.처리.성공");
			    this.fn_search();
				break;

			case "selectEvlScoreList":	// GET 평가점수목록
				break;

			case "selectFileList":      // 첨부파일 조회
				break;

			case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				this.fn_loadComboNonCodeTbl();
				break;
			case "getQleEvlManList":
				break;
		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

