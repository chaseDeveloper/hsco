//----------------------------------------------
this.div_search_cmb_orgnztSe_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
//	this.ds_cond.setColumn(0, "ORGNZT_CODE", e.postvalue);

	var evlCycle = this.ds_evlCycle.getColumn(0, "EVL_CYCLE");
	var odrSe = this.ds_cond.getColumn(0,"ODR_SE");
	
	this.div_search.cmb_seq.set_enableevent(false);
	this.ds_seqSe.clearData();
	switch (evlCycle) {
		case "1": // 1주기
		
			for (var i = 1; i <= 2; i++){
				var nRow = this.ds_seqSe.addRow();
				if (i == 1){
					var sMMcode = "6";
				}else if (i == 2){
					var sMMcode = "12";
				}
				this.ds_seqSe.setColumn(nRow, "CODE", sMMcode);
				this.ds_seqSe.setColumn(nRow, "CODE_NM", sMMcode + "월");
			}
			break;

		case "2": // 2주기
			if(odrSe == '1'){
				var nRow = this.ds_seqSe.addRow();
				
				this.ds_seqSe.setColumn(nRow, "CODE", "6");
				this.ds_seqSe.setColumn(nRow, "CODE_NM", "6월");
			}
			
			if(odrSe == '2'){
				var nRow = this.ds_seqSe.addRow();
				
				this.ds_seqSe.setColumn(nRow, "CODE", "12");
				this.ds_seqSe.setColumn(nRow, "CODE_NM", "12월");
			}
			break;

		case "4": // 4주기
			if(odrSe == '1'){
				var nRow = this.ds_seqSe.addRow();
				
				this.ds_seqSe.setColumn(nRow, "CODE", "3");
				this.ds_seqSe.setColumn(nRow, "CODE_NM", "3월");
			}
			
			if(odrSe == '2'){
				var nRow = this.ds_seqSe.addRow();
				
				this.ds_seqSe.setColumn(nRow, "CODE", "6");
				this.ds_seqSe.setColumn(nRow, "CODE_NM", "6월");
			}
			
			if(odrSe == '3'){
				var nRow = this.ds_seqSe.addRow();
				
				this.ds_seqSe.setColumn(nRow, "CODE", "9");
				this.ds_seqSe.setColumn(nRow, "CODE_NM", "9월");
			}
			
			if(odrSe == '4'){
				var nRow = this.ds_seqSe.addRow();
				
				this.ds_seqSe.setColumn(nRow, "CODE", "12");
				this.ds_seqSe.setColumn(nRow, "CODE_NM", "12월");
			}
			break;

		case "12": // 12주기
			var nRow = this.ds_seqSe.addRow();
			this.ds_seqSe.setColumn(0, "CODE", odrSe);
			this.ds_seqSe.setColumn(0, "CODE_NM", odrSe + "월");
			break;
	}
	this.ds_cond.setColumn(0, "SEQ", this.ds_seqSe.getColumn(0, "CODE"));
	this.div_search.cmb_seq.set_enableevent(true);
	
	this.fn_search();
}

