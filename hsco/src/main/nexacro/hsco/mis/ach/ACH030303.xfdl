//------------------------------------
// List Item 변경시
//------------------------------------
this.ds_evlList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 평가등급 Value 변경시
	if(e.columnid == "EVL_GRAD"){

		var sModGrad = e.newvalue;
		var nRow = e.row;
		var nFindRow = this.ds_evlScoreList.findRowExpr("EVL_GRAD == '" + sModGrad + "'" );

		if (nFindRow == -1 ){
		    this.gfn_message("errors.없음.요청","해당 등급의 등록된 기준점수가|등급 설정 정보를");
		    obj.set_updatecontrol(false);
			obj.setColumn(e.row, e.columnid, e.oldvalue);
			obj.set_updatecontrol(true);
			return false;

		}else{
			var nEvlScore 	= 0;
			var nEvlWghval 	= 0;
			var nTempCalc 	= 0;

			this.ds_evlList.setColumn(nRow, "EVL_SCORE", this.ds_evlScoreList.getColumn(nFindRow,"EVL_SCORE"));

			nEvlScore 	= this.ds_evlList.getColumn(nRow,"EVL_SCORE");
			nEvlWghval 	= this.ds_evlList.getColumn(nRow, "EVL_WGHTVAL");

			var nTempCalc = nEvlScore * nEvlWghval / 100 ;

			this.ds_evlList.setColumn(nRow, "RSLT_SCORE_C", nTempCalc );
		}
	}
}

