this.fn_setCurInfo = function(strComp)
{
	if (this.ds_curInfo.rowcount ==  1 ){

		if (strComp == "evalYear"){
			var sCurEvlYear = this.ds_curInfo.getColumn(0,"CUR_EVL_YEAR");
			this.div_search.spn_evalYear.set_value(sCurEvlYear);

		}else if (strComp == "odrSe"){
			var sCurOdrSe 	= this.ds_curInfo.getColumn(0,"CUR_ODR_SE");
			this.div_search.cmb_odrSe.set_value(sCurOdrSe);
		}
	}
}


/*
 * 함수명  	: ds_atchListDtl_onrowposchanged
 * 설명 		: 실적첨부내역 row 변경 후 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_atchListDtl_onrowposchanged(obj,e);
 */
this.ds_atchListDtl_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_cond_file.clearData();
	this.ds_cond_file.addRow();
	
	if(this.ds_atchListDtl.rowposition > -1){
		this.ds_cond_file.setColumn(0, "FILE_SN", this.ds_atchListDtl.getColumn(this.ds_atchListDtl.rowposition, "ATCH_FID"));
		this.ds_fileList.clearData();
		
		var cnfirmAt = obj.getColumn(e.newrow, "CNFIRM_AT");
		if(cnfirmAt == '1'){
			this.div_work.btn_delFiles.set_enable(false);
			this.div_work.btn_uploadFiles.set_enable(false);
		} else {
			this.div_work.btn_delFiles.set_enable(true);
			this.div_work.btn_uploadFiles.set_enable(true);
		}
		
		this.fn_transaction("selectFileList");
	}
}


