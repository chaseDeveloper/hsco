}

//-------
// 저장
//-------
this.fn_save = function()
{
	if(Ex.util.isUpdated(this.ds_atchListDtl)){
		if(this.ds_fileList.rowcount > 0){
			if(this.gfn_message("confirm.저장.여부"))	{
				this.nRowPos = this.ds_qKpiListMst.rowposition;
				this.nRowPos2 = this.ds_mmList.rowposition;
				this.fn_transaction("dtlCUD");
			}
		}else{
			this.gfn_message("info.처리불가", "실적자료 첨부파일을 1건이상 등록해 주세요.");
			return false;
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectListMst": 		// ListMst 불러오기
			var sController   	= "hsco/mis/ach/ACH030301/selectListMst.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_qKpiListMst=output1";
			break;

		case "selectListDtl": 		// ListDtl 불러오기
			var sController   	= "hsco/mis/ach/ACH030301/selectListDtl.do";
			var sInDatasets   	= "input1=ds_condDtl";  //ds_condDtl
			var sOutDatasets  	= "ds_atchListDtl=output1";
			break;

		case "dtlCUD": 				// 저장하기( Dtl )
			var sController   	= "hsco/mis/ach/ACH030301/dtlCUD.do";
			var sInDatasets   	= "input1=ds_atchListDtl:A";
			var sOutDatasets  	= "";
			break;
		case "selectFileList":		// 첨부파일 조회
			var sController   	= "hsco/mis/ach/ACH030301/selectFileList.do";
			var sInDatasets   	= "input1=ds_cond_file";
			var sOutDatasets  	= "ds_fileList=output1";
			break;

		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;
		case "saveTemp": // 파일업로드 전 현재 row의 기본정보 생성
			var sController   	= "hsco/mis/ach/ACH030301/saveTemp.do";
			var sInDatasets   	= "input1=ds_temp";
			var sOutDatasets  	= "";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectListMst": // ListMst 불러오기
				
				//Dtl Search
				if(this.ds_qKpiListMst.rowcount > 0 ){
					if(this.nRowPos > -1 ){
						this.ds_qKpiListMst.set_enableevent(false);
						this.ds_qKpiListMst.set_rowposition(this.nRowPos);
						this.ds_qKpiListMst.set_enableevent(true);
						nCurRow = this.nRowPos;
						this.nRowPos = -1;
					}
				}
				break;

			case "selectListDtl": // List Dtl 불러오기
				break;

			case "dtlCUD": // 저장하기( Dtl )
			    this.fn_search();
			    this.gfn_message("success.처리.성공");
				break;
			case "selectFileList":		// 조회(파일정보)
				this.ds_atchListDtl.set_updatecontrol(false);
				var nRow = this.ds_atchListDtl.rowposition;
				var rowCnt = this.ds_fileList.rowcount;
					if(rowCnt > 0){
						var fileNm = this.ds_fileList.getColumn(0, "ORGINL_FILE_NM");
						var tempFileNm;
						
						if(rowCnt > 1){
							tempFileNm = fileNm +" 외 "+ (rowCnt-1) +"건";
						} else {
							tempFileNm = fileNm;
						}
						
						this.ds_atchListDtl.setColumn(nRow, "ORGINL_FILE_NM", tempFileNm);
					} else {
						this.ds_atchListDtl.setColumn(nRow, "ORGINL_FILE_NM", null);
					}
				this.ds_atchListDtl.set_updatecontrol(true);
				break;

			case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				this.fn_loadComboNonCodeTbl();
				break;
				
			case "deleteFile":
				this.fn_transaction("selectFileList");
				break;
			case "saveTemp":
				this.fn_fileUpload();
				break;
		}
	}
}

