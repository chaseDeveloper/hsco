}


//----------------------------------------------
// 평가연도 text 변경시 - ds_cond 에 강제set
//----------------------------------------------
this.div_search_spn_evalYear_ontextchanged = function(obj:Spin, e:nexacro.TextChangedEventInfo)
{
	this.ds_cond.setColumn(0, "EVL_YEAR", e.posttext);
	this.fn_loadComboNonCodeTbl();
}
//----------------------------------------------
// 평가차수 item 변경시 - ds_cond 에 강제set
//----------------------------------------------

this.div_search_cmb_odrSe_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.ds_cond.setColumn(0, "ODR_SE", e.postvalue);
	
	// 집계처리일자
	var curDate = this.ds_odrSe.getColumn(e.postindex, "CODE_CN");
	this.div_search.sta_closeInfo.set_text("집계 기준일 : "+curDate);
	
	this.fn_loadComboNonCodeTbl2();	
}

//----------------------------------------------
// 조직 item 변경시 - ds_cond 에 강제set
//----------------------------------------------
this.div_search_cmb_orgnztSe_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.ds_cond.setColumn(0, "ORGNZT_CODE", e.postvalue);
	this.fn_search();
}


/***********************************************************************
* 사용자 정의 함수
************************************************************************/
//-------------------------------------------------------
// KEY 검색조건 컴포넌트 초기값(현채차수) 부여
//-------------------------------------------------------
this.fn_setCurInfo = function(strComp)
{
	if (this.ds_curInfo.rowcount ==  1 ){

		if (strComp == "evalYear"){
			var sCurEvlYear = this.ds_curInfo.getColumn(0,"CUR_EVL_YEAR");
			this.ds_cond.setColumn(0, "EVL_YEAR", sCurEvlYear);
			this.fn_loadComboNonCodeTbl();
			//this.div_search.spn_evalYear.set_value(sCurEvlYear);

		}else if (strComp == "odrSe"){
			var sCurOdrSe 	= this.ds_curInfo.getColumn(0,"CUR_ODR_SE");
			this.div_search.cmb_odrSe.set_value(sCurOdrSe);
			
			// 집계처리일자
			var fRow = this.ds_odrSe.findRow("CODE", sCurOdrSe);
			var curDate = this.ds_odrSe.getColumn(fRow, "CODE_CN");
			this.div_search.sta_closeInfo.set_text("집계 기준일 : "+curDate);
		}
	}
}

//-------------------------------------------------------
// 각 관점별 FilterCopy (전략목표)
//-------------------------------------------------------
this.fn_setFilterCopy = function(strViewPointSe)
{
    var sFilterExp = "UPPER_IX_CODE == '" + strViewPointSe + "'";
    var obj = eval("this.ds_view" + strViewPointSe);
	this.ds_refrnList.filter(sFilterExp);
	obj.copyData(this.ds_refrnList,true);
	this.ds_refrnList.filter("");
}

//-------------------------------------------------------
// 각 관점별 SetValue (관점)
//-------------------------------------------------------
this.fn_setValue = function(strViewPointSe)
{
    var nRow = this.ds_refrnList.findRow("IX_CODE", strViewPointSe );
    var sACHIV_RT = this.ds_refrnList.getColumn(nRow,"ACHIV_RT");

    if ( sACHIV_RT != "" && sACHIV_RT != undefined ){
		var sACHIV_RT = this.ds_refrnList.getColumn(nRow,"ACHIV_RT") + "%";
    }else{
        var sACHIV_RT = "";
    }
    var obj = eval("this.div_work.sta_" + strViewPointSe);
    obj.set_text("");
    obj.set_text(sACHIV_RT);
}

//-------------------------------------------------------
// TOTAL SetValue (공사전체)
//-------------------------------------------------------
this.fn_setTotalValue = function()
{
    var nRow = this.ds_refrnList.findRow("IX_CODE", "00000" );
    var sACHIV_RT = this.ds_refrnList.getColumn(nRow,"ACHIV_RT");

    if ( sACHIV_RT != "" && sACHIV_RT != undefined ){
		var sACHIV_RT = this.ds_refrnList.getColumn(nRow,"ACHIV_RT") + "%";
    }else{
        var sACHIV_RT = "";
    }
    this.div_work.sta_total.set_text("");
    this.div_work.sta_total.set_text(sACHIV_RT);
}


//-------------------------------------------------------
// 관점명 셋팅
//-------------------------------------------------------
this.fn_setViewPoint = function()
{
    var nRow = this.ds_refrnList.findRow("IX_CODE", "C" );
    if(nRow > -1){
		this.div_work.cmb_viewPointSeC.set_value("C");
    }
    
    nRow = this.ds_refrnList.findRow("IX_CODE", "F" );
    if(nRow > -1){
		this.div_work.cmb_viewPointSeF.set_value("F");
    }
    
    nRow = this.ds_refrnList.findRow("IX_CODE", "L" );  
    if(nRow > -1){
		this.div_work.cmb_viewPointSeL.set_value("L");
    }
  
    nRow = this.ds_refrnList.findRow("IX_CODE", "P" );
    if(nRow > -1){
		this.div_work.cmb_viewPointSeP.set_value("P");
    }
}

