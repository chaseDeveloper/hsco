/*
 * 함수명  	: btn_uploadFiles_onclick
 * 설명 		: '파일업로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_uploadFiles_onclick(obj,e);
 */
this.btn_uploadFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 첨부파일 업로드(fileSn 자동채번 / 첨부파일 Master에 ROW추가 / 파일 업로드 및 자동저장 / 테이블에 fileSn반영 / 데이터셋에 fileSn반영, 단 신규 ROW가 아닐경우 ROWTYPE은 유지)
	this.fn_autoFileSn(
		 "MIS"																				// 대분류   (SYS_SE)
		,"ACH"																				// 업무구분 (JOB_SE)
		, this.File_UpDownManager															// fileManager 오브젝트
		, this.ds_fileList																	// fileObject
		, this.ds_acmsltList																// fileSn을 불러오거나 반영해야 하는 컬럼
		, "TBACH_QYEVAL_DSTB_ACMSLT"														// fileSn을 저장할 테이블
		, "BASIS_FID"																		// fileSn 컬럼명이 다를 경우를 대비
		, ["EVL_YEAR","ODR_SE","ORGNZT_CODE","IX_CODE","ACMSLT_YY","ACMSLT_MT"]				// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
	);
}

/*
 * 함수명  	: btn_downFiles_onclick
 * 설명 		: '선택다운로드' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_downFiles_onclick(obj,e);
 */
this.btn_downFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
		return;
	}
	
	for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
		var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
		var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
		var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
		var chk       = this.ds_fileList.getColumn(fRow, "CHK");
		if(chk == '1') {
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}


/*
 * 함수명  	: btn_delFiles_onclick
 * 설명 		: '선택삭제' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_delFiles_onclick(obj,e);
 */
this.btn_delFiles_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_fileList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
			return;
	}
	
	if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
		return false;
	}
	
	for(var fRow = this.ds_fileList.getRowCount()-1; fRow >= 0; fRow--) {
		var chkVal = this.ds_fileList.getColumn(fRow, "CHK");
		if(chkVal == '1') {
			this.ds_fileList.deleteRow(fRow);
		}
	}
	
	var strSvcId   	 = "deleteFile";
	var sController  = "hsco/cmm/file/deleteFile.do";
	var sInDatasets  = "input1=ds_fileList:U";
	var sOutDatasets = "ds_fileList=output1";
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/*
 * 함수명  	: ds_acmsltList_oncolumnchanged
 * 설명 		: 월별실적값 변경시 달성율 및 월성과값 자동계산
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_acmsltList_oncolumnchanged(obj,e);
 */
this.ds_acmsltList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
    // Case. 월별실적 등록/변경시
	if(e.columnid == "MT_ACMSLT_VAL"){  
		if(!obj.getColumn(e.row, "MT_GOAL")){
			return false;
		}
		var nRow = this.ds_qKpiListMst.rowposition;
		var frmla = this.ds_qKpiListMst.getColumn(nRow, "ACHIV_RT_CALC_FRMLA");			// 현재 지표의 달성율 계산식
		var inputTy = this.ds_qKpiListMst.getColumn(nRow, "ACMSLT_INPUT_TY");			// 현재 지표의 실적 등록방식
		var mthd = this.ds_qKpiListMst.getColumn(nRow, "ACMSLT_VALUE_MANAGE_MTHD");     // 현재 지푱의 실적값 관리방식
		var mxmmLmt = this.ds_qKpiListMst.getColumn(nRow, "MXMM_EVL_LMT");		    	// 현재 지표의 최대평가한도
		var excpFrmla = this.ds_qKpiListMst.getColumn(nRow, "EXCP_RSLT_CALC_FRMLA");	// 현재 지표의 예외성과계산식	
			
		var evlWghtval = this.ds_acmsltList.getColumn(this.ds_acmsltList.rowposition, "EVL_WGHTVAL"); //평가가중치
		
		// Calc. 월별 실적에 따른 월별 달성율 계산
		var goal = obj.getColumn(e.row, "MT_GOAL");	// 월별 목표치
		var result = 0;
		var scoreResult = 0;
		
		if(frmla == "001"){		// 실적치/목표치
				
			if(goal > 0 && e.newvalue > 0){			
				result = (e.newvalue/goal) * 100;
				if(mxmmLmt > 0 && result > mxmmLmt ){	// 최대평가한도 적용			
					result = mxmmLmt;
				}			
			}else if(mthd == 'C' && goal == 0 && e.newvalue == 0){ // 마일리지(가감점)타입인 경우
				result = 100;
			}else if(excpFrmla == '3' && goal == 0 && e.newvalue == 0){ // 예외성과계산식 존재
				result = 100;					
			}else{
				result = 0;
			}				
		}		
		if(frmla == "002"){		// 목표치/실적치
			
			if(goal > 0 && e.newvalue > 0){			
				result = (goal/e.newvalue) * 100;				
				if(mxmmLmt > 0 && result > mxmmLmt ){	// 최대평가한도 적용			
					result = mxmmLmt;
				}			
			}else if(excpFrmla == '4' && e.newvalue == 0){ // 예외성과계산식 존재
				result = 100;					
			}else{
				result = 0;
			}
		}
		// Calc. 월별 실적에 따른 월별 성과점수 계산
		if (mthd == 'C' && evlWghtval == 0 && e.newvalue != 0 ){ // 마일리지(가감점)타입인 경우
			scoreResult = e.newvalue ;
		}else{
			//scoreResult = evlWghtval * result / 100;
			//scoreResult = goal / e.newvalue * evlWghtval;
			if(e.newvalue >= goal){
				scoreResult = evlWghtval;
			} else {
				scoreResult = this.fn_round((e.newvalue * evlWghtval / goal), 2);	// 실적 * 가중치 / 목표  (반올림하여 소수점 2자리까지 표현)
			}
			
		}
		
		obj.set_updatecontrol(false);
		obj.setColumn(e.row, "MT_ACHIV_RT", result);  			// 월별달성율
		obj.setColumn(e.row, "MT_RESULT_SCORE", scoreResult);	// 월별 성과점수	
		obj.set_updatecontrol(true);		
		
		// Calc. 월별 실적에 따른 최종실적 계산 -> Calc는 실적저장이후 집계처리로 변경
// 		var tot_val = obj.getColumn(e.row, "TOT_ACMSLT_VAL");	// 현재월 까지의 누계 실적치
// 		result = 0;
// 		
// 		if(inputTy == "001"){			// 누계합산
// 			result = (tot_val - e.oldvalue + e.newvalue);
// 		}else if(inputTy == "002"){		// 누계평균
// 			result = nexacro.round((tot_val - e.oldvalue + e.newvalue)/(this.ds_mmList.rowposition+1),2);
// 		}else {							// 누계실적치
// 			result = e.newvalue;
// 		}
// 		obj.set_updatecontrol(false);
// 		obj.setColumn(e.row, "FINAL_ACMSLT", result);
// 		obj.set_updatecontrol(true);	
	}
}

/*
 * 함수명  	: ds_acmsltList_cancolumnchange
 * 설명 		: 월별실적값 변경 전 월별목표의 유효성 체크
