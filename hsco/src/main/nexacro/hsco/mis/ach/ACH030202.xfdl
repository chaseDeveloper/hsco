this.fn_indvdlIxAt = function(){
	var nRow = this.ds_qKpiListMst.rowposition;
	
	var at = this.ds_qKpiListMst.getColumn(nRow, "INDVDL_IX_AT");
	
	var result = at == '1' ? false : true;
	
	return result;
}

this.fn_round = function(val, cphr) {
	var calNum = 1;
	for(var fRow =  0; fRow <cphr; fRow++) {
		calNum *= 10;
	}
	
	var rtnVal = nexacro.round(nexacro.ceil(val * calNum, 9)) / calNum;
	return rtnVal;
}
