 ***********************************************************************
 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.sClosAt = "";
this.nRowPos1 = 0;
this.nRowPos2 = -1;
this.nSetInit = 0;
var sysSe = "MIS";
var jobSe = "ACH";
this.File_UpDownManager = null;
this.checkDs = [this.ds_acmsltList];

var EVL_YEAR;
var ODR_SE;
 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH030202_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	
	this.fn_misFormInit(this);
	this.fn_init_form();
	
	this.fn_init_dataset();
}

this.ACH030202_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// 링크로 통해 들어온 파라메터 읽기
	this.fn_getLinkParam();
	// Combo에 사용되는 Dataset Load
	this.fn_loadCombo();
	// 검색조건 컴포넌트 초기값 부여
	this.fn_init_current();
	
	// 파일 메니저 설정
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "ACH");
	this.File_UpDownManager.init(fileConfig);
}

//----------------------------------
// Form/Dataset 오브젝트 초기화
//----------------------------------
this.fn_init_form = function()
{
	this.gfn_initForm(this);
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);
}

//----------------------------------
// Get Current 평가차수
//----------------------------------
this.fn_init_current = function()
{
    
    // 링크타고 들어온 경우
    if(EVL_YEAR != null && ODR_SE != null) {
		this.ds_cond.setColumn(0, "EVL_YEAR", EVL_YEAR);    
		this.ds_cond.setColumn(0, "ODR_SE"	, ODR_SE);
		
		this.fn_loadComboNonCodeTbl();
    }else {
		this.ds_curInfo.clearData();
		this.fn_transaction("selectCurEvlOdrInfo");
	}

}

//-----------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (공통코드)
//-----------------------------------------------------
 this.fn_loadCombo = function ()
{
    // ----------------------------------------- Condition Area -----------------------------------------------
	// 적용범위구분
