//-------------------------------------------------------
// TOTAL SetValue (공사전체)
//-------------------------------------------------------
this.fn_setTotalValue = function()
{
    this.ds_refrnList01.filter("");

    var nRow = this.ds_refrnList01.findRow("IX_CODE", "00000" );
    var sACHIV_RT = this.ds_refrnList01.getColumn(nRow,"ACHIV_RT");

    if ( this.comUtils.isNullEmpty(sACHIV_RT) || sACHIV_RT == "undefined" ) {
		sACHIV_RT = "0.00";
    }

    this.div_work.sta_total.set_text("");
    this.div_work.sta_total.set_text(sACHIV_RT + " %");
}


//-------------------------------------------------------
// 전략관점별 달성율 현황 Graph
//-------------------------------------------------------
this.fn_drawChart1 = function()
{
	var arrData = [];
	var argTitle = "관점별 성과 달성율";
		
	if(!this.comUtils.isNull(this.ds_refrnList01)) {
		
		for(var i = 0; i < this.ds_refrnList01.rowcount; i++) {
			var ixSe = this.ds_refrnList01.getColumn(i, "IX_SE");
			
			if("001" == ixSe){
				var sub_arr = [];
				
				sub_arr[0] = nexacro.trim(this.ds_refrnList01.getColumn(i, "IX_NM"));		// 관점명 
				sub_arr[1] = nexacro.toNumber(this.ds_refrnList01.getColumn(i, "ACHIV_RT"));			// 달성률
				sub_arr[2] = nexacro.toNumber(this.ds_refrnList01.getColumn(i, "ACHIV_RT"));			// 달성률 annotation
				arrData.push(sub_arr);
			}
		}
	}

	this.div_work.wb_graph01.callMethod("drawChart", arrData, argTitle);
}

