    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : ACH040701
 * 화면명   : 종합성과분석
 * 화면설명 : 전략관점별 달성율,부서성과순위,성과달성율현황,주요성과지표 달성율 현황을 조회한다.
 * 작성일   : 2015.11.10
 * 작성자   : 박병일
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */


/***********************************************************************
 * Script Include
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.sClosAt = "";
this.nRowPos = 0;
this.nSetInit = 0;
this.toggleView = 0;

this.isChartLoad1 = false;
this.isChartLoad2 = false;
this.isChartLoad3 = false;

 /***********************************************************************
 * 메뉴 변수 선언부
 ************************************************************************/

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.ACH040701_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	
	this.fn_init_form();
	
	this.fn_init_dataset();
}

this.ACH040701_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();
	this.fn_init_current();
	
	// 관점별 성과 달성율 차트 로딩
	var wbChartUrl01 = application.services["svcurl"].url + "web/hsco/chart/" + "ACH040701_01.html";
	this.div_work.wb_graph01.set_url(wbChartUrl01);
	
	// 조직별 성과 달성율 차트 로딩
	var wbChartUrl02 = application.services["svcurl"].url + "web/hsco/chart/" + "ACH040701_02.html";
	this.div_work.wb_graph02.set_url(wbChartUrl02);
	
	// 평가군별 성과 달성율 차트 로딩
	var wbChartUrl03 = application.services["svcurl"].url + "web/hsco/chart/" + "ACH040701_03.html";
	this.div_work.wb_graph03.set_url(wbChartUrl03);
	
	
	// 차트 로딩완료를 체크 위함
	this.fn_chartLoadCheck = function(obj, e) {
		if(this.isChartLoad1) {
			this.fn_drawChart1();
			this.killTimer(97);
		}
		
		if(this.isChartLoad2) {
			this.fn_drawChart2();
			this.killTimer(98);
		}
		
		if(this.isChartLoad3) {
			this.fn_drawChart3();
			this.killTimer(99);
		}
	}
	this.addEventHandler("ontimer", this.fn_chartLoadCheck, this);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);
}
//----------------------------------
// Get Current 평가차수
//----------------------------------
this.fn_init_current = function()
{
	this.ds_curInfo.clearData();
	this.fn_transaction("selectCurEvlOdrInfo");
}

//--------------------------
// Dataset을 초기화한다.
//--------------------------
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

//-----------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (공통코드)
//-----------------------------------------------------
 this.fn_loadCombo = function ()
{
	var comboParams = [
			["ds_refrnSe", 			"ACH020", "1", ""],		// 참조구분
			["ds_mesure_unit_se", 	"ACH011", "1", ""], 	// 측정단위
			["ds_me_cycle_se", 		"ACH005", "1", ""], 	// 측정주기
			["ds_acmsltValueMSe", 	"ACH014", "1", ""] 		// 실적값관리방식
		];

	this.gfn_comboLoad(comboParams, this.fn_callbackAfterLoad);
}

//-----------------------------------------
// 공통 Callback 함수에서 호출할 함수
//-----------------------------------------
this.fn_callbackAfterLoad = function ()
{
}

//--------------------------------------------------------
// 화면의 Combo 에 데이터를 불러온다. (Non-공통코드)
//--------------------------------------------------------
this.fn_loadComboNonCodeTbl = function (strDs, obj)
{
	var comboParams = [
	        // 평가차수
			["ds_odrSe", 	"ds_cond", "", "", "hsco/mis/ach/ACHCommon/selectOdrList.do", 0]
		];

	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
}

//-----------------------------------------
// ( Non-공통코드) Callback 함수에서 호출할 함수
//-----------------------------------------
this.fn_callbackAfter = function (svcId)
{
	switch (svcId) {
		case "ds_odrSe" :
			// 초기화
			this.div_search.sta_closeInfo.set_text("집계 기준일 : ");
			this.ds_refrnList01.clearData();
			this.ds_refrnList02.clearData();
			this.ds_refrnList03.clearData();
			this.ds_refrnList.clearData();
			
			if(this.ds_odrSe.rowcount > 0){
				// 집계기준일
				var curDate = this.ds_odrSe.getColumn(0, "CODE_CN");
				this.div_search.sta_closeInfo.set_text("집계 기준일 : "+curDate);
				
				if (nSetInit == 1){ // Load시 1회만 적용
					this.fn_setCurInfo("odrSe");
					nSetInit = 0;
				}
				this.fn_search();
			} else {
				this.gfn_message("info.처리불가", "해당년도의 등록된 평가차수가 없습니다.");
			}
			break;

	}
}


/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러
 **********************************************************************/
//-------
// 조회
//-------
this.fn_search = function()
{
	// validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return false;
	}

	this.ds_refrnList01.clearData();
	this.ds_refrnList02.clearData();
	this.ds_refrnList03.clearData();
	this.ds_refrnList.clearData();
	this.fn_transaction("selectList01");
 	this.fn_transaction("selectList02");
 	this.fn_transaction("selectList03");
 	this.fn_transaction("selectList04");
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {

		case "selectList01": // LIST (전략관점별 달성율)
			var sController   	= "hsco/mis/ach/ACH040201/selectListMst.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_refrnList01=output1";
			break

		case "selectList02": // LIST (부서성과순위)
			var sController   	= "hsco/mis/ach/ACH040501/selectList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_refrnList02=output1";
			break;
		
		case "selectList03": // LIST (편성군성과순위)
			var sController   	= "hsco/mis/ach/ACH040503/selectList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_refrnList03=output1";
			break;

		case "selectList04": // LIST (주요성과지표 달성율현황)
			var sController   	= "hsco/mis/ach/ACH040401/selectList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_refrnList=output1";
			break;

		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
			var sController   	= "hsco/mis/ach/ACHCommon/selectCurEvlOdrInfo.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_curInfo=output1";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId) {

			case "selectList01": // LIST
				this.fn_setTotalValue();
				//this.fn_report01();
				this.setTimer(97, 1000);
				break;

			case "selectList02": // LIST 불러오기
				//this.fn_report02();
				this.setTimer(98, 1000);
				break;

			case "selectList03": // LIST 불러오기
				//this.fn_report03();
				this.setTimer(99, 1000);
				break;
				
			case "selectList04": // LIST 불러오기
				break;

     		case "selectCurEvlOdrInfo": // GET 현재 평가차수(평가연도&차수)
				this.fn_setCurInfo("evalYear");
				nSetInit = 1;
				// Combo에 사용되는 개별 Dataset Load
				//this.fn_loadComboNonCodeTbl();
				break;
		}
	}
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/


//-----------------------------------------------------------------------------
// Grid Head 클릭 이벤트 처리(Grid 데이터 정렬)
// Grid의 Head의 체크 버튼을 클릭하였을 때, 실 데이터의 체크버튼 처리
//-----------------------------------------------------------------------------
this.div_work_grd_list_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
	}else{
	    this.gfn_gridSort(obj, e); // Grid 데이터 정렬
	}
}

//------------------------------------------------------------------------
// Body의 체크 버튼이 눌러졌을 때, Head 부분의 체크 버튼을 꺼 줌.
//------------------------------------------------------------------------
this.div_work_grd_list_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
}

//-----------------------------------------------------------
// ListMst EnterKey downUp 시
//-----------------------------------------------------------
this.div_work_grd_list_onkeyup = function(obj:Grid, e:nexacro.KeyEventInfo)
{
