			this.ds_anytmAsignDemand.setColumn(nRow, "DECSN_AMOUNT"     , decsnAm);
			this.ds_anytmAsignDemand.setColumn(nRow, "ASIGN_EXCUT_BLCE" , asignExcutBlce);
 		}
	}
}



