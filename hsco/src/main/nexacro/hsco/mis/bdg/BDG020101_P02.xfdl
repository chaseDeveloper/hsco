		if(useAt != '1') {
			this.gfn_message("info.항목.선택.알림", "[해당 예산은 이미 마감되었습니다]\n지출 가능한 예산");
			return false;
		}
	}
	
	var dsTemp = new Dataset();
	dsTemp.copyData(this.ds_expndtrUnitCode);
	dsTemp.clearData();
	var nRow = dsTemp.addRow();
	dsTemp.copyRow(nRow, this.ds_expndtrUnitCode, this.ds_expndtrUnitCode.rowposition);
