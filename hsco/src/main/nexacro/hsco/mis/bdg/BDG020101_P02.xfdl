/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectAcntCode":	// 예산계정과목 조회
			// ds_cond -> ds_acntCode
			var year         = this.ds_cond.getColumn(0, "YEAR");
			var bsnsSe       = this.ds_cond.getColumn(0, "BSNS_SE");
			var balcSe       = this.ds_cond.getColumn(0, "BALC_SE");
			var deptCode     = this.ds_cond.getColumn(0, "DEPT_CODE");
			var semokCode    = this.ds_cond.getColumn(0, "SEMOK_CODE");
			var acntCode     = this.ds_cond.getColumn(0, "ACNT_CODE");
			var planRegistAt = this.ds_cond.getColumn(0, "PLAN_REGIST_AT");
			var se           = this.ds_cond.getColumn(0, "SE");
			var gwan         = this.ds_cond.getColumn(0, "GWAN");
			var hang         = this.ds_cond.getColumn(0, "HANG");
			var sehang       = this.ds_cond.getColumn(0, "SEHANG");
			var mok          = this.ds_cond.getColumn(0, "MOK");
			var semok        = this.ds_cond.getColumn(0, "SEMOK");
			var acntType     = this.ds_cond.getColumn(0, "ACNT_TYPE");
			
			// set
			this.ds_condSub1.clearData();
			var nRow = this.ds_condSub1.addRow();
			this.ds_condSub1.setColumn(nRow, "YEAR",           year         );
			this.ds_condSub1.setColumn(nRow, "BSNS_SE",        bsnsSe       );
			this.ds_condSub1.setColumn(nRow, "BALC_SE",        balcSe       );
			this.ds_condSub1.setColumn(nRow, "DEPT_CODE",      deptCode     );
			this.ds_condSub1.setColumn(nRow, "SEMOK_CODE",     semokCode    );
			this.ds_condSub1.setColumn(nRow, "ACNT_CODE",      acntCode     );
			this.ds_condSub1.setColumn(nRow, "PLAN_REGIST_AT", planRegistAt );
			this.ds_condSub1.setColumn(nRow, "SE",             se           );
			this.ds_condSub1.setColumn(nRow, "GWAN",           gwan         );
			this.ds_condSub1.setColumn(nRow, "HANG",           hang         );
			this.ds_condSub1.setColumn(nRow, "SEHANG",         sehang       );
			this.ds_condSub1.setColumn(nRow, "MOK",            mok          );
			this.ds_condSub1.setColumn(nRow, "SEMOK",          semok        );
			this.ds_condSub1.setColumn(nRow, "ACNT_TYPE",      acntType     );
			
			var sController   	= "hsco/mis/bdg/BDG020101/selectAcntCode.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_acntCode=output1";
			break;
		case "selectExpndtrUnitCode":	// 세목/지출단위코드 조회
			// ds_acntCode -> ds_expndtrUnitCode
			var acRow = this.ds_acntCode.rowposition;
			var year           = this.ds_acntCode.getColumn(acRow, "YEAR");
			var bsnsSe         = this.ds_acntCode.getColumn(acRow, "BSNS_SE");
			var balcSe         = this.ds_acntCode.getColumn(acRow, "BALC_SE");
			var budgetAcntCode = this.ds_acntCode.getColumn(acRow, "BUDGET_ACNT_CODE");
			var acntLevel      = this.ds_acntCode.getColumn(acRow, "ACNT_LEVEL");
			var month          = this.ds_cond.getColumn(0, "MONTH");
			
			if(acntLevel < 4) {
				return false;	// 관ㆍ항ㆍ세항의 경우, 재조회할 필요가 없음
			}
			
			// ds_cond -> ds_expndtrUnitCode
			var deptCode       = this.ds_cond.getColumn(0, "DEPT_CODE");
			var deptNm         = this.ds_cond.getColumn(0, "DEPT_NM");
			var detailBsnsCode = this.ds_cond.getColumn(0, "DETAIL_BSNS_CODE");
			var bsnsNm         = this.ds_cond.getColumn(0, "BSNS_NM");
			var se             = this.ds_cond.getColumn(0, "SE");
			var bukipCode      = this.ds_cond.getColumn(0, "BUKIP_CODE");
			
			// set
			this.ds_condSub2.clearData();
			var nRow = this.ds_condSub2.addRow();
			this.ds_condSub2.setColumn(nRow, "YEAR",             year          );
			this.ds_condSub2.setColumn(nRow, "BSNS_SE",          bsnsSe        );
			this.ds_condSub2.setColumn(nRow, "BALC_SE",          balcSe        );
			this.ds_condSub2.setColumn(nRow, "DEPT_CODE",        deptCode      );
			this.ds_condSub2.setColumn(nRow, "DEPT_NM",          deptNm        );
			this.ds_condSub2.setColumn(nRow, "DETAIL_BSNS_CODE", detailBsnsCode);
			this.ds_condSub2.setColumn(nRow, "BSNS_NM",          bsnsNm        );
			this.ds_condSub2.setColumn(nRow, "BUDGET_ACNT_CODE", budgetAcntCode);
			this.ds_condSub2.setColumn(nRow, "MONTH",            month         );
			this.ds_condSub2.setColumn(nRow, "SE",               se            );
			this.ds_condSub2.setColumn(nRow, "BUKIP_CODE",       bukipCode     );
			
			var sController   	= "hsco/mis/bdg/BDG020101/selectExpndtrUnitCode.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_expndtrUnitCode=output1";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectAcntCode":		// 조회(계정코드)
				if(acntCode) {
					var findRow = this.ds_acntCode.findRow("BUDGET_ACNT_CODE", acntCode);
					if(findRow > -1) {
						this.ds_acntCode.set_rowposition(findRow);
					}
				}
				break;
			case "selectExpndtrUnitCode":	// 조회(세목/지출단위 코드)
				if(unitCode) {
					var findRow = this.ds_expndtrUnitCode.findRow("EXPNDTR_UNIT_CODE", unitCode);
					if(findRow > -1) {
						this.ds_expndtrUnitCode.set_rowposition(findRow);
					}
				}
				break;
		}
	}
}



