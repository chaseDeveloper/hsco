/**********************************************************************
* Script Include 
***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";



/**********************************************************************
* 화면 변수 선언부
***********************************************************************/



/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.BDG040101_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// form 초기화
	this.fn_init_form();

	// dataset 초기화
	this.fn_init_dataset();
}

this.BDG040101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// TODO..
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 초기 검색조건
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
 	this.ds_cond.setColumn(nRow, "YEAR", this.comUtils.getClientDate("YYYY")-1);	// 당해년도
 	this.ds_cond.setColumn(nRow, "MTH",  '12');	// 당월
	
	// 공통코드
	var comboParams = [
	    ["ds_bdg001", "BDG001", "Y", "D", "B"]	// ds_bdg001 : 사업구분
	   ,["ds_bdg002", "BDG002", "Y", "D", "B"]	// ds_bdg002 : 수지구분
	   ,["ds_bdg002", "BDG002", "Y", "D", "B"]	// ds_bdg002 : 수지구분
	];
	
	var callback2 = function (svcId, errorCode, errorMsg) {
		// 공통코드와 연계된 초기검색조건 설정
		this.ds_cond.setColumn(nRow, "BSNS_SE", "1");	// (사업구분) 1:사업예산, 2:자본예산, 3:자금운용
		this.ds_cond.setColumn(nRow, "BALC_SE", "2");	// (수지구분) 1:수입, 2:지출
		this.fn_search();	//  최초조회
	};
	this.gfn_comboLoad (comboParams, callback2);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(수입지출자료)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation 체크(수입지출자료)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	// clear Datasets
	this.ds_budgetStacnt.clearData();
	
	// transaction
	this.fn_transaction("selectBudgetStacnt");
}


/*
 * 함수명  	: fn_exec
 * 설명 		: 취합자료 생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_exec();
 */
this.fn_exec = function()
{
	var year   = this.ds_cond.getColumn(0, "YEAR");
	var bsnsSe = this.ds_bdg001.getColumn(this.ds_bdg001.findRow("CODE", this.ds_cond.getColumn(0,"BSNS_SE")),"CODE_NM");
	var balcSe = this.ds_bdg002.getColumn(this.ds_bdg002.findRow("CODE", this.ds_cond.getColumn(0,"BALC_SE")),"CODE_NM");
	var mth    = nexacro.toNumber(this.ds_cond.getColumn(0, "MTH"),0,0,0);
	
	var confirmMsg = "[수입지출에 대한 취합자료를 생성합니다.]" + "\n\n";
	confirmMsg    += "현재의 조회조건을 기준으로," + "\n";
	confirmMsg    += mth + "월의 취합정보를 삭제한 후 재생성합니다." + "\n";
	confirmMsg    += "(조회조건 : " + year + "년 " + bsnsSe + " " + balcSe + ")" + "\n\n";
	confirmMsg    += "계속 진행하시겠습니까?" + "\n";
	confirmMsg    += "(재생성 후 자동저장됩니다.)";
	
	if(confirm(confirmMsg))	{
		this.fn_transaction("saveExec");
	}
}


/*
 * 함수명  	: fn_exec
 * 설명 		: 취합자료 생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_exec();
 */
this.fn_execAcmtl = function()
{
	var year   = this.ds_cond.getColumn(0, "YEAR");
	var bsnsSe = this.ds_bdg001.getColumn(this.ds_bdg001.findRow("CODE", this.ds_cond.getColumn(0,"BSNS_SE")),"CODE_NM");
	var balcSe = this.ds_bdg002.getColumn(this.ds_bdg002.findRow("CODE", this.ds_cond.getColumn(0,"BALC_SE")),"CODE_NM");
	var mth    = nexacro.toNumber(this.ds_cond.getColumn(0, "MTH"),0,0,0);
	
	var confirmMsg = "[수입지출에 대한 취합자료를 생성합니다.]" + "\n\n";
	confirmMsg    += "현재의 조회조건을 기준으로," + "\n";
	confirmMsg    += mth + "월까지의 취합정보를 삭제한 후 재생성합니다." + "\n";
	confirmMsg    += "(조회조건 : " + year + "년 " + bsnsSe + " " + balcSe + ")" + "\n\n";
	confirmMsg    += "계속 진행하시겠습니까?" + "\n";
	confirmMsg    += "(재생성 후 자동저장됩니다.)";
	
	if(confirm(confirmMsg))	{
		this.fn_transaction("saveExecAcmtl");
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBudgetStacnt":	// 조회
			var sController   	= "/hsco/mis/bdg/BDG040101/selectBudgetStacnt.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_budgetStacnt=output1";
			break;
			
		case "saveExec":	// 취합자료 생성
			var sController   	= "hsco/mis/bdg/BDG040101/saveExec.do";	
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
			
		case "saveExecAcmtl":	// 취합자료 생성
			var sController   	= "hsco/mis/bdg/BDG040101/saveExecAcmtl.do";	
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	//alert("nErrorCode = " + nErrorCode);
	// error message
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	// callback process
	if (nErrorCode == '0'){
		switch (strSvcId) {
			case "selectBudgetStacnt":	// 조회
				//alert("selectBudgetStacnt");
				break;
			
			case "saveExec": 
				//alert("saveExec");
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
				
			case "saveExecAcmtl":
				//alert("saveExecAcmtl");
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
	
	// grid format
	var balcSe = this.ds_cond.getColumn(0, "BALC_SE");
	
	switch(balcSe) {
		case "1":
			this.grd_budgetStacnt.set_formatid("incme");
			break;
		case "2":
			this.grd_budgetStacnt.set_formatid("expndtr");
			break;
	}
}



/**********************************************************************
* Transaction 영역 끝
***********************************************************************/



/**********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
***********************************************************************
*
* 1. Button 관련 이벤트 처리
* 
***********************************************************************/



/**********************************************************************
