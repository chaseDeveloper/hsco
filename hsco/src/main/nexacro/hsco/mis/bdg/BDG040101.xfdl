***********************************************************************/

/*
 * 함수명  	: btn_exec_onclick
 * 설명 		: '자료취합' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_exec_onclick(obj,e);
 */
this.btn_exec_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_exec();
}


/*
 * 함수명  	: div_search_btn_execAcmtl_onclick
 * 설명 		: '자료취합' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_search_btn_execAcmtl_onclick(obj,e);
 */
this.div_search_btn_execAcmtl_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_execAcmtl();
}


/*
 * 함수명  	: btn_expand_onclick
 * 설명 		: '특정 레벨까지 펼치기' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_expand_onclick(obj,e);
 */
this.btn_expandLevel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.grd_budgetStacnt.set_treeinitstatus("collapse,all");
	var grdRow = 0;
	var maxLvl = this.cmb_level.value;
	while(grdRow < this.grd_budgetStacnt.rowcount) {
		var treeLevel = this.grd_budgetStacnt.getCellValue(grdRow, 3);
		if(treeLevel < maxLvl) {
			this.grd_budgetStacnt.setTreeStatus(grdRow, true);
		}
		grdRow++;
	}
}


/*
 * 함수명  	: btn_expand_onclick
 * 설명 		: '전체접기' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_expand_onclick(obj,e);
 */
this.btn_collapse_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.grd_budgetStacnt.set_treeinitstatus("collapse,all");
}


/*
 * 함수명  	: btn_expand_onclick
 * 설명 		: '전체펼치기' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_expand_onclick(obj,e);
 */
this.btn_expand_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
