/**********************************************************************
* Script Include 
***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";



/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
var saveRow = -1;

this.inpYn = false;
this.chargerAt = false;

/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.BDG050101_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}

this.BDG050101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 오브젝트 등에 대한 처리
	this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	this.div_search.div_srchDept.parentFormId = "BDG050101";

	var year = this.comUtils.getClientDate("YYYY");
	
	// 공통코드
	var comboParams = [
	    ["ds_bdg001", "BDG001", "Y", "A", "B"]	// ds_bdg001 : 사업구분
	   ,["ds_bdg002", "BDG002", "Y", "D", "B"]	// ds_bdg002 : 수지구분
	   ,["ds_bdg003", "BDG003", "Y", "D", "B"]	// ds_bdg003 : 예산구분
	   ,["ds_bdg024", "BDG024", "Y", "D", "B"]	// ds_bdg024 : 집행계획확정여부
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 집행계획 예산구분만
		this.ds_bdg003.filter("CODE == 'A'");
		
		// 공통코드와 연계된 초기검색조건 설정
		this.ds_cond.clearData();
		var nRow = this.ds_cond.addRow();
		this.ds_cond.setColumn(nRow, "YEAR",      year);	// 당해년도
		this.ds_cond.setColumn(nRow, "BSNS_SE",   "1");		// (사업구분) 1:사업예산, 2:자본예산
		this.ds_cond.setColumn(nRow, "BALC_SE",   "2");		// (수지구분) 1:수입, 2:지출
		this.ds_cond.setColumn(nRow, "BUDGET_SE", "A");		// (예산구분) A:집행계획(본예산)
		
		//예산담당자
		if(this.gfn_authGrpId("BDG_ADMIN") != -1) {
			this.chargerAt = true;
		}
		// 예산배정
		if(this.gfn_authGrpId("BDG_ASIGN") != -1) {
			this.div_search.div_srchDept.set_enable(true);
		}
		// 재무회계팀장
		if(this.gfn_authGrpId("FNR_ACC_TIMHDER") != -1) {
			this.div_search.div_srchDept.set_enable(true);
		}
		
		if(this.chargerAt){
			this.div_search.div_srchDept.set_enable(true);
			this.ds_cond.setColumn(nRow, "DEPT_CODE", "000000"        );		// 부서코드
			this.ds_cond.setColumn(nRow, "DEPT_NM",   "화성도시공사");		// 부서명
		}
		else{
			//해당 부서만 2016.08.31
			this.ds_cond.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId()  );	// 부서코드
			this.ds_cond.setColumn(nRow, "DEPT_NM",   this.gfn_getDeptName());	// 부서명
		}
		
		// 2018-04-06 김형태
		// 국민체육센터팀일 경우 거점체력센터운영 예산에 접근 가능해야 함
		// 추후 특이 케이스 또 발생할 때를 대비해서 수정할 수 있게 해놓음, deptCode_SQL 수정하면 됨
		if(this.gfn_getDeptId() == "040200") {
			this.div_search.div_srchDept.set_enable(true);
		}
		
		this.ds_elpdExcutRate.setColumn(0, "ELPD_EXCUT_RATE", '50');
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(입력가능여부)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// clear Datasets
	this.ds_budgetExcutPlan.clearData();
	
	var year  = this.ds_cond.getColumn(0, "YEAR");
	var nYear = nexacro.toNumber(year, 0);
	
	if(nYear < 2018) {
		this.ds_cond.setColumn(0, "YEAR_SE", "1");
	}
	else {
		this.ds_cond.setColumn(0, "YEAR_SE", "2");
	}
	
	this.ds_condAtmcCalc.copyRow(0, this.ds_cond, 0);
	
	// transaction
	this.fn_transaction("selectBudgetExcutPlan");
}



/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(집행계획등록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 저장가능 체크여부
	var inpYn = this.ds_inpYn.getColumn(0, "INP_YN") == '1' ? true : false;
	if(!inpYn){
		this.gfn_message("info.처리불가", "집행계획 등록기간이 아닙니다.");
		this.ds_budgetExcutPlan.reset();
		return false;
	}
	
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_budgetExcutPlan)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	for(var i=0; i < this.ds_budgetExcutPlan.getRowCount(); i++) {
		if(this.ds_budgetExcutPlan.getRowType(i) == Dataset.ROWTYPE_UPDATE) {
			var totalAm = this.ds_budgetExcutPlan.getColumn(i, "TOT_BUDGET_AM");
			var asignAm = this.ds_budgetExcutPlan.getColumn(i, "ASIGN_AM");
			
			var budgetAcntCode = this.ds_budgetExcutPlan.getColumn(i, "BUDGET_ACNT_CODE");
			var aCode = budgetAcntCode.substr(0, 1);
			
			if(aCode == "8") {
				if(totalAm < asignAm) {
					this.gfn_message("fail.정보수정.실패", "예비비의 월별배정금액의 합계가 총예산액보다 큽니다.");
					return false;
				}
				continue;
			}
			
			if(!this.cmprAmt(totalAm, asignAm)) {
				this.gfn_message("fail.정보수정.실패", (i+1) + "번째 행의 월별배정금액의 합계가 총예산액과 일치하지 않습니다.");
				return false;
			}
		}
	}
	
	// transaction
	if(this.gfn_message("confirm.삭제.진행", "변경된 내역이 존재합니다. 저장시 확정여부가 미확정으로 변경됩니다.")) {
		saveRow = this.ds_budgetExcutPlan.rowposition;
		this.fn_transaction("save");
	}
}



/*
