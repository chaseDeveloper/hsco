this.btn_auto_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var elpdExcutRate = this.ds_elpdExcutRate.getColumn(0, "ELPD_EXCUT_RATE");
	this.ds_condAtmcCalc.setColumn(0, "ELPD_EXCUT_RATE", elpdExcutRate);
