 */
this.ds_budgetExcutPlan_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "ASIGN_AM_01" || e.columnid == "ASIGN_AM_02" || e.columnid == "ASIGN_AM_03"
	   || e.columnid == "ASIGN_AM_04" || e.columnid == "ASIGN_AM_05" || e.columnid == "ASIGN_AM_06"
	   || e.columnid == "ASIGN_AM_07" || e.columnid == "ASIGN_AM_08" || e.columnid == "ASIGN_AM_09"
	   || e.columnid == "ASIGN_AM_10" || e.columnid == "ASIGN_AM_11" || e.columnid == "ASIGN_AM_12") {
		var asignAm01 = obj.getColumn(e.row, "ASIGN_AM_01");
		var asignAm02 = obj.getColumn(e.row, "ASIGN_AM_02");
		var asignAm03 = obj.getColumn(e.row, "ASIGN_AM_03");
		var asignAm04 = obj.getColumn(e.row, "ASIGN_AM_04");
		var asignAm05 = obj.getColumn(e.row, "ASIGN_AM_05");
		var asignAm06 = obj.getColumn(e.row, "ASIGN_AM_06");
		var asignAm07 = obj.getColumn(e.row, "ASIGN_AM_07");
		var asignAm08 = obj.getColumn(e.row, "ASIGN_AM_08");
		var asignAm09 = obj.getColumn(e.row, "ASIGN_AM_09");
		var asignAm10 = obj.getColumn(e.row, "ASIGN_AM_10");
		var asignAm11 = obj.getColumn(e.row, "ASIGN_AM_11");
		var asignAm12 = obj.getColumn(e.row, "ASIGN_AM_12");
		
		if(this.comUtils.isNull(asignAm01)) {
			asignAm01 = 0;
		}
		if(this.comUtils.isNull(asignAm02)) {
			asignAm02 = 0;
		}
		if(this.comUtils.isNull(asignAm03)) {
			asignAm03 = 0;
		}
		if(this.comUtils.isNull(asignAm04)) {
			asignAm04 = 0;
		}
		if(this.comUtils.isNull(asignAm05)) {
			asignAm05 = 0;
		}
		if(this.comUtils.isNull(asignAm06)) {
			asignAm06 = 0;
		}
		if(this.comUtils.isNull(asignAm07)) {
			asignAm07 = 0;
		}
		if(this.comUtils.isNull(asignAm08)) {
			asignAm08 = 0;
		}
		if(this.comUtils.isNull(asignAm09)) {
			asignAm09 = 0;
		}
		if(this.comUtils.isNull(asignAm10)) {
			asignAm10 = 0;
		}
		if(this.comUtils.isNull(asignAm11)) {
			asignAm11 = 0;
		}
		if(this.comUtils.isNull(asignAm12)) {
			asignAm12 = 0;
		}
		
		var asignAm   = asignAm01 + asignAm02 +asignAm03 + asignAm04 + asignAm05 + asignAm06 + asignAm07 + asignAm08 + asignAm09 + asignAm10 + asignAm11 + asignAm12;
		obj.setColumn(e.row, "ASIGN_AM", asignAm);
	}
}

