/*
 * 함수명  	: btn_dcsnPlan_onclick
 * 설명 		: '집행계획확정' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_dcsnPlan_onclick(obj,e);
 */
this.btn_dcsnPlan_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.gfn_message("comm.행위여부", "검색조건의 집행계획을 모두 확정합니다. 계속")) {
		if(Ex.util.isUpdated(this.ds_budgetExcutPlan)) {
			this.gfn_message("info.처리불가", "변경된 데이터가 존재합니다. 저장 후 다시 시도해주십시오.");
		}
		
		for(var i = 0; i < this.ds_budgetExcutPlan.getRowCount(); i++) {
			var totalAm = this.ds_budgetExcutPlan.getColumn(i, "TOT_BUDGET_AM");
			var asignAm = this.ds_budgetExcutPlan.getColumn(i, "ASIGN_AM");
			if(!this.cmprAmt(totalAm, asignAm)) {
				this.gfn_message("info.처리불가", (i+1) + " 번째 행의 배정이 완료되지 않았습니다. 금액 입력 후 다시 시도해주십시오.");
			}
		}
		
		this.fn_transaction("saveDcsnExcutPlan");
	}
}


/*
 * 함수명  	: btn_dcsn_onclick
 * 설명 		: '배정계획확정' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_dcsn_onclick(obj,e);
 */
this.btn_dcsn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.gfn_message("comm.행위여부", "기등록된 배정계획을 모두 삭제한 뒤 검색조건의 자료를 새로 배정계획을 등록합니다. (수시배정 자료는 삭제하지 않습니다.)\n계속")) {
		if(Ex.util.isUpdated(this.ds_budgetExcutPlan)) {
			this.gfn_message("info.처리불가", "변경된 데이터가 존재합니다. 저장 후 다시 시도해주십시오.");
		}
		
		for(var i = 0; i < this.ds_budgetExcutPlan.getRowCount(); i++) {
			var totalAm = this.ds_budgetExcutPlan.getColumn(i, "TOT_BUDGET_AM");
			var asignAm = this.ds_budgetExcutPlan.getColumn(i, "ASIGN_AM");
			
			var budgetAcntCode = this.ds_budgetExcutPlan.getColumn(i, "BUDGET_ACNT_CODE");
			var aCode = budgetAcntCode.substr(0, 1);
			
			if(aCode == "8") {
				if(totalAm < asignAm) {
					this.gfn_message("fail.정보수정.실패", "예비비의 월별배정금액의 합계가 총예산액보다 큽니다.");
					return false;
				}
				continue;
			}
			
			if(!this.cmprAmt(totalAm, asignAm)) {
				this.gfn_message("info.처리불가", (i+1) + " 번째 행의 배정이 완료되지 않았습니다. 금액 입력 후 다시 시도해주십시오.");
				return false;
			}
		}
		
		this.fn_transaction("saveDcsnAsignPlan");
	}
}


/*
 * 함수명  	: btn_saveHist_onclick
 * 설명 		: '집행계획 이력남기기' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_saveHist_onclick(obj,e);
 */
this.btn_saveHist_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.chargerAt) {
		if(this.gfn_message("comm.행위여부", "현재 설정된 예산년도에 해당하는 예산집행계획이력을 저장합니다.\n계속")) {
			var year  = this.ds_cond.getColumn(0, "YEAR");
			var nYear = nexacro.toNumber(year, 0);
			
			if(nYear < 2018) {
				this.ds_cond.setColumn(0, "YEAR_SE", "1");
			}
			else {
				this.ds_cond.setColumn(0, "YEAR_SE", "2");
			}
			
			this.fn_transaction("saveBudgetExcutPlnHist");
		}
	} else {
		this.gfn_message("info.처리불가", "집행계획 이력남기기 권한이 없습니다.\n(예산담당자만 가능한 기능입니다.)");
		return false;
	}
}


