 * 설명 		: 숫자값에 대하여 천원단위로 콤마(,) 적용
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_setComma(msg);
 */
this.fn_setComma = function(msg)
{
	return this.comUtils.setComma(msg+"");
}


/*
 * 함수명  	: cmprAmt
 * 설명 		: 총예산과 합계금액을 비교한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.cmprAmt();
 */
this.cmprAmt = function(totAm, asignAm, budgetAcntCode)
{
	var sTotAm = totAm + "";
	var sAsignAm = asignAm + "";
	var aCode = "";
	
	if(!this.comUtils.isNull(budgetAcntCode)) {
		aCode = budgetAcntCode.substr(0, 1);
	}
	
	if(aCode == "8") {
		return true;
	}
	
	return (sTotAm == sAsignAm);
}


