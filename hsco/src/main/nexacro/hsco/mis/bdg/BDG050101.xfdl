/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBudgetExcutPlan":		// 집행계획 조회
			var sController   	= "/hsco/mis/bdg/BDG050101/selectBudgetExcutPlan.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_inpYn=output1 ds_budgetExcutPlan=output2";
			break;
		case "save":						// 저장
			var sController   	= "/hsco/mis/bdg/BDG050101/save.do";
			var sInDatasets   	= "input1=ds_budgetExcutPlan:U";
			var sOutDatasets  	= "";
			break;
		case "saveAtmcCalc":				// 자동계산
			var sController   	= "/hsco/mis/bdg/BDG050101/saveAtmcCalc.do";
			var sInDatasets   	= "input1=ds_condAtmcCalc";
			var sOutDatasets  	= "";
			break;
		case "saveDcsnAsignPlan":			// 배정계획확정
			var sController   	= "/hsco/mis/bdg/BDG050101/saveDcsnAsignPlan.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "saveDcsnExcutPlan":			// 집행계획확정
			var sController   	= "/hsco/mis/bdg/BDG050101/saveDcsnExcutPlan.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "saveBudgetExcutPlnHist":		// 집행계획 이력남기기
			var sController   	= "/hsco/mis/bdg/BDG050101/saveBudgetExcutPlnHist.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectBudgetExcutPlan":		// 조기집행계획 조회
				// 집행계획 기간일 때에만 자동계산 버튼 활성화
				this.inpYn = this.ds_inpYn.getColumn(0, "INP_YN") == '1' ? true : false;
				this.btn_auto.set_enable(this.inpYn);
				
				// 담당자일 때 집행계획확정, 배정계획확정 버튼 활성화
				//this.btn_dcsnPlan.set_enable(this.chargerAt);
				this.btn_dcsn.set_enable(this.chargerAt);
				
				if(saveRow > 0) {
					this.ds_budgetExcutPlan.set_rowposition(saveRow);
					saveRow = -1;
				}
				break;
			case "save":						// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "saveAtmcCalc":				// 자동계산
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "saveDcsnAsignPlan":			// 배정계획확정
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "saveDcsnExcutPlan":			// 집행계획확정
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "saveBudgetExcutPlnHist":		// 집행계획 이력남기기
				this.gfn_message("success.처리.성공");
				break;
		}
	}
}

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 사업코드(조회조건)
	if(sPopupId == "popBsns") {
