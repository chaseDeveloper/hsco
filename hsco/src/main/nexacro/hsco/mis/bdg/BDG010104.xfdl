}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 오브젝트 등에 대한 처리 2016.08.31
	this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	// 초기 검색조건
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
 	this.ds_cond.setColumn(nRow, "YEAR",      this.comUtils.getClientDate("YYYY"));	// 당해년도
 	
	//예산담당자 2016.09.04
	if(this.gfn_authcode("108") != -1){
		this.div_search.div_srchDept.set_enable(true);
		this.ds_cond.setColumn(nRow, "DEPT_CODE", "000000"        );	// 부서코드
		this.ds_cond.setColumn(nRow, "DEPT_NM",   "화성도시공사");	// 부서명
	}
	else{
		//해당 부서만 2016.08.31
		this.ds_cond.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId()  );	// 부서코드
		this.ds_cond.setColumn(nRow, "DEPT_NM",   this.gfn_getDeptName());	// 부서명
	}
	
 	
	// 공통코드
	var comboParams = [
	    ["ds_bdg001", "BDG001", "Y", "D", "B"]	// ds_bdg001 : 사업구분
	   ,["ds_bdg002", "BDG002", "Y", "D", "B"]	// ds_bdg002 : 수지구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 공통코드와 연계된 초기검색조건 설정
		this.ds_cond.setColumn(nRow, "BSNS_SE", "1");	// (사업구분) 1:사업예산, 2:자본예산, 3:자금운용
		this.ds_cond.setColumn(nRow, "BALC_SE", "2");	// (수지구분) 1:수입, 2:지출
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
	
	// 2016.10.05 박세현 주석처리
	/*
	if(this.ds_cond.getColumn(nRow, "YEAR")=="2017"){
		this.btn_copy.set_enable(false);
	}
	*/
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건 Dataset 외에 초기화가 필요한 Dataset에 대한 초기화 코드를 추가
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(예산계정)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// validation check(검색조건)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	// clear Datasets
	this.ds_acntCode.clearData();
	this.ds_expndtrUnitCode.clearData();
	
	// transaction
	this.fn_transaction("selectBudgetAcntCode");
}

/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(지출단위)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function()
{
	// clear Datasets
	this.ds_expndtrUnitCode.clearData();
	
	// transaction
	if(this.ds_acntCode.getRowCount() > 0) {		// parent check
		this.fn_transaction("selectExpndtrUnitCode");
	}
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(지출단위)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// validation 체크(지출단위코드)
	if(!this.gfn_checkValidation(this.ds_expndtrUnitCode, this.ds_validation)) {
		return;
	}
	
	// transaction
	if(this.gfn_message("confirm.저장.여부")) {
		this.fn_transaction("saveExpndtrUnitCode");
	}
}


/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBudgetAcntCode":		// 예산계정과목 조회
			var sController   	= "/hsco/mis/bdg/BDG010104/selectBudgetAcntCode.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_acntCode=output1 ds_inpYn=output2";
			break;
		case "selectExpndtrUnitCode":		// 지출단위코드 조회
			var tcRow          = this.ds_acntCode.rowposition;
			var year           = this.ds_acntCode.getColumn(tcRow, "YEAR");
			var bsnsSe         = this.ds_acntCode.getColumn(tcRow, "BSNS_SE");
			var balcSe         = this.ds_acntCode.getColumn(tcRow, "BALC_SE");
			var budgetAcntCode = this.ds_acntCode.getColumn(tcRow, "BUDGET_ACNT_CODE");
			var deptCode	   = this.ds_cond.getColumn(0,"DEPT_CODE");
			//alert(this.ds_cond.getColumn(0,"DEPT_CODE"));
			this.ds_condSub.clearData();
			var nRow = this.ds_condSub.addRow();
			this.ds_condSub.setColumn(nRow, "YEAR",             year          );
			this.ds_condSub.setColumn(nRow, "BSNS_SE",          bsnsSe        );
			this.ds_condSub.setColumn(nRow, "BALC_SE",          balcSe        );
			this.ds_condSub.setColumn(nRow, "BUDGET_ACNT_CODE", budgetAcntCode);
			this.ds_condSub.setColumn(nRow, "DEPT_CODE", 		deptCode);
			
			var sController   	= "/hsco/mis/bdg/BDG010104/selectExpndtrUnitCode.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_expndtrUnitCode=output1";
			break;
		case "saveExpndtrUnitCode":			// 저장
			var sController   	= "hsco/mis/bdg/BDG010104/saveExpndtrUnitCode.do";
			var sInDatasets   	= "input1=ds_expndtrUnitCode:U";
			var sOutDatasets  	= "";
			break;
		case "selectUseYearCnt":		// 현 연도를 사용하고 있는 테이블을 탐색
			var year = this.ds_cond.getColumn(0, "YEAR");
			
			this.ds_condSub2.clearData();
			var nRow = this.ds_condSub2.addRow();
			this.ds_condSub2.setColumn(nRow, "YEAR", year );
			
			this.ds_useYearCnt.clearData();
			
			var sController   	= "/hsco/mis/bdg/BDG010104/selectUseYearCnt.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_useYearCnt=output1";
			break;
		case "selectUseAcntCnt":		// 현 연도를 사용하고 있는 테이블을 탐색
			var euRow    = this.ds_expndtrUnitCode.rowposition;
			var year     = this.ds_expndtrUnitCode.getColumn(euRow, "YEAR");
			var bsnsSe   = this.ds_expndtrUnitCode.getColumn(euRow, "BSNS_SE");
			var balcSe   = this.ds_expndtrUnitCode.getColumn(euRow, "BALC_SE");
			var acntCode = this.ds_expndtrUnitCode.getColumn(euRow, "BUDGET_ACNT_CODE");
			var unitCode = this.ds_expndtrUnitCode.getColumn(euRow, "EXPNDTR_UNIT_CODE");
			
			this.ds_condSub2.clearData();
			var nRow = this.ds_condSub2.addRow();
			this.ds_condSub2.setColumn(nRow, "YEAR", year );
			this.ds_condSub2.setColumn(nRow, "BSNS_SE", bsnsSe );
			this.ds_condSub2.setColumn(nRow, "BALC_SE", balcSe );
			this.ds_condSub2.setColumn(nRow, "BUDGET_ACNT_CODE", acntCode );
			this.ds_condSub2.setColumn(nRow, "EXPNDTR_UNIT_CODE", unitCode );
			
			this.ds_useAcntCnt.clearData();
			
			var sController   	= "/hsco/mis/bdg/BDG010104/selectUseAcntCnt.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_useAcntCnt=output1";
			break;
		case "saveCopyPrvYear":
			var sController   	= "hsco/mis/bdg/BDG010104/saveCopyPrvYear.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "selectBsnsAcntCode":
			var sController   	= "hsco/mis/bdg/BDG010104/selectBsnsAcntCode.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_expndtrBsnsCode=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectBudgetAcntCode":	// 조회(예산계정)
				//if(this.ds_acntCode.getRowCount() == 0) {
				//	this.gfn_message("info.데이터.없음");
				//}
				
				// message set
				var year = this.ds_cond.getColumn(0, "YEAR");
				if(year < 2016) {
					this.sta_titleAcnt.set_text("('목'단위만 지출단위의 추가가 가능합니다)");
 					this.sta_titleUnit.set_text("(세목단위 지출시에도 지출단위코드를 입력바랍니다)");
				} else {
					this.sta_titleAcnt.set_text("('세목'단위만 지출단위의 추가가 가능합니다)");
 					this.sta_titleUnit.set_text("(2016년도 이후의 세목명은 사업명으로 고정됩니다)");
				}
				
				// Grid Head
		
			//	this.grd_expndtrUnitCode.setCellProperty("Head", 2, "text", headTxt);	// 세목코드/명
				
				// 마감기간에 따른 수정여부 결정
				this.inpAt = '1' //this.ds_inpYn.getColumn(0, "INP_YN"); 2017.01.03 신규/수정 기간 제어 해제
				
				if(this.inpAt != '1') {
					this.sta_titleAcnt.set_text("");
					this.sta_titleUnit.set_text("(해당 조회조건에 부합하는 마감기간이 존재하지 않습니다.)");
					
					this.grd_expndtrUnitCode.setCellProperty("Body", 0, "edittype", "none");	// 집행단위
					this.grd_expndtrUnitCode.setCellProperty("Body", 2, "edittype", "none");	// 세목코드
					this.grd_expndtrUnitCode.setCellProperty("Body", 5, "edittype", "none");	// 세목명
					this.grd_expndtrUnitCode.setCellProperty("Body", 8, "edittype", "none");	// 지출단위코드
					this.grd_expndtrUnitCode.setCellProperty("Body", 9, "edittype", "none");	// 지출단위명
					this.grd_expndtrUnitCode.setCellProperty("Body", 10, "edittype", "none");	// 지출단위명
					this.grd_expndtrUnitCode.setCellProperty("Body", 11, "edittype", "none");	// 조기집행여부
					this.grd_expndtrUnitCode.setCellProperty("Body", 12, "edittype", "none");	// 조기집행율
				} else {
					this.grd_expndtrUnitCode.setCellProperty("Body", 0, "edittype", "checkbox");											// 집행단위
					this.grd_expndtrUnitCode.setCellProperty("Body", 2, "edittype", "none");	// 세목코드
					this.grd_expndtrUnitCode.setCellProperty("Body", 5, "edittype", "expr:YEAR < '2016' ? 'text' : 'none'");				// 세목명
					this.grd_expndtrUnitCode.setCellProperty("Body", 8, "edittype", "expr:YEAR < '2016' ? 'text' : 'none'");
					this.grd_expndtrUnitCode.setCellProperty("Body", 9, "edittype", "expr:dataset.getRowType(currow) == Dataset.ROWTYPE_INSERT ? 'mask' : 'none'");						// 지출단위코드
					this.grd_expndtrUnitCode.setCellProperty("Body", 10, "edittype", "text");												// 지출단위명
					this.grd_expndtrUnitCode.setCellProperty("Body", 11, "edittype", "checkbox");											// 조기집행여부
					this.grd_expndtrUnitCode.setCellProperty("Body", 12, "edittype", "expr:ELPD_EXCUT_AT=='1' ? 'masknumber' : 'none'");		// 조기집행율
				}
				break;
			case "selectExpndtrUnitCode":	// 조회(지출단위)
				break;
			case "saveExpndtrUnitCode": 	// 저장
				this.fn_transaction("selectExpndtrUnitCode");
				break;
			case "selectUseAcntCnt":		// 삭제 조건
				var rtnMsg = this.ds_useAcntCnt.getColumn(0, "RTN_MSG");
				var totCnt = nexacro.toNumber(this.ds_useAcntCnt.getColumn(0, "TOT_CNT"));
				if(totCnt > 0) {
					this.gfn_message("info.확인.진행가능", "현 계정을 사용하는 <" + rtnMsg + "> 삭제 ", rtnMsg);
					return false;
				}
				
				if(!this.gfn_message("confirm.삭제여부")) {
					return false;	// 아니오
				}
				this.ds_expndtrUnitCode.deleteRow(this.ds_expndtrUnitCode.rowposition);
				break;
			case "selectUseYearCnt":
				var rtnMsg = this.ds_useYearCnt.getColumn(0, "RTN_MSG");
				var totCnt = nexacro.toNumber(this.ds_useYearCnt.getColumn(0, "TOT_CNT"));
				if(totCnt > 0) {
					this.gfn_message("info.확인.진행가능", "현 연도를 사용하는 <" + rtnMsg + "> 삭제 ", rtnMsg);
					return false;
				}
				
				if(this.gfn_message("confirm.복사.여부", "(자동저장) 전년 지출단위|당해 지출단위")) {
					this.fn_transaction("saveCopyPrvYear");
				}
				break;
			case "saveCopyPrvYear":			// 전년도복사
				this.fn_search();
				break;
			case "selectBsnsAcntCode":
				if( this.ds_cond00.getColumn(0,"YEAR") > 2016 ){
					var currow = this.ds_expndtrUnitCode.rowposition;
					this.ds_expndtrUnitCode.setColumn( currow,"BSNS_UNIT_CODE", this.ds_expndtrBsnsCode.getColumn(0, "EXPNDTR_BSNS_CODE"));
				}
				break;

		}
	} else if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "bsnsPop") {
		var ds_bsnsCode = new Dataset();
		var loadCnt = ds_bsnsCode.loadXML(sReturn);
		if(loadCnt) {
			var bsnsCode = ds_bsnsCode.getColumn(0, "SE_ACCNUT_ACNT_CODE");
			var bsnsNm   = ds_bsnsCode.getColumn(0, "SE_ACCNUT_ACNT_NM");
			var euRow    = this.ds_expndtrUnitCode.rowposition;
			this.ds_expndtrUnitCode.setColumn(euRow, "DETAIL_BSNS_CODE", bsnsCode);
			this.ds_expndtrUnitCode.setColumn(euRow, "DETAIL_BSNS_NM",        bsnsNm  );
			this.ds_cond00.clearData()
			var nRow = this.ds_cond00.addRow();
			this.ds_cond00.copyRow(nRow, this.ds_cond, 0);
			this.ds_cond00.setColumn(nRow, "DETAIL_BSNS_CODE" , bsnsCode)
			this.fn_transaction("selectBsnsAcntCode");
		}

	}
	
	if(sPopupId == "deptPop") {
		var ds_deptCode = new Dataset();
		var loadCnt = ds_deptCode.loadXML(sReturn);
		if(loadCnt) {
			var deptCode = ds_deptCode.getColumn(0, "DEPT_CODE");
			var deptNm   = ds_deptCode.getColumn(0, "DEPT_NM");
			var euRow    = this.ds_expndtrUnitCode.rowposition;
			this.ds_expndtrUnitCode.setColumn(euRow, "DEPT_CODE", deptCode);
			this.ds_expndtrUnitCode.setColumn(euRow, "DEPT_NM",   deptNm  );
		}
	}
}



