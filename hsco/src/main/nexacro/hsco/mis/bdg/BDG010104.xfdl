/*
 * 함수명  	: btn_Add_onclick
 * 설명 		: '행추가' 버튼 클릭 후 이벤트 처리(지출간위코드 Grid)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_Add_onclick(obj,e);
 */
this.fn_insert = function()
{
	if(this.inpAt != '1') {
		this.gfn_message("errors.없음.요청", "해당 조회조건에 부합하는 마감기간이|예산편성마감기간 화면을");
		return false;
	}
	
 	// 지출단위코드 등록이 가능하도록 처리(Leaf / 목 혹은 세목단위)
 	var year = this.ds_cond.getColumn(0, "YEAR");
 	var inpAcntLvl = year < 2016 ? 4 : 5;
 	var inpAcntNm  = year < 2016 ? "목" : "세목";
 	
 	var dbRow = this.ds_acntCode.rowposition;
	var acntLevel = this.ds_acntCode.getColumn(dbRow, "ACNT_LEVEL");
	var isView    = (acntLevel == inpAcntLvl ? true : false);
	if(!isView) {
		this.gfn_message("info.저장.초과", "'" + inpAcntNm + "'단위의 지출단위만");
		return false;
	}
	var nRow = this.ds_expndtrUnitCode.addRow();
	this.ds_expndtrUnitCode.copyRow(nRow, this.ds_acntCode, dbRow);
	this.ds_expndtrUnitCode.setColumn(nRow, "EXCUT_UNIT_AT" , 1);
	if( this.ds_cond.getColumn(0,"DEPT_CODE") != "000000"){				// div_search 부서코드 화성도시공사(000000) 아닐때 부서코드 복사
		var deptCode   = this.ds_cond.getColumn(0,"DEPT_CODE");
		var deptNm 	   = this.ds_cond.getColumn(0,"DEPT_NM");
		this.ds_expndtrUnitCode.setColumn(nRow, "DEPT_CODE",  deptCode);
		this.ds_expndtrUnitCode.setColumn(nRow, "DEPT_NM",  deptNm);
 		this.ds_expndtrUnitCode.setColumn(nRow, "DEPT_CHK", "N");		// 부서코드 팝업버튼 visible false
	} else {
		this.ds_expndtrUnitCode.setColumn(nRow, "DEPT_CODE",  this.ds_expndtrUnitCode.getColumn(nRow -1, "DEPT_CODE")); // 위 행의 부서코드 복사
		this.ds_expndtrUnitCode.setColumn(nRow, "DEPT_NM", this.ds_expndtrUnitCode.getColumn(nRow -1, "DEPT_NM"));
		this.ds_expndtrUnitCode.setColumn(nRow, "DEPT_CHK", "Y");		// 부서코드 팝업버튼 visible true
	}
	this.ds_expndtrUnitCode.setColumn(nRow, "EXPNDTR_UNIT_CODE", "        "    );	// mask에 대한 자릿수 임의처리
 	this.ds_expndtrUnitCode.setColumn(nRow, "BUDGET_CNRL_AT", "1"    );			    // 예산통제여부 default 체크
	
	 	// 2016년도 이후에는, 세목코드는 자동으로 계정에서 끌어와 채번
	if(year >= 2016) {
		var budgetAcntCode = this.ds_acntCode.getColumn(dbRow, "BUDGET_ACNT_CODE");
		var taxitmCode = budgetAcntCode.substr(6,1).padLeft(2, "0")					
		var taxitmNm   = this.ds_acntCode.getColumn(dbRow, "BUDGET_ACNT_NM");		
		 
		this.ds_expndtrUnitCode.setColumn(nRow, "TAXITM_CODE", taxitmCode);			// 세목코드
		this.ds_expndtrUnitCode.setColumn(nRow, "TAXITM_NM", taxitmNm);				// 세목코드명
			
		var deptCode = this.ds_expndtrUnitCode.getColumn(nRow, "DEPT_CODE");
	
		if(year == 2016){
			this.ds_expndtrUnitCode.setColumn(nRow,"BSNS_UNIT_CODE","01");
			this.ds_expndtrUnitCode.setColumn(nRow, "UNIT_CODE", "0100"    );	// mask에 대한 자릿수 임의처리
		}else {
			this.ds_expndtrUnitCode.setColumn(nRow,"BSNS_UNIT_CODE","00");
			this.ds_expndtrUnitCode.setColumn(nRow, "EXPNDTR_UNIT_CODE", "00000000"    );	// mask에 대한 자릿수 임의처리
			this.ds_expndtrUnitCode.setColumn(nRow, "UNIT_CODE", "0100"    );	// mask에 대한 자릿수 임의처리
			
		}
		
		if(nRow-1 <0){
			this.ds_expndtrUnitCode.setColumn(nRow, "DEPT_CODE", "000000");
		}else {
			this.ds_expndtrUnitCode.setColumn(nRow,"DEPT_CODE","000000");				// onrowchange 를 위해 기본값을 먼저 넣어줌
			this.ds_expndtrUnitCode.setColumn(nRow,"DEPT_CODE",deptCode);				
		}
	}

}


