/*
 * 함수명  	: ds_expndtrUnitCode_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(지출단위코드 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_expndtrUnitCode_oncolumnchanged(obj,e);
 */
this.ds_expndtrUnitCode_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 세목코드 - 예산계정과목의 마지막 번호 채번 ( 00-__-__-__ ) 첫번째자리
	if(e.columnid == "TAXITM_CODE") {
		var taxitmCode = e.newvalue;
		if(taxitmCode.indexOf(" ") > -1) {
			this.gfn_message("errors.항목.타입.숫자형", "세목코드");
			obj.setColumn(e.row, e.columnid, e.oldvalue);
		} else if(!this.comUtils.isNull(e.newvalue) && (e.newvalue+"").length == 2) {
			var newVal = taxitmCode;
			var unitCodeAf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(2);
			obj.setColumn(e.row, "EXPNDTR_UNIT_CODE", newVal+""+unitCodeAf);
		}
	}
	
	// 사업코드 - 수기 채번 ( 00-__--__ ) 첫번째자리 
	if(e.columnid == "BSNS_UNIT_CODE") {
		var bsnsUnitCode = e.newvalue;
		if(bsnsUnitCode.indexOf(" ") > -1) {
			this.gfn_message("errors.항목.타입.숫자형", "부서코드");
			obj.setColumn(e.row, e.columnid, e.oldvalue);
		} else if(!this.comUtils.isNull(e.newvalue) && (e.newvalue+"").length == 2) {
			//var unitCodeBf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(0,2);
			var newVal     = bsnsUnitCode;
			var unitCodeAf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(2);
			obj.setColumn(e.row, "EXPNDTR_UNIT_CODE", newVal + "" + unitCodeAf);
		}
	}

	// 부서코드 - 부서코드의 2~4 번호 채번 ( __-00-__-__ ) 두번째자리 
	if(e.columnid == "DEPT_CODE") {
		var deptCode = e.newvalue;
		if(deptCode.indexOf(" ") > -1) {
			this.gfn_message("errors.항목.타입.숫자형", "부서코드");
			obj.setColumn(e.row, e.columnid, e.oldvalue);
		} else if(!this.comUtils.isNull(e.newvalue) && (e.newvalue+"").length == 6) {
			var unitCodeBf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(0,2);
			var newVal     = deptCode.substr(2,2);
			var unitCodeAf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(4);
			obj.setColumn(e.row, "EXPNDTR_UNIT_CODE", unitCodeBf + "" + newVal + "" + unitCodeAf);
		}
	}
	
	// 사업코드 - 
	if(e.columnid == "DETAIL_BSNS_CODE"){
		var bsnsCode = e.newvalue;
		if(bsnsCode.indexOf(" ") > -1) {
			this.gfn_message("errors.항목.타입.숫자형", "부서코드");
			obj.setColumn(e.row, e.columnid, e.oldvalue);
		} else if(!this.comUtils.isNull(e.newvalue) && (e.newvalue+"").length == 7) {
			var unitCodeBf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(0,4);
			/*
			var newVal = "00";
			var findRow = obj.findRow("DETAIL_BSNS_CODE",  bsnsCode);
			if(findRow == e.row){
				findRow = -1;
			}
			if( findRow > -1 ){
				newVal = (obj.getColumn(findRow, "EXPNDTR_UNIT_CODE")+"").substr(4,2);
			} else {
				var bsnsUnitCode =  new String(Number(obj.getMax("BSNS_UNIT_CODE")) + 1);
				if( bsnsUnitCode < 10 ){
					newVal = "0" + bsnsUnitCode;
				} else {
					newVal = bsnsUnitCode;
				}
			}
			var unitCodeAf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(6);
			obj.setColumn(e.row, "BSNS_UNIT_CODE", newVal);
			obj.setColumn(e.row, "EXPNDTR_UNIT_CODE", unitCodeBf + "" + newVal + "" + unitCodeAf);
			*/
		}
	}

	// 단위코드 - 해당 계정의 사업코드별 채번 ( __-__-00-00 ) 세번째네번째자리 
	if(e.columnid == "UNIT_CODE") {
		var unitCode = e.newvalue;
		if(unitCode.indexOf(" ") > -1) {
			this.gfn_message("errors.항목.타입.숫자형", "지출단위코드");
			obj.setColumn(e.row, e.columnid, e.oldvalue);
		} else if(!this.comUtils.isNull(e.newvalue) && (e.newvalue+"").length == 4) {
			var unitCodeBf = (obj.getColumn(e.row, "EXPNDTR_UNIT_CODE")+"").substr(0,4);
			var newVal     = unitCode;
			obj.setColumn(e.row, "EXPNDTR_UNIT_CODE", unitCodeBf + "" + newVal);
		}
	}
	
	// 조기집행여부
	if(e.columnid == "ELPD_EXCUT_AT") {
		if(e.newvalue == '1') {
			obj.set_enableevent(false);
			obj.setColumn(e.row, "ELPD_EXCUT_RT", 85);	// 기본 조기집행율은 0.85 고정(이후 조정 가능)
			obj.set_enableevent(true);
		} else {
			obj.set_enableevent(false);
			obj.setColumn(e.row, "ELPD_EXCUT_RT", 0);	// 조기집행율 초기화
			obj.set_enableevent(true);
		}
	}
}




