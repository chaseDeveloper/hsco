	/**********************************************************************
	 * 자동분개유형코드
	 * ACC020203 - 신규 - 거래유형팝업
	***********************************************************************/
	
	/**********************************************************************
	 * 예산계정코드의 자동분개유형에서, 운영수익으로 되는 녀석들을 연결해서 테스트해볼것!
	***********************************************************************/
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(레포츠센터수입)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// clear Datasets
	this.ds_operIncme.clearData();
	
	// transaction
	this.fn_transaction("selectOperIncme");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(레포츠센터수입)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// validation 체크(레포츠센터수입)
	if(!this.gfn_checkValidation(this.ds_operIncme, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.fn_transaction("saveOperIncme");
	}
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete= function()
{	
 	if(this.ds_operIncme.getRowCount() > 0) {
		if( this.fn_isChit(this.ds_operIncme.rowposition)) {
			this.gfn_message("fail.삭제.불가");
			return;
		}
		
		if (this.gfn_message("confirm.삭제여부") ) {
			this.ds_operIncme.deleteRow(this.ds_operIncme.rowposition);
		}
	}
}
/*
 * 함수명  	: fn_sanction
 * 설명 		: 결제 버튼 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_sanction = function(){

	var rowType = this.ds_operIncme.getRowType(this.ds_operIncme.rowposition);
	
	trace(rowType);
	
	if(Dataset.ROWTYPE_NORMAL != this.ds_operIncme.getRowType(this.ds_operIncme.rowposition)) {
		if(this.gfn_message("confirm.변경.저장.여부")) {
			// validation 체크(레포츠센터수입)
			if(!this.gfn_checkValidation(this.ds_operIncme, this.ds_validation)) {
				return;
			}
			
			this.fn_transaction("saveOperIncme");

			return;
		}
	}

	var sanValue = this.ds_operIncme.getColumn(this.ds_operIncme.rowposition, "LAST_SANCTN_STTUS");

	if(null != sanValue){
		if(!("S08" == sanValue || "S09" == sanValue)) {
			this.gfn_message("fail.실행.불가", "결재 실행을");
			return;
		}
	}
	
	var gridObj = this.ds_operIncme;			//그리드
	var gridSelectedRow = gridObj.rowposition;	//그리드 선택로우
	
	//보고서변환
	this.ds_cond00.clearData();
	this.ds_cond00.addRow();
	this.ds_cond00.copyRow(0, gridObj, gridSelectedRow);

	trace(this.ds_cond00.saveXML());

	var sanctnId = "";
	var strSvcId = "innerSanctnUbiDataset";
	var reportTitle  = "수납/수입결의서";
	var reportUri    = "/mis/bdg/BDG040102_R01.jrf";
	var saveFileName = "수납/수입결의서";
	var sController   	= "hsco/mis/bdg/BDG040102/sanSelectOperIncme.do";
	var sInDatasets   	= "input1=ds_cond00"; 
	var sOutDatasets  	= "ds_operIncmeSan=output1";
	
	this.gfn_makeReportTranInfo(sanctnId, strSvcId, sController, sInDatasets, sOutDatasets, reportTitle, reportUri, saveFileName);
	
	//내부결재요청
	//업무키 조합한다.
	var key = gridObj.getColumn(gridSelectedRow, "JOB_KEY");
	
	this.ds_innerSanctnParam.clearData();
	this.ds_innerSanctnParam.addRow();
	this.ds_innerSanctnParam.setColumn(0, "INNER_SANCTN_MDL_ID", "BDG-000001");	/*내부결재모형ID*/
	this.ds_innerSanctnParam.setColumn(0, "SYS_SE_CODE", "MIS");				/*시스템구분 COM024*/
	this.ds_innerSanctnParam.setColumn(0, "JOB_SE", "BDG");						/*업무구분 COM025*/
	this.ds_innerSanctnParam.setColumn(0, "JOB_KEY", key);						/*시스템 업무별 UNIQUE KEY*/
	this.ds_innerSanctnParam.setColumn(0, "SANCTN_INFO_FILESN", this.ds_operIncme.getColumn(this.ds_operIncme.rowposition, "FILE_SN"));	/* 첨부파일*/		
	//alert(this.ds_innerSanctnParam.setColumn(0, "SANCTN_INFO_FILESN"));
	var fn_cb = function(sPopupId, sReturn){
		var sanctnResult = sReturn.split("^"); /*index 0: 응답코드, index 1 : 내부결재ID*/

		trace("sanctnResult : " + sanctnResult[0] + "," + sanctnResult[1]);

		if("0000"==sanctnResult[0]){
			trace("info.정상처리");
			this.fn_search();
		}
		else {
			trace("에러");
		}
	}
	
	trace(gridObj.getColumn(gridSelectedRow, "REGIST_DT"));
	
	//공통요청 2016.08.30
	var subRegistDt = new String(gridObj.getColumn(gridSelectedRow, "REGIST_DT"));
	var registDt = subRegistDt.substring(0,8); 
	
	var result = this.gfn_openInnerSanctionPopup(this.ds_innerSanctnParam, fn_cb, registDt);
}

/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectOperIncme":	// 조회
			var sController   	= "hsco/mis/bdg/BDG040102/selectOperIncme.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_operIncme=output1";
			break;
		case "saveOperIncme":	// 저장
			var sController   	= "hsco/mis/bdg/BDG040102/saveOperIncme.do";	
			var sInDatasets   	= "input1=ds_operIncme:U";
			var sOutDatasets  	= "";
			break;
		case "insertOperIncme":	// 레포츠센터수입 일괄생성
			var sController   	= "hsco/mis/bdg/BDG040102/insertOperIncme.do";	
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "saveMakeChit":	// 레포츠센터수입 전표 일괄생성
			this.ds_arg.clearData();
			for( var i=0; i<this.ds_operIncme.getRowCount(); i++ ){
				if( this.ds_operIncme.getColumn(i,"CHK") == 1 ){
					var oiRow    = i;
					var year     = this.ds_operIncme.getColumn(oiRow, "YEAR");
					var bsnsSe   = this.ds_operIncme.getColumn(oiRow, "BSNS_SE");
					var balcSe   = this.ds_operIncme.getColumn(oiRow, "BALC_SE");
					var acntCode = this.ds_operIncme.getColumn(oiRow, "BUDGET_ACNT_CODE");
					var incmeDt  = this.ds_operIncme.getColumn(oiRow, "INCME_DT");
					var registId = this.gfn_getUserId();	
					
					var nRow = this.ds_arg.addRow();
					this.ds_arg.setColumn(nRow, "IN_YEAR",             year    );
					this.ds_arg.setColumn(nRow, "IN_BSNS_SE",          bsnsSe  );
					this.ds_arg.setColumn(nRow, "IN_BALC_SE",          balcSe  );
					this.ds_arg.setColumn(nRow, "IN_BUDGET_ACNT_CODE", acntCode);
					this.ds_arg.setColumn(nRow, "IN_INCME_DT",         incmeDt );
					this.ds_arg.setColumn(nRow, "IN_REGISTER_ID",      registId);
				}
			}
 			var sController   	= "hsco/mis/bdg/BDG040102/saveMakeChit.do";	
 			var sInDatasets   	= "input1=ds_arg";
 			var sOutDatasets  	= "";
 			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if (nErrorCode == '0'){
	
		switch (strSvcId) {
			case "selectOperIncme":		// 조회
				var count = this.ds_operIncme.getRowCount();
				if( count > 0) {
				
						for(var i=0; i<count; i++ ){
							if(parseInt(this.ds_operIncme.getColumn(i,"INCME_DT")) <= 20160930){
								this.ds_operIncme.setColumn(i,"LAST_SANCTNSTS_IMG", "theme://images/sanctn/sttus_S01.png");
								this.ds_operIncme.setColumn(i,"LAST_SANCTN_STTUS", "S01");
								this.ds_operIncme.setColumn(i,"CHK", "1");
							}
						}
				
				}
				break;
			case "saveOperIncme":		// 저장
				alert("처리되었습니다");
				this.fn_search();
				break;
			case "insertOperIncme":		// 레포츠센터수입 일괄생성
				this.fn_search();
				break;
			case "saveMakeChit":		// 레포츠센터수입  전표일괄생성
				alert("처리되었습니다");
				this.fn_search();
				break;
				
		}
	} else {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
}


