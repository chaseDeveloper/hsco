		}
	} else if(sPopupId == "ACC000000_P01") {
		//this.fn_search();
	} else if(sPopupId == "BDG040102_P01"){
		
		if(sReturn != undefined){
			this.ds_operIncme.setColumn(this.ds_operIncme.rowposition , "FILE_SN" , sReturn);
		}
	}
}



