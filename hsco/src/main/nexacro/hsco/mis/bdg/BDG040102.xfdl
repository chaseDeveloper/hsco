/*
 * 함수명  	: btn_insert_onclick
 * 설명 		: '생성' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_insert_onclick(obj,e);
 */
this.btn_insert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var inpDt = this.ds_cond.getColumn(0, "INP_DT");
	if(this.comUtils.isNull(inpDt)) {
		this.gfn_message("comm.항목.입력.을", "'생성일자'")
		return false;
	}
	
	// TODO : 생성은 팝업 만들어서, 선택적으로 추가할 수 있도록
	// TODO : 조건부 일괄삭제 기능도 필요함
	
	var inputDate = this.cal_insert.text;
	
	var confirmMsg = "[레포츠센터수입 자료를 생성합니다]" + "\n\n";
	confirmMsg    += "수입일자가 " + inputDate + "인 데이터를 제거한 후," + "\n";
	confirmMsg    += "레포츠센터 예산을 기준으로" + "\n";
	confirmMsg    += "수입일자가 " + inputDate + "인 데이터를 자동 생성합니다." + "\n\n";
	confirmMsg    += "계속 진행하시겠습니까?" + "\n";
	confirmMsg    += "(전표가 생성된 정보는 삭제 또는 수정되지 않습니다)" + "\n";
	confirmMsg    += "(변경 후 자동저장됩니다.)";
	
	if(confirm(confirmMsg)) {	
	//if(this.gfn_message("confirm.생성.여부", confirmMsg + "|레포츠센터수입 자료를")) {
		// transaction
		this.fn_transaction("insertOperIncme");	// 레포츠센터수입 일괄생성
	}
}

/*
 * 함수명  	: btn_makeChit_onclick
 * 설명 		: '전표' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_makeChit_onclick(obj,e);
 */
this.btn_makeChit_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var findRow = this.ds_operIncme.findRowExpr("CHK == '1' && LVDC_AM==0 && INCME_AM==0 && DECME_AM==0 && LEV_CHIT_NO==0 && INCME_CHIT_NO==0");
	if(findRow > -1) {
		this.gfn_message("comm.항목.입력.을", "'조정액' 또는 '수입액'");
		this.grd_operIncme.selectRow(findRow);
		return false;
	}

// 	if(this.comUtils.isDatasetUpdated(this.ds_operIncme)) {
// 		this.gfn_message("comm.정보.저장.요청", "전표 생성하기 전, 수정하신");
// 		return false;
// 	}
	
	//체크여부 2016.07.07
	var rowCount = this.ds_operIncme.getCaseCount("CHK == '1'");	
	//체크한 행이 없을 경우
	if(rowCount < 1){
		this.gfn_message("comm.데이터.선택.없음");
		return false;
	}	
	
	var findRow = this.ds_operIncme.findRowExpr("CHK_ORG=='0' && CHK=='1' && ATMC_JRNLZ_TY_CODE==null");
	if(findRow > -1) {
		this.gfn_message("comm.항목.입력.을", "'자동분개유형'");
		return false;
	}
	
	if(this.gfn_message("comm.행위여부", "전표를 생성")) {
		this.fn_transaction("saveMakeChit");	// 전표생성
	}
}


