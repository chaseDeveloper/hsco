/*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(검색조건 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged(obj,e);
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 연도가 바뀔때, 조회기간 및 생성일자의 연도필드를 자동으로 변경
	if(e.columnid == "YEAR") {
		var year = e.newvalue;
		if(!this.comUtils.isNull(year) && year.length == 4) {
			var inpDt = this.ds_cond.getColumn(e.row, "INP_DT");
			if(!this.comUtils.isNull(inpDt)) {
				this.ds_cond.setColumn(e.row, "INP_DT", year+inpDt.substr(4));
			}
			var stDt  = this.ds_cond.getColumn(e.row, "ST_DT");
			if(!this.comUtils.isNull(stDt)) {
				this.ds_cond.setColumn(e.row, "ST_DT", year+stDt.substr(4));
			}
			var edDt  = this.ds_cond.getColumn(e.row, "ED_DT");
			if(!this.comUtils.isNull(edDt)) {
				this.ds_cond.setColumn(e.row, "ED_DT", year+edDt.substr(4));
			}
		}
	}
}



