 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	var year  = this.comUtils.getClientDate("YYYY");
	var today = this.comUtils.getClientDate("YYYYMMDD");
	
	this.ds_cond.clearData();
	var nRow  = this.ds_cond.addRow();
 	
	// 공통코드
	var comboParams = [
	    ["ds_bdg001", "BDG001", "Y", "D", "B"]	// ds_bdg001 : 사업구분
	   ,["ds_bdg002", "BDG002", "Y", "D", "B"]	// ds_bdg002 : 수지구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 초기검색조건 설정
		this.ds_cond.setColumn(nRow, "YEAR",    year        );	// 연도
		this.ds_cond.setColumn(nRow, "BSNS_SE", "1"         );	// (사업구분) 1:사업예산, 2:자본예산, 3:자금운용
		this.ds_cond.setColumn(nRow, "BALC_SE", "1"         );	// (수지구분) 1:수입, 2:지출
		this.ds_cond.setColumn(nRow, "ST_DT",   year+"0101" );	// 조회시작일
		this.ds_cond.setColumn(nRow, "ED_DT",   today       );	// 조회종료일
		this.ds_cond.setColumn(nRow, "INP_DT",  today       );	// 레포츠센터 생성용 날짜
		
		this.fn_search();	//  최초조회
	};
	this.gfn_comboLoad (comboParams, callback);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
