}


/*
 * 함수명  	: saveExecutBudget
 * 설명 		: 실행 이벤트 처리(예산확정)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.saveExecutBudget();
 */
this.execFrmtnToExecutBudget = function()
{
	// update chk
	if(Ex.util.isUpdated(this.ds_frmtnBudget) || Ex.util.isUpdated(this.ds_frmtnComputBsis)) {
		var errMsg = "저장하지 않은 데이터가 존재합니다." + "\n";
		errMsg    += "현재 작업을 취소하신 후 계속 진행하시겠습니까?";
		if(!confirm(errMsg)) {
			return false;	// 아니오
		}
	}
	
	// transaction
	this.fn_transaction("execFrmtnToExecutBudget");
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBusinessDistrictList":	// 사업지구 코드 조회
			var sController   	= "hsco/mis/bdg/BDG030301/businessDistrictList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_bsnsType=output1";
			break;
		case "selectAcntCode":	// 세부사업 조회
			
			this.ds_cond.setColumn(0,"DETAIL_BSNS_CODE", this.div_search.edt_bsnsAreaCode.value);
			var sController   	= "/hsco/mis/bdg/BDG030301/selectAcntCode.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_acntCode=output1 ds_inpYn=output2";
			break;
		case "selectFrmtnBudget":		// sub데이터 조회(편성예산)
			// ds_acntCode -> ds_frmtnBudget
			var acRow    = this.ds_acntCode.rowposition;
			var year     = this.ds_acntCode.getColumn(acRow, "YEAR");
			var bsnsSe   = this.ds_acntCode.getColumn(acRow, "BSNS_SE");
			var balcSe   = this.ds_acntCode.getColumn(acRow, "BALC_SE");
			var acntCode = this.ds_acntCode.getColumn(acRow, "BUDGET_ACNT_CODE");
			
			// ds_cond -> ds_frmtnBudget
			var deptCode = this.ds_cond.getColumn(0, "DEPT_CODE");
			var deptNm   = this.ds_cond.getColumn(0, "DEPT_NM");
			var budgetSe = this.ds_cond.getColumn(0, "BUDGET_SE");
			
			// cond set
			this.ds_condSub1.clearData();
			var nRow = this.ds_condSub1.addRow();
			this.ds_condSub1.setColumn(nRow, "YEAR",             year     );	// 예산년도
			this.ds_condSub1.setColumn(nRow, "BSNS_SE",          bsnsSe   );	// 사업구분
			this.ds_condSub1.setColumn(nRow, "BALC_SE",          balcSe   );	// 수지구분
			this.ds_condSub1.setColumn(nRow, "BUDGET_ACNT_CODE", acntCode );	// 예산계정코드
			this.ds_condSub1.setColumn(nRow, "DEPT_CODE",        deptCode );	// 부서코드
			this.ds_condSub1.setColumn(nRow, "DEPT_NM",          deptNm   );	// 부서명
			this.ds_condSub1.setColumn(nRow, "BUDGET_SE",        budgetSe );	// 예산구분
			this.ds_condSub1.setColumn(nRow, "DETAIL_BSNS_CODE", this.div_search.edt_bsnsAreaCode.value);
			
			var sController   	= "/hsco/mis/bdg/BDG030301/selectFrmtnBudget.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_frmtnBudget=output1";
			break;
		case "selectFrmtnComputBsis":	// sub데이터 조회(산출기초)
			// ds_frmtnBudget -> ds_frmtnComputBsis
			var fbRow           = this.ds_frmtnBudget.rowposition;
