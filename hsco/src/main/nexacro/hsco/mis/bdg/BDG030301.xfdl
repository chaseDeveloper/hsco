		case "selectAllBsnsPrint":		// 전체 예산서 출력
			var sController   	= "/hsco/mis/bdg/BDG030301/selectAllBsnsPrint.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_print=output1";
