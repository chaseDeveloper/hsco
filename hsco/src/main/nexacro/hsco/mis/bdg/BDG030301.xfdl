{
	// ds_acntCode -> ds_frmtnBudget
	var acRow    = this.ds_acntCode.rowposition;
	var year     = this.ds_acntCode.getColumn(acRow, "YEAR");
	var bsnsSe   = this.ds_acntCode.getColumn(acRow, "BSNS_SE");
	var balcSe   = this.ds_acntCode.getColumn(acRow, "BALC_SE");
	var acntCode = this.ds_acntCode.getColumn(acRow, "BUDGET_ACNT_CODE");
	
	// ds_cond -> ds_frmtnBudget
	var deptCode = this.ds_cond.getColumn(0, "DEPT_CODE");
	var deptNm   = this.ds_cond.getColumn(0, "DEPT_NM");
	var budgetSe = this.ds_cond.getColumn(0, "BUDGET_SE");
	
	// cond set
	this.ds_condSub1.clearData();
	var nRow = this.ds_condSub1.addRow();
	this.ds_condSub1.setColumn(nRow, "YEAR",             year     );	// 예산년도
	this.ds_condSub1.setColumn(nRow, "BSNS_SE",          bsnsSe   );	// 사업구분
	this.ds_condSub1.setColumn(nRow, "BALC_SE",          balcSe   );	// 수지구분
	this.ds_condSub1.setColumn(nRow, "BUDGET_ACNT_CODE", acntCode   );	// 예산계정코드
	this.ds_condSub1.setColumn(nRow, "DEPT_CODE",        deptCode );	// 부서코드
	this.ds_condSub1.setColumn(nRow, "DEPT_NM",          deptNm   );	// 부서명
	this.ds_condSub1.setColumn(nRow, "BUDGET_SE",        budgetSe );	// 예산구분
	
	// popup set
	var nLeft    = system.clientToScreenX(this, 0);
	var nTop     = system.clientToScreenY(this, 0);
	var newChild = new ChildFrame;
	newChild.init("BDG030301_P01", "absolute", nLeft, nTop, 800, 620, null, null , "mis_bdg::BDG030301_P01.xfdl");
	newChild.set_openalign("center middle");
	newChild.set_dragmovetype("all");
	newChild.showModal(this.getOwnerFrame(), {ds_cond:this.ds_condSub1.saveXML()}, this, "fn_popupCallBack");
}


