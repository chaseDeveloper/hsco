/*
 * 함수명  	: ds_frmtnBudget_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(편성예산 dataset) 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_frmtnBudget_oncolumnchanged(obj,e);
 */
this.ds_frmtnBudget_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	try{
		// 산출기초 변경 시
		if(e.columnid == "COMPUT_BSIS_DTLS") {
			var strBsis = new String(e.newvalue);
			if(strBsis) {
				var result = "";
				for(var cRow = 0; cRow < strBsis.length; cRow++) {
					var chkStr = strBsis.charAt(cRow);
					if (chkStr.match(/[\\{|0-9.|\\}]+|[\(|\)|\+|\-|\*|\/]/)) {
						result += chkStr;
					} else if(chkStr == "%") {
						result += "/100";
					}
				}
				obj.set_enableevent(false);
			var budgetSe = this.ds_cond.getColumn(0, "BUDGET_SE");
			var acntRow = this.ds_acntCode.rowposition;
			if(budgetSe == 0 ){
				obj.setColumn(e.row, "BUDGET_0_AM", nexacro.floor(eval(result)));
				

			} else if(budgetSe == 1){
				obj.setColumn(e.row, "BUDGET_1_AM", nexacro.floor(eval(result)));
				

			} else if(budgetSe == 2){
				obj.setColumn(e.row, "BUDGET_2_AM", nexacro.floor(eval(result)));
				

			} else if(budgetSe == 3){
				obj.setColumn(e.row, "BUDGET_3_AM", nexacro.floor(eval(result)));
				

			}
			
				obj.set_enableevent(true);
			}
		}
		this.fn_acntCode_budgetAm_set(1);
	} catch(e){
		obj.set_enableevent(true);
		this.gfn_message("err.에러.발생", "", e.message);
	}
}


/*
 * 함수명  	: ds_frmtnComputBsis_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(편성예산 산출기초 dataset) 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_frmtnComputBsis_oncolumnchanged(obj,e);
 */
this.ds_frmtnComputBsis_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 산출기초코드 LENGTH 체크
	if(e.columnid == "COMPUT_BSIS_CODE"){
		var computBsisCode =  new String(e.newvalue);
		var lengthCode = computBsisCode.indexOf(" ")
		if( lengthCode > -1 ){
			if( computBsisCode.length - 1 == lengthCode ){
				computBsisCode = "0" + computBsisCode.substr(0,1);
			} else {
				computBsisCode = "0" + computBsisCode.substr(1,1);
			}
		}
		obj.setColumn(e.row, "COMPUT_BSIS_CODE", computBsisCode);
	}
	
	try{
		// 산출기초 변경 시
		if(e.columnid == "COMPUT_BSIS_DTLS") {
			var strBsis = new String(e.newvalue);

			if(strBsis) {
				var result = "";
				for(var cRow = 0; cRow < strBsis.length; cRow++) {
					var chkStr = strBsis.charAt(cRow);
					if (chkStr.match(/[\\{|0-9.|\\}]+|[\(|\)|\+|\-|\*|\/]/)) {
						result += chkStr;
					} else if(chkStr == "%") {
						result += "/100";
					}
				}
				obj.set_enableevent(false);

				// 백원단위 이하 올림처리
				// 123,100 => 124,000
				// 2016.10.04 박세현 수정
				//obj.setColumn(e.row, "BUDGET_AM", nexacro.floor(eval(result)));
				obj.setColumn(e.row, "BUDGET_AM", nexacro.ceil(eval(result)/1000)*1000);
				
				var frmtnBudgetAm  = obj.getColumn(e.row, "FRMTN_BUDGET_AM");
				//obj.setColumn(e.row, "MIN_AM", 
				obj.set_enableevent(true);
			}
		}
		this.fn_frmtnAm_set();
	}catch(e){
		obj.set_enableevent(true);
		this.gfn_message("err.에러.발생", "", e.message);
	}

}

/* 산출기초리스트 수정시 편성예산 반영*/
this.fn_frmtnAm_set = function(){
	var budgetAm = this.ds_frmtnComputBsis.getSum("BUDGET_AM");
	var budgetSe = this.div_search.cbo_BUDGET_SE.value;
	if( budgetSe == 0 ){
		this.ds_frmtnBudget.setColumn(this.ds_frmtnBudget.rowposition, "BUDGET_0_AM",budgetAm)
		
	} else if( budgetSe == 1 ){
		this.ds_frmtnBudget.setColumn(this.ds_frmtnBudget.rowposition, "BUDGET_1_AM",budgetAm)
		
	} else if( budgetSe == 2 ){
		this.ds_frmtnBudget.setColumn(this.ds_frmtnBudget.rowposition, "BUDGET_2_AM",budgetAm)
		
	} else if( budgetSe == 3 ){
		this.ds_frmtnBudget.setColumn(this.ds_frmtnBudget.rowposition, "BUDGET_3_AM",budgetAm)
		
	}
	this.fn_acntCode_budgetAm_set(1);
}


