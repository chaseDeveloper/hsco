		case "selectAcntClassPrint":		// 예산과목 분류 예산서 출력
			var sController   	= "/hsco/mis/bdg/BDG030301/selectAcntClassPrint.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_print1=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



