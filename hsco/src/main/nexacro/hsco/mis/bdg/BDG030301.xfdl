/*편성등록 조정액 반영 구분 budChk = 1:편성예산리스트 수정 */
this.fn_acntCode_budgetAm_set = function( budChk ){
		var budgetSe = this.ds_cond.getColumn(0, "BUDGET_SE");
		var acntRow = this.ds_acntCode.rowposition;
		var B = 0;
		if(budgetSe == 0 ){
			B = this.ds_frmtnBudget.getSum("BUDGET_0_AM");					// (편성조정액) 본예산
			var A = this.ds_acntCode.getColumn(acntRow, "DEMAND_BUDGET_AM");	// 기준액 (요구액(본예산)(A)
			this.ds_acntCode.setColumn( acntRow, "BUDGET_0_AM" , B);			// 본예산(B)
		} else if(budgetSe == 1){
			B = this.ds_frmtnBudget.getSum("BUDGET_1_AM");					// (편성조정액) 본예산
			var A = this.ds_acntCode.getColumn(acntRow, "BUDGET_0_AM");			// 본예산(A)
			this.ds_acntCode.setColumn( acntRow, "BUDGET_1_AM" , B);			// 추경1차(B)
		} else if(budgetSe == 2){
			B = this.ds_frmtnBudget.getSum("BUDGET_2_AM");					// (편성조정액) 본예산
	
			var A = this.ds_acntCode.getColumn(acntRow, "BUDGET_1_AM");			// 추경1차(A)
			this.ds_acntCode.setColumn( acntRow, "BUDGET_2_AM" , B);			// 추경2차(B)
		} else if(budgetSe == 3){
	
			B = this.ds_frmtnBudget.getSum("BUDGET_3_AM");									// (편성조정액) 본예산
			var A = this.ds_acntCode.getColumn(acntRow, "BUDGET_2_AM");			// 추경2차(A)
			this.ds_acntCode.setColumn( acntRow, "BUDGET_3_AM" , B);			// 추경3차(B)
		}

}
