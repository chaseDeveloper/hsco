/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(편성예산, 산출기초)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{

	var inpYn = this.ds_inpYn.getColumn(0, "INP_YN") == '1' ? true : false;
	if(!inpYn){
		alert("예산편성 등록기간이 아닙니다.");
		this.ds_frmtnBudget.reset();
		return false;
	}
	// validation 체크(편성예산, 산출기초)
	if(!Ex.util.isUpdated(this.ds_frmtnBudget) && !Ex.util.isUpdated(this.ds_frmtnComputBsis)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_frmtnBudget, this.ds_validation)) {
	
		return;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_frmtnComputBsis, this.ds_validation)) {
	
		return;
	}
	
	this.ds_frmtnBudget.set_enableevent(false);
	
	if(this.ds_frmtnComputBsis.getRowCount() > 0) {
		var budgetAmt = this.ds_frmtnComputBsis.getSum("BUDGET_AM");
		var fbRow = this.ds_frmtnBudget.rowposition;
		
		this.ds_frmtnBudget.setColumn(fbRow, "BUDGET_AM", budgetAmt);
	}
	
	for(var fRow = 0; fRow < this.ds_frmtnBudget.getRowCount(); fRow++) {
		var seAccnutCode = this.ds_frmtnBudget.getColumn(fRow, "SE_ACCNUT_ACNT_CODE");
		var budgetSe = this.ds_cond.getColumn(0, "BUDGET_SE");
		if(this.comUtils.isNull(seAccnutCode) && budgetSe == '0') {

		}
	}
	this.ds_frmtnBudget.set_enableevent(true);
	
	// transaction
	if(this.gfn_message("confirm.저장.여부"))	{
		var rvspbgtYm = new String(this.div_search.spn_YEAR.value) +  new String(this.cbo_choMonth.value);
		this.ds_frmtnBudget.setColumn(fbRow, "RVSPBGT_YM", rvspbgtYm);
		this.fn_transaction("save");
	}
}


/*
 * 함수명  	: saveDemandToFrmtn
 * 설명 		: 실행 이벤트 처리(요구예산 불러오기)
