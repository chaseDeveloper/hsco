 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectBusinessDistrictList":
				var today = new String(this.dateUtils.today());
				var month = today.substring(4,6);
				this.cbo_choMonth.set_value(month);
				break;
			case "selectAcntCode":			// 조회(계정리스트, 입력가능여부)
				
				var budgetSe = this.ds_cond.getColumn(0, "BUDGET_SE");
				switch(budgetSe) {
					case '0':
						this.grd_acntCode.set_formatid("budget0");
						this.grd_frmtnBudget.set_formatid("budget0");
						break;
					case '1':
						this.grd_acntCode.set_formatid("budget1");
						this.grd_frmtnBudget.set_formatid("budget1");
						break;
					case '2':
						this.grd_acntCode.set_formatid("budget2");
						this.grd_frmtnBudget.set_formatid("budget2");
						break;
					case '3':
						this.grd_acntCode.set_formatid("budget3");
						this.grd_frmtnBudget.set_formatid("budget3");
						break;
				}
				// 입력가능여부에 따라서 수정조건 처리
				var inpYn = this.ds_inpYn.getColumn(0, "INP_YN") == '1' ? true : false;

				
				this.btn_getDemandBudget.set_enable(inpYn);
				this.btn_ExecutBudget.set_enable(inpYn);
				var fRow =  this.ds_acntCode.findRow("EXECUT_CONFIRM_AT","1");
				if(fRow > -1){
					this.div_search.btn_executConfirmAt.set_visible(true);
					this.div_search.btn_executConfirmAt.style.set_background("red");
					this.killTimer(0);
					this.setTimer(0, 800);
				}else{
					this.killTimer(0);
					this.div_search.btn_executConfirmAt.set_visible(false);
				}
				this.btn_Add_M.set_enable(false);
				this.btn_Delete_M.set_enable(false);
				this.btn_Cancel_M.set_enable(false);
				this.btn_Add_B.set_enable(false);
				this.btn_Delete_B.set_enable(false);
				this.btn_Cancel_B.set_enable(false);
				this.btn_Save_M.set_enable(false);
				this.btn_Save_M00.set_enable(false);
				
				//예산담당자 아니면 비활성 2016.09.04
				if(this.gfn_authcode("108") == -1){
					
					this.btn_getDemandBudget.set_enable(false);
					this.btn_ExecutBudget.set_enable(false);
					
					this.btn_Add_M.set_enable(false);
					this.btn_Delete_M.set_enable(false);
					this.btn_Cancel_M.set_enable(false);
					this.btn_Add_B.set_enable(false);
					this.btn_Delete_B.set_enable(false);
					this.btn_Cancel_B.set_enable(false);
					this.btn_Save_M.set_enable(false);
					this.btn_Save_M00.set_enable(false);
				}				
				
				this.grd_frmtnBudget.setCellProperty("Body", 2,  "expandsize", inpYn==true?'13'        :'0'   );	// 사업코드팝업
				this.grd_frmtnBudget.setCellProperty("Body", 4,  "expandsize", inpYn==true?'13'        :'0'   );	// 예산계정코드팝업
				this.grd_frmtnBudget.setCellProperty("Body", 6,  "expandsize", inpYn==true?'13'        :'0'   );	// 부기코드 팝업
				this.grd_frmtnBudget.setCellProperty("Body",12,  "expandsize", inpYn==true && budgetSe=='0'?'13':'0');	// 구분회계코드 팝업(본예산일때만 입력가능)
				this.grd_frmtnComputBsis.setCellProperty("Body", 0, "edittype", inpYn==true?'mask'      :'none');	// 산출기초코드
				this.grd_frmtnComputBsis.setCellProperty("Body", 1, "edittype", inpYn==true?'normal'    :'none');	// 산출기초명
				this.grd_frmtnComputBsis.setCellProperty("Body", 3, "edittype", inpYn==true?'normal'    :'none');	// 산출기초내역
				this.grd_frmtnComputBsis.setCellProperty("Body", 4, "edittype", inpYn==true?'normal'    :'none');	// 산출기초참조
				
				break;
			case "selectFrmtnBudget":		// 조회(편성예산)
				break;
			case "selectFrmtnComputBsis": 	// 조회(산출기초)
				break;
			case "save": 					// 저장
				alert("저장됐습니다.");
				this.ds_acntCode.applyChange();
				break
			case "execDemandToFrmtnBudget":	// 요구예산불러오기(요구->편성)
				this.fn_search();
				break;
			case "execFrmtnToExecutBudget":	// 예산확정(편성->실행)
				this.fn_search();
				break;
			case "selectBsnsPrint":				// 사업별 증감 예산서 출력
				var vOpts = new this.cf_Opts();
				vOpts.setToolbar(false);
				vOpts.setScale(100);
				vOpts.setArgs("BSNS_NM", this.ds_cond.getColumn(0,"BSNS_NM"));
				if(this.ds_cond.getColumn(0,"BUDGET_SE") == "0"){
					vOpts.setReportPath("/mis/bdg/BDG030301_R04.jrf");
				} else {
					vOpts.setReportPath("/mis/bdg/BDG030301_R01.jrf");
				}
				vOpts.setSaveFilename("사업별증감예산서");
				vOpts.setLocalDs(["ds_print", this.ds_print]);
				vOpts.setArgs("title", "사업별증감예산서");
				var parm = {
					arg_0 : this,
					opts : vOpts
				};
				this.gfn_popup("Popup_Report", 884, 735, "사업별증감예산서", parm, "common::frmReportPopup.xfdl", "report_callback");
				break;
			case "selectAllBsnsPrint":				// 사업별 전체 예산서 출력
				var vOpts = new this.cf_Opts();
				vOpts.setToolbar(false);
				vOpts.setScale(100);
				if(this.ds_cond.getColumn(0,"BUDGET_SE") == "0"){
					vOpts.setReportPath("/mis/bdg/BDG030301_R04.jrf");
				} else {
					vOpts.setReportPath("/mis/bdg/BDG030301_R03.jrf");
				}
				vOpts.setSaveFilename("사업별전체예산서");
				vOpts.setLocalDs(["ds_print", this.ds_print]);
				vOpts.setArgs("title", "사업별전체별예산서");
				var parm = {
					arg_0 : this,
					opts : vOpts
				};
				this.gfn_popup("Popup_Report", 884, 735, "사업별전체예산서", parm, "common::frmReportPopup.xfdl", "report_callback");
				break;
			case "selectAcntClassPrint":				// 예산과목 분류 예산서 출력
				var title = "";
				var title_1 = "";
				var title_2 = "";
				var bsnsSe = this.ds_cond.getColumn(0,"BSNS_SE");
				var balcSe = this.ds_cond.getColumn(0,"BALC_SE");
				if( bsnsSe == "1"){
					title_1 = "사업예산";
				}else if(bsnsSe == "2"){
					title_1 = "자본예산";
				}else if(bsnsSe == "3"){
					title_1 = "자금운용";
				}else{
					title_1 = "";
				}
				if(balcSe == "1"){
					title_2 = "수입";
				} else if(balcSe == "2"){
					title_2 = "지출";
				} else {
					title_1 = "";
				}
				title = title_1 + " " + title_2 + " 예산과목 분류";
				var vOpts = new this.cf_Opts();
				vOpts.setToolbar(false);
				vOpts.setScale(100);
				vOpts.setReportPath("/mis/bdg/BDG030301_R05.jrf");
				vOpts.setSaveFilename("예산과목분류예산서");
				vOpts.setLocalDs(["ds_print1", this.ds_print1]);
				vOpts.setArgs("title", title);
				var parm = {
					arg_0 : this,
					opts : vOpts
				};
				this.gfn_popup("Popup_Report", 884, 735, "예산과목분류예산서", parm, "common::frmReportPopup.xfdl", "report_callback");
				break;
		}
	}
}


 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 사업코드 선택
	if(sPopupId == "popBsns") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var fbRow    = this.ds_frmtnBudget.rowposition;
			var bsnsCode = dsTemp.getColumn(0, "SE_ACCNUT_ACNT_CODE");
			var bsnsNm   = dsTemp.getColumn(0, "SE_ACCNUT_ACNT_NM");
			
			this.ds_frmtnBudget.setColumn(fbRow, "DETAIL_BSNS_CODE", bsnsCode);	// 사업코드
			this.ds_frmtnBudget.setColumn(fbRow, "DETAIL_BSNS_NM",   bsnsNm  );	// 사업명
		}
	}
	
	if(sPopupId == "deptPop") {
		var dsDeptCode = new Dataset();
		var loadCnt = dsDeptCode.loadXML(sReturn);
		if(loadCnt) {
			var fbRow    = this.ds_frmtnBudget.rowposition;
			var deptCode = dsDeptCode.getColumn(0, "DEPT_CODE");
			var deptNm   = dsDeptCode.getColumn(0, "DEPT_NM");
			
			this.ds_frmtnBudget.setColumn(fbRow, "DEPT_CODE", deptCode);
			this.ds_frmtnBudget.setColumn(fbRow, "DEPT_NM",   deptNm  );
		}
	}
	
	if(sPopupId == "BDG030101_P01") {
		var dsRtn = new Dataset();
		var loadCnt = dsRtn.loadXML(sReturn);
		if(loadCnt) {
			var unitCode   = dsRtn.getColumn(0, "EXPNDTR_UNIT_CODE");
			var unitNm     = dsRtn.getColumn(0, "EXPNDTR_UNIT_NM");

			var deptCode = dsRtn.getColumn(0, "DEPT_CODE");
			var deptNm = dsRtn.getColumn(0, "DEPT_NM");
			
			var detailBsnsCode = dsRtn.getColumn(0, "BSNS_CODE");
			var detailBsnsNm = dsRtn.getColumn(0, "BSNS_NM");
			
			
			var fbRow = this.ds_frmtnBudget.rowposition;
			this.ds_frmtnBudget.setColumn(fbRow, "EXPNDTR_UNIT_CODE", unitCode  );
			this.ds_frmtnBudget.setColumn(fbRow, "EXPNDTR_UNIT_NM",   unitNm    );
			this.ds_frmtnBudget.setColumn(fbRow, "DETAIL_BSNS_CODE",  detailBsnsCode    );
			this.ds_frmtnBudget.setColumn(fbRow, "DETAIL_BSNS_NM",    detailBsnsNm    );
			this.ds_frmtnBudget.setColumn(fbRow, "DEPT_CODE",     	  deptCode );
			this.ds_frmtnBudget.setColumn(fbRow, "DEPT_NM",     	  deptNm );
		}
	}
	
	if(sPopupId == "ACC000000_P03") {
		var dsRtn = new Dataset();
		var loadCnt = dsRtn.loadXML(sReturn);
		if(loadCnt) {
			var seAccnutCode = dsRtn.getColumn(0, "SE_ACCNUT_ACNT_CODE");
			
			var fbRow = this.ds_frmtnBudget.rowposition;
			this.ds_frmtnBudget.setColumn(fbRow, "SE_ACCNUT_ACNT_CODE", seAccnutCode);
		}
	}
}



