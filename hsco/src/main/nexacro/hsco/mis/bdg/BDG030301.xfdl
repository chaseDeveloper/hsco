			var deptCode        = this.ds_frmtnBudget.getColumn(fbRow, "DEPT_CODE");
			var deptNm          = this.ds_frmtnBudget.getColumn(fbRow, "DEPT_NM");
			var bsnsSe          = this.ds_frmtnBudget.getColumn(fbRow, "BSNS_SE");
			var balcSe          = this.ds_frmtnBudget.getColumn(fbRow, "BALC_SE");
			var budgetSe        = this.ds_frmtnBudget.getColumn(fbRow, "BUDGET_SE");
			var bsnsCode        = this.ds_frmtnBudget.getColumn(fbRow, "DETAIL_BSNS_CODE");
			var acntCode        = this.ds_frmtnBudget.getColumn(fbRow, "BUDGET_ACNT_CODE");
			var expndtrUnitCode = this.ds_frmtnBudget.getColumn(fbRow, "EXPNDTR_UNIT_CODE");
			
			// cond set
			this.ds_condSub2.clearData();
			var nRow = this.ds_condSub2.addRow();
			this.ds_condSub2.setColumn(nRow, "YEAR",              year            );	// 예산년도
			this.ds_condSub2.setColumn(nRow, "DEPT_CODE",         deptCode        );	// 부서코드
			this.ds_condSub2.setColumn(nRow, "DEPT_NM",           deptNm          );	// 부서명
			this.ds_condSub2.setColumn(nRow, "BSNS_SE",           bsnsSe          );	// 사업구분
			this.ds_condSub2.setColumn(nRow, "BALC_SE",           balcSe          );	// 수지구분
			this.ds_condSub2.setColumn(nRow, "BUDGET_SE",         budgetSe        );	// 예산구분
			this.ds_condSub2.setColumn(nRow, "DETAIL_BSNS_CODE",  bsnsCode        );	// 사업코드
			this.ds_condSub2.setColumn(nRow, "BUDGET_ACNT_CODE",  acntCode        );	// 예산계정코드
			this.ds_condSub2.setColumn(nRow, "EXPNDTR_UNIT_CODE", expndtrUnitCode );	// 부기코드
		//	this.ds_condSub2.setColumn(nRow, "DETAIL_BSNS_CODE", this.div_search.edt_bsnsAreaCode.value);
			var sController   	= "/hsco/mis/bdg/BDG030301/selectFrmtnComputBsis.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_frmtnComputBsis=output1";
			break;
		case "save":	// 저장
			var sController   	= "hsco/mis/bdg/BDG030301/save.do";
			var sInDatasets   	= "input1=ds_frmtnBudget:U input2=ds_frmtnComputBsis:U";
			var sOutDatasets  	= "";
			break;
		case "execDemandToFrmtnBudget":	// 요구예산불러오기(요구->편성)
			var sController   	= "hsco/mis/bdg/BDG030301/execDemandToFrmtnBudget.do";
			var sInDatasets   	= "input1=ds_cond"
			var sOutDatasets  	= "";
			break;
		case "execFrmtnToExecutBudget":	// 예산확정(편성->실행)
			this.ds_cond.setColumn(0,"RVSPBGT_YM", this.cbo_choMonth.value);
			var sController   	= "hsco/mis/bdg/BDG030301/execFrmtnToExecutBudget.do";
			var sInDatasets   	= "input1=ds_cond"
			var sOutDatasets  	= "";
			break;
		case "selectBsnsPrint":		// 증감예산서 출력
			var sController   	= "/hsco/mis/bdg/BDG030301/selectBsnsPrint.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_print=output1";
