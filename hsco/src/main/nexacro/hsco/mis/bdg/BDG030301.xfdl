/*
 * 함수명  	: ds_acntCode_canrowposchange
 * 설명 		: Dataset 행 변경 전 이벤트 처리(계정리스트 dataset) 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_acntCode_canrowposchange(obj,e);
 */
this.ds_acntCode_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(Ex.util.isUpdated(this.ds_frmtnBudget)) {
	
		var errMsg = "[예산계정상세내역 Grid]" + "\n";
		errMsg    += "저장하지 않은 데이터가 존재합니다." + "\n";
		errMsg    += "현재 작업을 취소하신 후 계속 진행하시겠습니까?";
		
		if(!confirm(errMsg)) {
			return false;	// 아니오
		} else {
			this.ds_acntCode.reset();
			this.ds_frmtnComputBsis.clearData();
			this.ds_frmtnBudget.clearData();
		}
			
	}
}

/*
 * 함수명  	: ds_acntCode_onrowposchanged
 * 설명 		: Dataset 행 변경 후 이벤트 처리(계정리스트 dataset) 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_acntCode_onrowposchanged(obj,e);
 */
this.ds_acntCode_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
 	// 버튼 활성화여부 결정
 	var inpYn     = this.ds_inpYn.getColumn(0, "INP_YN");
 	var rowCount = this.ds_acntCode.findRow( "UPPER_ACNT_CODE", this.ds_acntCode.getColumn(e.newrow,"BUDGET_ACNT_CODE"));
 	var bEnable = rowCount > -1 || inpYn != '1' ? false : true;
	
	this.btn_Add_M.set_enable(bEnable);
	this.btn_Delete_M.set_enable(bEnable);
	this.btn_Cancel_M.set_enable(bEnable);
	this.btn_Save_M.set_enable(bEnable);
	this.btn_Save_M00.set_enable(bEnable);
	this.btn_Add_B.set_enable(bEnable);
	this.btn_Delete_B.set_enable(bEnable);
	this.btn_Cancel_B.set_enable(bEnable);
	
	
	// sub 데이터 조회
	this.fn_search_sub1();
}


/*
 * 함수명  	: ds_frmtnBudget_canrowposchange
 * 설명 		: Dataset 행 변경 전 이벤트 처리(편성예산 dataset) 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_frmtnBudget_canrowposchange(obj,e);
 */
this.ds_frmtnBudget_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(Ex.util.isUpdated(this.ds_frmtnComputBsis)) {
		var errMsg = "[산출기초리스트 Grid]" + "\n";
		errMsg    += "저장하지 않은 데이터가 존재합니다." + "\n";
		errMsg    += "현재 작업을 취소하신 후 계속 진행하시겠습니까?";
		
		if(!confirm(errMsg)) {
			return false;	// 아니오
		} else {
			this.ds_frmtnBudget.reset();
		}
	}
}


/*
 * 함수명  	: ds_frmtnBudget_onrowposchanged
 * 설명 		: Dataset 행 변경 후 이벤트 처리(편성예산 dataset) 
