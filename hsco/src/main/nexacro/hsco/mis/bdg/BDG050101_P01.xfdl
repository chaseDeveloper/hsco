      <BindItem id="item17" compid="msk_MVMN_AM" propid="value" datasetid="ds_cond" columnid="MVMN_AM"/>
      <BindItem id="item19" compid="chk_ELPD_EXCUT_AT" propid="value" datasetid="ds_cond" columnid="ELPD_EXCUT_AT"/>
