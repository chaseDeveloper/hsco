***********************************************************************/

/*
 * 함수명  	: fn_getEnableAt
 * 설명 		: enable 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_getEnableAt();
 */
this.fn_getEnableAt = function(nRow)
{
	var inpYn = this.ds_inpYn.getColumn(0, "INP_YN");
	var dcsnAt = this.ds_stacntDetail.getColumn(nRow, "CYFD_DCSN_AT");
	
	if(inpYn != "1") {
		return false;
	}
	else {
		if(dcsnAt == "1") {
			return false;
		}
		else {
			return true;
		}
	}
}
