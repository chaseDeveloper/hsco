/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(계정별 예산결산)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// clear Datasets
	this.ds_stacntGrp.clearData();
	this.ds_stacntDetail.clearData();
	
	var year  = this.ds_cond.getColumn(0, "YEAR");
	var nYear = nexacro.toNumber(year, 0);
	
	if(nYear < 2018) {
		this.ds_cond.setColumn(0, "YEAR_SE", "1");
	}
	else {
		this.ds_cond.setColumn(0, "YEAR_SE", "2");
	}
	
	// transaction
	this.fn_transaction("selectStacntGrp");
}


/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(부기별 예산결산)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub1();
 */
this.fn_search_sub = function()
{
	// clear Datasets
	this.ds_stacntDetail.clearData();
	
	// transaction
	if(this.ds_stacntGrp.getRowCount() > 0) {
		this.fn_transaction("selectStacntDetail");
	}
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if(!this.comUtils.isDatasetUpdated(this.ds_stacntGrp) && !this.comUtils.isDatasetUpdated(this.ds_stacntDetail)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	var pRow = this.ds_stacntGrp.rowposition;
	var year = this.ds_stacntGrp.getColumn(pRow, "YEAR");
	var blce = this.ds_stacntGrp.getColumn(pRow, "BLCE");
	
	var cyfdDemandAm = 0;
	var rCnt = this.ds_stacntDetail.getRowCount();
	
	if(year < '2018') {	// 2017년 이전(이월요구금액이 예산결산내역의 잔액을 넘지 않도록)
		if(!this.gfn_checkValidation(this.ds_stacntDetail, this.ds_validation1)) {
			return;
		}
		
		for(var i = 0; i <rCnt; i++) {
			cyfdDemandAm += this.ds_stacntDetail.getColumn(i, "BUDGET_AM");
		}
		
		if(blce < cyfdDemandAm) {
			this.gfn_message("info.처리불가", "이월요구금액의 합계가 잔액보다 많습니다.\n확인 후 다시 시도하여 주십시오.")
			return false;
		}
	} else {			// 2018년 이후(이월요구금액이 잔액을 넘지 않도록)
		if(!this.gfn_checkValidation(this.ds_stacntDetail, this.ds_validation2)) {
			return;
		}
		
		for(var i = 0; i <rCnt; i++) {
			cyfdDemandAm += this.ds_stacntDetail.getColumn(i, "BUDGET_AM");
		}
		
		if(blce < cyfdDemandAm) {
			this.gfn_message("info.처리불가", "이월요구금액의 합계가 잔액보다 많습니다.\n확인 후 다시 시도하여 주십시오.")
			return false;
		}
	}
	
	if(this.gfn_message("confirm.저장.여부"))	{
		this.fn_transaction("save");
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectStacntGrp":		// 조회(예산결산내역)
			var sController   	= "/hsco/mis/bdg/BDG030102/selectStacntGrp.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_stacntGrp=output1 ds_inpYn=output2";
			break;
		case "selectStacntDetail":	// 조회(이월예산요구등록)			
			var sController   	= "/hsco/mis/bdg/BDG030102/selectStacntDetail.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_stacntDetail=output1";
			break;
		case "save":				// 저장
			var sController   	= "hsco/mis/bdg/BDG030102/save.do";
			var sInDatasets   	= "input1=ds_stacntDetail:U";
			var sOutDatasets  	= "";
			break;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectStacntGrp":			// 조회(계정별 예산결산)
				if(this.saveRow > -1) {
					this.ds_stacntGrp.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				
				var year = this.ds_cond.getColumn(0, "YEAR");
				if (year < 2018) {	// 2017년 이하
					this.grd_stacntGrp.setFormat("b2017");
					this.grd_stacntDetail.setFormat("b2017");
				}
				else {				// 2018년 이상
					this.grd_stacntGrp.setFormat("a2018");
					this.grd_stacntDetail.setFormat("a2018");
				}
				break;
			case "selectStacntDetail":		// 조회(부기별 예산결산)
				if(this.ds_stacntDetail.getRowCount() > 0 && this.saveRow2 > -1) {
					this.ds_stacntDetail.set_rowposition(this.saveRow2);
					this.saveRow2 = -1;
				}
				break;
			case "save": 					// 저장
				this.saveRow  = this.ds_stacntGrp.rowposition;
				this.saveRow2 = this.ds_stacntDetail.rowposition;
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}


 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	var dsRtn = new Dataset();
	var loadCnt = dsRtn.loadXML(sReturn);
	
	// 사업코드(조회조건)
	if(sPopupId == "popBsns") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
 		if(loadCnt) {
			var bsnsCode = dsTemp.getColumn(0, "BSNS_CODE");
 			var bsnsNm   = dsTemp.getColumn(0, "BSNS_NM");
			this.ds_cond.setColumn(0, "BSNS_CODE", bsnsCode);
			this.ds_cond.setColumn(0, "BSNS_NM"  , bsnsNm);
 		}
	}
	
	// 이월예산정보
	if(sPopupId == "cyfdAcntPop") {
		if(loadCnt){
			var detailBsnsCode  = dsRtn.getColumn(0, "DETAIL_BSNS_CODE");
			var detailBsnsNm    = dsRtn.getColumn(0, "DETAIL_BSNS_NM");
			var deptCode        = dsRtn.getColumn(0, "DEPT_CODE");
			var deptNm          = dsRtn.getColumn(0, "DEPT_NM");
			var budgetAcntCode  = dsRtn.getColumn(0, "BUDGET_ACNT_CODE");
			var budgetAcntNm    = dsRtn.getColumn(0, "BUDGET_ACNT_NM");
			var expndtrUnitCode = dsRtn.getColumn(0, "EXPNDTR_UNIT_CODE");
			var expndtrUnitNm   = dsRtn.getColumn(0, "EXPNDTR_UNIT_NM");
			
			var sdRow = this.ds_stacntDetail.rowposition;
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_DETAIL_BSNS_CODE",  detailBsnsCode  );
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_DETAIL_BSNS_NM",    detailBsnsNm    );
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_DEPT_CODE",         deptCode        );
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_DEPT_NM",           deptNm          );
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_BUDGET_ACNT_CODE",  budgetAcntCode  );
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_BUDGET_ACNT_NM",    budgetAcntNm    );
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_EXPNDTR_UNIT_CODE", expndtrUnitCode );
			this.ds_stacntDetail.setColumn(sdRow, "CYFD_EXPNDTR_UNIT_NM",   expndtrUnitNm   );
		}
	}
	
	// 예산계정 및 부기
	if(sPopupId == "bdgAcntPop") {
		if(loadCnt){
			var detailBsnsCode  = dsRtn.getColumn(0, "DETAIL_BSNS_CODE");
			var detailBsnsNm    = dsRtn.getColumn(0, "DETAIL_BSNS_NM");
			var expndtrUnitCode = dsRtn.getColumn(0, "EXPNDTR_UNIT_CODE");
			var expndtrUnitNm   = dsRtn.getColumn(0, "EXPNDTR_UNIT_NM");
			
			var sdRow = this.ds_stacntDetail.rowposition;
			this.ds_stacntDetail.setColumn(sdRow, "DETAIL_BSNS_CODE",  detailBsnsCode  );
			this.ds_stacntDetail.setColumn(sdRow, "DETAIL_BSNS_NM",    detailBsnsNm    );
			this.ds_stacntDetail.setColumn(sdRow, "EXPNDTR_UNIT_CODE", expndtrUnitCode );
			this.ds_stacntDetail.setColumn(sdRow, "EXPNDTR_UNIT_NM",   expndtrUnitNm   );
		}
	}
}



