this.grd_stacntDetail_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	var sgRow          = this.ds_stacntGrp.rowposition;
	var year           = this.ds_stacntGrp.getColumn(sgRow, "YEAR");
	var nYear          = nexacro.toNumber(year, 0);
	var aYear          = nYear + 1;
	var bsnsSe         = this.ds_stacntGrp.getColumn(sgRow, "BSNS_SE");
	var balcSe         = this.ds_stacntGrp.getColumn(sgRow, "BALC_SE");
	var bsnsCode       = this.ds_stacntGrp.getColumn(sgRow, "DETAIL_BSNS_CODE");
	var bsnsNm         = this.ds_stacntGrp.getColumn(sgRow, "DETAIL_BSNS_NM");
	var deptCode       = this.ds_stacntGrp.getColumn(sgRow, "DEPT_CODE");
	var deptNm         = this.ds_stacntGrp.getColumn(sgRow, "DEPT_NM");
	var budgetAcntCode = this.ds_stacntGrp.getColumn(sgRow, "BUDGET_ACNT_CODE");
	
	if(year < 2018) {	// 2017년 이하
		if(e.cell == 10) {			// 이월(사업, 부서, 예산계정, 부기)
			var parm = {
				 arg_0        : this
				,searchDs     : this.ds_cond.saveXML()							// 검색조건(YEAR, BSNS_SE, BALC_SE, DEPT_CODE 컬럼만 이용)
				,strObj       : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE"				// 수정불가능 처리할 오브젝트 명
				,year_S       : aYear											// 이월년도
				,bsnsCode_S   : bsnsCode										// 사업코드
				,bsnsNm_S     : bsnsNm											// 사업명
				,deptCode_S   : deptCode										// 부서코드
				,deptNm_S     : deptNm											// 부서명
				,acntType     : "MVMN"											// 수혜계정
				,acntPopup    : false											// (true : 예산계정코드선택팝업, false : 부기선택팝업)
			};
			this.gfn_popup("cyfdAcntPop", 900, 635, "예산선택팝업", parm, "mis_bdg::BDG020101_P02.xfdl", "fn_popupCallBack", true);
		} else if(e.cell == 15) {	// 사업/예산계정/부기
			var parm = {
				 arg_0      : this
				,strObj     : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE"
				,year_S     : nYear
				,bsnsSe_S   : bsnsSe
				,balcSe_S   : balcSe
				,bsnsCode_S : bsnsCode
				,bsnsNm_S   : bsnsNm
				,deptCode_S : deptCode
				,deptNm_S   : deptNm
				,acntCode_S	: budgetAcntCode
			};
			this.gfn_popup("bdgAcntPop", 1000, 635, "", parm, "com::bdgAcntPop.xfdl", "fn_popupCallBack", true);
		}
	} else if(year > 2017) {	// 2018년 이상
		if(e.cell == 6) {			// 이월(사업, 부서, 예산계정, 부기)
			var parm = {
				 arg_0        : this
				,searchDs     : this.ds_cond.saveXML()							// 검색조건(YEAR, BSNS_SE, BALC_SE, DEPT_CODE 컬럼만 이용)
				,strObj       : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE"				// 수정불가능 처리할 오브젝트 명
				,year_S       : aYear											// 이월년도
				,bsnsCode_S   : bsnsCode										// 사업코드
				,bsnsNm_S     : bsnsNm											// 사업명
				,deptCode_S   : deptCode										// 부서코드
				,deptNm_S     : deptNm											// 부서명
				,acntType     : "MVMN"											// 수혜계정
				,acntPopup    : false											// (true : 예산계정코드선택팝업, false : 부기선택팝업)
			};
			this.gfn_popup("cyfdAcntPop", 900, 635, "예산선택팝업", parm, "mis_bdg::BDG020101_P02.xfdl", "fn_popupCallBack", true);
		}
	}
}


this.btn_Add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_stacntGrp.getRowCount() == 0) {
		this.gfn_message("info.데이터.없음");
		return false;
	}
	
	var sgRow           = this.ds_stacntGrp.rowposition;
	var year            = this.ds_stacntGrp.getColumn(sgRow, "YEAR");
	var nYear           = nexacro.toNumber(year, 0);
	var bsnsSe          = this.ds_stacntGrp.getColumn(sgRow, "BSNS_SE");
	var balcSe          = this.ds_stacntGrp.getColumn(sgRow, "BALC_SE");
	var detailBsnsCode  = this.ds_stacntGrp.getColumn(sgRow, "DETAIL_BSNS_CODE");
	var detailBsnsNm    = this.ds_stacntGrp.getColumn(sgRow, "DETAIL_BSNS_NM");
	var deptCode        = this.ds_stacntGrp.getColumn(sgRow, "DEPT_CODE");
	var deptNm          = this.ds_stacntGrp.getColumn(sgRow, "DEPT_NM");
	var budgetAcntCode  = this.ds_stacntGrp.getColumn(sgRow, "BUDGET_ACNT_CODE");
	var budgetAcntNm    = this.ds_stacntGrp.getColumn(sgRow, "BUDGET_ACNT_NM");
	var expndtrUnitCode = this.ds_stacntGrp.getColumn(sgRow, "EXPNDTR_UNIT_CODE");
	var expndtrUnitNm   = this.ds_stacntGrp.getColumn(sgRow, "EXPNDTR_UNIT_NM");
	
	var se = "";
	var rCnt = this.ds_bdg008.rowcount;
	if(rCnt > 0) {
		se = this.ds_bdg008.getColumn(0, "CODE");
	}
	
	var nRow = this.ds_stacntDetail.addRow();
	this.ds_stacntDetail.setColumn(nRow, "YEAR",              year           );	// 예산년도
	this.ds_stacntDetail.setColumn(nRow, "BSNS_SE",           bsnsSe         );	// 사업구분
	this.ds_stacntDetail.setColumn(nRow, "BALC_SE",           balcSe         );	// 수지구분
	this.ds_stacntDetail.setColumn(nRow, "DETAIL_BSNS_CODE",  detailBsnsCode );	// 사업코드
	this.ds_stacntDetail.setColumn(nRow, "DETAIL_BSNS_NM",    detailBsnsNm   );	// 사업명
	this.ds_stacntDetail.setColumn(nRow, "DEPT_CODE",         deptCode       );	// 부서코드
	this.ds_stacntDetail.setColumn(nRow, "DEPT_NM",           deptNm         );	// 부서명
	this.ds_stacntDetail.setColumn(nRow, "BUDGET_ACNT_CODE",  budgetAcntCode );	// 예산계정코드
	this.ds_stacntDetail.setColumn(nRow, "BUDGET_ACNT_NM",    budgetAcntNm   );	// 예산계정명
	this.ds_stacntDetail.setColumn(nRow, "SE",                se             );	// 이월구분
	if(nYear > 2017) {	// 2018년 이상
		this.ds_stacntDetail.setColumn(nRow, "EXPNDTR_UNIT_CODE", expndtrUnitCode );	// 부기코드
		this.ds_stacntDetail.setColumn(nRow, "EXPNDTR_UNIT_NM",   expndtrUnitNm   );	// 부기명
	}
	
}


this.btn_Delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_stacntGrp.getRowCount() == 0) {
		this.gfn_message("info.데이터.없음");
		return false;
	}
	
	var nRow = this.ds_stacntDetail.rowposition;
	var dcsnAt = this.ds_stacntDetail.getColumn(nRow, "CYFD_DCSN_AT");
	if(dcsnAt == "1") {
		this.gfn_message("info.처리불가", "이월확정된 정보는 수정할 수 없습니다.");
		return false;
	}
	
	if(!this.gfn_message("confirm.삭제여부")) {
		return false;	// 아니오
	}
	this.ds_stacntDetail.deleteRow(this.ds_stacntDetail.rowposition);
}


this.btn_Cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_stacntGrp.getRowCount() == 0) {
		this.gfn_message("info.데이터.없음");
		return false;
	}
	
	if(Ex.util.isUpdated(this.ds_stacntDetail)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	this.ds_stacntDetail.reset();
}


