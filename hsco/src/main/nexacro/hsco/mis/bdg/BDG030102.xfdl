/*
 * 함수명  	: ds_stacntGrp_onrowposchanged
 * 설명 		: Dataset 행 변경 후 이벤트 처리(계정별 예산결산 dataset) 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_stacntGrp_onrowposchanged(obj,e);
 */
this.ds_stacntGrp_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1) {
		// ds_stacntGrp -> ds_stacntDetail
		var pRow            = this.ds_stacntGrp.rowposition;
		var year            = this.ds_stacntGrp.getColumn(pRow, "YEAR");
		var bsnsSe          = this.ds_stacntGrp.getColumn(pRow, "BSNS_SE");
		var balcSe          = this.ds_stacntGrp.getColumn(pRow, "BALC_SE");
		var detailBsnsCode  = this.ds_stacntGrp.getColumn(pRow, "DETAIL_BSNS_CODE");
		var deptCode        = this.ds_stacntGrp.getColumn(pRow, "DEPT_CODE");
		var budgetAcntCode  = this.ds_stacntGrp.getColumn(pRow, "BUDGET_ACNT_CODE");
		var expndtrUnitCode = this.ds_stacntGrp.getColumn(pRow, "EXPNDTR_UNIT_CODE");
		var nYear           = nexacro.toNumber(year, 0);
		
		// cond set
		this.ds_condSub.clearData();
		var nRow = this.ds_condSub.addRow();
		this.ds_condSub.setColumn(nRow, "YEAR",              year            );
		this.ds_condSub.setColumn(nRow, "BSNS_SE",           bsnsSe          );
		this.ds_condSub.setColumn(nRow, "BALC_SE",           balcSe          );
		this.ds_condSub.setColumn(nRow, "DETAIL_BSNS_CODE",  detailBsnsCode  );
		this.ds_condSub.setColumn(nRow, "DEPT_CODE",         deptCode        );
		this.ds_condSub.setColumn(nRow, "BUDGET_ACNT_CODE",  budgetAcntCode  );
		this.ds_condSub.setColumn(nRow, "EXPNDTR_UNIT_CODE", expndtrUnitCode );
		
		if(nYear < 2018) {
			this.ds_condSub.setColumn(nRow, "YEAR_SE", "1");
		}
		else {
			this.ds_condSub.setColumn(nRow, "YEAR_SE", "2");
		}
		
		this.fn_search_sub();
	}
}


