this.div_DetailMvmn_btn_bdgAcntPop_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var mbRow = this.ds_mvmnBudget.rowposition;
	
	var deptCode = this.ds_mvmnBudget.getColumn(mbRow, "MVMN_BUDGET_DEPT");	// 수혜계정의 예산부서
	if( this.comUtils.isNull(deptCode)) {
		this.gfn_message("info.확인.진행가능", "'예비비신청계정_예산부서' 입력여부 ");
		return false;
	}
	
	var provdAcntCode = this.ds_mvmnBudget.getColumn(mbRow, "PROVD_ACNT_CODE");	// 제공계정의 계정코드
	if( this.comUtils.isNull(provdAcntCode)) {
		this.gfn_message("info.확인.진행가능", "'제공계정' 입력여부 ");
		return false;
	}
	
	var mbRow    = this.ds_mvmnBudget.rowposition;
	var se       = this.parent.se;
	var year     = this.ds_mvmnBudget.getColumn(mbRow, "YEAR");
	var bsnsSe   = this.ds_mvmnBudget.getColumn(mbRow, "MVMN_BSNS_SE");
	var balcSe   = this.ds_mvmnBudget.getColumn(mbRow, "MVMN_BALC_SE");
	var deptCode = this.ds_mvmnBudget.getColumn(mbRow, "MVMN_BUDGET_DEPT");
	var deptNm   = this.ds_mvmnBudget.getColumn(mbRow, "MVMN_BUDGET_DEPT_NM");
	var acntType  = "MVMN";
	
	this.ds_cond.set_enableevent(false);
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	this.ds_cond.setColumn(nRow, "YEAR",      year     );
	this.ds_cond.setColumn(nRow, "BSNS_SE",   bsnsSe   );
	this.ds_cond.setColumn(nRow, "BALC_SE",   balcSe   );
	this.ds_cond.setColumn(nRow, "DEPT_CODE", deptCode );
	this.ds_cond.setColumn(nRow, "DEPT_NM",   deptNm   );
	this.ds_cond.setColumn(nRow, "SE",        se       );
	this.ds_cond.set_enableevent(true);
	
	var parm = {
	     arg_0     : this
	    ,searchDs  : this.ds_cond.saveXML()							// 검색조건
	    ,strObj    : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE"				// 수정불가능 처리할 오브젝트
	    ,acntType  : acntType										// 수혜계정
	    ,acntPopup : false											// (true : 예산계정코드선택팝업, false : 부기선택팝업)
	};
	this.gfn_popup("mvmnBdgAcntPop", 900, 635, "예산선택팝업", parm, "mis_bdg::BDG020101_P02.xfdl", "fn_popupCallBack", true);
}


