    <Script type="xscript5.0"><![CDATA[/**********************************************************************
* 화면ID   : BDG020103_P01.xfdl
* 화면명   : 예비비신청등록
* 화면설명 : 예비비를 신청한다
* 작성일   : 2017.05.23
* 작성자   : 김형태
* 수정이력 : 
***********************************************************************
*     수정일     수정자   내용
***********************************************************************
*
***********************************************************************
*/

/**********************************************************************
* Script Include 
***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";



/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
var isUpdate = false;
this.chargerAt = false;


/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.BDG020103_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}

this.BDG020103_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 기본 bind 처리
	this.div_DetailMvmn.div_MVMN_BUDGET_DEPT.fn_setBind("ds_mvmnBudget", "MVMN_BUDGET_DEPT",  "MVMN_BUDGET_DEPT_NM" );
	this.div_DetailProvd.div_PROVD_BUDGET_DEPT.fn_setBind("ds_mvmnBudget", "PROVD_BUDGET_DEPT", "PROVD_BUDGET_DEPT_NM");
	
	// 초기 검색조건
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	this.ds_cond.setColumn(nRow, "BALC_SE", "2");	// 지출 고정
	
	// 공통코드
	var comboParams = [
	    ["ds_bdg001", "BDG001", "Y", "D", "B"]	// ds_bdg001 : 사업구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		if(this.gfn_authGrpId("BDG_ADMIN") != -1) {			// 예산결재자
			this.chargerAt = true;
		}
		
		// 기본 입력가능여부 처리
		this.div_DetailMvmn.div_MVMN_BUDGET_DEPT.fn_setReadonly(true);
		this.div_DetailProvd.div_PROVD_BUDGET_DEPT.fn_setReadonly(true);
		//this.div_DetailProvd.div_PROVD_BUDGET_DEPT.div_Search.btn_dept.set_enable(true);	// (제공계정_부서)버튼만 이용 가능하도록 처리
		
		if(this.chargerAt){
			this.div_DetailMvmn.div_MVMN_BUDGET_DEPT.div_Search.btn_dept.set_enable(true);	// (이동계정_부서)버튼만 이용 가능하도록 처리
		}
	};
	this.gfn_comboLoad (comboParams, callback);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	if(!this.comUtils.isNull(this.parent.mvmnSn)) {
		this.ds_cond.setColumn(0, "YEAR", this.parent.mvmnYear);
		this.ds_cond.setColumn(0, "SE",   this.parent.mvmnSe);
		this.ds_cond.setColumn(0, "SN",   this.parent.mvmnSn);
		this.btn_New.set_text("조회");
		this.fn_search();
	} else {
		// default RowType : 신규 추가상태
		if(Ex.util.isUpdated(this.ds_mvmnBudget) && isUpdate) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
		}
		
		isUpdate = false;
		
		this.ds_mvmnBudget.clearData();
		var nRow = this.ds_mvmnBudget.addRow();
		
		var today    = this.comUtils.getClientDate("YYYYMMDD");
		var year     = this.parent.year;
		var bsnsSe   = this.parent.bsnsSe;
		var deptCode = this.parent.deptCode;
		var deptNm   = this.parent.deptNm;
		var se       = this.parent.se;
		
		// 예비비신청 정보
		this.ds_mvmnBudget.set_enableevent(false);
		this.ds_mvmnBudget.setColumn(nRow, "SE",                   se       );	// 1 : 이용(항), 2 : 전용(세항), 3 : 전용(목), 4 : 조정(세목), 0 : 예비비
		this.ds_mvmnBudget.setColumn(nRow, "MVMN_DE",              today    );	// 예비비신청일자
		this.ds_mvmnBudget.setColumn(nRow, "YEAR",                 year     );
		
		// 수혜계정 정보
		this.ds_mvmnBudget.setColumn(nRow, "MVMN_BSNS_SE",         bsnsSe   );
		this.ds_mvmnBudget.setColumn(nRow, "MVMN_BALC_SE",         "2"      );
		this.ds_mvmnBudget.setColumn(nRow, "MVMN_BUDGET_DEPT",     deptCode );
		this.ds_mvmnBudget.setColumn(nRow, "MVMN_BUDGET_DEPT_NM",  deptNm   );
		
		// 제공계정
		this.ds_mvmnBudget.setColumn(nRow, "PROVD_BSNS_SE",        bsnsSe   );
		this.ds_mvmnBudget.setColumn(nRow, "PROVD_BALC_SE",        "2"      );
		this.ds_mvmnBudget.setColumn(nRow, "PROVD_BUDGET_DEPT",    deptCode );
		this.ds_mvmnBudget.setColumn(nRow, "PROVD_BUDGET_DEPT_NM", deptNm   );
		this.ds_mvmnBudget.set_enableevent(true);
	}
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(예비비)
 *                -> 해당 기능은 사용하지 않으며, 이후 추가요청을 대비하여 미리 만들어만 둠.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_mvmnBudget) && isUpdate) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	
	// clear Datasets
	this.ds_mvmnBudget.clearData();
	
	// transaction
	this.fn_transaction("selectMvmnBudget");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(예비비)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// validation 체크
	if(!this.gfn_checkValidation(this.ds_mvmnBudget, this.ds_validation)) {
		return;
	}
	
	// transaction
	if(this.gfn_message("confirm.저장.여부"))	{
		this.fn_transaction("saveMvmnBudget");
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectMvmnBudget":	// 예비비신청 조회
			var sController   	= "/hsco/mis/bdg/BDG020103/selectMvmnBudget.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_mvmnBudget=output1";
			break;
		case "saveMvmnBudget":		// 저장
			var sController   	= "/hsco/mis/bdg/BDG020103/saveMvmnBudget.do";
			var sInDatasets   	= "input1=ds_mvmnBudget:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if (nErrorCode == 0){
		switch (strSvcId) {
			case "selectMvmnBudget":	// 조회(예비비)
				break;
			case "saveMvmnBudget":		// 저장(예비비)
				this.gfn_message("success.처리.성공", null, null, null, this.fn_saveCallBack);
				//this.close();
				break;
		}
	}
}


 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_saveCallBack = function()
{
	this.fn_search();
}

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 수혜계정
	if(sPopupId == "mvmnBdgAcntPop") {
		var dsBdgAcntPop = new Dataset();
		var loadCnt = dsBdgAcntPop.loadXML(sReturn);
		if(loadCnt) {
			// popup에서의 반환값
			var bsnsSe          = dsBdgAcntPop.getColumn(0, "BSNS_SE");
			var balcSe          = dsBdgAcntPop.getColumn(0, "BALC_SE");
			var deptCode        = dsBdgAcntPop.getColumn(0, "DEPT_CODE");
			var deptNm          = dsBdgAcntPop.getColumn(0, "DEPT_NM");
			var detailbsnsCode  = dsBdgAcntPop.getColumn(0, "DETAIL_BSNS_CODE");
			var budgetAcntCode  = dsBdgAcntPop.getColumn(0, "BUDGET_ACNT_CODE");
			var expndtrUnitCode = dsBdgAcntPop.getColumn(0, "EXPNDTR_UNIT_CODE");
			var detailbsnsNm    = dsBdgAcntPop.getColumn(0, "DETAIL_BSNS_NM");
			var budgetAcntNm    = dsBdgAcntPop.getColumn(0, "BUDGET_ACNT_NM");
			var expndtrUnitNm   = dsBdgAcntPop.getColumn(0, "EXPNDTR_UNIT_NM");
			var budgetAm        = dsBdgAcntPop.getColumn(0, "BUDGET_AM");
			var decsnAmount     = dsBdgAcntPop.getColumn(0, "DECSN_AMOUNT");
			var expndtrUnitBalc = dsBdgAcntPop.getColumn(0, "EXPNDTR_UNIT_BALC");
			
			// 현재 지정된 제공예산계정
			var provdBsnsSe   = this.ds_mvmnBudget.getColumn(0, "PROVD_BSNS_SE");			// 제공_사업구분
			var provdBalcSe   = this.ds_mvmnBudget.getColumn(0, "PROVD_BALC_SE");			// 제공_수지구분
			var provdAcntCode = this.ds_mvmnBudget.getColumn(0, "PROVD_ACNT_CODE");			// 제공_예산계정코드
			var provdUnitCode = this.ds_mvmnBudget.getColumn(0, "PROVD_EXPNDTR_UNIT_CODE");	// 제공_부기코드
			
			// 제공예산과 똑같이 선택할 수 없도록 제어
			if(bsnsSe==provdBsnsSe && balcSe==provdBalcSe && budgetAcntCode==provdAcntCode && expndtrUnitCode==provdUnitCode) {
				this.gfn_message("info.항목.선택.알림", "제공예산과 다른 계정과목(세목)");
				return false;
			}
			
			var mbRow = this.ds_mvmnBudget.rowposition;
			this.ds_mvmnBudget.setColumn(mbRow, "MVMN_BSNS_SE",           bsnsSe         );	// 사업구분
			this.ds_mvmnBudget.setColumn(mbRow, "MVMN_BALC_SE",           balcSe         );	// 수지구분
