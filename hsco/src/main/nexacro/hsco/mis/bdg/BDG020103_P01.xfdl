/*
 * 함수명  	: ds_mvmnBudget_cancolumnchange
 * 설명 		: Dataset 데이터 변경 전 이벤트 처리(예비비신청 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_mvmnBudget_cancolumnchange(obj,e);
 */
this.ds_mvmnBudget_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 변경액
	if(e.columnid == "MVMN_AM") {
		var provdBudgetBlce = nexacro.toNumber(obj.getColumn(e.row, "PROVD_BUDGET_BLCE"));
		var newVal = nexacro.toNumber(e.newvalue);
		if(newVal > provdBudgetBlce) {
			this.gfn_message("errors.항목.입력값.이하", "'예비비 신청액'|'제공계정 예산잔액'");
			return false;
		}
	}
	
	// 제공계정의 예산잔액
	if(e.columnid == "PROVD_BUDGET_BLCE") {
		var mvmnAm = nexacro.toNumber(obj.getColumn(e.row, "MVMN_AM"));
		var newVal = nexacro.toNumber(e.newvalue);
		if(newVal < mvmnAm) {
			this.gfn_message("errors.항목.입력값.이하", "'예비비 신청액'|'제공계정 예산잔액'");
			obj.set_enableevent(false);
			obj.setColumn(e.row, "MVMN_AM",             null);	// 이동신청액
			obj.setColumn(e.row, "MVMN_DEMAND_AMOUNT",  null);	// 요구금액(증)
			obj.setColumn(e.row, "PROVD_DEMAND_AMOUNT", null);	// 요구금액(감)
			obj.set_enableevent(true);
		}
	}
}


/*
 * 함수명  	: ds_mvmnBudget_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(예비비신청 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_mvmnBudget_oncolumnchanged(obj,e);
 */
this.ds_mvmnBudget_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	isUpdate = true;
	
	// 변경액
	if(e.columnid == "MVMN_AM") {
		obj.setColumn(e.row, "MVMN_DEMAND_AMOUNT",  nexacro.toNumber(e.newvalue+"",0)     );		// 요구금액(증)
		obj.setColumn(e.row, "PROVD_DEMAND_AMOUNT", nexacro.toNumber(e.newvalue+"",0) * -1);		// 요구금액(감)
	}
	
	// 제공계정의 예산부서
	if(e.columnid == "PROVD_BUDGET_DEPT") {
		this.ds_mvmnBudget.setColumn(e.row, "PROVD_ACNT_CODE",         "");
		this.ds_mvmnBudget.setColumn(e.row, "PROVD_ACNT_NM",           "");
		this.ds_mvmnBudget.setColumn(e.row, "PROVD_EXPNDTR_UNIT_CODE", "");
		this.ds_mvmnBudget.setColumn(e.row, "PROVD_EXPNDTR_UNIT_NM",   "");
		
		this.ds_mvmnBudget.setColumn(e.row, "PROVD_BUDGET_CRNTAM",     "");
		this.ds_mvmnBudget.setColumn(e.row, "PROVD_EXCUT_AM",          "");
		this.ds_mvmnBudget.setColumn(e.row, "PROVD_BUDGET_BLCE",       "");
	}
	
	// 수혜계정의 예산부서
	if(e.columnid == "MVMN_BUDGET_DEPT") {
		this.ds_mvmnBudget.setColumn(e.row, "MVMN_ACNT_CODE",         "");
		this.ds_mvmnBudget.setColumn(e.row, "MVMN_ACNT_NM",           "");
		this.ds_mvmnBudget.setColumn(e.row, "MVMN_EXPNDTR_UNIT_CODE", "");
		this.ds_mvmnBudget.setColumn(e.row, "MVMN_EXPNDTR_UNIT_NM",   "");
		
		this.ds_mvmnBudget.setColumn(e.row, "MVMN_BUDGET_CRNTAM",     "");
		this.ds_mvmnBudget.setColumn(e.row, "MVMN_EXCUT_AM",          "");
		this.ds_mvmnBudget.setColumn(e.row, "MVMN_BUDGET_BLCE",       "");
	}
}


