			this.gfn_message("info.파일.업로드.허용건수초과", "1");
			return false;
		}
		
		this.fn_autoFileSn(
			 "MIS"							// 대분류   (SYS_SE)
			,"BDG"							// 업무구분 (JOB_SE)
			,this.File_UpDownManager		// fileManager 오브젝트
			,this.ds_fileList				// fileObject
			,this.ds_drctMatterFlflList		// fileSn을 불러오거나 반영해야 하는 컬럼
