this.fn_uploadCallback = function() {

	var nRow = this.ds_drctMatterFlflList.rowposition;
	var fileSn = this.ds_drctMatterFlflList.getColumn(nRow, "FILE_SN");
				
	this.ds_cond_file.clearData();
