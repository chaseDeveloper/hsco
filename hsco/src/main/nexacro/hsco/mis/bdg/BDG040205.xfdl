 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.BDG040205_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();
	this.fn_misFormInit(this);
	
	// 오브젝트 등에 대한 처리
	this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();

	// DataSet 초기화
	this.fn_init_dataset();	
	
	this.div_print.set_visible(false);

	// 공통코드
	var comboParams = [
		["ds_bdg001", "BDG001", "Y", "A", "B"]	// ds_bdg001 : 사업구분
	   ,["ds_bdg002", "BDG002", "Y", "D", "B"]	// ds_bdg002 : 수지구분
	   ,["ds_bdg029", "BDG029", "Y", "A", "B"]	// ds_bdg029 : 사업종류
	   ,["ds_bdg030", "BDG030", "Y", "A", "B"]	// ds_bdg030 : 예산종류
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 초기 검색조건
		var year  = this.comUtils.getClientDate("YYYY");
		var today = this.comUtils.getClientDate("YYYYMMDD");
		this.ds_cond.setColumn(nRow, "YEAR" , year);		// 당해년도
		this.ds_cond.setColumn(nRow, "BGNDE", year+"0101");	// 시작일(당해 1월1일)
		this.ds_cond.setColumn(nRow, "ENDDE", today);		// 종료일(오늘)
		
		// 공통코드와 연계된 초기검색조건 설정
		this.ds_cond.setColumn(nRow, "BSNS_SE", "1");	// (사업구분) 1:사업예산, 2:자본예산, 3:자금운용
		this.ds_cond.setColumn(nRow, "BALC_SE", "2");	// (수지구분) 1:수입, 2:지출
		
		this.fn_search();	//  최초조회
	};
	this.gfn_comboLoad (comboParams, callback);
}

