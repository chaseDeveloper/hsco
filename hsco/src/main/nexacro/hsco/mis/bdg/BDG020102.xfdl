{	/*
	var mbRow = this.ds_mvmnBudget.rowposition;
	var sanctnKnd = this.ds_mvmnBudget.getColumn(mbRow, "SANCTN_KND")
	if(sanctnKnd == '1' || sanctnKnd == '4' || sanctnKnd == '5') {	// 0:기안, 4:중간결재, 5:결재완료
		var sanctnKndNm = this.ds_com022.getColumn(this.ds_com022.findRow("CODE", sanctnKnd), "CODE_NM");
		this.gfn_message("info.확인.진행가능", "결재상태(현재 : " + sanctnKndNm + ")가 '기안, 중간결재, 결재완료' 가 아님을 ");
		return false;
	}
	*/
	
	// 전자결재 상태 체크 2016.7
	// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
	// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
	// 기안전인것 : 재상신 및 수정/삭제
	// 결재상태가 상신전 외는 신규로 다시 진행.
	var SANCTN_STS = this.ds_mvmnBudget.getColumn(this.ds_mvmnBudget.rowposition, "SANCTN_STS");

	switch (SANCTN_STS) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");
		return;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return;
		
		default:
	}
	
	this.fn_transaction("saveSanc");
}



/**********************************************************************
* Grid 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: grd_mvmnBudget_oncellclick
 * 설명 		: Grid cell 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_mvmnBudget_oncellclick(obj,e);
 */
this.grd_mvmnBudget_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var curEditType = obj.getCurEditType();
	if(curEditType == "button" && e.col == '1') {
		
		var mvmnRow = this.ds_mvmnBudget.rowposition;
		var dscnAt = this.ds_mvmnBudget.getOrgColumn(mvmnRow, "DSCN_AT");
		if(dscnAt=='1') {
			this.gfn_message("fail.수정.불가", "승인된 건이므로");
			return false;
		}
		/*
		var sanctnKnd = this.ds_mvmnBudget.getColumn(mvmnRow, "SANCTN_KND");
		if(sanctnKnd == '1' || sanctnKnd == '4' || sanctnKnd == '5') {
			this.gfn_message("fail.수정.불가", "결재가 진행중이거나 완료된 건이므로 승인으로");
			return false;
		}
		*/
		
		// 전자결재 상태 체크 2016.7
		// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
		// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
		// 기안전인것 : 재상신 및 수정/삭제
		// 결재상태가 상신전 외는 신규로 다시 진행.
		var sanctnKnd = this.ds_mvmnBudget.getColumn(this.ds_mvmnBudget.rowposition, "SANCTN_STS");

		switch (sanctnKnd) {
			case "1": case "2": case "4": case "8": case "1024": case "DUP":
			this.gfn_message("comm.결재.진행");
			return;
			
			case "32": case "256": case "512": case "4096": 
			this.gfn_message("comm.결재.완료");
			return;
			
			default:
		}

		var parm = {
			 arg_0    : this
			,year     : this.ds_cond.getColumn(0, "YEAR")
			,bsnsSe   : this.ds_cond.getColumn(0, "BSNS_SE")
			,deptCode : this.ds_cond.getColumn(0, "DEPT_CODE")
			,deptNm   : this.ds_cond.getColumn(0, "DEPT_NM")
			,mvmnYear : this.ds_mvmnBudget.getColumn(mvmnRow, "YEAR")
			,mvmnSn   : this.ds_mvmnBudget.getColumn(mvmnRow, "SN")
		};
		this.gfn_popup("BDG020102_P01", 800, 530, "예산전용등록", parm, "mis_bdg::BDG020102_P01.xfdl", "fn_popupCallBack", true);
	}
	

	if(e.col == 4){
		var sanctnNo = this.ds_mvmnBudget.getColumn(e.row, "SANCTN_NO");
		var SANCTN_STS = this.ds_mvmnBudget.getColumn(this.ds_mvmnBudget.rowposition, "SANCTN_STS");
		//공통요청 2016.08.30
		var registDt = this.ds_mvmnBudget.getColumn(this.ds_mvmnBudget.rowposition, "REGIST_DT");
		if(SANCTN_STS < 1 || SANCTN_STS == 'NUL'){
			return;
		}
		if(sanctnNo){
			var parm = {
				arg_0 : this,  
				arg_1 : sanctnNo,
				arg_2 : registDt //공통요청 2016.08.30
			};
			this.gfn_popup("sancPopup", 710, 518, "전자결재진행 현황", parm, "com::COM010400_P01.xfdl", "fn_popupCallBack");
		}
	}
}

