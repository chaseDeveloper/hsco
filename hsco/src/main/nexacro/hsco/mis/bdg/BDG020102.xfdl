/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectMvmnBudget":	// 예산전용 조회
			var sController   	= "/hsco/mis/bdg/BDG020102/selectMvmnBudget.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_mvmnBudget=output1";
			break;
		case "selectMvmnDetail":	// 예산상세 조회
			var mbRow = this.ds_mvmnBudget.rowposition;
			var year  = this.ds_mvmnBudget.getColumn(mbRow, "YEAR");
			var se    = this.ds_mvmnBudget.getColumn(mbRow, "SE");
			var sn    = this.ds_mvmnBudget.getColumn(mbRow, "SN");
			
			this.ds_condSub.clearData();
			var nRow = this.ds_condSub.addRow();
			this.ds_condSub.setColumn(nRow, "YEAR", year );
			this.ds_condSub.setColumn(nRow, "SE",   se   );
			this.ds_condSub.setColumn(nRow, "SN",   sn   );
			
			var sController   	= "/hsco/mis/bdg/BDG020102/selectMvmnDetail.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_mvmnDetail=output1";
			break;
		case "saveExecutBudget":	// 실행예산 적용
			var sController   	= "/hsco/mis/bdg/BDG020102/saveExecutBudget.do";
			var sInDatasets   	= "input1=ds_mvmnBudget:U input2=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "saveMvmnMaster":		// 저장(삭제기능 전용)
			var sController   	= "/hsco/mis/bdg/BDG020102/saveMvmnMaster.do";
			var sInDatasets   	= "input1=ds_mvmnBudget:U input2=ds_cond";
			var sOutDatasets  	= "";
			break;
		case "saveSanc":
			var mbRow = this.ds_mvmnBudget.rowposition;
			//var year = this.ds_mvmnBudget.getColumn(mbRow, "YEAR");
			
			var p_acntNm     = this.ds_mvmnDetail.getColumn(0, "ACNT_CODE_NM"        );	// 제공_계정명
			var p_unitCode   = "";														// 제공_지출단위코드(사용x)
			var p_unitNm     = this.ds_mvmnDetail.getColumn(0, "EXPNDTR_UNIT_CODE_NM");	// 제공_지출단위명
			var p_budgetBlce = this.ds_mvmnDetail.getColumn(0, "BUDGET_BLCE"         );	// 제공_예산잔액
			var p_demandAm   = this.ds_mvmnDetail.getColumn(0, "DEMAND_AMOUNT"       );	// 제공_변경금액
			
			//이동 내역 전자결재내용에 추가 2016.07.25
			var p_acntNm2     = this.ds_mvmnDetail.getColumn(1, "ACNT_CODE_NM"        );	// 이동_계정명
			var p_unitCode2   = "";														// 이동_지출단위코드(사용x)
			var p_unitNm2     = this.ds_mvmnDetail.getColumn(1, "EXPNDTR_UNIT_CODE_NM");	// 이동_지출단위명
			var p_budgetBlce2 = this.ds_mvmnDetail.getColumn(1, "BUDGET_BLCE"         );	// 이동_예산잔액
			var p_demandAm2   = this.ds_mvmnDetail.getColumn(1, "DEMAND_AMOUNT"       );	// 이동_변경금액
			
			var MVMN_PRVONSH  = this.ds_mvmnBudget.getColumn(mbRow, "MVMN_PRVONSH"); //사유
			
			// sanctnNo update key
			var year     = this.ds_mvmnBudget.getColumn(mbRow, "YEAR");
			var se       = this.ds_mvmnBudget.getColumn(mbRow, "SE");
			var sn       = this.ds_mvmnBudget.getColumn(mbRow, "SN");
			
			p_acntNm     = this.comUtils.isNull(p_acntNm)     ? ""  : p_acntNm;
			p_unitNm     = this.comUtils.isNull(p_unitNm)     ? ""  : p_unitNm;
			p_budgetBlce = this.comUtils.isNull(p_budgetBlce) ? "0" : p_budgetBlce;
			p_demandAm   = this.comUtils.isNull(p_demandAm)   ? "0" : p_demandAm;
			
			p_budgetBlce = this.comUtils.formatComma(p_budgetBlce);
			p_demandAm   = this.comUtils.formatComma(p_demandAm);
			
			//이동 내역 전자결재내용에 추가 2016.07.25
			p_acntNm2     = this.comUtils.isNull(p_acntNm2)     ? ""  : p_acntNm2;
			p_unitNm2     = this.comUtils.isNull(p_unitNm2)     ? ""  : p_unitNm2;
			p_budgetBlce2 = this.comUtils.isNull(p_budgetBlce2) ? "0" : p_budgetBlce2;
			p_demandAm2   = this.comUtils.isNull(p_demandAm2)   ? "0" : p_demandAm2;
			
			p_budgetBlce2 = this.comUtils.formatComma(p_budgetBlce2);
			p_demandAm2   = this.comUtils.formatComma(p_demandAm2);
			
			var title     = "예산전용요청";
			var dataTitle = year + "년도 예산변경 협조요청(예산전용)";
			var dataValue = p_acntNm + "|" + 	//제공 계정명
							p_unitCode + "|" + 	//제공 부기코드
							p_unitNm + "|" + 	//제공 부기코드명
							p_budgetBlce + "|" + 	//기존 예산잔액
							p_demandAm + "|" +		//기존 변경금액
							MVMN_PRVONSH + "|" +	//사유 //이동 내역 전자결재내용에 추가 2016.07.25
							p_acntNm2 + "|" +	//이동 계정명
							//p_unitCode2 + "|" +	//이동 부기코드
							p_unitNm2 + "|" +	//이동 부기코드명
							p_budgetBlce2 + "|" +	//이동 예산잔액
							p_demandAm2 + "|" +	//이동 변경금액
							"예산전용" + "|" +
							year + "|";	//년도

			this.ds_sanc.clearData();
			var nRow = this.ds_sanc.addRow();
			this.ds_sanc.setColumn(nRow, "TITLE",      title      );
			this.ds_sanc.setColumn(nRow, "DATA_TITLE", dataTitle  );
			this.ds_sanc.setColumn(nRow, "DATA_VALUE", dataValue  );
			this.ds_sanc.setColumn(nRow, "DEPT_CD",    this.gfn_getDeptId() );
			this.ds_sanc.setColumn(nRow, "EMP_CD",     this.gfn_getUserId() );
			this.ds_sanc.setColumn(nRow, "YEAR",       year       );
			this.ds_sanc.setColumn(nRow, "SE",         se         );
			this.ds_sanc.setColumn(nRow, "SN",         sn         );
			
			// 전자결재 2016.7
			var sanctn_no = this.ds_mvmnBudget.getColumn(nRow, "SANCTN_NO");
			this.ds_sanc.setColumn(nRow, "SANCTN_NO",     sanctn_no );
			
			trace("ssssssss : " + this.ds_mvmnBudget.saveXML());
			trace("ddddddddddd : " + this.ds_sanc.saveXML());
			
			var sController   	= "hsco/mis/bdg/BDG020101/saveSanc.do";
			var sInDatasets   	= "input1=ds_sanc";
			var sOutDatasets  	= "ds_sanc=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



