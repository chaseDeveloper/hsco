/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS010207_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010207_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_search.cal_bgnde.setFocus(true);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "BGN_DE", this.gfn_today().substring(0,6) + '01');
	this.ds_cond.setColumn(0, "END_DE", this.gfn_today());

	var comboParams = [["ds_actSe",         "CUS022", 1, "전체"],
					   ["ds_mntrSeSerach",  "CUS023", 1, "전체"],
					   ["ds_rcognAt"  ,     "CUS024", 1, ""],
					   ["ds_sacnctnSttusSe","COM022", 1, ""],
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "CUS");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	//고객모니터단지출관리 목록 조회 
	this.fn_transaction("selectCstmrMntgStgExpndtr");
}

/***********************************************************************
* 상단 신규
************************************************************************/
this.fn_insert = function()
{
	var nCnt = this.ds_cstmrMntgStgExpndtr.getCaseCount("!PYMNT_SN");
	if(nCnt > 0){
		this.gfn_message("info.저장.초과", "신규 지급정보는 1건씩")
		return;
	};
	this.fn_transaction("selectMntrStgActList");
}

/***********************************************************************
* 상단삭제
************************************************************************/
this.fn_delete = function()
{
	var sanctnKnd = this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "SANCTN_STS");

	if(!this.comUtils.isNull(sanctnKnd))
	{
		//결재상태가 기안전, 반송, 취소가 아닐경우
		if(sanctnKnd != '0'){
			if(sanctnKnd == '1' || sanctnKnd == '2' || sanctnKnd =='4' || sanctnKnd =='8' || sanctnKnd =='1024' || sanctnKnd =='DUP'){		// 기안, 중간결재
				this.gfn_message("fail.삭제.불가", "이미 결재가 진행중인 내역은");
				return false;
			} else {										// 결재완료
				this.gfn_message("fail.삭제.불가", "이미 결재가 완료된 내역은");
				return false;
			}
		}
	}

	if(this.gfn_message("confirm.삭제여부")){
		var nowRow = this.ds_cstmrMntgStgExpndtr.rowposition;
		var rowCnt = this.ds_cstmrMntgStgExpndtr.rowcount;
		
		this.ds_cstmrMntgStgExpndtr.deleteRow(nowRow);
		
		if((rowCnt > 1) && (nowRow != rowCnt-1)){
			this.ds_cstmrMntgStgExpndtr_onrowposchanged();
		}
	}
}

/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_cstmrMntgStgExpndtr))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_cstmrMntgStgExpndtr.reset();		//DATASET ROLLBACK
			this.ds_cstmrMntgStgExpndtr.applyChange();	//DATASET COMMIT
			this.ds_cstmrMntgStgExpndtr_onrowposchanged();
			return;
		}
	}
}
/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{
	//validation check
	if(!this.gfn_checkValidation(this.ds_cstmrMntgStgExpndtr, this.ds_validation)){
		return;
	}
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.ds_cstmrMntgStgExpndtr.rowposition;
		this.fn_transaction("cstmrMntgStgExpndtrCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	
		//고객모니터단지출관리 목록 조회 
		case "selectCstmrMntgStgExpndtr" :
			this.ds_cstmrMntgStgExpndtr.clearData();
			this.ds_cstmrMntrStgActList.clearData();
			this.ds_fileList.clearData();
			var sController   	= "hsco/mis/cus/CUS010207/selectCstmrMntgStgExpndtr.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cstmrMntgStgExpndtr=output01";
		break;		
		
		//고객모니터단활동관리 목록조회(지출목록 신규 등록시)
		case "selectMntrStgActList" :
			this.ds_cstmrMntrStgActList.clearData();
			var sController   	= "hsco/mis/cus/CUS010207/selectMntrStgActList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cstmrMntrStgActList=output01";
		break;
		
		//고객모니터단활동관리 목록조회(지출목록 조회시 상세조회)
		case "selectMntrStgActList2" :
			var sController   	= "hsco/mis/cus/CUS010207/selectMntrStgActList2.do";
			var sInDatasets   	= "input01=ds_cond3";
			var sOutDatasets  	= "ds_cstmrMntrStgActList=output01";
		break;
		
		//고객모니터단지출관리 CUD
		case "cstmrMntgStgExpndtrCUD" :
			var sController   	= "hsco/mis/cus/CUS010207/cstmrMntgStgExpndtrCUD.do";
			var sInDatasets   	= "input02=ds_cstmrMntgStgExpndtr:U input03=ds_cstmrMntrStgActList:A";
			var sOutDatasets  	= "";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
		case "GetInnerSanctnId": // 내부결재id 조회   
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
		break;
		
		case "innerSanctnUbiDataset": // 시스템연계 내부결재보고서 정보 조회
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0, "PYMNT_SN",      this.ds_mntrStgExpndtrList.getColumn(this.ds_mntrStgExpndtrList.rowposition, "PYMNT_SN"));
			var sController   	= "hsco/mis/cus/CUS010207/innerSanctnUbiDataset.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_ubiDataset=output1";
		break; 
		
		//최초 내부결재 팝업 호출 시 저장버튼을 눌러 시작되었는지, 닫기버튼을 눌러 취소를 했는지 여부check
		case "selectInnerSanctnMasterCheck"	:
			var sController   	= "hsco/mis/cus/CUS010207/selectInnerSanctnMasterCheck.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_innerSanctnChek=output1";
		break;	
		
		case "approve":			  // 전자결재
			var sController   	= "hsco/mis/cus/CUS010207/approve.do";	
			var sInDatasets   	= "input1=ds_approve";
			var sOutDatasets  	= "ds_approve=output1";
			break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//고객지출목록 CUD 콜백
			case "cstmrMntgStgExpndtrCUD":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//고객모니터단지출관리 목록 콜백
			case "selectCstmrMntgStgExpndtr":
				//저장트랜잭션 직전 rowposition값으로 이동
// 				if(save_row != -1)
// 				{
// 					this.ds_cstmrMntgStgExpndtr.set_rowposition(-1);
// 					this.ds_cstmrMntgStgExpndtr.set_rowposition(save_row);
// 					save_row = -1;
// 				}
			break;
			
			//활동목록 조회 콜백
			case "selectMntrStgActList":
					
					if(this.ds_cstmrMntrStgActList.rowcount > 0){
						
						this.insertYn = 'Y';
						
						this.ds_temp.copyData(this.ds_cstmrMntrStgActList);			// 임시 데이터셋어 활동목록내역 임시저장(행 변경 이벤트에서 사용)
						
						var nRow = this.ds_cstmrMntgStgExpndtr.addRow();
						
						this.ds_cstmrMntgStgExpndtr.setColumn(nRow, "EXPNDTR_PLAN_DE" , this.gfn_today());
						this.ds_cstmrMntgStgExpndtr.setColumn(nRow, "SANCTN_KND" ,      '0');
						
						var sumPymntAmSm = 0;
						
						for(var i = 0; i < this.ds_cstmrMntrStgActList.rowcount; i++)
						{
							this.ds_cstmrMntrStgActList.setColumn(i,"RM","1");
							sumPymntAmSm = sumPymntAmSm + nexacro.toNumber(this.ds_cstmrMntrStgActList.getColumn(i, "PYMNT_AMOUNT"));
						}
						
						this.ds_cstmrMntgStgExpndtr.setColumn(nRow, "PYMNT_AM_SM" , sumPymntAmSm);
						
					}else{
						if(this.ds_cstmrMntgStgExpndtr.rowposition > -1){
							this.fn_transaction("selectMntrStgActList2");
						}
						this.gfn_message("fail.정보검색.실패","고객모니터단활동목록을");
					}
			break;
			
			// 내부결재id 조회
			case "GetInnerSanctnId":
			    this.fn_popup_InnerSanctionDrft();
			break;
			
			//최초 내부결재 진행여부체크
			case "selectInnerSanctnMasterCheck":
				if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
				{
					this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "DRFT_JOB_CONFM_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
					this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "DRFT_DE",           this.gfn_today());
					this.fn_transaction("cstmrMntgStgExpndtrCUD");
				}else{
					
				}
			break;
			
			// 전자결재
			case "approve":
// 				var sanctnNO = this.ds_approve.getColumn(0,"SANCTN_NO");
// 				
// 				this.ds_cstmrMntgStgExpndtr.set_updatecontrol(false);
// 				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "DRFT_JOB_CONFM_NO", sanctnNO);
// 				this.ds_cstmrMntgStgExpndtr.set_updatecontrol(true);
				
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
			break;
			
			case "selectMntrStgActList2" :
				this.fn_transaction("selectAtchFile");
			break;
		}
	}
}


/**********************************************************************
* 자금배정신청 버튼 클릭시
***********************************************************************/ 
this.div_work_btn_attach02_onclick = function()
{	
	if(this.ds_cstmrMntgStgExpndtr.rowposition < 0){
		this.gfn_message("comm.항목.선택.요청","고객모니터단활동비지급목록");
	}else{		
		if(!this.comUtils.isNull(this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "REGIST_DT")))
		{
			if(this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "CONFM_AT") == 1){
				this.gfn_message("comm.승인.완료");
			}else{
				var ds_temp = new Dataset();
				ds_temp.addColumn( "DEPT_CODE", "string" );
				ds_temp.addColumn( "DEPT_NM", "string" );
				ds_temp.addColumn( "WIK_CPTAL_PLAN_KEY", "string" );
				ds_temp.addRow();
				ds_temp.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
				ds_temp.setColumn(0, "DEPT_NM",   this.gfn_getDeptName());
				ds_temp.setColumn(0, "WIK_CPTAL_PLAN_KEY",   this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "CPTAL_ASIGN_CONFM_NO"));
				
				var parm = {
					arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
					arg_1 : ds_temp
				};
				this.gfn_popup("FUN000000_P01", 950, 700, "주간지출계획등록 팝업", parm, "mis_fun::FUN000000_P01.xfdl", "fn_PopUpCallback");		
			}
		}else{
			this.gfn_message("comm.정보.저장.요청","먼저 고객모니터단활동비지급내역을");
		}
	}
}

/**********************************************************************
* 자금배정신청 팝업 콜백
***********************************************************************/ 
this.fn_PopUpCallback = function(sPopupId, sReturn)
{
    
    switch(sPopupId)
	{
		case "expDsnPopup":
			if(sReturn != 'C'){
				// 신규 결의서 신규, 변경 저장시 재조회
				this.fn_transaction("selectCstmrMntgStgExpndtr");
			}
		break;
			
		case "FUN000000_P01":
			var dsRtn = new Dataset();
			nRowCnt = dsRtn.loadXML(sReturn);
		if(nRowCnt > 0){
			if(dsRtn.getColumn(0,"TYPE") == "C" || dsRtn.getColumn(0,"TYPE") == "D"){
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "CPTAL_ASIGN_CONFM_NO",dsRtn.getColumn(0,"WIK_CPTAL_PLAN_KEY"));   // 지출계획과 맵핑될때 사용됨. 각테이블에 저장바람
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "BUDGET_ACNT_CODE",    dsRtn.getColumn(0,"BUDGET_ACNT_CODE"));     // 계정코드
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "BUDGET_ACNT_CODE_NM", dsRtn.getColumn(0,"BUDGET_ACNT_CODE_NM"));
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "EXPNDTR_UNIT_NM",     dsRtn.getColumn(0,"EXPNDTR_UNIT_NM"));
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "EXPNDTR_UNIT_CODE",   dsRtn.getColumn(0,"EXPNDTR_UNIT_CODE"));	   // 지출단위코드
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "CPTAL_ASIGN_REQST_DE", this.gfn_today());
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "CONFM_AT", 0);
				this.fn_transaction("cstmrMntgStgExpndtrCUD");
			}	
			if(dsRtn.getColumn(0,"TYPE") == "D"){	
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "CPTAL_ASIGN_CONFM_NO", "");   // 지출계획과 맵핑될때 사용됨. 각테이블에 저장바람
				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "CPTAL_ASIGN_REQST_DE", "");	
				this.fn_transaction("cstmrMntgStgExpndtrCUD");
			}
		}		
		break;
	}
}
