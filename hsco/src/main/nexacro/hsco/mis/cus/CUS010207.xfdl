	}else{
		this.parent.btn_sanction.set_enable(true);
	}
	
	this.ds_fileList.clearData();
	
	if(rowType != Dataset.ROWTYPE_INSERT){		// 신규 행이 아닐 경우에만 상세 지출활동 내역 조회
		this.ds_cond3.clearData();
		this.ds_cond3.addRow();
		this.ds_cond3.setColumn(0, "PYMNT_SN", this.ds_cstmrMntgStgExpndtr.getColumn(nRow, "PYMNT_SN"));
		
		this.ds_cstmrMntrStgActList.clearData();
		
		this.fn_transaction("selectMntrStgActList2");
	}else {
