/***********************************************************************
* 조회기간 변경 시 
************************************************************************/
this.ds_cond_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	//시작일 변경시
	if(e.columnid == "BGN_DE")
	{
		//필수입력 체크 2016.07.12
		if(this.comUtils.isNullEmpty(e.newvalue)){
			obj.setColumn(e.row,e.oldvalue);
			return false;
		}
		
		if(e.newvalue > this.div_search.cal_endde.value) 
		{
			this.gfn_message("errors.시작일자.종료일자.이하","|");
			return false;
		}
		return true;
	}
	//종료일 변경시
	if(e.columnid == "END_DE")
	{
		//필수입력 체크 2016.07.12
		if(this.comUtils.isNullEmpty(e.newvalue)){
			obj.setColumn(e.row,e.oldvalue);
			return false;
		}
		
		if(e.newvalue < this.div_search.cal_bgnde.value) 
		{
			this.gfn_message("errors.종료일자.시작일자.이상","|");
			return false;
		}
		return true;

	}
}
/***********************************************************************
 * 설명 : 기안버튼 클릭 시 전자결재 
 ***********************************************************************/
this.div_work_btn_drft_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_cstmrMntgStgExpndtr)){
		this.gfn_message("comm.정보.저장.요청","먼저 변경된");
		return false;
	}
	
	var sanctnknd = this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "SANCTN_KND");

	if(sanctnknd != '0'){
		if(sanctnknd == '1' || sanctnknd == '4'){		// 기안, 중간결재
			this.gfn_message("comm.결재.진행");
			return false;
		}else if(sanctnknd == '5'){						// 결재 완료
			this.gfn_message("comm.결재.완료");
			return false;
		}
	}	
	
	var key = this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "PYMNT_SN");
	var sj  = this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "SJ");
	
	var dataTitle = sj;
	var title     = sj;
	
	this.ds_approve.clearData();	
	this.ds_approve.addRow();

	this.ds_approve.setColumn(0, "TITLE",      title                );
	this.ds_approve.setColumn(0, "DATA_TITLE", dataTitle            );
	this.ds_approve.setColumn(0, "DATA_VALUE", null                 );
	this.ds_approve.setColumn(0, "SEQ",        key);
 	this.ds_approve.setColumn(0, "DEPT_CD",    this.gfn_getDeptId() );
 	this.ds_approve.setColumn(0, "EMP_CD",     application.gds_userInfo.getColumn(0, "EMPNO") );
 	
 	this.fn_transaction("approve");
}

