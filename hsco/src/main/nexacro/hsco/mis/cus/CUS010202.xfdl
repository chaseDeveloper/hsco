/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";
include "lib::comInnerSanct.xjs";
include "lib::comSms.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
  this.ds_cstmrPartcptn
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;

/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS010202_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010202_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_search.cal_registBgnDt.setFocus(true);
	
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "PARTCPTN_SE", "2");
	this.div_search.cal_registBgnDt.set_value(this.gfn_today().substring(0,6) + '01');
	this.div_search.cal_registEndDt.set_value(this.gfn_today());
	var comboParams = [["ds_prcStatus",     "CUS009", 1, "전체"],
					   ["ds_processSe",     "CUS008", 1, "선택"],
					   ["ds_sacnctnSttusSe","COM032", 1, ""],
					   ["ds_cus030",		"CUS030", "Y", "D", "B"]
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		// SMS Default 메시지 셋팅
		this.ds_smsMsg.setColumn(0, "MSSAGE_CN", this.fn_getCusSmsMsg("MSG0000009"));
		// SMS 화성도시공사 대표전화번호 셋팅
		var fRow = this.ds_cus030.findRow("CODE", "TEL");
		var tel	 = fRow > 0 ? this.ds_cus030.getColumn(fRow, "VAL1") : "0533500301";
		this.ds_smsMsg.setColumn(0, "ANSWTEL", tel);
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);	
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
  var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "CUS");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{
	this.fn_transaction("selectCstmrPartcptnList");
}

/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{
	//validation check
	if(!this.gfn_checkValidation(this.ds_cstmrPartcptn, this.ds_validation)){
		return;
	}
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.ds_cstmrPartcptn.rowposition;
		this.fn_transaction("cstmrPartcptnListU");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		
		//고객제안 목록 조회
		case "selectCstmrPartcptnList":
			this.ds_fileList.clearData();
			this.ds_fileList2.clearData();
			var sController   	= "hsco/mis/cus/CUS010201/selectCstmrPartcptnList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cstmrPartcptn=output01";
		break;
		
		//고객제안 저장/수정/삭제
		case "cstmrPartcptnListU":
			var sController   	= "hsco/mis/cus/CUS010201/cstmrPartcptnListU.do";
			var sInDatasets   	= "input02=ds_cstmrPartcptn:U";
			var sOutDatasets  	= "";
		break;
		
		//제안정보 첨부파일조회
		case "selectAtchFile":
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_cstmrPartcptn.getColumn(this.ds_cstmrPartcptn.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
		
		//처리정보 첨부파일조회
		case "selectAtchFile2":
			this.div_work.grd_cmmnAtchmnfl00.setCellProperty("head", 0, "text", "0");
			this.ds_fileList2.clearData();
			this.ds_fileList2.addRow();
			this.ds_fileList2.setColumn(0, "FILE_SN", this.ds_cstmrPartcptn.getColumn(this.ds_cstmrPartcptn.rowposition, "PROCESS_ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList2";
			var sOutDatasets = "ds_fileList2=output1";
		break;
		
		case "GetInnerSanctnId": // 내부결재id 조회   
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
		break;
		
		case "innerSanctnUbiDataset": // 시스템연계 내부결재보고서 정보 조회
			this.ds_cond02.clearData();
