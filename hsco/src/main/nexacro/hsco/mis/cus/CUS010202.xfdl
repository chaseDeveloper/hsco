this.div_work_btn_send_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_smsClient1.clearData();	// sms 수신자 목록 초기화
	
	var chkCnt = this.ds_cstmrPartcptn.getCaseCount("CHK == 1");
	
	if(chkCnt<1){
		this.gfn_message("info.항목.선택.알림", "수신자");
		return;
	}
	
	var rowCnt = this.ds_cstmrPartcptn.rowcount;
	
	for(var i=0; i<rowCnt; i++){
		var chkYn = this.ds_cstmrPartcptn.getColumn(i, "CHK");
		if(chkYn == '1'){
			var mbtlNo = this.ds_cstmrPartcptn.getColumn(i, "MBTLNO");
			if(!mbtlNo){
				this.gfn_message("info.처리불가", "민원인 연락처 정보가 존재하지 않습니다.");
				return;
			}
			
			var nRow = this.ds_smsClient1.addRow();
			this.ds_smsClient1.setColumn(nRow, "RECIPIENT_NUM", mbtlNo.replace('-',''));
			this.ds_smsClient1.setColumn(nRow, "CHANGE_WORD5", this.ds_cstmrPartcptn.getColumn(i, "CSTMR_NM"));
		}
	}
	
	//validation check		
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_smsMsg, this.ds_validation)){
		return;
	}
		
	var chrctrCn = this.ds_smsMsg.getColumn(0, "MSSAGE_CN");		// 문자내용
	var telNo    = this.ds_smsMsg.getColumn(0, "ANSWTEL");			// 발신자번호
	var deptId   = this.gfn_getDeptId();
	var smsType = this.div_work.txt_messageCn.getLength("ascii") <= 80 ? '0' : '2';	// 0:SMS , 2:MMS
	
	//문자전송 소스필요
	var sSndDeptcode = this.gfn_getDeptId();      	// 부서코드(필수)
	var sAnswTel = telNo;       					// 발신번호(필수)
    var sServiceType = smsType;     				// sms(단문80 byte):0, mms(장문 2000 byte):2 (필수)
    var sMsge = chrctrCn;  

    //수신 Dataset (필수)
    var sSndDt = ""           // 예약전송시간 YYYYMMDDHH24MISS "20160410153050" , 미입력시 즉시전송
    var sChangWord1 = ""      // 동보메시지단어1
    var sChangWord2 = ""      // 동보메시지단어2
    var sChangWord3 = ""      // 동보메시지단어3
    var sChangWord4 = ""      // 동보메시지단어4    
    
	this.sms_MultiMessage(this, sSndDeptcode, sAnswTel, sServiceType, sMsge, this.ds_smsClient1, sSndDt, sChangWord1, sChangWord2, sChangWord3, sChangWord4) ;	
}

