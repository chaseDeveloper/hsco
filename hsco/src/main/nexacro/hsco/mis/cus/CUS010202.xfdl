this.fn_callback2 = function(sPopupId, sReturn)
{
	if(sPopupId == "CUS010201_P01")
	{	
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);
		
		if(!nRowCnt){
			
		} else {
			var rowCnt = this.ds_cstmrPartcptn.rowcount;
			for(var i=0; i<rowCnt; i++){
				var chkYn = this.ds_cstmrPartcptn.getColumn(i, "CHK1")
				
				if(chkYn == '1'){
					this.ds_cstmrPartcptn.setColumn(i, "PROCESS_DE", dsRtn.getColumn(0,"PROCESS_DE"));
					this.ds_cstmrPartcptn.setColumn(i, "PROCESS_SE", dsRtn.getColumn(0,"PROCESS_SE"));
					this.ds_cstmrPartcptn.setColumn(i, "EXPNDTR_TRGET_AT", dsRtn.getColumn(0,"EXPNDTR_TRGET_AT"));
					this.ds_cstmrPartcptn.setColumn(i, "PROCESS_CN", dsRtn.getColumn(0,"PROCESS_CN")); 
					this.ds_cstmrPartcptn.setColumn(i, "OPETR",    application.gds_userInfo.getColumn(0, "EMPNO"));	// 처리자사번
					this.ds_cstmrPartcptn.setColumn(i, "OPETR_NM", this.fn_getEmpnm());	// 처리자명
				}
			}
		}		
	}
}

