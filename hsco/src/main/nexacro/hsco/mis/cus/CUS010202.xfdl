			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_ubiDataset=output1";
		break; 
		
		//최초 내부결재 팝업 호출 시 저장버튼을 눌러 시작되었는지, 닫기버튼을 눌러 취소를 했는지 여부check
		case "selectInnerSanctnMasterCheck"	:
			var sController   	= "hsco/mis/cus/CUS010201/selectInnerSanctnMasterCheck.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_innerSanctnChek=output1";
		break;
		
		//내부결재 번호 업데이트
		case "saveInnerSanc":
			var sController   	= "hsco/mis/cus/CUS010201/saveInnerSanc.do";
			var sInDatasets   	= "input1=ds_sanctnResult:U";
			var sOutDatasets  	= "";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//상단저장 콜백
			case "cstmrPartcptnListU":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//고객제안목록조회 콜백
			case "selectCstmrPartcptnList":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_cstmrPartcptn.set_rowposition(save_row);
					save_row = -1;
				}
			break;
			
			//첨부파일 조회 콜백
			case "selectAtchFile":
				this.fn_transaction("selectAtchFile2");
			break;
			
			// 내부결재id 조회
			case "GetInnerSanctnId":
			    this.fn_popup_InnerSanctionDrft();
			break;
			
			//최초 내부결재 진행여부체크
			case "selectInnerSanctnMasterCheck":
				if(this.ds_innerSanctnChek.getColumn(0, "CHK") == 1)
				{	
					var nRow = this.ds_cstmrPartcptn.rowposition;
					var lastSttus = this.ds_innerSanctnChek.getColumn(0, "LAST_SANCTN_STTUS");
					
					this.ds_cstmrPartcptn.set_updatecontrol(false);
					this.ds_cstmrPartcptn.setColumn(nRow, "INNER_SANCTN_ID",    this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
					this.ds_cstmrPartcptn.setColumn(nRow, "DRFT_DE",            this.gfn_today());
					this.ds_cstmrPartcptn.setColumn(nRow, "LAST_SANCTN_STTUS",  lastSttus);
					if(lastSttus == 'T12'){
						this.ds_cstmrPartcptn.setColumn(nRow, "PRC_STATUS",  "1");
					}
					this.ds_cstmrPartcptn.set_updatecontrol(false);
					
					this.ds_cstmrPartcptn.set_rowposition(-1);
					this.ds_cstmrPartcptn.set_rowposition(nRow);
				}
			break;
			
			//내부결재 번호 업데이트
			case "saveInnerSanc":
				this.fn_search();
			break;
		}
	}
}
/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_cstmrPartcptn))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_cstmrPartcptn.reset();		//DATASET ROLLBACK
			this.ds_cstmrPartcptn.applyChange();	//DATASET COMMIT
			return;
		}
	}
}
