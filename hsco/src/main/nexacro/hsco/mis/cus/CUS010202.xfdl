		var gridObj = this.ds_cstmrPartcptn;		//그리드
		var gridSelectedRow = gridObj.rowposition;	//그리드 선택로우
		
		var inner_sanctn_sttus = gridObj.getColumn( gridSelectedRow , "INNER_SANCTN_STTUS");
		var registDt = gridObj.getColumn( gridSelectedRow , "REGIST_DT");
		
