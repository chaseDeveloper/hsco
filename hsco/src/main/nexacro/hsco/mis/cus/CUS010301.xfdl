/***********************************************************************
*  엑셀 업로드 버튼 클릭 시
************************************************************************/
this.div_work_Button01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.setWaitCursor(true,true);
	this.ds_hmpgCstmrInfo.clearData();
	var arg = {
		targetDs : this.ds_excelExport,
		headSet : false,
		callBack : function () {
			if(this.ds_excelExport.rowcount > 1){
				var Hrow_con = this.ds_excelExport.getRowCount()-1;
				for(i=1; i< Hrow_con; i++) {
					this.ds_hmpgCstmrInfo.addRow();
					
					if(this.comUtils.isNotNull(this.ds_excelExport.getColumn(i+1, "Column1"))){
						this.ds_hmpgCstmrInfo.setColumn(i-1, "SE",       this.ds_excelExport.getColumn(i+1, "Column1").replace("/\s\*/g", ""));  //핸드폰번호
					}
					this.ds_hmpgCstmrInfo.setColumn(i-1, "CSTMR_NM",    this.ds_excelExport.getColumn(i+1, "Column2"));  //성명
					
					if(this.comUtils.isNotNull(this.ds_excelExport.getColumn(i+1, "Column3"))){
						this.ds_hmpgCstmrInfo.setColumn(i-1, "MBTLNO",       this.ds_excelExport.getColumn(i+1, "Column3").replace("/\s\*/g", ""));  //핸드폰번호
					}
					if(this.comUtils.isNotNull(this.ds_excelExport.getColumn(i+1, "Column4"))){
						this.ds_hmpgCstmrInfo.setColumn(i-1, "GNRL_TLPHON",    this.ds_excelExport.getColumn(i+1, "Column4").replace("/\s\*/g", ""));  //전화번호
					}
					if(this.comUtils.isNotNull(this.ds_excelExport.getColumn(i+1, "Column5"))){
						var brthdy = this.ds_excelExport.getColumn(i+1, "Column5");
						var brthdy2 = this.dateUtils.format(brthdy,"yyyymmdd");
						this.ds_hmpgCstmrInfo.setColumn(i-1, "BRTHDY",   brthdy2);  //생년월일
					}
					this.ds_hmpgCstmrInfo.setColumn(i-1, "EMAIL",    this.ds_excelExport.getColumn(i+1, "Column6"));  //이메일
					this.ds_hmpgCstmrInfo.setColumn(i-1, "ADRES",     this.ds_excelExport.getColumn(i+1, "Column7"));  //주소
					this.ds_hmpgCstmrInfo.setColumn(i-1, "PARTCLR_MATTER",     this.ds_excelExport.getColumn(i+1, "Column8"));  //특이사항
					
					this.ds_hmpgCstmrInfo.setRowType(i-1,"I");
				}
			}
			this.setWaitCursor(false,true);
     	}	
	};
	this.gfn_importExcel(arg);	
}

