    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID     : CUS010301.xfdl
 * 화면명    : 고객관리
 * 화면설명  : 고객 정보를 관리한다.
 * 작성일    : 2015.08.07
 * 작성자    :  이지영
 * 수정이력  : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************/
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
  this.ds_hmpgCstmrInfo
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS010301_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
 	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010301_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	application.set_httptimeout(60);
	var comboParams = [["ds_cstmrGrad",  "CUS021", 1, ""],
					   ["ds_indvdlTy",   "CUS025", 1, ""],
					   ["ds_cstmrGradS", "CUS021", 1, "전체"],
					   ["ds_indvdlTyS",  "CUS025", 1, "전체"],
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
	this.fn_transaction("selectGetSe");
	this.div_work.Tab00.set_tabindex(1);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.	
	this.fn_misFormInit(this);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	this.fn_transaction("selectCstmrList");
}

/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{	
	if(Ex.util.isUpdated(this.ds_hmpgCstmrInfo) || Ex.util.isUpdated(this.ds_cvplCstmrList)) {              //데이터셋의 수정여부 체크
		// validation 체크
		if(!this.gfn_checkValidation_NoUpdChk(this.ds_hmpgCstmrInfo, this.ds_validation)) {
			return false;
		}
		
		if(this.gfn_message("confirm.저장.여부")){
			//현재 rowpostion값을 저장
			save_row = this.ds_hmpgCstmrInfo.rowposition;
			this.fn_transaction("cstmrListCUD");
		}
		
	} else {	
	    this.gfn_message("comm.데이터.변경.없음");
	}	
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//고객 목록 조회
		case "selectCstmrList":
			var sController   	= "hsco/mis/cus/CUS010301/selectCstmrList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cvplCstmrList=output01 ds_hmpgCstmrInfo=output02";
		break;
		
		//고객 목록 저장/수정/삭제
		case "cstmrListCUD":
			var sController   	= "hsco/mis/cus/CUS010301/cstmrListCUD.do";
			var sInDatasets   	= "input01=ds_cvplCstmrList:U input02=ds_hmpgCstmrInfo:U";
			var sOutDatasets  	= "";
		break;
		
		//고객 목록 구분(코드용 조회)
		case "selectGetSe":
			var sController   	= "hsco/mis/cus/CUS010301/selectGetSe.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_cmbSe=output01";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			
			//상단저장 콜백
			case "cstmrListCUD":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//고객 목록 조회 콜백
			case "selectCstmrList":
			//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_hmpgCstmrInfo.set_rowposition(save_row);
					save_row = -1;
				}
				this.fn_transaction("selectGetSe");
			break;
			//고객 목록 구분(코드용 조회)
			case "selectGetSe":
				this.ds_cmbSe.insertRow(0);
				this.ds_cmbSe.setColumn(0, "CODE_NM", "전체");
				if(this.ds_cond.getColumn(0, "SE") == null || this.ds_cond.getColumn(0, "SE") == ""){
					this.div_search.cmb_se.set_index(0);
				}
			break;
		}
	}
}

/**********************************************************************
* 그리드 헤드 클릭 시 
***********************************************************************/ 
this.div_work_grd_hmpgCstmrInfo_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0)
	{
		//체크박스 전체 선택
		this.gfn_setGridCheckAll(obj, e);
	}else{	
		//선택한 헤드에 따른 정렬
		this.gfn_gridSort(obj, e);
	}
}

/**********************************************************************
* 위 주소와 동일하게 체크박스 클릭시 
***********************************************************************/ 
this.div_work_chk_samenss_onchanged = function(obj:CheckBox, e:nexacro.CheckBoxChangeEventInfo)
{
	if(this.div_work.Tab00.tpg2.chk_samenss.value == true)
	{
		this.div_work.Tab00.tpg2.msk_zip2.set_value(this.div_work.Tab00.tpg2.msk_zip.value);
		this.div_work.Tab00.tpg2.edt_pstmtrAdres.set_value(this.div_work.Tab00.tpg2.edt_adres.value);
	}else{
		this.div_work.Tab00.tpg2.msk_zip2.set_value('');
		this.div_work.Tab00.tpg2.edt_pstmtrAdres.set_value('');
	}
}
/**********************************************************************
* 개인고객목록 셀 영역 변경 시 (dataset)
***********************************************************************/ 
this.ds_hmpgCstmrInfo_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.div_work.Tab00.tpg2.chk_samenss.set_value(0);	
	
	if(this.ds_hmpgCstmrInfo.getRowType(e.newrow) == Dataset.ROWTYPE_INSERT)
	{
		this.div_work.Tab00.tpg2.edt_cstmrNm.set_readonly(false);
		this.div_work.Tab00.tpg2.edt_cstmrNm.set_cssclass("edt_WF_Essential");
		this.div_work.Tab00.tpg2.edt_mbtlno.set_readonly(false);
		this.div_work.Tab00.tpg2.edt_mbtlno.set_cssclass("edt_WF_Essential");
	}else{
		this.div_work.Tab00.tpg2.edt_cstmrNm.set_readonly(true);
