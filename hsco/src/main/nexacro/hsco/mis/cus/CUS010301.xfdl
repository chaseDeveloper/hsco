/***********************************************************************
* 기타고객 행삭제 버튼 클릭 시
************************************************************************/
this.div_work_Tab00_tpg2_btn_delRow_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var ro_count = this.ds_hmpgCstmrInfo.rowcount;
	var delete_count = 0 ;
	//데이터셋에서 모든 체크된 행을 삭제
	for(var i = ro_count; i >= 0 ; i-- ){
		if(this.ds_hmpgCstmrInfo.getColumn(i,"CHK") == "1")
		{
			delete_count = delete_count +1;
		}
	}
	
	//체크된 행이 없을 경우
	if(delete_count < 1)
	{
		this.gfn_message("comm.데이터.선택.없음");
	}else{
		if(this.gfn_message("confirm.삭제여부"))
		{
			for(var i = ro_count; i >= 0 ; i-- ){
				if(this.ds_hmpgCstmrInfo.getColumn(i,"CHK") == "1")
				{
					this.ds_hmpgCstmrInfo.deleteRow(i);	
					delete_count = delete_count +1;
				}
			}
			this.div_work.Tab00.tpg2.grd_hmpgCstmrInfo.setCellProperty("Head", 0 , "text", "0");
		}
	}
}

/***********************************************************************
* 기타고객 행취소 버튼 클릭 시
************************************************************************/
this.div_work_Tab00_tpg2_btn_canRow_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(!this.comUtils.isDatasetUpdated(this.ds_hmpgCstmrInfo))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_hmpgCstmrInfo.reset();		//DATASET ROLLBACK
			this.ds_hmpgCstmrInfo.applyChange();	//DATASET COMMIT
			return;
		}
	}
}


