/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{
	if(Ex.util.isUpdated(this.ds_teamCvplDstb)){
		var nCnt = this.ds_teamCvplDstb.rowcount;
		
		for(var i=0; i<nCnt; i++){
			var rowType = this.ds_teamCvplDstb.getRowType(i);
			
			if(rowType != Dataset.ROWTYPE_NORMAL){
				if(this.gfn_authcode("101") > -1 && this.ds_teamCvplDstb.getColumn(i, "MAIN_HELP_SE") == null || this.ds_teamCvplDstb.getColumn(i, "MAIN_HELP_SE") == ""){
					this.ds_teamCvplDstb.setColumn(i, "MAIN_HELP_SE", 1);
				}
				if(this.ds_teamCvplDstb.getColumn(i, "MAIN_HELP_SE") == 1){				// 주관부서
					if(this.ds_teamCvplDstb.getColumn(i, "CVPL_STTUS") == 4){			// 담당자지정 상태
						if(!this.ds_teamCvplDstb.getColumn(i, "APPN_CHARGER_NM")){		// 담당자 null 체크
							this.gfn_message("comm.항목.입력.을", "담당자");
							return;
						}
						if(!this.ds_teamCvplDstb.getColumn(i, "PROCESS_TMLMT")){	// 처리기한 null 체크
							this.gfn_message("comm.항목.입력.을", "처리기한");
							return;
						}
					}
				}
			}
			
			if(rowType != Dataset.ROWTYPE_NORMAL){
				if(this.ds_teamCvplDstb.getColumn(i, "MAIN_HELP_SE") == 2){				// 협조부서
					if(this.ds_teamCvplDstb.getColumn(i, "CVPL_STTUS") == 4){			// 담당자지정 상태
						if(!this.ds_teamCvplDstb.getColumn(i, "HELP_APPN_CHARGER_NM")){	// 담당자 null 체크
							this.gfn_message("comm.항목.입력.을", "담당자");
							return;
						}
						if(!this.ds_teamCvplDstb.getColumn(i, "PROCESS_TMLMT")){	// 처리기한 null 체크
							this.gfn_message("comm.항목.입력.을", "처리기한");
							return;
						}
					}
				}
			}
		}
			
		if(this.gfn_message("confirm.저장.여부")){
			//현재 rowpostion값을 저장
			this.save_row = this.ds_teamCvplDstb.rowposition;
			this.fn_transaction("teamCvplDstbListCUD");
		}
	}else{
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
	
	
}

/***********************************************************************
