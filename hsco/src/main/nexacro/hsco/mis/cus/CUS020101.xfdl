/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//부서콤보 조회
		case "selectDeptcodeList":
			this.ds_cond03.clearData();
			var nRow = this.ds_cond03.addRow();
			var sController   	= "hsco/mis/cus/CUS010102/selectDeptCodeList.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_dept=output1 ds_permList=output2";
		break;	
		
		//민원목록 리스트 조회
		case "selectTeamCvplDstbList" :	
			this.ds_fileList.clearData();
			this.ds_cond.setColumn(0, "EMPNO", application.gds_userInfo.getColumn(0, "EMPNO"));
			this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
			if(this.gfn_authcode("101") > -1){	//고객VOC담당
				this.ds_cond.setColumn(0, "AUTH_CODE", "101");
			}
			var sController   	= "hsco/mis/cus/CUS020101/selectTeamCvplDstbList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_teamCvplDstb=output01";
			break;	
			
		//담당자정보 조회
		case "selectChargerAppnInfo":
			var sController   	= "hsco/mis/cus/CUS010102/selectChargerAppnInfo.do";
			var sInDatasets   	= "input02=ds_cond01";
			var sOutDatasets  	= "ds_cargerAppnInfo=output02";
		break;	
	
		//우리팀 민원분배 저장(상단 저장)
		case "teamCvplDstbListCUD":
			var sController   	= "hsco/mis/cus/CUS020101/teamCvplDstbListCUD.do";
			var sInDatasets   	= "input03=ds_teamCvplDstb:U";
			var sOutDatasets  	= "";
		break;
		
		//접수반려
		case "returnCVPLU":
 			var sController   	= "hsco/mis/cus/CUS020101/returnCVPLU.do";
 			var sInDatasets   	= "input03=ds_teamCvplDstb:U";
 			var sOutDatasets  	= "";
		break;
			
		//첨부파일조회
		case "selectAtchFile":
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_teamCvplDstb.getColumn(this.ds_teamCvplDstb.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//상단조회 콜백
			case "selectCvplList":
				if(select_at){
					this.div_work.grd_hmpgCstmrCvpl.selectRow(rowpos);	
				}
				select_at = false;
			    //저장트랜잭션 직전 rowposition값으로 이동
				if(this.save_row != -1)
				{
					this.ds_teamCvplDstb.set_rowposition(this.save_row);
					this.save_row = -1;
				}
			break;
			
			//부서코드 조회 콜백
			case "selectDeptcodeList":
				this.fn_search();
			break;
			
			//담당자정보조회 콜백
			case "selectChargerAppnInfo":
			    //ds_cargerAppnInfo로 받은 값을 ds_hmpgCstmrCvpl에 셋팅.
			    this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "CHARGER_EMPNO", this.ds_cargerAppnInfo.getColumn(0,"DEPT_CHARGER_EMPNO"));
				this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "CHARGER_NM",    this.ds_cargerAppnInfo.getColumn(0,"DEPT_CHARGER_NM"));
				this.div_work.cmb_dclzSe.set_value(this.ds_cargerAppnInfo.getColumn(0,"DCLZ_SE"));
			break;
			
			//저장 콜백
			case "teamCvplDstbListCUD":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//부서 반려 콜백
			case "returnCVPLU":
				this.fn_search();
			break;
			
			//첨부파일 조회 콜백
			case "selectAtchFile":
			break;
		}
	}
}
/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_hmpgCstmrCvpl))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_hmpgCstmrCvpl.reset();		//DATASET ROLLBACK
			this.ds_hmpgCstmrCvpl.applyChange();	//DATASET COMMIT
			return;
		}
	}
}

