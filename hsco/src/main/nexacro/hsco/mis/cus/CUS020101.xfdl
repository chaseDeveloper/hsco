/***********************************************************************
* 우리팀 민원목록 행 변경 시 첨부파일 조회
************************************************************************/
this.ds_teamCvplDstb_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	//주부서 처리건
	if(this.ds_teamCvplDstb.getColumn(this.ds_teamCvplDstb.rowposition, "MAIN_HELP_SE") == '1'
	 || this.gfn_authcode("101") > -1) 	//고객VOC담당)
	{
		this.div_work.cmb_chrgDeptCode.set_visible(true);  
		this.div_work.edt_appnCharger.set_visible(true);
		this.div_work.sta_chargerNm.set_visible(true);
		this.div_work.cmb_helpDeptCode.set_visible(false);  
		this.div_work.edt_helpCharger.set_visible(false);  
		this.div_work.sta_helpCharger.set_visible(false);
		this.div_work.btn_change1.set_visible(true);
		this.div_work.btn_change2.set_visible(false);
		this.div_work.btn_backUp.set_visible(true);
		this.div_work.cmb_rceptReturn.set_enable(true);
		this.div_work.txt_returnPrvonsh.set_enable(true);
		
		//민원상태가 담당자미지정 경우 담당자, 반려 정보 수정가능
		var oldCvplSttus = this.ds_teamCvplDstb.getColumn(this.ds_teamCvplDstb.rowposition, "OLD_CVPL_STTUS");
		if(oldCvplSttus == '2' || oldCvplSttus == '8')
		{
			this.div_work.cmb_rceptReturn.set_readonly(false);
			this.div_work.cmb_rceptReturn.set_cssclass("");
			this.div_work.txt_returnPrvonsh.set_readonly(false);
			this.div_work.txt_returnPrvonsh.set_cssclass("");
			this.div_work.btn_change1.set_visible(true);
			this.div_work.btn_backUp.set_visible(true);
		}else{
		//민원상태가 담당자지정 경우 담당자, 반려 정보 수정 불가능
			this.div_work.cmb_rceptReturn.set_readonly(true);
			this.div_work.cmb_rceptReturn.set_cssclass("cmb_WF_Readonly");
			this.div_work.txt_returnPrvonsh.set_readonly(true);
			this.div_work.txt_returnPrvonsh.set_cssclass("txt_WF_Readonly");
			this.div_work.btn_change1.set_visible(false);
			this.div_work.btn_backUp.set_visible(false);
		}
	//협조부서 처리건
	}else if(this.ds_teamCvplDstb.getColumn(this.ds_teamCvplDstb.rowposition, "MAIN_HELP_SE") == '2'){ 
		this.div_work.cmb_chrgDeptCode.set_visible(false);  
		this.div_work.edt_appnCharger.set_visible(false);   
		this.div_work.sta_chargerNm.set_visible(false);
		this.div_work.btn_change1.set_visible(false);
		this.div_work.btn_backUp.set_visible(true);
		this.div_work.cmb_helpDeptCode.set_visible(true);  
		this.div_work.edt_helpCharger.set_visible(true);
		this.div_work.sta_helpCharger.set_visible(true);
		this.div_work.cmb_rceptReturn.set_enable(false);
		this.div_work.txt_returnPrvonsh.set_enable(false);
		
		//민원상태가 담당자미지정 경우 담당자 정보 수정가능
		var oldCvplSttus = this.ds_teamCvplDstb.getColumn(this.ds_teamCvplDstb.rowposition, "OLD_CVPL_STTUS");
		if(oldCvplSttus == '2' || oldCvplSttus == '8')
		{
			this.div_work.btn_change2.set_visible(true);
			this.div_work.btn_backUp.set_visible(true);
		}else{
		//민원상태가 담당자지정 경우 담당자 정보 수정 불가능
			this.div_work.btn_change2.set_visible(false);
			this.div_work.btn_backUp.set_visible(false);
		}
	}else{
		this.div_work.cmb_chrgDeptCode.set_visible(true);  
		this.div_work.edt_appnCharger.set_visible(true);
		this.div_work.sta_chargerNm.set_visible(true);
		this.div_work.cmb_helpDeptCode.set_visible(false);  
		this.div_work.edt_helpCharger.set_visible(false);  
		this.div_work.sta_helpCharger.set_visible(false);
		this.div_work.btn_change1.set_visible(false);
		this.div_work.btn_change2.set_visible(false);
		this.div_work.btn_backUp.set_visible(false);
		this.div_work.cmb_rceptReturn.set_enable(false);
		this.div_work.txt_returnPrvonsh.set_enable(false);
	}
	this.fn_transaction("selectAtchFile");
}

