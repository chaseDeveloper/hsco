this.fn_uploadCallback = function()
{
	if(!this.comUtils.isNull(this.ds_tmp.getColumn(0, "ATCH_FILE")))
	{
		if(this.comUtils.isNull(this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "PROCESS_ATCH_FILE")))
		{
			this.ds_offlineCvplList.setColumn(this.ds_offlineCvplList.rowposition, "PROCESS_ATCH_FILE", this.ds_tmp.getColumn(0, "ATCH_FILE"))
			this.fn_transaction("selectAtchFile");
			return true;
		}else{
			this.fn_transaction("selectAtchFile");
			return true;
		}
	}else{
		this.fn_transaction("selectAtchFile");
		return true;
	}
	
}

