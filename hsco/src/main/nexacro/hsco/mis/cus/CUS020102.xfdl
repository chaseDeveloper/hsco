		this.div_work.btn_uploadFiles00.set_enable(true);
		
		this.div_work.btn_drft.set_enable(false);
		this.div_work.btn_change2.set_enable(true);
		this.div_work.btn_backUp.set_enable(true);
		
// 		this.div_work.txt_snsCn.set_readonly(true);			// 문자내용(수정불가능)
// 		this.div_work.btn_send.set_enable(false);			// 문자발송버튼(수정불가능)
		
	//저장후(접수 후)
	}else{
		this.div_work.cal_rceptDt.set_readonly(true);
