			}
		}
		
		// 현재사용자 = 처리자
		if(empno == empno2){
			
			if(sttus == '3'){	// 처리중(결재전)
				this.div_work.cal_processDt.set_readonly(false);	// 처리일자(수정가능)
				this.div_work.cal_processDt.set_cssclass("");
				
				this.div_work.btn_change2.set_enable(false);		// 처리자선택(수정불가능)
				this.div_work.btn_backUp.set_enable(false);			// 처리자초기화(수정불가능)
				this.div_work.txt_processCn.set_readonly(false);	// 처리내용(수정불가)
				this.div_work.txt_processCn.set_cssclass("");
				
				this.div_work.btn_delFiles00.set_enable(true);		// 처리 첨부파일삭제 (수정가능)
				this.div_work.btn_uploadFiles00.set_enable(true);	// 처리 첨부파일업로드(수정가능)
				
				if(rowType == Dataset.ROWTYPE_NORMAL && processDt){	// 처리일자가 있고, 수정안된 상태일 때 기안 가능
					this.div_work.btn_drft.set_enable(true);
				}
			} else {	// 결재중, 처리완료, 응답완료
				this.div_work.btn_drft.set_enable(true);			// 기안버튼(수정불가능)
				this.div_work.btn_change2.set_enable(false);		// 처리자선택(수정불가능)
				this.div_work.btn_backUp.set_enable(false);			// 처리자초기화(수정불가능)
				this.div_work.cal_processDt.set_readonly(true);		// 처리일자(수정불가)
				this.div_work.cal_processDt.set_cssclass("cal_WF_Readonly");
				this.div_work.txt_processCn.set_readonly(true);		// 처리내용(수정불가)
				this.div_work.txt_processCn.set_cssclass("txt_WF_Readonly");
				this.div_work.btn_delFiles00.set_enable(false);
				this.div_work.btn_uploadFiles00.set_enable(false);
				
//  				if(sttus == '5'){ // 처리완료
// 					this.div_work.txt_snsCn.set_readonly(false);		// 문자내용(수정가능)
// 					this.div_work.btn_send.set_enable(true);			// 문자발송버튼(수정가능)
//  				}
			}
		}
		
		// 현재사용자 = 접수자 && 처리자
		if(empno == empno2 && empno == empno1){
			var rowType = this.ds_offlineCvplList.getRowType(nRow);
			
			if(sttus == '3'){	// 처리중(결재전)
				
				this.div_work.btn_change2.set_enable(true);			// 처리자선택(수정가능)
				this.div_work.btn_backUp.set_enable(true);			// 처리자초기화(수정가능)
				
				this.div_work.btn_delFiles.set_enable(true);		// 접수첨부파일삭제(수정가능)
				this.div_work.btn_uploadFiles.set_enable(true);		// 접수첨부파일업로드(수정가능)
				
				if(rowType == Dataset.ROWTYPE_NORMAL && processDt){	// 처리일자가 있고, 수정안된 상태일 때 기안 가능
					this.div_work.btn_drft.set_enable(true);
				}
			}
		}		
	}
		
	//내부결재가 시작 된 오프라인민원의 경우 처리정보를 변경 불가능하게.
// 	if(!this.comUtils.isNull(this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "DRFT_JOB_CONFM_NO")))
// 	{
// 		this.div_work.cal_processDt.set_readonly(true);
// 		this.div_work.cal_processDt.set_cssclass("cal_WF_Readonly");
// 		this.div_work.btn_empno.set_enable(false);
// 		this.div_work.txt_processCn.set_readonly(true);
// 		this.div_work.txt_processCn.set_cssclass("txt_WF_Readonly");
// 	}else{
// 		this.div_work.cal_processDt.set_readonly(false);
// 		this.div_work.cal_processDt.set_cssclass("");
// 		this.div_work.btn_empno.set_enable(true);
// 		this.div_work.txt_processCn.set_readonly(false);
// 		this.div_work.txt_processCn.set_cssclass("");
// 	}
	
	this.fn_transaction("selectAtchFile");
}
/***********************************************************************************
* 설명	: 조회기간 변경 시 
***********************************************************************************/
this.ds_cond_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
		//시작일 변경시
	if(e.columnid == "RCEPT_BGN_DT")
	{
		if(e.newvalue > this.div_search.cal_rceptEndDt.value) 
		{
			this.gfn_message("errors.시작일자.종료일자.이하","|");
			return false;
		}
		return true;
	}
	//종료일 변경시
	if(e.columnid == "RCEPT_END_DT")
	{
		if(e.newvalue < this.div_search.cal_rceptBgnDt.value) 
		{
			this.gfn_message("errors.종료일자.시작일자.이상","|");
			return false;
		}
		return true;
	}
}

