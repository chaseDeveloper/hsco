this.fn_save = function()
{
	//validation check		
	if(!this.gfn_checkValidation(this.ds_offlineCvplList, this.ds_validation)){
		return;
	} 
	
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.ds_offlineCvplList.rowposition;
		//처리일자가 없을 경우에만 오늘날짜로셋팅
		if(this.comUtils.isNull(this.ds_offlineCvplList.getColumn(save_row, "PROCESS_DT"))){
			this.ds_offlineCvplList.setColumn(save_row, "PROCESS_DT", this.gfn_today());
		}
		this.fn_transaction("offlineCvplListCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//오프라인 민원목록 조회
		case "selectOfflineCvplList": 
			this.ds_fileList.clearData();
			this.ds_fileList2.clearData();
			var sController   	= "hsco/mis/cus/CUS020102/selectOfflineCvplList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_offlineCvplList=output01";
			break;	
				
		//오프라인 민원 목록 저장
		case "offlineCvplListCUD" :
			if(this.comUtils.isNull(this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "PROCESS_ATCH_FILE")))
			{
				this.ds_offlineCvplList.setColumn(this.ds_offlineCvplList.rowposition, "PROCESS_ATCH_FILE", this.ds_tmp.getColumn(0, "ATCH_FILE"));
			}
// 			if(!this.comUtils.isNull(this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "PROCESS_DT")))
// 			{
// 				this.ds_offlineCvplList.setColumn(this.ds_offlineCvplList.rowposition, "CVPL_STTUS", 10);
// 			}
// 			else{
// 				this.ds_offlineCvplList.setColumn(this.ds_offlineCvplList.rowposition, "CVPL_STTUS", 5);
// 			}
			var sController   	= "hsco/mis/cus/CUS020102/offlineCvplListCUD.do";
			var sInDatasets   	= "input02=ds_offlineCvplList:U";
			var sOutDatasets  	= "";
			break;	
		
		//고객정보 조회
		case "selectHmpgCstmrInfo":
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0,"CSTMR_NM",this.div_work.edt_nm.value)
			this.ds_cond02.setColumn(0,"MBTLNO",this.div_work.edt_mbtlno.value)
			
			var sController   	= "hsco/mis/cus/CUS010102/selectHmpgCstmrInfo.do";
			var sInDatasets   	= "input03=ds_cond02";
			var sOutDatasets  	= "ds_hmpgCstmrInfo=output03";
			break;
		break;		
		
		//첨부파일조회
		case "selectAtchFile":
			// 접수첨부파일
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			// 처리첨부파일
			this.div_work.grd_cmmnAtchmnfl00.setCellProperty("head", 0, "text", "0");
			this.ds_fileList2.clearData();
			this.ds_fileList2.addRow();
			this.ds_fileList2.setColumn(0, "FILE_SN", this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "PROCESS_ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			
			var sController  = "/hsco/mis/cus/CUS020102/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList input2=ds_fileList2";
			var sOutDatasets = "ds_fileList=output1 ds_fileList2=output2";
		break;
		
		case "GetInnerSanctnId": // 내부결재id 채번 
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
		break;
		
		case "innerSanctnUbiDataset": // 시스템연계 내부결재보고서 정보 조회
			this.ds_cond04.clearData();
			this.ds_cond04.addRow();
			this.ds_cond04.setColumn(0, "RCEPT_NO", this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "RCEPT_NO"));
			this.ds_cond04.setColumn(0, "CVPL_NO",  this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "CVPL_NO"));
			var sController   	= "hsco/mis/cus/CUS020102/innerSanctnUbiDataset.do";
			var sInDatasets   	= "input1=ds_cond04";
			var sOutDatasets  	= "ds_ubiDataset=output1";
		break; 
		
		//최초 내부결재 팝업 호출 시 저장버튼을 눌러 시작되었는지, 닫기버튼을 눌러 취소를 했는지 여부check
		case "selectInnerSanctnMasterCheck"	:
			var sController   	= "hsco/mis/cus/CUS020102/selectInnerSanctnMasterCheck.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "ds_innerSanctnChek=output1";
		break;
		
		//로그인 사용자 정보 조회
		case "selectUserInfo" :		
			
			var sController   	= "hsco/mis/cus/CUS000000/selectUserInfo.do";
			var sInDatasets   	= "input01=ds_cond01";
			var sOutDatasets  	= "ds_session=output01";
		break;
		
		//내부결재
		case "innerSanctU":
			var sController   	= "hsco/mis/cus/CUS020102/innerSanctU.do";
			var sInDatasets   	= "input01=ds_sanctnResult";
			var sOutDatasets  	= "";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//상단저장 콜백
			case "offlineCvplListCUD":
				this.fn_search();
			break;	
			
			//상단조회콜백
			case "selectOfflineCvplList":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_offlineCvplList.set_rowposition(save_row);
					save_row = -1;
				}	
			break;
			//고객정보 조회 콜백
	   		case "selectHmpgCstmrInfo":
				if(this.ds_hmpgCstmrInfo.rowcount > 0){
					//가져온 고객정보를 통합(민원+고객정보)데이터셋에 복사
					this.div_work.edt_nm.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"CSTMR_NM"));
					this.div_work.edt_mbtlno.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"MBTLNO"));
					this.div_work.edt_zipcode.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"ZIP"));
					this.div_work.edt_adres.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"ADRES"));
				}else{
					this.gfn_message("fail.정보검색.실패","고객정보를");
				}
			break;
			
			case "selectAtchFile":
				var sttus = this.ds_offlineCvplList.getColumn(this.ds_offlineCvplList.rowposition, "CVPL_STTUS");				// 현재 row의 처리상태
				var empno = application.gds_userInfo.getColumn(0, "EMPNO");		// 현재 로그인 사용자 사번
//				if(sttus == '5'){	// 처리완료
					this.ds_cond01.clearData();
					this.ds_cond01.addRow();
					this.ds_cond01.setColumn(0, "EMPNO", empno);
					this.fn_transaction("selectUserInfo");	
//				}
			break;
			
			// 내부결재id 채번
			case "GetInnerSanctnId":
			    this.fn_popup_InnerSanctionDrft();
			break;
			
			//최초 내부결재 진행여부체크
			case "selectInnerSanctnMasterCheck":
				
				if(this.ds_innerSanctnChek.rowcount > 0){
					if(this.ds_innerSanctnChek.getColumn(0, "CHK") == 1)
					{
						var lastSttus = this.ds_innerSanctnChek.getColumn(0, "LAST_SANCTN_STTUS");
						var nRow = this.ds_offlineCvplList.rowposition;
						
						this.ds_offlineCvplList.set_updatecontrol(false);
						this.ds_offlineCvplList.setColumn(nRow, "DRFT_JOB_CONFM_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
						this.ds_offlineCvplList.setColumn(nRow, "LAST_SANCTN_STTUS", lastSttus);
						this.ds_offlineCvplList.setColumn(nRow, "DRFT_DE",           this.gfn_today());
						
						var imprtySttus = ["S03","T03","T04","T06","T08","T09","T10","T12","T13","T14","T16"]; // 수정불가 결재상태
						
						if(imprtySttus.indexOf(lastSttus) > -1) {
							if(lastSttus == 'T12'){
								this.ds_offlineCvplList.setColumn(nRow, "CVPL_STTUS", '5');	// 처리완료
							}else if(imprtySttus.indexOf(lastSttus) > -1){
								this.ds_offlineCvplList.setColumn(nRow, "CVPL_STTUS", '4');	// 결재중
							}else {
								this.ds_offlineCvplList.setColumn(nRow, "CVPL_STTUS", '3');	// 처리중
							}
						}else{
							
						}
						this.ds_offlineCvplList.set_updatecontrol(true);
						
						this.ds_offlineCvplList.set_rowposition(-1);
						this.ds_offlineCvplList.set_rowposition(nRow);						
					}
				}
			break;
			
			//로그인 사용자 정보
			case "selectUserInfo":
				var telNo = this.ds_session.getColumn(0, "TELNO_1");
				this.ds_chrctrCnInfo.clearData();
				this.ds_chrctrCnInfo.addRow();
				this.ds_chrctrCnInfo.setColumn(0, "SEND_TEL_NO", telNo);
			break;
			
			case "innerSanctU":
				this.fn_search();
			break;
		}
	}
}
/***********************************************************************
* 상단 신규
************************************************************************/
this.fn_insert = function()
{
	var nRow = this.ds_offlineCvplList.addRow();
	this.ds_offlineCvplList.setColumn(nRow, "RCEPT_DT" , this.gfn_today());      
	this.ds_offlineCvplList.setColumn(nRow, "RCEPT_EMPNO" , application.gds_userInfo.getColumn(0, "EMPNO"));        	
	this.ds_offlineCvplList.setColumn(nRow, "RCEPT_EMPNM" , this.fn_getEmpnm());
	
	this.ds_offlineCvplList.setColumn(this.ds_offlineCvplList.rowposition, "CVPL_STTUS", "3");	// 코드3 : 처리중
}

/***********************************************************************
* 상단삭제
************************************************************************/
this.fn_delete = function()
{
	if(this.gfn_message("confirm.삭제여부"))
	{
		var nRow = this.ds_offlineCvplList.rowposition;
		this.ds_offlineCvplList.deleteRow(nRow);
		this.ds_offlineCvplList_onrowposchanged();
	}
}

/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_offlineCvplList))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{	
		if(this.gfn_message("confirm.취소여부")){
			this.ds_offlineCvplList.reset();		//DATASET ROLLBACK
			this.ds_offlineCvplList.applyChange();	//DATASET COMMIT
			return;
		}
	}
}

