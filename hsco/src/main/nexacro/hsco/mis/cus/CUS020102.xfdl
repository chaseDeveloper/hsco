}

/***********************************************************************************
* 설명	: 팝업 콜백
***********************************************************************************/
this.fn_callback5 = function(sPopupId, sReturn)
{
	if(sPopupId == "Popup2")
	{	
		this.ds_rtn_1.clearData();
        this.ds_rtn_1.loadXML(sReturn);  
        if(this.ds_rtn_1.rowcount > 0){
			
			var nRow = this.ds_offlineCvplList.rowposition;
			this.ds_offlineCvplList.setColumn(nRow, "CHRG_DEPT_CODE",  this.gfn_getDeptId());
			this.ds_offlineCvplList.setColumn(nRow, "CHRG_DEPT_NM",    this.gfn_getDeptName());
			this.ds_offlineCvplList.setColumn(nRow, "APPN_CHARGER_NM", this.ds_rtn_1.getColumn(0,"DEPT_CHARGER_NM"));		
			this.ds_offlineCvplList.setColumn(nRow, "APPN_CHARGER",    this.ds_rtn_1.getColumn(0,"DEPT_CHARGER_EMPNO"));  
			
			this.ds_offlineCvplList.setColumn(nRow, "CVPL_STTUS", "3");		// 코드3 : 처리중
			this.ds_offlineCvplList_onrowposchanged();
        }
	}
}

