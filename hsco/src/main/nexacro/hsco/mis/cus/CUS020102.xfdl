		this.div_work.txt_cn.set_cssclass("txt_WF_Readonly");
// 		this.div_work.rdo_rceptMth.set_readonly(true);
// 		this.div_work.rdo_rceptMth.set_cssclass("rdo_WF_Readonly");
		this.div_work.cmb_rceptMth.set_readonly(true);
		this.div_work.cmb_rceptMth.set_cssclass("cmb_WF_Readonly");
		
		this.div_work.cal_processDt.set_readonly(true);
		this.div_work.cal_processDt.set_cssclass("cal_WF_Readonly");
