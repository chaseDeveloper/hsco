		var sttus = this.ds_offlineCvplList.getColumn(nRow, "CVPL_STTUS");
		
		// 현재사용자 = 접수자
		if(empno == empno1){
			if(sttus == '1' || sttus == '3'){	// 접수대기(처리담당자 미지정상태), 처리중
