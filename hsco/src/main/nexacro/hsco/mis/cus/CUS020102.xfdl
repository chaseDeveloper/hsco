		this.div_work.btn_uploadFiles.set_enable(false);
		this.div_work.btn_change2.set_enable(false);
		this.div_work.btn_backUp.set_enable(false);
		this.div_work.btn_drft.set_enable(false);
		
		//this.div_work.txt_snsCn.set_readonly(true);			// 문자내용(수정불가능)
		//this.div_work.btn_send.set_enable(false);			// 문자발송버튼(수정불가능)
		
		var empno = application.gds_userInfo.getColumn(0, "EMPNO");	//현재사용자	
		
		var empno1 = this.ds_offlineCvplList.getColumn(nRow, "RCEPT_EMPNO");	//접수자
		var empno2 = this.ds_offlineCvplList.getColumn(nRow, "APPN_CHARGER");	//처리자
