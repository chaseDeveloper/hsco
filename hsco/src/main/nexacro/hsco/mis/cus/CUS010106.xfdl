			this.ds_cstmrVisitEvlCard.setColumn(this.ds_cstmrVisitEvlCard.rowposition, 'CHARGER_NM',   this.ds_rtn_1.getColumn(this.ds_rtn_1.rowposition, 'EMPNM'));
