 */
this.div_work_btn_address00_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var fn_callback = function(sPopupId, sReturn) {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var zipNo         = dsTemp.getColumn(0, "ZIPNO").replace("-","");	// 우편번호
			var roadAddrPart1 = dsTemp.getColumn(0, "ROADADDRPART1");			// 도로명주소(일반)
			var roadAddrPart2 = dsTemp.getColumn(0, "ROADADDRPART2");			// 도로명주소(상세)
			var jibunAddr     = dsTemp.getColumn(0, "JIBUNADDR");				// 지번주소
			
			var nRow = this.ds_cstmrVisitEvlCard.rowposition;
			this.ds_cstmrVisitEvlCard.setColumn(nRow, "ZIP",          zipNo        );
			this.ds_cstmrVisitEvlCard.setColumn(nRow, "ADRES",        roadAddrPart1);
			this.ds_cstmrVisitEvlCard.setColumn(nRow, "ADRES_DETAIL", roadAddrPart2);
		}
	}
	
	this.gfn_popup('zipPopup', 500, 500, '', null, "common::frmZipAddressPopup.xfdl", fn_callback);		
}
