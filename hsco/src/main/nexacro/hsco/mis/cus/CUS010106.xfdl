this.div_work_btn_excelUpload_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{

	this.ds_cstmrVisitEvlCard.clearData();
	var arg = {
		targetDs : this.ds_excelExport,
		headSet : false,
		callBack : function () {
	//	this.grd_SLARY_TABLE.createFormat();
	
	    if(this.ds_excelExport.rowcount > 1){
	        var Hrow_con = this.ds_excelExport.getRowCount();
		    for(i=1; i< Hrow_con; i++) {
				this.ds_cstmrVisitEvlCard.addRow();
				var visitDe = this.ds_excelExport.getColumn(i, "Column1");
				var visitDe2 = this.dateUtils.format(visitDe,"yyyymmdd");
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "VISIT_DE",      visitDe2);
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "DEPT_CODE_NM",  this.ds_excelExport.getColumn(i, "Column2"));
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "CHARGER_NM",    this.ds_excelExport.getColumn(i, "Column3"));				
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_SCORE",   this.ds_excelExport.getColumn(i, "Column4"));
				switch(this.ds_excelExport.getColumn(i, "Column4"))
				{
					//엑셀에서 점수가 1인 경우 (매우불만)
					case '1':
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_SCORE", '-1');
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "STSFDG_EVL",   '5');
					break;
					//엑셀에서 점수가 2인 경우 (불만)
					case '2':
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_SCORE", '-0.5');
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "STSFDG_EVL",  '4');
					break;
					//엑셀에서 점수가 3인 경우 (보통)
					case '3':
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_SCORE", '0');
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "STSFDG_EVL",  '3');
					break;
					//엑셀에서 점수가 4인 경우 (만족)
					case '4':
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_SCORE", '0.5');
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "STSFDG_EVL",  '2');
					break;
					//엑셀에서 점수가 5인 경우 (매우만족)
					case '5':
 						this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_SCORE", '1');
						this.ds_cstmrVisitEvlCard.setColumn(i-1, "STSFDG_EVL",  '1');
					break;
				
				}
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_OPINION", this.ds_excelExport.getColumn(i, "Column5"));
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "CSTMR_NM",      this.ds_excelExport.getColumn(i, "Column6"));
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "TELNO",         this.ds_excelExport.getColumn(i, "Column7"));
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "ADRES",         this.ds_excelExport.getColumn(i, "Column8"));
				this.ds_cond02.clearData();
				this.ds_cond02.addRow();
				this.ds_cond02.setColumn(0, "DEPT_NM", this.ds_excelExport.getColumn(i, "Column2"))
				this.ds_cond02.setColumn(0, "EMP_NM",  this.ds_excelExport.getColumn(i, "Column3"))
				this.ds_empNoDeptCode.clearData();
				
				//부서코드, 사번 조회 
				var strSvcId        = "selectEmpNoDeptCode";   
				var sController   	= "hsco/mis/cus/CUS010106/selectEmpNoDeptCode.do";
				var sInDatasets   	= "input03=ds_cond02";
				var sOutDatasets  	= "ds_empNoDeptCode=output03";
				var argument = "";
				var callbackfunc = "";
				var showprogress = "";
				var bAsync = false;
				Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, argument, callbackfunc, showprogress, bAsync);  
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "EMPNO",      this.ds_empNoDeptCode.getColumn(0, "EMPNO"));
				this.ds_cstmrVisitEvlCard.setColumn(i-1, "DEPT_CODE",  this.ds_empNoDeptCode.getColumn(0, "DEPT_CODE"));
				this.ds_cstmrVisitEvlCard.setRowType(i-1,"I");
				}
			}
     	}	
	};
	this.gfn_importExcel(arg);
}

/***********************************************************************************
* 설명	: 점수값 변경 시
***********************************************************************************/
this.ds_cstmrVisitEvlCard_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "STSFDG_EVL")
	{
		switch(e.newvalue)
		{	
			case '1' :
				this.ds_cstmrVisitEvlCard.setColumn(this.ds_cstmrVisitEvlCard.rowposition, "CSTMR_SCORE", "5");
			break;
			case '2':
				this.ds_cstmrVisitEvlCard.setColumn(this.ds_cstmrVisitEvlCard.rowposition, "CSTMR_SCORE", "4");
			break;
			case '3':
				this.ds_cstmrVisitEvlCard.setColumn(this.ds_cstmrVisitEvlCard.rowposition, "CSTMR_SCORE", "3");
			break;
			case '4':
				this.ds_cstmrVisitEvlCard.setColumn(this.ds_cstmrVisitEvlCard.rowposition, "CSTMR_SCORE", "2");
			break;
			case '5':
				this.ds_cstmrVisitEvlCard.setColumn(this.ds_cstmrVisitEvlCard.rowposition, "CSTMR_SCORE", "1");
			break;
		}
	}
}

