 ***********************************************************************/
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
  this.ds_cstmrVisitEvlCard
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS010106_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
 	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010106_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_search.cal_visitBgnDe.setFocus(true);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.div_search.cal_visitBgnDe.set_value(this.gfn_today().substring(0,6)+'01');
	this.div_search.cal_visitEndDe.set_value(this.gfn_today());
	var comboParams = [["ds_stsfdgEvl",        "CUS034", 1, ""],
					   ["ds_stsfdgEvlSearch",  "CUS034", 1, "전체"],
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "CUS");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	this.fn_transaction("selectVisitEvlCardList");
}

/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{
	//validation check
	if(!this.gfn_checkValidation(this.ds_cstmrVisitEvlCard, this.ds_validation)){
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")){
		//현재 rowpostion값을 저장
		save_row = this.ds_cstmrVisitEvlCard.rowposition;
		this.fn_transaction("visitEvlCardListCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//고객방문카드 목록 조회
		case "selectVisitEvlCardList":
			this.ds_fileList.clearData();
			var sController   	= "hsco/mis/cus/CUS010106/selectVisitEvlCardList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cstmrVisitEvlCard=output01";
		break;
	
		//고객방문카드 저장/수정/삭제
		case "visitEvlCardListCUD":
			var sController   	= "hsco/mis/cus/CUS010106/visitEvlCardListCUD.do";
			var sInDatasets   	= "input02=ds_cstmrVisitEvlCard:U";
			var sOutDatasets  	= "";
		break;
		
		//고객방문카드 첨부파일 조회 
		case "selectAtchFile":
			this.div_work.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			this.ds_fileList.addRow();
			this.ds_fileList.setColumn(0, "FILE_SN", this.ds_cstmrVisitEvlCard.getColumn(this.ds_cstmrVisitEvlCard.rowposition, "ATCH_FILE"));	// 재조회목적(좋은 방법은 아니지만, 공통을 최대한 안바꾸려면)
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_fileList";
			var sOutDatasets = "ds_fileList=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//상단저장 콜백
			case "visitEvlCardListCUD":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//상단조회콜백
			case "selectVisitEvlCardList":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_cstmrVisitEvlCard.set_rowposition(save_row);
					save_row = -1;
				}
			break;
		}
	}
}
/***********************************************************************
* 상단 신규
************************************************************************/
this.fn_insert = function()
{
	var nRow = this.ds_cstmrVisitEvlCard.addRow();
	this.ds_cstmrVisitEvlCard.setColumn(nRow, "VISIT_DE" , this.gfn_today());        	
}

/***********************************************************************
* 상단삭제
************************************************************************/
this.fn_delete = function()
{
	//변수선언
	var ro_count = this.ds_cstmrVisitEvlCard.rowcount;
	var delete_count = 0 ;
	//데이터셋 에서 체크된 행들을 삭제
	for(var i = ro_count; i >= 0 ; i-- ){
		if(this.ds_cstmrVisitEvlCard.getColumn(i,"CHK") == "1")
		{
			delete_count = delete_count +1;
		}
	}
	//체크한 행이 없을 경우
	if(delete_count < 1)
	{
		this.gfn_message("comm.데이터.선택.없음");
	}else{
		if(this.gfn_message("confirm.삭제여부")){
			for(var i = ro_count; i >= 0 ; i-- ){
				if(this.ds_cstmrVisitEvlCard.getColumn(i,"CHK") == "1")
				{
					this.ds_cstmrVisitEvlCard.deleteRow(i);	
				}
			}
		}
	}
	this.div_work.grd_cstmrVisitEvlCard.setCellProperty("Head", 0 , "text", "0");
}

/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_cstmrVisitEvlCard))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_cstmrVisitEvlCard.reset();		//DATASET ROLLBACK
			this.ds_cstmrVisitEvlCard.applyChange();	//DATASET COMMIT
			return;
		}
	}
}

