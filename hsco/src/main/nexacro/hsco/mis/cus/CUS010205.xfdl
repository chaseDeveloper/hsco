/***********************************************************************
* 발송버튼 클릭시
************************************************************************/
this.div_Cont_btn_Attach_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	this.ds_smsClient1.clearData();	// sms 수신자 목록 초기화
	this.ds_smsClient1.addRow();
	
	var chkCnt = this.ds_cstmrMntrStgList.getCaseCount("CHK == 1");
	
	if(chkCnt<1){
		this.gfn_message("info.항목.선택.알림", "수신자");
		return;
	}
	
	var rowCnt = this.ds_cstmrMntrStgList.rowcount;
	for(var i=0; i<rowCnt; i++){
		var chkYn = this.ds_cstmrMntrStgList.getColumn(i, "CHK");
		if(chkYn == '1'){
			var mbtlNo = this.ds_cstmrMntrStgList.getColumn(i, "CTTPC");
			if(!mbtlNo){
				this.gfn_message("info.처리불가", "고객 연락처 정보가 존재하지 않습니다.");
				return;
			}
			var nRow = this.ds_smsClient1.addRow();
			this.ds_smsClient1.setColumn(nRow, "RECIPIENT_NUM", mbtlNo.replace('-',''));
			this.ds_smsClient1.setColumn(nRow, "CHANGE_WORD5", this.ds_cstmrMntrStgList.getColumn(i, "NM"));
		}
	}
	
	//validation check		
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_chrctrCnInfo, this.ds_validation)){
		return;
	}
	
	var chrctrCn = this.ds_chrctrCnInfo.getColumn(0, "CHRCTR_CN");		// 문자내용
	var telNo    = this.ds_chrctrCnInfo.getColumn(0, "SEND_TEL_NO");	// 발신자번호
	var deptId   = this.gfn_getDeptId();
	
	var nMsgSize = this.div_work.txt_chrctrCn.getLength("ascii");
	if(nMsgSize > 80 ) {
			var smsType = "2";
	}else {
		var smsType = "0";
	}
	
	//문자전송 소스필요
	var sSndDeptcode = deptId;      // 부서코드(필수)
	var sAnswTel = telNo;       	// 발신번호(필수)
    var sServiceType = smsType;          // sms(단문80 byte):0, mms(장문 2000 byte):2 (필수)
    var sMsge = chrctrCn;  /* 전송메시지 동보메시지단어 입력시: 사용예) 전송메시지에 '%CHANGEWORD%님이 %CHANGEWORD%에 문의하신 민원을 접수하였습니다. %CHANGEWORD%님 감사합니다.'	
								        (필수)                           결과:  '홍길동님이 2016년3월20일에 문의하신 민원을 접수하였습니다. 홍길동님 감사합니다.' */
    //수신 Dataset (필수)
    var sSndDt = ""           // 예약전송시간 YYYYMMDDHH24MISS "20160410153050" , 미입력시 즉시전송
    var sChangWord1 = ""      // 동보메시지단어1
    var sChangWord2 = ""      // 동보메시지단어2
    var sChangWord3 = ""      // 동보메시지단어3
    var sChangWord4 = ""      // 동보메시지단어4    

	this.sms_MultiMessage(this,sSndDeptcode, sAnswTel, sServiceType, sMsge, this.ds_smsClient1, sSndDt, sChangWord1, sChangWord2, sChangWord3, sChangWord4) ;	

	//내용저장 체크시 
	if(this.div_work.chk_saveCn.value == true){
		this.fn_transaction("chrctrCnC");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
		//로그인 사용자 정보 조회
		case "selectUserInfo" :			
			var sController   	= "hsco/mis/cus/CUS000000/selectUserInfo.do";
			var sInDatasets   	= "input01=ds_cond01";
			var sOutDatasets  	= "ds_session=output01";
		break;
		
		//고객모니터단관리 목록조회
		case "selectCstmrMntrStgList" :
			this.ds_cond.clearData();
			this.ds_cond.addRow();
			this.ds_cond.setColumn(0, "HRSMN",   this.div_search.edt_hrsmn.value);
			this.ds_cond.setColumn(0, "MNTR_SE", this.div_search.cmb_mntrSe.value);
			this.ds_cond.setColumn(0, "NM",      this.div_search.edt_nm.value);
			
			var sController   	= "hsco/mis/cus/CUS010205/selectCstmrMntrStgList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cstmrMntrStgList=output01";
		break;
		
		//고객모니터단 저장/수정/삭제	
		case "cstmrMntrStgListCUD" :
			var sController   	= "hsco/mis/cus/CUS010205/cstmrMntrStgListCUD.do";
			var sInDatasets   	= "input02=ds_cstmrMntrStgList:U";
			var sOutDatasets  	= "";
		break;
		
		//문자내용 샘플 조회
		case "selectSndngCnList":			
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();
			this.ds_cond2.setColumn(0, "CHRCTR_SE", "02");
			
			var sController   	= "hsco/mis/cus/CUS010104/selectSndngCnList.do";
			var sInDatasets   	= "input01=ds_cond2";
			var sOutDatasets  	= "ds_sndngCnList=output01";
		break;	
		
		//문자내용 저장
		case "chrctrCnC":
			this.ds_chrctrCnInfo.setColumn(this.ds_chrctrCnInfo.rowposition, "CHRCTR_SE", "02");
			var sController   	= "hsco/mis/cus/CUS010104/chrctrCnC.do";
			var sInDatasets   	= "input03=ds_chrctrCnInfo";
			var sOutDatasets  	= "";
		break;
		
		case "selectExpCode":	// 지출결의서 금융기관 코드정보
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0, "USE_AT", "1");
			
			var sController   	= "hsco/mis/acc/ACC020102/fnncInsttCodeList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_fnncInstt=output1";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//로그인 사용자 정보
			case "selectUserInfo":
				var telNo = this.ds_session.getColumn(0, "TELNO_1");
				this.ds_chrctrCnInfo.clearData();
				this.ds_chrctrCnInfo.addRow();
				this.ds_chrctrCnInfo.setColumn(0, "SEND_TEL_NO", telNo);
				this.fn_search();
			break;
			
			//상단조회 콜백
			case "selectCstmrMntrStgList":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_cstmrMntrStgList.set_rowposition(save_row);
					save_row = -1;
				}
				this.fn_transaction("selectSndngCnList");
			break;
			
			//상단저장 콜백
			case "cstmrMntrStgListCUD":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//문자발송 내용 저장 후 샘플 내용 조회 콜백
			case "chrctrCnC":
				this.fn_transaction("selectSndngCnList");  //발송내용문자 목록 조회
			break;
			
			case "selectExpCode":		// 결의서 코드정보
				this.ds_fnncInstt.insertRow(0);
				this.ds_fnncInstt.setColumn(0, "FNNC_INSTT_CODE_NM", "미선택");
			break;
		}
	}
}

