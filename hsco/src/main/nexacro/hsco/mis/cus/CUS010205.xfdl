				{
					this.ds_cstmrMntrStgList.setColumn(i-1, "MNTR_SE",  1); 
				}else if(this.ds_excelExport.getColumn(i, "Column2").substring(0,2) == "대학"){
					this.ds_cstmrMntrStgList.setColumn(i-1, "MNTR_SE",  2); 
				}else{
					this.ds_cstmrMntrStgList.setColumn(i-1, "MNTR_SE",  "");
				}
				this.ds_cstmrMntrStgList.setColumn(i-1, "NM",       this.ds_excelExport.getColumn(i, "Column3"));  //성명
				this.ds_cstmrMntrStgList.setColumn(i-1, "CTTPC",    this.ds_excelExport.getColumn(i, "Column4"));  //연락처
				this.ds_cstmrMntrStgList.setColumn(i-1, "CTTPC2",   this.ds_excelExport.getColumn(i, "Column5"));  //연락처2
				this.ds_cstmrMntrStgList.setColumn(i-1, "EMAIL",    this.ds_excelExport.getColumn(i, "Column6"));  //이메일
				this.ds_cstmrMntrStgList.setColumn(i-1, "HMPG",     this.ds_excelExport.getColumn(i, "Column7"));  //홈페이지
				this.ds_cstmrMntrStgList.setColumn(i-1, "BLOG",     this.ds_excelExport.getColumn(i, "Column8"));  //블로그
				this.ds_cstmrMntrStgList.setColumn(i-1, "BANK_NM",  this.ds_excelExport.getColumn(i, "Column9"));  //은행명
				this.ds_cstmrMntrStgList.setColumn(i-1, "ACNUTNO",  this.ds_excelExport.getColumn(i, "Column10"));  //계좌번호
				this.ds_cstmrMntrStgList.setColumn(i-1, "DPSTR",    this.ds_excelExport.getColumn(i, "Column11")); //예금주
				this.ds_cstmrMntrStgList.setColumn(i-1, "ADRES",    this.ds_excelExport.getColumn(i, "Column12")); //주소
				var brthdy = this.ds_excelExport.getColumn(i, "Column13");
				var brthdy2 = this.dateUtils.format(brthdy,"yyyymmdd");
				this.ds_cstmrMntrStgList.setColumn(i-1, "BRTHDY",   brthdy2); //생년월일
				this.ds_cstmrMntrStgList.setColumn(i-1, "SCHUL",    this.ds_excelExport.getColumn(i, "Column14")); //학교
				this.ds_cstmrMntrStgList.setRowType(i-1,"I");
				}
			}
     	}	
	};
	this.gfn_importExcel(arg);	
}

this.ds_cstmrMntrStgList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	//등록전 수정가능
	if(obj.getRowType(e.newrow) == Dataset.ROWTYPE_INSERT)
	{
		this.div_work.grd_cstmrMntrStgList.setCellProperty("body", 2, "edittype", "normal");
	}
	else{
	//등록이후 키값 수정 불가
		this.div_work.grd_cstmrMntrStgList.setCellProperty("body", 2, "edittype", "none");
	}
}

this.div_work_grd_cstmrMntrStgList_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	var fn_cb = function(sPopupId, sReturn) {
		
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var zipNo         = dsTemp.getColumn(0, "ZIPNO").replace("-","");	// 우편번호
			var roadAddrPart1 = dsTemp.getColumn(0, "ROADADDRPART1");			// 도로명주소(일반)
			var roadAddrPart2 = dsTemp.getColumn(0, "ROADADDRPART2");			// 도로명주소(상세)
			var jibunAddr     = dsTemp.getColumn(0, "JIBUNADDR");				// 지번주소
			
			var mRow = this.ds_cstmrMntrStgList.rowposition;
			this.ds_cstmrMntrStgList.setColumn(mRow, "ZIP",            zipNo        );
			this.ds_cstmrMntrStgList.setColumn(mRow, "ADRES",          roadAddrPart1);
			this.ds_cstmrMntrStgList.setColumn(mRow, "ADRES_DETAIL",   roadAddrPart2);
		}
	}
	
	this.gfn_popup('zipPopup', 500, 500, '', null, "common::frmZipAddressPopup.xfdl", fn_cb);
}

/**************************************************************************
* SMS 발송 팝업 호출
**************************************************************************/
this.div_work_btn_sendSMS_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var sRow = this.ds_cstmrMntrStgList.rowposition;
	
	var MBTLNO = this.ds_cstmrMntrStgList.getColumn( sRow ,"CTTPC");
	var NM = this.ds_cstmrMntrStgList.getColumn( sRow, "NM");
	this.ds_smsList.clearData();
		
	if(!this.comUtils.isNullEmpty(MBTLNO)){
		var nRow = this.ds_smsList.addRow();
		this.ds_smsList.setColumn(nRow , "MBTLNO", MBTLNO);
		this.ds_smsList.setColumn(nRow , "NM", NM);
	}	
	
	var CONTENT = "테스트 문자 메세지 발송";
	var param = {
		CONTENT : CONTENT,
		JOB_SE : "CUS", 
		ds : this.ds_smsList
	};
		
	this.gfn_popup("COM010700_P01.xfdl"
			   , 800
			   , 500
			   , "SMS발송"
			   , param
			   , "com::COM010700_P01.xfdl"
			   , "sms_callback");
}

