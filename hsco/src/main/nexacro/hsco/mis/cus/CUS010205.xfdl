 * 설명 		: 팝업 콜백 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                sReturn  : 팝업에서 넘어오는 데이터
 * return Type 	: None
 * 사용법		: this.fn_PopBsnsCallback();
 */
this.fn_PopCallback = function(sPopupId, sReturn)
{
    if(sPopupId == "CUS000000_P01")
    {	
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(nRowCnt > 0){
			for(i = 0; i<nRowCnt; i++){
				var nRow = this.ds_cstmrMntrStgList.addRow();
				this.ds_cstmrMntrStgList.setColumn(nRow, "MEMBER_ID", dsRtn.getColumn(i, "MEMBER_ID"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "NM", dsRtn.getColumn(i, "MEMBER_NAME"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "CTTPC", dsRtn.getColumn(i, "MEMBER_HP"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "CTTPC2", dsRtn.getColumn(i, "MEMBER_TEL"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "ZIP", dsRtn.getColumn(i, "ZIP_CODE"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "EMAIL", dsRtn.getColumn(i, "MEMBER_EMAIL"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "ADRES", dsRtn.getColumn(i, "MEMBER_ADDR1"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "ADRES_DETAIL", dsRtn.getColumn(i, "MEMBER_ADDR2"));
				this.ds_cstmrMntrStgList.setColumn(nRow, "BRTHDY", dsRtn.getColumn(i, "BIRTH"));
			}	
		}
    }
}
