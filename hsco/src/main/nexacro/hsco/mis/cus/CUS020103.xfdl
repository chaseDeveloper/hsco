/******************************************************************
 * 온라인 민원 : 내부결재 처리
******************************************************************/
this.fn_inner_sanction = function(){
	if(this.gfn_message("comm.행위여부","내부결재 상신을 진행")){
		var gridObj = this.ds_cvplPrcRsltmang;		//그리드
		var gridSelectedRow = gridObj.rowposition;	//그리드 선택로우
		
	 	var inner_sanctn_sttus = gridObj.getColumn( gridSelectedRow , "INNER_SANCTN_STTUS");
	 	var registDt = gridObj.getColumn( gridSelectedRow , "REGIST_DT");

		
		//기요청건 검증
		if( !(this.comUtils.isNullEmpty(inner_sanctn_sttus)
			|| "S08"==inner_sanctn_sttus
			|| "S09"==inner_sanctn_sttus) ){	
			this.gfn_message("info.내부결재.등록된ID","");
			return;
		}

		var sanctnId = gridObj.getColumn(gridSelectedRow, "INNER_SANCTN_ID");  //내부결재ID	
		var strSvcId= "innerSanctnUbiDataset";
		this.ds_cond04.clearData();
		this.ds_cond04.addRow();
		this.ds_cond04.setColumn(0, "RCEPT_NO", this.ds_cvplPrcRsltmang.getColumn(gridSelectedRow, "RCEPT_NO"));
		this.ds_cond04.setColumn(0, "CVPL_NO",  this.ds_cvplPrcRsltmang.getColumn(gridSelectedRow, "CVPL_NO"));
		
		var sController   	= "hsco/mis/cus/CUS020103/innerSanctnUbiDataset.do";
		var sInDatasets   	= "input1=ds_cond04";
		var sOutDatasets  	= "ds_ubiDataset=output1";
		
		var reportTitle = "민원처리결과관리내부결재";
		var reportUri = "/mis/cus/CUS020103_R01.jrf";
		var saveFileName = "민원처리결과관리내부결재";

		this.gfn_makeReportTranInfo(sanctnId, strSvcId, sController, sInDatasets, sOutDatasets, reportTitle, reportUri, saveFileName);
		
		
		
		//내부결재요청
		//업무키 조합한다.
		var key = gridObj.getColumn(gridSelectedRow, "CVPL_NO") + "^" 
				+ gridObj.getColumn(gridSelectedRow, "RCEPT_NO") + "^"
				+ gridObj.getColumn(gridSelectedRow, "INNER_SANCTN_KEY"); 

		this.ds_innerSanctnParam.clearData();
		this.ds_innerSanctnParam.addRow();
		this.ds_innerSanctnParam.setColumn(0, "INNER_SANCTN_MDL_ID", "CUS-100029");	/*내부결재모형ID*/
		this.ds_innerSanctnParam.setColumn(0, "SYS_SE_CODE", "MIS");			/*시스템구분 COM024*/
		this.ds_innerSanctnParam.setColumn(0, "JOB_SE", "CUS");				/*업무구분 COM025*/
		this.ds_innerSanctnParam.setColumn(0, "JOB_KEY", key);				/*시스템 업무별 UNIQUE KEY*/
		
		var fn_cb = function(sPopupId, sReturn){
			var sanctnResult = sReturn.split("^"); /*index 0: 응답코드, index 1 : 내부결재ID*/
			if("0000"==sanctnResult[0]){
				if(this.comUtils.isNotNull(sanctnResult[1])){
					//sanctnResult[1] : INNER_SANCTN_ID 내부결재ID
					//내부결재ID가 필요한경우 처리
					var cRow = this.ds_sanctnResult.addRow();
					this.ds_sanctnResult.setColumn(cRow, "CVPL_NO", gridObj.getColumn(gridSelectedRow, "CVPL_NO"));
					this.ds_sanctnResult.setColumn(cRow, "RCEPT_NO", gridObj.getColumn(gridSelectedRow, "RCEPT_NO"));
					this.ds_sanctnResult.setColumn(cRow, "INNER_SANCTN_ID", sanctnResult[1]);
					this.fn_transaction("innerSanctU");
				}			
			}
		}
		var result = this.gfn_openInnerSanctionPopup(this.ds_innerSanctnParam, fn_cb, registDt);
	}
}

/******************************************************************
 * 오프라인 민원 : 전자결재 처리
******************************************************************/
this.fn_elec_sanction = function(){
	// 전자결재 상태 체크 2016.7
	// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
	// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
	// 기안전인것 : 재상신 및 수정/삭제
	// 결재상태가 상신전 외는 신규로 다시 진행.
	var nRow = this.ds_cvplPrcRsltmang.rowposition;
	var sanctnKnd = this.ds_cvplPrcRsltmang.getColumn(nRow, "RESULT_SANCTN_STS");

	switch (sanctnKnd) {
		case "1": case "2": case "4": case "8": case "1024": case "DUP":
		this.gfn_message("comm.결재.진행");
		return false;
		
		case "32": case "256": case "512": case "4096": 
		this.gfn_message("comm.결재.완료");
		return false;
		
		default:
	}
	
	if(Ex.util.isUpdated(this.ds_cvplPrcRsltmang)){
		this.gfn_message("comm.정보.저장.요청", "수정된");
		return false;
	}
	
	var rceptNo = this.ds_cvplPrcRsltmang.getColumn(nRow, "RCEPT_NO");
	var cvplNo  = this.ds_cvplPrcRsltmang.getColumn(nRow, "CVPL_NO");
	var processSj = this.ds_cvplPrcRsltmang.getColumn(nRow, "SJ");				//제목
	var processCn = this.ds_cvplPrcRsltmang.getColumn(nRow, "PROCESS_CN");		//처리내용
	var sanctnNo  = this.ds_cvplPrcRsltmang.getColumn(nRow, "SANCTN_NO");
	
	var filds = new Array(2);
	filds[0] = processCn;
	
	var dataValue = filds.join("|");
	
	var dataTitle = processSj;
	var title     = processSj;
	
	this.ds_approve.clearData();	
	this.ds_approve.addRow();

	this.ds_approve.setColumn(0, "TITLE",           title           );
	this.ds_approve.setColumn(0, "DATA_TITLE",      dataTitle       );
	this.ds_approve.setColumn(0, "DATA_VALUE",      dataValue       );
	this.ds_approve.setColumn(0, "RCEPT_NO",        rceptNo		);
	this.ds_approve.setColumn(0, "CVPL_NO", 	    cvplNo			);
 	this.ds_approve.setColumn(0, "DEPT_CD",    this.gfn_getDeptId() );
 	this.ds_approve.setColumn(0, "EMP_CD",     this.fn_getEmpno()   );
 	this.ds_approve.setColumn(0, "SANCTN_NO",     sanctnNo   );
	
	if(this.gfn_message("comm.행위여부", "전자결재 상신을 진행"))	{
		this.fn_transaction("approve");
	}
}


