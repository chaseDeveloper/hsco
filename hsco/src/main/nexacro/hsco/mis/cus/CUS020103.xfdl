 ***********************************************************************
 *    
 ***********************************************************************/
 

/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";
include "lib::comInnerSanct.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 this.checkDs = [
  this.ds_cvplPrcRsltmang
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
var gvPid = application.gv_pid;	//get param
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS020103_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS020103_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	//this.div_search.cmb_inqryknd.setFocus(true);
}

/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_fileList.clearData();
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	if(this.gfn_authcode("101") > -1 || this.gfn_authcode("034") > -1){	//고객VOC담당 || 감사담당자
		this.ds_cond.setColumn(0, "AUTH_CODE", "101");
		this.div_search.div_dept.set_enable(true);
	}else{
		this.ds_cond.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId());
		this.ds_cond.setColumn(nRow, "DEPT_NM",   this.gfn_getDeptName());
	}
	this.ds_cond.setColumn(nRow, "EMPNO", application.gds_userInfo.getColumn(0, "EMPNO"));
	this.ds_cond.setColumn(nRow, "APPN_CHARGER", this.gfn_getUserId());
	
	this.div_search.div_dept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	//감사담당자 접근시 버튼권한 제어
	if(this.gfn_authcode("034") > -1){
		this.parent.btn_save.set_enable(false);
		this.parent.btn_cancel.set_enable(false);
		this.div_work.CUS020103.T01.btn_rcept.set_enable(false);
		this.div_work.CUS020103.T03.btn_drft.set_enable(false);
		this.div_work.CUS020103.T03.btn_sancComp.set_enable(false);
		this.div_work.CUS020103.T03.btn_delFiles.set_enable(false);
		this.div_work.CUS020103.T03.btn_uploadFiles.set_enable(false);
	}else{
		this.parent.btn_save.set_enable(true);
		this.parent.btn_cancel.set_enable(true);
		this.div_work.CUS020103.T01.btn_rcept.set_enable(true);
		this.div_work.CUS020103.T03.btn_drft.set_enable(true);
		this.div_work.CUS020103.T03.btn_sancComp.set_enable(true);
		this.div_work.CUS020103.T03.btn_delFiles.set_enable(true);
		this.div_work.CUS020103.T03.btn_uploadFiles.set_enable(true);
	}
	
	//검색일 지정
	//포털 진입시 기본 조회조건
	if(this.comUtils.isNullEmpty(gvPid)){
		this.div_search.cal_rceptBgnDt.set_value(this.gfn_today().substring(0,4) + '0101');
	}else{
		this.ds_cond.setColumn(0, "RCEPT_BGN_DT", '19990101');
	}
	
	this.div_search.cal_rceptEndDt.set_value(this.gfn_today());
	
	this.comboLoad(); 	//공통콤보 로드
}

