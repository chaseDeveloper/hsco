 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
					
			//저장 콜백
			case "cvplPrcResltmangListCUD":
				this.fn_search();
			break;
			//개인반려 콜백
			case "returnCVPLU":
				this.fn_search();	
			break;
			
			//조회콜백
			case "selectCvplPrcResltmangList":
			//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					var temp = save_row;
					save_row = -1;
					this.ds_cvplPrcRsltmang.set_rowposition(temp);					
				}
			break;
			
			// 내부결재id 조회
			case "GetInnerSanctnId":
			    this.fn_popup_InnerSanctionDrft();
			break;
			
			//최초 내부결재 진행여부체크
			case "selectInnerSanctnMasterCheck":
				if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
				{
					this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "DRFT_JOB_CONFM_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
					this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "DRFT_DE",           this.gfn_today());
					
// 					if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "LAST_SANCTN_STTUS") == 'T12'){
// 						this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "CVPL_STTUS",        '5');
// 					}else {
// 						this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "CVPL_STTUS",        '4');
// 					}
					
					var lastSttus = this.ds_innerSanctnChek.getColumn(0, "LAST_SANCTN_STTUS");
					var imprtySttus = ["S03","T03","T04","T06","T08","T09","T10","T12","T13","T14","T16"]; // 수정불가 결재상태
					
					if(imprtySttus.indexOf(lastSttus) > -1) {
					
						if(lastSttus == 'T12'){
							this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "CVPL_STTUS", '5');	// 처리완료
						}else if(imprtySttus.indexOf(lastSttus) > -1){
							this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "CVPL_STTUS", '4');	// 결재중
						}else {
							this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "CVPL_STTUS", '3');	// 처리중
						}
					}
					this.fn_transaction("cvplPrcResltmangListCUD");
				}else{
					
				}
			break;
			
			case "sendEmail":
				this.fn_transaction("cvplSttusU");
				this.gfn_message("success.처리.성공");
			break;
			
			//전자결재 콜백
			case "approve":
// 				var sanctnNo = this.ds_approve.getColumn(0,"DRFT_JOB_CONFM_NO");
// 				//alert("sanctnNo = " + sanctnNo);
// 				this.ds_cvplPrcRsltmang.set_updatecontrol(false);
// 				this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "SANCTN_NO", sanctnNo);
// 				this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "DRFT_DE", this.gfn_today().substring(0,8));
// 				this.ds_cvplPrcRsltmang.setColumn(this.ds_cvplPrcRsltmang.rowposition, "CVPL_STTUS", 4);
// 				this.ds_cvplPrcRsltmang.set_updatecontrol(true);
				
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				//alert("O_URL = " + oUrl);

				system.execBrowser(oUrl);
				
			break;
			case "viewSanction":
				if(this.ds_esUrl.getRowCount() < 1){
					this.gfn_message("info.전자결재.완료.조회실패"); // 결재완료 문서를 조회하지 못하였습니다.\n관리자에게 문의하십시오.
					return false;
				}
				//trace("URL ---------- " + this.ds_esUrl.getColumn(0, "ES_URL"));
				var oUrl = this.ds_esUrl.getColumn(0, "ES_URL");
				system.execBrowser(oUrl);
			break;
			case "innerSanctU":
				this.fn_search();
			break;
			//처리상태 업데이트
			case "cvplSttusU":
				this.fn_search();
			break;
			
			//접수처리 
			case "rceptProcU":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//전자결재 처리완료
			case "sancComp":
				this.fn_search();
				this.gfn_message("success.처리.성공");
			break;
		}
	}
}

