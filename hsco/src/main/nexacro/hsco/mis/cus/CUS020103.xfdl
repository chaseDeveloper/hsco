/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{	
	var rowPos = this.ds_cvplPrcRsltmang.rowposition;
	
	//반려정보가 null이 아닌 경우
	if(!this.comUtils.isNull(this.ds_cvplPrcRsltmang.getColumn(rowPos, "RETURN_SE")))
	{
		this.ds_cvplPrcRsltmang.setColumn(rowPos, "CVPL_STTUS", 8);
		if(this.gfn_message("confirm.반려.여부","민원을"))
		{
			this.fn_transaction("returnCVPLU");
		}else{
			return false;
		}
				
	//반려정보가 null인경우
	}else{
		//접수처리 우선
		if(this.comUtils.isNull(this.ds_cvplPrcRsltmang.getColumn(rowPos, "RCEPT_DT"))){
			this.gfn_message("info.처리불가", "접수처리를 먼저 해주시기 바랍니다.");
			return false;
		}
		
		//자주묻는질문등록 체크 없을 시 저장 트랜잭션
		if(Ex.util.isUpdated(this.ds_cvplPrcRsltmang)) {
			if(this.gfn_message("confirm.저장.여부")){
			
				//처리일자가 없을 경우에만 오늘날짜로셋팅
				if(this.comUtils.isNull(this.ds_cvplPrcRsltmang.getColumn(rowPos, "PROCESS_DT"))){
					this.ds_cvplPrcRsltmang.setColumn(rowPos, "PROCESS_DT", this.gfn_today());
				}
				if(this.div_work.CUS020103.tabindex==2){
					if(!this.gfn_checkValidation(this.ds_cvplPrcRsltmang, this.ds_validation)) {
						return;
					}
				}
				//현재 rowpostion값을 저장
				save_row = this.ds_cvplPrcRsltmang.rowposition;
				this.fn_transaction("cvplPrcResltmangListCUD");
			}
		} else {
			this.gfn_message("comm.내역.변경.없음");
		}
	}	
}

/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_hmpgCstmrCvpl))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_hmpgCstmrCvpl.reset();		//DATASET ROLLBACK
			this.ds_hmpgCstmrCvpl.applyChange();	//DATASET COMMIT
			return;
		}
	}
}


