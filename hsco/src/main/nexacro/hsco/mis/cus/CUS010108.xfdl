	var rowType   = this.ds_cstmrMntgStgExpndtr.getRowType(nRow);
	
	// 기안상태 COM022 (0:기안전 1:기안 9:기안오류)
	// 결재상태 COM023 (1: 진행 2:보류 4:대기 8:협조대기 32:삭제 256:완료 512:반송 4096:취소 1024:참조 NUL:상신전 DUP:결재중복)
	// 기안전인것 : 재상신 및 수정/삭제
	// 결재상태가 상신전 외는 신규로 다시 진행.
	if(!this.comUtils.isNull(sanctnKnd))
	{
		//결재가 시작된 경우 전자결재진행현황버튼 활성
		if(sanctnKnd == "1"
		|| sanctnKnd == "2"
		|| sanctnKnd == "4"
		|| sanctnKnd == "8"
		|| sanctnKnd == "1024"
		|| sanctnKnd == "DUP"
		|| sanctnKnd == "NUL"){
			this.parent.btn_sanction.set_enable(true);
			this.div_work.btn_expndtrDecsn.set_enable(false);
		}else if(sanctnKnd == "32"
			  || sanctnKnd == "256"
			  || sanctnKnd == "512"
			  || sanctnKnd == "4069"){
			this.parent.btn_sanction.set_enable(false);
			if(confmAt=='1'){	// 자금 승인이 된 이후는 지출결의 수정 불가
				this.div_work.btn_expndtrDecsn.set_enable(false);
			}else{
				this.div_work.btn_expndtrDecsn.set_enable(true);
			}
		}
		
		
// 		//결재상태가 기안전, 반송, 취소인 경우 기안 수정가능
// 		if(sanctnKnd == '0' || sanctnKnd == '2' || sanctnKnd == '3')
// 		{
// 			this.div_work.cal_expndtrPlanDe.set_readonly(false);
// 			this.div_work.cal_expndtrPlanDe.set_cssclass("cal_WF_Essential");
// 			
// 			this.div_work.edt_sj.set_readonly(false);
// 			this.div_work.edt_sj.set_cssclass("edt_WF_Essential");
// 			
// 			this.div_work.txt_cn.set_readonly(false);
// 			this.div_work.txt_cn.set_cssclass("txt_WF_Essential");
// 			
// 			this.div_work.btn_delFiles.set_enable(true);			
// 			this.div_work.btn_uploadFiles.set_enable(true);
// 			
// 			this.div_work.btn_drft.set_enable(true);
// 		}else{
// 			this.div_work.cal_expndtrPlanDe.set_readonly(true);
// 			this.div_work.cal_expndtrPlanDe.set_cssclass("cal_WF_Readonly");
// 			
// 			this.div_work.edt_sj.set_readonly(true);
// 			this.div_work.edt_sj.set_cssclass("edt_WF_Readonly");
// 			
// 			this.div_work.txt_cn.set_readonly(true);
// 			this.div_work.txt_cn.set_cssclass("txt_WF_Readonly");
// 			
// 			this.div_work.btn_delFiles.set_enable(false);
// 			this.div_work.btn_uploadFiles.set_enable(false);
// 			
// 			this.div_work.btn_drft.set_enable(false);
// 		}
		
	}else{
		this.parent.btn_sanction.set_enable(true);
	}
	
	this.ds_fileList.clearData();
	
	if(rowType != Dataset.ROWTYPE_INSERT){		// 신규 행이 아닐 경우에만 상세 지출활동 내역 조회
		this.ds_cond3.clearData();
		this.ds_cond3.addRow();
		this.ds_cond3.setColumn(0, "PYMNT_SN", this.ds_cstmrMntgStgExpndtr.getColumn(nRow, "PYMNT_SN"));
		
		this.ds_cstmrVisitEvlCard.clearData();
		
		this.fn_transaction("selectMntrStgActList2");
	}else {
		if(this.insertYn == 'N'){
			this.ds_cstmrVisitEvlCard.clearData();
			this.ds_cstmrVisitEvlCard.copyData(this.ds_temp);
		}else {
			this.insertYn = 'N';
		}
	}	
}

