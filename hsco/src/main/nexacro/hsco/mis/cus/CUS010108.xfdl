    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID     : CUS010108.xfdl
 * 화면명    : 고객평가지출관리
 * 화면설명  : 고객방문카드 평가 지출 목록을 관리한다.
 * 작성일    : 2015.10.22
 * 작성자    :  이지영
 * 수정이력  : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************/
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comInnerSanct.xjs";
include "lib::comUpDownUtils.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 this.checkDs = [
  this.ds_cstmrMntgStgExpndtr
];
//저장시 rowposition값 저장할 변수
var save_row = -1;
this.File_UpDownManager = null;
this.insertYn = 'N';
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS010108_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}
/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010108_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	this.div_search.cal_expndtrPlanBgnDe.setFocus(true);	
	this.fn_search();
}
/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.div_search.cal_expndtrPlanBgnDe.set_value(this.gfn_today().substring(0,6)+'01');
	this.div_search.cal_expndtrPlanEndDe.set_value(this.gfn_today());	
	var comboParams = [ ["ds_confmAtSearch",  "CUS016", 1, "전체"],
						["ds_confmAt",        "CUS016", 1, ""],
						["ds_sacnctnSttusSe", "COM022", 1, ""],
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	var fileConfig = {
      useMultiUpload : true,
      onChange: this.fn_onFileSelected,   // 파일 선택 이벤트 핸들러
      onSuccess : this.fn_onFileSuccess   // 전송 성공 이벤트 핸들러
   };
   this.File_UpDownManager = new this.fileUpDownManager(this, "MIS", "CUS");
   this.File_UpDownManager.init(fileConfig);
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{	
	this.fn_transaction("selectCstmrMntgStgExpndtrList");
}

/***********************************************************************
* 상단 저장처리
************************************************************************/
this.fn_save = function()
{
	//validation check
	if(!this.gfn_checkValidation(this.ds_cstmrMntgStgExpndtr, this.ds_validation)){
		return;
	}
	if(this.gfn_message("confirm.저장.여부")){
		
		//현재 rowpostion값을 저장
		save_row = this.ds_cstmrMntgStgExpndtr.rowposition;
		this.fn_transaction("cstmrMntgStgExpndtrListCUD");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
