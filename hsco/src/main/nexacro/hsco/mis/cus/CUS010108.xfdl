this.ds_cstmrMntgStgExpndtr_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var nRow = this.ds_cstmrMntgStgExpndtr.rowposition;
	
	var sanctnKnd = this.ds_cstmrMntgStgExpndtr.getColumn(nRow, "SANCTN_STS");
