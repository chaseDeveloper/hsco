 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//상단저장 콜백
			case "cstmrMntgStgExpndtrListCUD":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//고객평가지출목록조회
			case "selectCstmrMntgStgExpndtrList":
				//저장트랜잭션 직전 rowposition값으로 이동
				if(save_row != -1)
				{
					this.ds_cstmrMntgStgExpndtr.set_rowposition(save_row);
					save_row = -1;
				}
			break;
			
			case "selectAtchFile":
				this.fn_transaction("selectAtchFile2");
			break;
			
			// 내부결재id 조회
			case "GetInnerSanctnId":
			    this.fn_popup_InnerSanctionDrft();
			break;
			
			//최초 내부결재 진행여부체크
			case "selectInnerSanctnMasterCheck":
				if(this.ds_innerSanctnChek.getColumn(this.ds_innerSanctnChek.rowposition, "CHK") == 1)
				{
					this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "DRFT_JOB_CONFM_NO", this.ds_recomInfo.getColumn(0, "INNER_SANCTN_ID"));
					this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "DRFT_DE",           this.gfn_today());
					this.fn_transaction("cstmrMntgStgExpndtrListCUD");
				}else{
					
				}
			break;
			
			//활동목록 조회 콜백
			case "selectMntrStgActList":
					
					if(this.ds_cstmrVisitEvlCard.rowcount > 0){
						
						this.insertYn = 'Y';
						
						this.ds_temp.copyData(this.ds_cstmrMntrStgActList);			// 임시 데이터셋어 활동목록내역 임시저장(행 변경 이벤트에서 사용)
						
						var nRow = this.ds_cstmrMntgStgExpndtr.addRow();
						
						this.ds_cstmrMntgStgExpndtr.setColumn(nRow, "EXPNDTR_PLAN_DE" , this.gfn_today());
						this.ds_cstmrMntgStgExpndtr.setColumn(nRow, "SANCTN_KND" ,      '0');
						
						var sumPymntAmSm = this.ds_cstmrVisitEvlCard.rowcount * 10000;	// 지출금액 : 지출대상 1건당 10,000원
						
						this.ds_cstmrMntgStgExpndtr.setColumn(nRow, "PYMNT_AM_SM" , sumPymntAmSm);
						
					}else{
						
						if(this.ds_cstmrMntgStgExpndtr.rowposition > -1){
							this.fn_transaction("selectMntrStgActList2");
						}
						this.gfn_message("fail.정보검색.실패","지출대상 목록을");
					}
			break;
			
			// 전자결재
			case "approve":
// 				var sanctnNO = this.ds_approve.getColumn(0,"SANCTN_NO");
// 				
// 				this.ds_cstmrMntgStgExpndtr.set_updatecontrol(false);
// 				this.ds_cstmrMntgStgExpndtr.setColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "DRFT_JOB_CONFM_NO", sanctnNO);
// 				this.ds_cstmrMntgStgExpndtr.set_updatecontrol(true);
				
				var oUrl = this.ds_approve.getColumn(0,"O_URL");
				system.execBrowser(oUrl);
			break;
			
			case "selectMntrStgActList2" :
				this.fn_transaction("selectAtchFile");
			break;
			
			case "innerSanctU" :
				this.fn_search();
			break;
		}
	}
}

/***********************************************************************
* 상단 신규
************************************************************************/
this.fn_insert = function()
{		
	var nCnt = this.ds_cstmrMntgStgExpndtr.getCaseCount("!PYMNT_SN");
	if(nCnt>0){
		this.gfn_message("info.저장.초과", "신규 지출정보는 1건씩")
		return;
	};
	
	this.fn_transaction("selectMntrStgActList");
}

/***********************************************************************
* 상단삭제
************************************************************************/
this.fn_delete = function()
{	
	var sanctnKnd = this.ds_cstmrMntgStgExpndtr.getColumn(this.ds_cstmrMntgStgExpndtr.rowposition, "SANCTN_STS");

	if(!this.comUtils.isNull(sanctnKnd))
	{
		//결재상태가 기안전, 반송, 취소가 아닐경우
		if(sanctnKnd != '0'){
			if(sanctnKnd == '1' || sanctnKnd == '2' || sanctnKnd =='4' || sanctnKnd =='8' || sanctnKnd =='1024' || sanctnKnd =='DUP'){		// 기안, 중간결재
				this.gfn_message("fail.삭제.불가", "이미 결재가 진행중인 내역은");
				return false;
			} else {										// 결재완료
				this.gfn_message("fail.삭제.불가", "이미 결재가 완료된 내역은");
				return false;
			}
		}
	}

	if(this.gfn_message("confirm.삭제여부")){
		var nowRow = this.ds_cstmrMntgStgExpndtr.rowposition;
		var rowCnt = this.ds_cstmrMntgStgExpndtr.rowcount;
		
		this.ds_cstmrMntgStgExpndtr.deleteRow(nowRow);
		
		if((rowCnt > 1) && (nowRow != rowCnt-1)){
			this.ds_cstmrMntgStgExpndtr_onrowposchanged();
		}
	}
}

/***********************************************************************
* 상단취소
************************************************************************/
this.fn_cancel = function()
{	
	if(!this.comUtils.isDatasetUpdated(this.ds_cstmrMntgStgExpndtr))
	{
		this.gfn_message("comm.내역.변경.없음");
	}else{
		if(this.gfn_message("confirm.취소여부")){
			this.ds_cstmrMntgStgExpndtr.reset();		//DATASET ROLLBACK
			this.ds_cstmrMntgStgExpndtr.applyChange();	//DATASET COMMIT
			this.ds_cstmrMntgStgExpndtr_onrowposchanged();
			return;
		}
	}
}

