/***********************************************************************
* 발송버튼 클릭시
************************************************************************/
this.div_Cont_btn_Attach_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	this.ds_smsClient1.clearData();	// sms 수신자 목록 초기화
	
	var chkCnt = this.ds_cstmrlList.getCaseCount("CHK == 1");
	
	if(chkCnt<1){
		this.gfn_message("info.항목.선택.알림", "수신자");
		return;
	}
	
	var rowCnt = this.ds_cstmrlList.rowcount;
	for(var i=0; i<rowCnt; i++){
		var chkYn = this.ds_cstmrlList.getColumn(i, "CHK");
		if(chkYn == '1'){
			var mbtlNo = this.ds_cstmrlList.getColumn(i, "MBTLNO");
			if(!mbtlNo){
				this.gfn_message("info.처리불가", "고객 연락처 정보가 존재하지 않습니다.");
				return;
			}
			
			var nRow = this.ds_smsClient1.addRow();
			this.ds_smsClient1.setColumn(nRow, "RECIPIENT_NUM", mbtlNo.replace('-',''));
			this.ds_smsClient1.setColumn(nRow, "CHANGE_WORD5", this.ds_cstmrlList.getColumn(i, "CSTMR_NM"));
		}
	}
	
	//validation check		
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_chrctrCnInfo, this.ds_validation)){
		return;
	}
		
	var chrctrCn = this.ds_chrctrCnInfo.getColumn(0, "CHRCTR_CN");		// 문자내용
	var telNo    = this.ds_chrctrCnInfo.getColumn(0, "SEND_TEL_NO");	// 발신자번호
	var deptId   = this.gfn_getDeptId();

	var nMsgSize = this.div_work.txt_chrctrCn.getLength("ascii");
	if(nMsgSize > 80 ) {
			var smsType = "2";
	}else {
		var smsType = "0";
	}
		
	//문자전송 소스필요
	var sSndDeptcode = deptId;      // 부서코드(필수)
	var sAnswTel = telNo;       	// 발신번호(필수)
    var sServiceType = smsType;     // sms(단문80 byte):0, mms(장문 2000 byte):2 (필수)
    var sMsge = chrctrCn;  /* 전송메시지 동보메시지단어 입력시: 사용예) 전송메시지에 '%CHANGEWORD%님이 %CHANGEWORD%에 문의하신 민원을 접수하였습니다. %CHANGEWORD%님 감사합니다.'	
								        (필수)                           결과:  '홍길동님이 2016년3월20일에 문의하신 민원을 접수하였습니다. 홍길동님 감사합니다.' */
    //수신 Dataset (필수)
    var sSndDt = ""           // 예약전송시간 YYYYMMDDHH24MISS "20160410153050" , 미입력시 즉시전송
    var sChangWord1 = ""      // 동보메시지단어1
    var sChangWord2 = ""      // 동보메시지단어2
    var sChangWord3 = ""      // 동보메시지단어3
    var sChangWord4 = ""      // 동보메시지단어4    
    
	this.sms_MultiMessage(this, sSndDeptcode, sAnswTel, sServiceType, sMsge, this.ds_smsClient1, sSndDt, sChangWord1, sChangWord2, sChangWord3, sChangWord4) ;
	
	//문자전송 소스필요
	if(this.div_work.chk_saveCn.value == true){
		this.fn_transaction("chrctrCnC");
	}
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		
		//로그인 사용자 정보 조회
		case "selectUserInfo" :			
			var sController   	= "hsco/mis/cus/CUS000000/selectUserInfo.do";
			var sInDatasets   	= "input01=ds_cond01";
			var sOutDatasets  	= "ds_session=output01";
		break;
		
		//고객목록 리스트 조회
		case "selectCstmrList" :
			var sController   	= "hsco/mis/cus/CUS010302/selectCstmrList.do";
			var sInDatasets   	= "input02=ds_cond";
			var sOutDatasets  	= "ds_cstmrlList=output02";
		break;	
		
		//문자내용 샘플 조회
		case "selectSndngCnList":			
			this.ds_cond2.clearData();
			this.ds_cond2.addRow();
			this.ds_cond2.setColumn(0, "CHRCTR_SE", "05");
			var sController   	= "hsco/mis/cus/CUS010104/selectSndngCnList.do";
			var sInDatasets   	= "input01=ds_cond2";
			var sOutDatasets  	= "ds_sndngCnList=output01";
		break;	
		
		//문자내용 저장
		case "chrctrCnC":
			this.ds_chrctrCnInfo.setColumn(this.ds_chrctrCnInfo.rowposition, "CHRCTR_SE", "05");		
			var sController   	= "hsco/mis/cus/CUS010104/chrctrCnC.do";
			var sInDatasets   	= "input03=ds_chrctrCnInfo";
			var sOutDatasets  	= "";
		break;
		
		//사업코드 조회 2016.07.13
		case "selectBsnsCodeList" :
			var sController   	= "hsco/mis/cus/CUS010302/selectBsnsCodeList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_bsnsAreaCode=output01";
		break;
		
		//주택코드 조회 2016.07.13
		case "selectHouseCodeList" :
			var sController   	= "hsco/mis/cus/CUS010302/selectHouseCodeList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_houseCodeSh=output01";
		break;
		
		//매입주택코드 조회 2016.07.13
		case "selectPuchasHouseCodeList" :
			var sController   	= "hsco/mis/cus/CUS010302/selectPuchasHouseCodeList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_puchasHouseCodeSh=output01";
		break;
		
		//토지코드 조회 2016.07.13
		case "selectLandCodeList" :
			var sController   	= "hsco/mis/cus/CUS010302/selectLandCodeList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_landCodeSh=output01";
		break;
		//고객 목록 구분(코드용 조회)
		case "selectGetSe":
			var sController   	= "hsco/mis/cus/CUS010301/selectGetSe.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_cmbSe=output01";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} 
	else {		
		
		switch (strSvcId) {
			//상단조회 콜백
			//로그인 사용자 정보
			case "selectUserInfo":
				var telNo = this.ds_session.getColumn(0, "TELNO_1");
				this.ds_chrctrCnInfo.clearData();
				this.ds_chrctrCnInfo.addRow();
				this.ds_chrctrCnInfo.setColumn(0, "SEND_TEL_NO", telNo);
				
				//this.fn_search();
				
			break;
			
			case "selectCstmrList":
				//alert("this.div_search.cmb_cstmrSe00.index = " + this.div_search.cmb_cstmrSe00.value);
				switch(this.div_search.cmb_cstmrSe00.value)
				{
					//온라인고객
					case "09" :
