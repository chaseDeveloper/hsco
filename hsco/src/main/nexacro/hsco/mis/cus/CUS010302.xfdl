* SMS 발송 팝업 호출
**************************************************************************/
this.div_work_btn_sendSMS_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
// 	var sRow = this.ds_cstmrlList.rowposition;
// 	
// 	var MBTLNO = this.ds_cstmrlList.getColumn( sRow ,"MBTLNO");
// 	var NM = this.ds_cstmrlList.getColumn( sRow, "CSTMR_NM");
 	this.ds_smsList.clearData();
	
	var findRow = this.ds_cstmrlList.findRow("CHK", "1");
	if(findRow < 0) {
		this.gfn_message("comm.데이터.선택.없음");
		return;
	}
	
	for(var fRow = 0; fRow < this.ds_cstmrlList.getRowCount(); fRow++) {
		var MBTLNO  = this.ds_cstmrlList.getColumn(fRow, "MBTLNO");
		var NM 		= this.ds_cstmrlList.getColumn(fRow, "CSTMR_NM");
		var chk     = this.ds_cstmrlList.getColumn(fRow, "CHK");
		if(chk == '1') {
			//this.File_UpDownManager.download(fileSn, fileOrder, fileName);
			if(!this.comUtils.isNullEmpty(MBTLNO)){
				var nRow = this.ds_smsList.addRow();
				this.ds_smsList.setColumn(nRow , "MBTLNO", MBTLNO);
				this.ds_smsList.setColumn(nRow , "NM", NM);
			}	
		}
	}
	
	var CONTENT = "";
	var param = {
		CONTENT : CONTENT,
		JOB_SE : "AAA", 
		ds : this.ds_smsList
	};
		
	this.gfn_popup("COM010700_P01.xfdl"
			   , 800
			   , 500
			   , "SMS발송"
			   , param
			   , "com::COM010700_P01.xfdl"
			   , "sms_callback");
}

