this.div_work_btn_sendMail_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_email.clearData();
	
	var chkCnt = this.ds_cstmrlList.getCaseCount("CHK == 1");
		
	if(chkCnt<1){
		this.gfn_message("info.항목.선택.알림", "수신자");
		return;
	} 
	
	var rowCnt = this.ds_cstmrlList.rowcount;
	for(var i=0; i<rowCnt; i++){
		var chkYn = this.ds_cstmrlList.getColumn(i, "CHK");
		if(chkYn == '1'){
			var nRow = this.ds_email.addRow();    
			this.ds_email.setColumn(nRow, "RECV_EMAIL", this.ds_cstmrlList.getColumn(0, "CSTMR_NM"));
			this.ds_email.setColumn(nRow, "RECV_NM", 	this.ds_cstmrlList.getColumn(0, "EMPNM"));	 
			this.ds_email.setColumn(nRow, "SEND_EMAIL", "hsco@smtp.hsco.or.kr");	// 보내는 사람 메일주소로 변경
			this.ds_email.setColumn(nRow, "SEND_NM"	  , "화성도시공사");	// 보내는 사람 이름으로 변경
			this.ds_email.setColumn(nRow, "TITLE", "화성도시공사");
			this.ds_email.setColumn(nRow, "SUBJECT", "[화성도시공사] 메일발송입니다.");
			this.ds_email.setColumn(nRow, "CONTENTS", this.div_work.txt_chrctrCn.value);
		}
	}
	
	if(this.gfn_message("comm.행위여부", "메일을 발송")){
		// 발송
		Ex.core.tran(this, "sendEmail", "hsco/mis/mis/MIS000000/sendEmail.do", "input1=ds_email", "", "", "fn_callBack", false); 
	}
}

