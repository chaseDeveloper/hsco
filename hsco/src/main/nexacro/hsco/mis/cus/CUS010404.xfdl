    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID     : CUS010404.xfdl
 * 화면명    : 기타민원처리조회
 * 화면설명  : 기타민원처리(접점별, 민원등급별, 반려사유별, 온라인,오프라인,직원별)을 조회한다.
 * 작성일    : 2015.09.17
 * 작성자    :  이지영
 * 수정이력  : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************/
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
  this.ds_ccntPrcSttusList
 ,this.ds_cvplGradPrcSttusList
 ,this.ds_returnPrcSttusList
 ,this.ds_onCvplPrcSttusList
 ,this.ds_offCvplPrcSttusList
 ,this.ds_empPrcSttusList
];
/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS010404_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010404_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	//화면 최초 오픈 시 조회조건 최좌측상단에 커서 위치 
	this.div_search.cal_bgnDe.setFocus(true);
}


/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.div_search.cal_bgnDe.set_value(this.gfn_today().substring(0,4) + '0101');
	this.div_search.cal_endDe.set_value(this.gfn_today());
	
	if(this.gfn_authcode("101") > -1){	//고객VOC담당
		this.ds_cond.setColumn(0, "AUTH_CODE", "101");
		this.div_search.div_dept.set_enable(true);
	}else{
		this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
		this.ds_cond.setColumn(0, "DEPT_NM",   this.gfn_getDeptName());
	}
	this.div_search.div_dept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	var comboParams = [["ds_inqryKnd",       "CUS001", 1, "전체"],
	                   ["ds_inqryKndDetail", "CUS003", 1, "전체"],
	                   ["ds_ccnt",           "CUS019", 1, ""],
					  ]; 
	var callback = function (svcId, errorCode, errorMsg) {
		this.deptcode();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/**********************************************************************
* Dataset 오브젝트 초기화
***********************************************************************/
this.fn_init_dataset = function()
{
	
}

/***********************************************************************
* 부서콤보박스 조회
************************************************************************/
this.deptcode = function()
{
	// clear Datasets
	this.ds_dept.clearData();
	// transaction
	this.fn_transaction("selectDeptcodeList");
}

/***********************************************************************
* 상단 조회처리
************************************************************************/
this.fn_search = function()
{		
	switch(this.div_work.CUS010404.tabindex)
	{
		//접점별 처리현황
		case 0:
			this.fn_transaction("selectCcntPrcSttusList");
		break;
		//민원등급별 현황
		case 1:
			this.fn_transaction("selectCvplGradPrcSttusList");
		break;
		//반려사유별 처리상태
		case 2:
			this.fn_transaction("selectReturnPrcSttusList");
		break;
		//온라인민원처리현황
		case 3:
			this.fn_transaction("selectOnCvplPrcSttusList");
		break;
		//오프라인민원처리현황
		case 4: 
		    this.fn_transaction("selectOffCvplPrcSttusList");
		break;
		//직원별 처리현황
		case 5: 
			this.fn_transaction("selectEmpPrcSttusList");
		break;
		
	}
}

/**********************************************************************
* TabPage 변경 시
***********************************************************************/ 
this.div_work_CUS010405_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	this.fn_search();
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		//부서콤보 조회
		case "selectDeptcodeList":
			this.ds_cond03.clearData();
			var nRow = this.ds_cond03.addRow();
			var sController   	= "hsco/mis/cus/CUS010102/selectDeptCodeList.do";
			var sInDatasets   	= "input5=ds_cond03";
			var sOutDatasets  	= "ds_dept=output5";
		break;	
		
		//T01_접점별 처리현황 리스트 조회
		case "selectCcntPrcSttusList" :				
			var sController   	= "hsco/mis/cus/CUS010404/selectCcntPrcSttusList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_ccntPrcSttusList=output01";
		break;	
		
		//T02_민원등급별 현황
		case "selectCvplGradPrcSttusList" :	
			var sController   	= "hsco/mis/cus/CUS010404/selectCvplGradPrcSttusList.do";
			var sInDatasets   	= "input02=ds_cond";
			var sOutDatasets  	= "ds_cvplGradPrcSttusList=output02";
		break;	
		
		//T03_반려사유별 처리상태
		case "selectReturnPrcSttusList":
			var sController   	= "hsco/mis/cus/CUS010404/selectReturnPrcSttusList.do";
			var sInDatasets   	= "input03=ds_cond";
			var sOutDatasets  	= "ds_returnPrcSttusList=output03";
		break;	

		//T04_온라인 민원처리현황
		case "selectOnCvplPrcSttusList":
			var sController   	= "hsco/mis/cus/CUS010404/selectOnCvplPrcSttusList.do";
			var sInDatasets   	= "input04=ds_cond";
			var sOutDatasets  	= "ds_deptList=output07 ds_onCvplPrcSttusList=output04";
		break;	

		//T05_오프라인 민원처리현황
		case "selectOffCvplPrcSttusList":
			var sController   	= "hsco/mis/cus/CUS010404/selectOffCvplPrcSttusList.do";
			var sInDatasets   	= "input05=ds_cond";
			var sOutDatasets  	= "ds_deptList=output05 ds_offCvplPrcSttusList=output06";
		break;	
		
		//T06_직원별 처리현황
		case "selectEmpPrcSttusList":
			var sController   	= "hsco/mis/cus/CUS010404/selectEmpPrcSttusList.do";
			var sInDatasets   	= "input06=ds_cond";
			var sOutDatasets  	= "ds_empPrcSttusList=output06";
		break;	
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

/**********************************************************************
* Transaction Callback 영역
***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//부서코드 조회 콜백
			case "selectDeptcodeList":
				this.fn_search();
			break;
			
			//온라인민원처리현황 콜백
			case "selectOnCvplPrcSttusList":			
			break;
			
			//오프라인민원처리현황 콜백
			case "selectOffCvplPrcSttusList":			
			break;
			
			
		}
	}
}

/**********************************************************************
* 조회기간 변경시
***********************************************************************/ 
this.ds_cond_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	//시작일 변경시
	if(e.columnid == "BGN_DE")
	{
		if(e.newvalue > this.div_search.cal_endDe.value) 
		{
			this.gfn_message("errors.시작일자.종료일자.이하","|");
			return false;
		}
		return true;
	}
	//종료일 변경시
	if(e.columnid == "END_DE")
	{
		if(e.newvalue < this.div_search.cal_bgnDe.value) 
		{
			this.gfn_message("errors.종료일자.시작일자.이상","|");
			return false;
		}
		return true;

	}
}

