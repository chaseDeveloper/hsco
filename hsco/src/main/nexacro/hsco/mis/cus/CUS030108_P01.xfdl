this.CUS030108_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	this.ds_cond.copyData(v_dsCond);
//	trace(v_dsCond.getColumn(0, "NOW_MONTH"));
	this.fn_transaction("selectTimeList");
	this.fn_transaction("selectDayList");
	this.fn_transaction("acmsltPlanList");
	this.fn_transaction("selectSendSMSCountList");
	this.fn_transaction("selectJobList");
	
}
