}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	
		//일별 상담목록 조회
		case "selectDayList" :
			var sController   	= "hsco/mis/cus/CUS030109/selectDayList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cllrDayList=output01 ds_minrank=output02 ds_maxrank=output03";
		break;
		
		// 운영실적내역 및 운영 계획
		case "selectSendSMSCountList": 
			var sController		= "/hsco/mis/cus/CUS030108/selectSendSMSCountList.do";
			var sInDatasets		= "input01=ds_cond";
			var sOutDatasets	= "ds_sendSMSCnt=output01";
			break;
		
		// 운영실적내역 및 운영 계획
		case "acmsltPlanList": 
			var sController		= "/hsco/mis/cus/CUS030112/CUS030112List.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_acmsltPlanList=output1";
			break;
		
		//업무별 상담목록 조회
		case "selectJobList" :
			var sController   	= "hsco/mis/cus/CUS030111/selectJobList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_inList=output01 ds_outList=output02 ds_smsList=output03";
		break;	
			
		//일별 상담목록 조회
		case "selectTimeList" :
			var sController   	= "hsco/mis/cus/CUS030110/selectTmzonList.do";
			var sInDatasets   	= "input01=ds_cond";
			var sOutDatasets  	= "ds_cllrDayList1=output01 ds_cllrDayList2=output02 ds_cllrTot=output03";
		break;
		
		//홍보고객선택 목록 저장/수정	
		case "prCstmrListActChoiseU" :
			
			var roCount = this.ds_prCstmrChoiseList.rowcount;
			for(var i = 0 ; i < roCount ; i++)
			{
				this.ds_prCstmrChoiseList.setColumn(i, "PR_ACT_SN", pr_act_sn);
			}
			var sController   	= "hsco/mis/cus/CUS010602_P01/prCstmrListActChoiseU.do";
			var sInDatasets   	= "input02=ds_prCstmrChoiseList:U";
			var sOutDatasets  	= "";
		break;
	}
	var sArgs = "";	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//상단저장 콜백
			case "prCstmrListActChoiseU":
				this.close();
			break;
			// 운영실적내역 및 운영 계획
			case "acmsltPlanList": 
				//this.ds_acmsltPlanList01.copyData(this.ds_acmsltPlanList);
				this.ds_acmsltPlanList.filter("ACMSLT_PLAN_SE == '1'");
				this.ds_acmsltPlanList01.copyData(this.ds_acmsltPlanList, true);
				
				this.ds_acmsltPlanList.filter("ACMSLT_PLAN_SE == '2'");
				this.ds_acmsltPlanList02.copyData(this.ds_acmsltPlanList, true);
			break;
			//일별 상담목록 조회
			case "selectTimeList" :
				//막대 그래프용 데이터
				this.ds_grp01.clearData();
				
				for(var z = 0; z < this.ds_cllrDayList1.rowcount; z++){
					var mon = (this.ds_cllrDayList1.getColumn(z, "TIT_SE")=="INBOUND호") ? this.ds_cond.getColumn(0, "NOW_MONTH") : "응답호";
					for(var a = 0; a < this.ds_cllrDayList1.getColCount(); a++){
						var nRow, cate="", val="";
						if(this.ds_cllrDayList1.getColID(a) == "TM09" || this.ds_cllrDayList1.getColID(a) == "TM10" 
						|| this.ds_cllrDayList1.getColID(a) == "TM11" || this.ds_cllrDayList1.getColID(a) == "TM12" 
						|| this.ds_cllrDayList1.getColID(a) == "TM13" || this.ds_cllrDayList1.getColID(a) == "TM14" 
						|| this.ds_cllrDayList1.getColID(a) == "TM15" || this.ds_cllrDayList1.getColID(a) == "TM16" 
						|| this.ds_cllrDayList1.getColID(a) == "TM17"
						){
							switch(this.ds_cllrDayList1.getColID(a)){
								case "TM09" : 
									nRow = this.ds_grp01.addRow();
									val = Number(this.ds_cllrDayList1.getColumn(z, "TM08")) + Number(this.ds_cllrDayList1.getColumn(z, "TM09"));
									cate = "09~10";
								break;
								case "TM10" : 
									nRow = this.ds_grp01.addRow();
									val = this.ds_cllrDayList1.getColumn(z, this.ds_cllrDayList1.getColID(a));
									cate = "10~11"; 
								break;
								case "TM11" : 
									nRow = this.ds_grp01.addRow();
									val = this.ds_cllrDayList1.getColumn(z, this.ds_cllrDayList1.getColID(a));
									cate = "11~12"; 
								break;
								case "TM12" : 
									nRow = this.ds_grp01.addRow();
									val = this.ds_cllrDayList1.getColumn(z, this.ds_cllrDayList1.getColID(a));
									cate = "12~13"; 
								break;
								case "TM13" : 
									nRow = this.ds_grp01.addRow();
									val = this.ds_cllrDayList1.getColumn(z, this.ds_cllrDayList1.getColID(a));
									cate = "13~14"; 
								break;
								case "TM14" : 
									nRow = this.ds_grp01.addRow();
									val = this.ds_cllrDayList1.getColumn(z, this.ds_cllrDayList1.getColID(a));
									cate = "14~15"; 
								break;
								case "TM15" : 
									nRow = this.ds_grp01.addRow();
									val = this.ds_cllrDayList1.getColumn(z, this.ds_cllrDayList1.getColID(a));
									cate = "15~16"; 
								break;
								case "TM16" :
									nRow = this.ds_grp01.addRow();
									val = this.ds_cllrDayList1.getColumn(z, this.ds_cllrDayList1.getColID(a));
									cate = "16~17"; 
								break;
								case "TM17" : 
									nRow = this.ds_grp01.addRow();
									val = Number(this.ds_cllrDayList1.getColumn(z, "TM17")) + Number(this.ds_cllrDayList1.getColumn(z, "TM18"));
									cate = "17~18"; 
								break;
							}
							
							this.ds_grp01.setColumn(nRow, "CATE", cate);
							this.ds_grp01.setColumn(nRow, "SERISE", mon);
							this.ds_grp01.setColumn(nRow, "VAL", val);
						}
					}
				}
				//전월 데이터 Row추가
				trace(this.ds_cllrDayList2.getColCount());
				for(var g = 0; g < this.ds_cllrDayList2.getColCount(); g++){
					var nRow, cate="", val="";
					if(this.ds_cllrDayList2.getColID(g) == "TM09" || this.ds_cllrDayList1.getColID(g) == "TM10" 
					|| this.ds_cllrDayList2.getColID(g) == "TM11" || this.ds_cllrDayList1.getColID(g) == "TM12" 
					|| this.ds_cllrDayList1.getColID(g) == "TM13" || this.ds_cllrDayList1.getColID(g) == "TM14" 
					|| this.ds_cllrDayList1.getColID(g) == "TM15" || this.ds_cllrDayList1.getColID(g) == "TM16" 
					|| this.ds_cllrDayList1.getColID(g) == "TM17"
					){
						switch(this.ds_cllrDayList2.getColID(g)){
							case "TM09" : 
								nRow = this.ds_grp01.addRow();
								val = Number(this.ds_cllrDayList2.getColumn(0, "TM08")) + Number(this.ds_cllrDayList2.getColumn(0, "TM09"));
								cate = "09~10";
							break;
							case "TM10" : 
								nRow = this.ds_grp01.addRow();
								val = this.ds_cllrDayList2.getColumn(0, this.ds_cllrDayList2.getColID(g)); 
								cate = "10~11"; 
							break;
							case "TM11" : 
								nRow = this.ds_grp01.addRow();
								val = this.ds_cllrDayList2.getColumn(0, this.ds_cllrDayList2.getColID(g)); 
								cate = "11~12"; 
							break;
							case "TM12" : 
								nRow = this.ds_grp01.addRow();
								val = this.ds_cllrDayList2.getColumn(0, this.ds_cllrDayList2.getColID(g)); 
								cate = "12~13"; 
							break;
							case "TM13" : 
								nRow = this.ds_grp01.addRow();
								val = this.ds_cllrDayList2.getColumn(0, this.ds_cllrDayList2.getColID(g)); 
								cate = "13~14"; 
							break;
							case "TM14" : 
								nRow = this.ds_grp01.addRow();
								val = this.ds_cllrDayList2.getColumn(0, this.ds_cllrDayList2.getColID(g)); 
								cate = "14~15"; 
							break;
							case "TM15" : 
								nRow = this.ds_grp01.addRow();
								val = this.ds_cllrDayList2.getColumn(0, this.ds_cllrDayList2.getColID(g)); 
								cate = "15~16"; 
							break;
							case "TM16" :
								nRow = this.ds_grp01.addRow();
								val = this.ds_cllrDayList2.getColumn(0, this.ds_cllrDayList2.getColID(g)); 
								cate = "16~17"; 
							break;
							case "TM17" : 
								nRow = this.ds_grp01.addRow();
								val = Number(this.ds_cllrDayList2.getColumn(0, "TM17")) + Number(this.ds_cllrDayList2.getColumn(0, "TM18"));
								cate = "17~18";
							break;
						}
						
						this.ds_grp01.setColumn(nRow, "CATE", cate);
						this.ds_grp01.setColumn(nRow, "SERISE", this.ds_cond.getColumn(0, "AGO_MONTH"));
						this.ds_grp01.setColumn(nRow, "VAL", val);
					}
					trace(this.ds_grp01.rowcount);
				}
			break;
			
			
			case "selectJobList" :
				//막대 그래프용 데이터
				this.ds_grp02.clearData();
 				for(var a = 0; a < this.ds_inList.rowcount; a++){
					var mon = this.ds_inList.getColumn(a, "MONTH");
 					for(var b = 0; b < this.ds_inList.getColCount(); b++){
 						var nRow, cate="", val="";
 						if(this.ds_inList.getColID(b) == "A01" || this.ds_inList.getColID(b) == "A02"
 						|| this.ds_inList.getColID(b) == "B01" || this.ds_inList.getColID(b) == "B02"
 						|| this.ds_inList.getColID(b) == "C01" || this.ds_inList.getColID(b) == "C02" || this.ds_inList.getColID(b) == "C03"
 						|| this.ds_inList.getColID(b) == "D01" || this.ds_inList.getColID(b) == "F01" || this.ds_inList.getColID(b) == "G01"
 						|| this.ds_inList.getColID(b) == "E01" || this.ds_inList.getColID(b) == "E02" || this.ds_inList.getColID(b) == "E03"
 						){
 							switch(this.ds_inList.getColID(b)){
								case "A01" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "A01"); cate = "토지";	break;
								case "A02" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "A02"); cate = "상가";	break;
								case "B01" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "B01"); cate = "분양주택";	break;
								case "B02" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "B02"); cate = "공공임대";	break;
								case "C01" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "C01"); cate = "영구임대";	break;
								case "C02" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "C02"); cate = "매입임대";	break;
								case "C03" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "C03"); cate = "전세임대";	break;
								case "D01" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "D01"); cate = "보상";	break;
								case "E01" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "E01"); cate = "분양/임대";	break;
								case "E02" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "E02"); cate = "영구임대";	break;
								case "E03" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "E03"); cate = "매입임대";	break;
								case "F01" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "F01"); cate = "언론/보도";	break;
								case "G01" :  nRow = this.ds_grp02.addRow(); val = this.ds_inList.getColumn(a, "G01"); cate = "기타";	break;
							}

							this.ds_grp02.setColumn(nRow, "CATE", cate);
							this.ds_grp02.setColumn(nRow, "SERISE", mon);
							this.ds_grp02.setColumn(nRow, "VAL", val);
 						}
 					}
 				}
 				
				//원형 그래프용 데이터
				var val01 = Number(this.ds_inList.getColumn(0, "A01")) + Number(this.ds_inList.getColumn(0, "A02"));
				var val02 = Number(this.ds_inList.getColumn(0, "B01")) + Number(this.ds_inList.getColumn(0, "B02"));
				var val03 = Number(this.ds_inList.getColumn(0, "C01")) + Number(this.ds_inList.getColumn(0, "C02")) + Number(this.ds_inList.getColumn(0, "C03"));
				var val04 = Number(this.ds_inList.getColumn(0, "D01"));
				var val05 = Number(this.ds_inList.getColumn(0, "E01")) + Number(this.ds_inList.getColumn(0, "E02")) + Number(this.ds_inList.getColumn(0, "E03"));
				var val06 = Number(this.ds_inList.getColumn(0, "F01")) + Number(this.ds_inList.getColumn(0, "G01"));

				var titArr = new Array();
				var valArr = new Array();
				titArr = ["토지/상가", "분양/공공임대", "영구임대", "보상", "하자", "홍보및기타"];
				valArr = [val01, val02, val03, val04, val05, val06];
				
				this.ds_grp03.clearData();
				for(var i = 0; i < 6; i++){
					var nRow = this.ds_grp03.addRow();
					this.ds_grp03.setColumn(nRow, "CATE", titArr[i]);
					this.ds_grp03.setColumn(nRow, "VAL", valArr[i]);
				}
			break;
		}
	}
}

 /**********************************************************************
 * 종료버튼 클릭시
