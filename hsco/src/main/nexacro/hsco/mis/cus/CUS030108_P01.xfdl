this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}



this.btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var vOpts = new this.cf_Opts;
	var arg1 = v_dsCond.getColumn(0, "NOW_MONTH").substring(0,4)+"년 "+v_dsCond.getColumn(0, "NOW_MONTH").substring(4,6)+"월";
	var arg2 = v_dsCond.getColumn(0, "NOW_MONTH").substring(4,6)+"월";
	var arg3 = this.dateUtils.addMonth(v_dsCond.getColumn(0, "NOW_MONTH"), + 1, 's').substring(4,6)+"월";
	var arg4 = "1. "+arg2+" 일별 상담 실적";
	var arg5 = "2. "+arg2+" 시간대별 상담 실적";
	var arg6 = "3. "+arg2+" 업무별 상담 실적";
	var arg7 = "4. "+arg2+" 문자발송 현황";
	var arg8 = arg2+" 운영 실적 및 "+arg3+" 운영 계획";
	var arg9 = "1. "+arg2+" 운영 실적";
	var arg10 = "2. "+arg3+" 운영 계획";
	var arg11 = (this.comUtils.isNotNull(this.txt_cliAppSe.value))?this.txt_cliAppSe.value:"";
	var arg12 = (this.comUtils.isNotNull(this.txt_deptAppSe.value))?this.txt_deptAppSe.value:"";
	var arg13 = Number(this.ds_cllrDayList1.getColumn(0, "TOT_CNT"))-Number(this.ds_cllrDayList2.getColumn(0, "TOT_CNT"));
	
	this.ds_hnfSttus.setColumn(1, "SE", arg1);
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setSaveFilename("청아람안내센터 운영 실적 보고");
	
	vOpts.setArgs("title", "청아람안내센터 운영 실적 보고");
	vOpts.setArgs("arg1", arg1);
	vOpts.setArgs("arg2", arg2);
	vOpts.setArgs("arg3", arg3);
	vOpts.setArgs("arg4", arg4);
	vOpts.setArgs("arg5", arg5);
	vOpts.setArgs("arg6", arg6);
	vOpts.setArgs("arg7", arg7);
	vOpts.setArgs("arg8", arg8);
	vOpts.setArgs("arg9", arg9);
	vOpts.setArgs("arg10", arg10);
	vOpts.setArgs("arg11", arg11);
	vOpts.setArgs("arg12", arg12);
	vOpts.setArgs("arg13", arg13);
	
	vOpts.clearLocalDs();
	vOpts.setLocalDs(["ds_hnfSttus", this.ds_hnfSttus]);	//인력현황
	vOpts.setLocalDs(["ds_hnfChg", this.ds_hnfChg]);		//인력변동현황
	vOpts.setLocalDs(["ds_hnfEtc", this.ds_hnfEtc]);		//기타인력운영현황
	vOpts.setLocalDs(["ds_minrank", this.ds_minrank]);		//기타인력운영현황
	vOpts.setLocalDs(["ds_maxrank", this.ds_maxrank]);		//기타인력운영현황
	vOpts.setLocalDs(["ds_inList", this.ds_inList]);		//업무별 실적(인바운드)
	vOpts.setLocalDs(["ds_outList", this.ds_outList]);		//업무별 실적(아웃바운드)
	vOpts.setLocalDs(["ds_smsList", this.ds_smsList]);		//업무별 실적(문자현황)
	vOpts.setLocalDs(["ds_cllrDayList", this.ds_cllrDayList]);		//일별 상담실적
	vOpts.setLocalDs(["ds_cllrDayList1", this.ds_cllrDayList1]);		//시간대별 상담실적
