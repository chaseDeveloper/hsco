 * 설명 		: 팝업 콜백 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                sReturn  : 팝업에서 넘어오는 데이터
 * return Type 	: None
 * 사용법		: this.fn_PopBsnsCallback();
 */
this.fn_PopCallback = function(sPopupId, sReturn)
{
    if(sPopupId == "CUS000000_P02")
    {	
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(sReturn);	
		
		if(nRowCnt > 0){
			var nRow = this.ds_hmpgCstmrCvpl.rowposition;
			this.ds_hmpgCstmrCvpl.setColumn(nRow, "NM", dsRtn.getColumn(0, "MEMBER_NAME"));
			this.ds_hmpgCstmrCvpl.setColumn(nRow, "MBTLNO", dsRtn.getColumn(0, "MEMBER_HP"));
			this.ds_hmpgCstmrCvpl.setColumn(nRow, "GNRL_TLPHON", dsRtn.getColumn(0, "MEMBER_TEL"));
			this.ds_hmpgCstmrCvpl.setColumn(nRow, "ZIP", dsRtn.getColumn(0, "ZIP_CODE"));
			this.ds_hmpgCstmrCvpl.setColumn(nRow, "EMAIL", dsRtn.getColumn(0, "MEMBER_EMAIL"));
			this.ds_hmpgCstmrCvpl.setColumn(nRow, "ADRES", dsRtn.getColumn(0, "MEMBER_ADDR"));
		}
    }
}

/***********************************************************************
* 조회기간 변경 시 
************************************************************************/
this.ds_cond_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	//시작일 변경시
	if(e.columnid == "RCEPT_BGN_DT"){
		//필수입력 체크 2016.07.12
		if(this.comUtils.isNullEmpty(e.newvalue)){
			obj.setColumn(e.row,e.oldvalue);
			return false;
		}
		if(e.newvalue > this.div_search.cal_rceptEndDt.value){
			this.gfn_message("errors.시작일자.종료일자.이하","|");
			return false;
		}
		return true;
	}
	
	//종료일 변경시
	if(e.columnid == "RCEPT_END_DT"){
		//필수입력 체크 2016.07.12
		if(this.comUtils.isNullEmpty(e.newvalue)){
			obj.setColumn(e.row,e.oldvalue);
			return false;
		}

		if(e.newvalue < this.div_search.cal_rceptBgnDt.value){
			this.gfn_message("errors.종료일자.시작일자.이상","|");
			return false;
		}
		return true;
	}
}
