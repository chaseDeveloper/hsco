// 			}
			var sController   	= "hsco/mis/cus/CUS010102/hmpgCvplDstbListCUD.do";
			var sInDatasets   	= "input04=ds_hmpgCstmrCvpl:U";
			var sOutDatasets  	= "";
		break;
		
		//첨부파일조회
		case "selectAtchFile":
			this.div_work.CUS010105.T01.grd_cmmnAtchmnfl.setCellProperty("head", 0, "text", "0");
			this.ds_fileList.clearData();
			
			this.ds_condSub.clearData();
			this.ds_condSub.addRow();
