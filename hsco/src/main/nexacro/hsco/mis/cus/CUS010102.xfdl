/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.CUS010102_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010102_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	this.ds_cond.setColumn(0, "DEPT_CODE", "000000");
	this.ds_cond.setColumn(0, "DEPT_NM",   "화성도시공사");
	
	// 데이터셋 동적 바인딩(부서팝업)
	this.div_search.div_dept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");
	
	//alert(this.gfn_dateInfo("NOW_DATE_ALL_TIME"));
	//포털 진입시 기본 조회조건
	if(this.comUtils.isNullEmpty(gvPid)){
		this.ds_cond.setColumn(0, "RCEPT_BGN_DT", this.gfn_today().substring(0,6)+'01');
	}else{
		this.ds_cond.setColumn(0, "RCEPT_BGN_DT", '19990101');
	}
	this.ds_cond.setColumn(0, "RCEPT_END_DT", this.gfn_today());
	this.ds_cond.setColumn(0, "DELETE_AT", "2");
	
	var comboParams = [["ds_inqryKnd",        "CUS001", 1, "전체"],
	                   ["ds_inqryKndDetail",  "CUS003", 1, "전체"], 
	                   ["ds_inqryKnd2",       "CUS001", 1, "선택"],
	                   ["ds_inqryKndDetail2", "CUS003", 1, ""],
	                   ["ds_prcStatus",       "CUS002", 1, "전체"],
	                   ["ds_dclzSe",          "SAL009", 1, ""],
	                   ["ds_cvplGrad",        "CUS021", 1, ""],
	                   ["ds_rceptCours",      "CUS004", 1, "전체"],
	                   ["ds_rceptMth",        "CUS006", 1, "선택"], //접수방법
	                   ["ds_hopeRicfldCours", "CUS014", 1, "선택"], //희망답변 경로
	                   ["ds_cvplSttus",       "CUS017", 1, ""],
	                   ["ds_cvplSttusSearch", "CUS017", 1, "전체"],
	                   ["ds_processTmlmt",    "CUS027", 1, ""],
	                   ["ds_p03_stsfdgEvl",   "CUS007", 1, ""],         //만족도평가
	                   ["ds_T03fdbckCn",      "CUS005", 1, ""],
	                   ["ds_prcMth",          "CUS019", 1, ""],
	                   ["ds_cus066",          "CUS066", 1, "전체"],		//분배여부
					   ["ds_cus030",		  "CUS030", "Y", "D", "B"]	//도시공사연락처
		              ];
	var callback = function (svcId, errorCode, errorMsg) {
		this.ds_chrctrCnInfo.clearData();
		this.ds_chrctrCnInfo.addRow();

		// SMS Default 메시지 셋팅
		this.ds_chrctrCnInfo.setColumn(0, "CHRCTR_CN", this.fn_getCusSmsMsg("MSG0000003"));
		
		// SMS 화성도시공사 대표전화번호 셋팅
		var fRow = this.ds_cus030.findRow("CODE", "TEL");
		var tel	 = fRow > 0 ? this.ds_cus030.getColumn(fRow, "VAL1") : "0533500301";
		this.ds_chrctrCnInfo.setColumn(0, "ANSWTEL", tel);
		
		//포털 진입시 기본 조회조건
		if(!this.comUtils.isNullEmpty(gvPid)){
			this.ds_cond.setColumn(0,"CVPL_STTUS", "1");
		}
		
		//부서콤보 및 화면접근 가능 사용자 리스트 조회
		this.fn_transaction("selectDeptcodeList");
	};
	this.gfn_comboLoad (comboParams, callback);
	
	//this.div_search.cmb_inqryknd.setFocus(true);
}

