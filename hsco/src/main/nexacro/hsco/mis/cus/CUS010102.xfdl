 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			//상단조회 콜백
			case "selectCvplList":
				if(select_at){
					this.div_work.CUS010105.T01.grd_hmpgCstmrCvpl.selectRow(rowpos);	
				}
				select_at = false;
				//저장트랜잭션 직전 rowposition값으로 이동
// 				if(save_row != -1)
// 				{
// 					this.ds_hmpgCstmrCvpl.set_rowposition(save_row);
// 					save_row = -1;
// 				}
			break;
			
			//부서코드 및 화면접근 가능 사용자 리스트 조회 콜백
			case "selectDeptcodeList":
				this.ds_dept.insertRow(0);
				this.ds_dept.setColumn(0, "DEPT_CODE", null);
				this.ds_dept.setColumn(0, "DEPT_NM", '선택');
				
				var empno = application.gds_userInfo.getColumn(0, "EMPNO");					//현재 사용자 사번
				var cnt1 = this.ds_permList.getCaseCount("EMPNO == '"+empno+"'");			// 현재 사용자가 고객담당자 인지 확인
				var cnt2 = this.ds_permList.getCaseCount("AGNCY_EMPNO == '"+empno+"'");		// 현재 사용자가 고객담당자의 대리자인지 확인
				
				if(this.gfn_authcode("101") > -1){
					this.chargerYn = 'Y';	// 화면접근 허용
				} else {
					this.chargerYn = 'N';	// 화면접근 허용 안함
				}
				
				this.fn_search();
			break;
			
			//선택한 담당부서의 담당자정보조회 콜백
			case "selectChargerAppnInfo":
			   
				if(this.comUtils.isNull(this.ds_cargerAppnInfo.getColumn(0,"DEPT_CHARGER_NM")))
				{
					this.gfn_message("info.담당자.지정.없음");
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "CHRG_DEPT_CODE", "");
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "CHARGER_EMPNO",  "");
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "CHARGER_NM",     "");
				}else{
					 //ds_cargerAppnInfo로 받은 값을 ds_hmpgCstmrCvpl에 셋팅.
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "CHARGER_EMPNO", this.ds_cargerAppnInfo.getColumn(0,"DEPT_CHARGER_EMPNO"));
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "CHARGER_NM",    this.ds_cargerAppnInfo.getColumn(0,"DEPT_CHARGER_NM"));
				}
			break;
			
			//선택한 협조부서의 담당자정보조회 콜백
			case "selectChargerAppnInfo2":
			if(this.comUtils.isNull(this.ds_cargerAppnInfo2.getColumn(0,"DEPT_CHARGER_NM")))
				{
					this.gfn_message("info.담당자.지정.없음");
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "HELP_DEPT_CODE",     "");
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "HELP_CHARGER_EMPNO", "");
					this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "HELP_CHARGER_NM",    "");
				}else{
				//ds_cargerAppnInfo로 받은 값을 ds_hmpgCstmrCvpl에 셋팅.
			    this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "HELP_CHARGER_EMPNO", this.ds_cargerAppnInfo2.getColumn(0,"DEPT_CHARGER_EMPNO"));
				this.ds_hmpgCstmrCvpl.setColumn(this.ds_hmpgCstmrCvpl.rowposition, "HELP_CHARGER_NM",    this.ds_cargerAppnInfo2.getColumn(0,"DEPT_CHARGER_NM"));
					
				}
			break;
			
			//저장 콜백
			case "hmpgCvplDstbListCUD":
				this.gfn_message("success.처리.성공");
				this.fn_search();
			break;
			
			//고객정보 조회 콜백
	   		case "selectHmpgCstmrInfo":
				if(this.ds_hmpgCstmrInfo.rowcount > 0){
			        //이름, 핸드폰번호로 가져온 고객정보를 세팅.
					this.div_work.CUS010105.T01.edt_nm.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"CSTMR_NM"));
					this.div_work.CUS010105.T01.msk_mbtlno.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"MBTLNO"));
					this.div_work.CUS010105.T01.msk_gnrlTlphon.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"GNRL_TLPHON"));
					this.div_work.CUS010105.T01.edt_email.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"EMAIL"));
					this.div_work.CUS010105.T01.msk_zip.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"ZIP"));
					this.div_work.CUS010105.T01.edt_adres.set_value(this.ds_hmpgCstmrInfo.getColumn(0,"ADRES"));
				}else{
					this.gfn_message("info.데이터.없음");
				}
			break;
			
			case "selectAtchFile":
				var cn = this.ds_cn.getColumn(0, "CN");
				
				if(cn){
					this.ds_hmpgCstmrCvpl.set_updatecontrol(false);
					var nRow = this.ds_hmpgCstmrCvpl.rowposition;
					this.ds_hmpgCstmrCvpl.setColumn(nRow, "CN", cn);
					this.ds_hmpgCstmrCvpl.set_updatecontrol(true);
				}
			break;
			
		}
	}
}
