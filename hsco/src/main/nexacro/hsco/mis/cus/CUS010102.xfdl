this.ds_hmpgCstmrCvpl_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var dept1 = obj.getColumn(e.row, "CHRG_DEPT_CODE");	// 주 부서
	var dept2 = obj.getColumn(e.row, "HELP_DEPT_CODE");	// 협조 부서
	
	if(e.columnid == "CHRG_DEPT_CODE"){
	// 주 부서
		var sttus = obj.getColumn(e.row, "OLD_CVPL_STTUS");
		
		// 주 부서가 null이 아닐 경우
		if(e.newvalue){
			if(e.newvalue == dept2){
				this.gfn_message("info.항목.중복","주민원부서와 협조부서");
				return false;
			}
			
			if(sttus == '1' || sttus == '7' || sttus == '8'){
				//현재 선택된 콤보값을 지정부서로 셋팅하기 위함.
				var selVal = this.div_work.CUS010105.T01.cmb_chrgDeptCode.text;
				var fRow = this.ds_dept.findRow("DEPT_NM", selVal);
				
				if(this.comUtils.isNotNull(this.ds_dept.getColumn(fRow, "EMPNO"))){
					obj.setColumn(e.row, "CHARGER_NM",    this.ds_dept.getColumn(fRow, "EMPNM"));
					obj.setColumn(e.row, "CHARGER_EMPNO", this.ds_dept.getColumn(fRow, "EMPNO"));
					obj.setColumn(e.row, "CVPL_STTUS", 2);	//민원상태 부서분배[2/6]
					obj.setColumn(e.row, "CHARGER_TELNO", this.ds_dept.getColumn(fRow, "TELNO"));
					
					
					this.div_work.CUS010105.T01.cal_deleteDe.set_readonly(true);				//삭제일자
					this.div_work.CUS010105.T01.cal_deleteDe.set_cssclass("cal_WF_Readonly");
					this.div_work.CUS010105.T01.edt_dtrn.set_readonly(true);					//삭제사유
					this.div_work.CUS010105.T01.edt_dtrn.set_cssclass("edt_WF_Readonly");
					this.div_work.CUS010105.T01.cmb_helpDeptCode.set_readonly(false);			//협조부서
					this.div_work.CUS010105.T01.cmb_helpDeptCode.set_cssclass("");
					//obj.setColumn(e.row, "APPN_DT", this.gfn_dateInfo("NOW_DATE_ALL_TIME"));	// 지정일시
					
					// 기존 접수 일자가 없을 경우, 접수일자, 접수자사번 입력
					if(this.comUtils.isNull(obj.getColumn(e.row, "RCEPT_DT"))){
						//obj.setColumn(e.row, "RCEPT_DT", this.gfn_dateInfo("NOW_DATE_ALL_TIME"));
						obj.setColumn(e.row, "RCEPT_EMPNO", application.gds_userInfo.getColumn(0, "EMPNO"));
						obj.setColumn(e.row, "RCEPT_EMPNM", application.gds_userInfo.getColumn(0, "USER_NM"));
					}
					
				}else{
					this.gfn_message("fail.정보검색.실패", "선택한부서의 담당자를");
					return false;
				}
				
			}
			
		// 주 부서가 null일 경우	
		} else {
			if(sttus == '1' || sttus == '7' || sttus == '8'){
				
				if(sttus == '1'){
					//obj.setColumn(e.row, "RCEPT_DT", null);	// 이전 처리상태가 접수대기 일 경우 접수 일자 초기화
					obj.setColumn(e.row, "RCEPT_EMPNO", null);	//접수자 초기화
					obj.setColumn(e.row, "RCEPT_EMPNM", null);	//접수자 초기화
				}
				
				obj.setColumn(e.row, "CHARGER_NM",    null);	//주 부서 담당자 초기화
				obj.setColumn(e.row, "CHARGER_EMPNO", null);
				//obj.setColumn(e.row, "APPN_DT",       null);		//지정일시
				
				// 주 부서가 null일 경우 실행 됨.
				this.div_work.CUS010105.T01.cal_deleteDe.set_readonly(false);				//삭제일자
				this.div_work.CUS010105.T01.cal_deleteDe.set_cssclass("");
				this.div_work.CUS010105.T01.edt_dtrn.set_readonly(false);					//삭제사유
				this.div_work.CUS010105.T01.edt_dtrn.set_cssclass("");
				this.div_work.CUS010105.T01.cmb_helpDeptCode.set_readonly(true);			//협조부서
				this.div_work.CUS010105.T01.cmb_helpDeptCode.set_cssclass("cmb_WF_Readonly");
				obj.setColumn(e.row, "HELP_DEPT_CODE",     null);	//협조부서 초기화
				obj.setColumn(e.row, "HELP_CHARGER_EMPNO", null);	//협조부서 담당자 초기화
				obj.setColumn(e.row, "HELP_CHARGER_NM",    null);	//협조부서 담당자 초기화
			}
			obj.setColumn(e.row, "CVPL_STTUS", sttus);
		}
	}else if(e.columnid == "HELP_DEPT_CODE"){
	// 협조부서
		var sttus = obj.getColumn(e.row, "OLD_CVPL_STTUS");
		
		// 협조부서가 null이 아닐경우
		if(e.newvalue){
			if(dept1 == e.newvalue){
				this.gfn_message("info.항목.중복","주민원부서와 협조부서");
				return false;
			}
			
			if(sttus == '1' || sttus == '7' || sttus == '8'){
				var selVal = this.div_work.CUS010105.T01.cmb_helpDeptCode.text;
				var fRow = this.ds_dept.findRow("DEPT_NM", selVal);
				if(this.comUtils.isNotNull(this.ds_dept.getColumn(fRow, "EMPNO"))){
					obj.setColumn(e.row, "HELP_CHARGER_NM",    this.ds_dept.getColumn(fRow, "EMPNM"));
					obj.setColumn(e.row, "HELP_CHARGER_EMPNO", this.ds_dept.getColumn(fRow, "EMPNO"));
					obj.setColumn(e.row, "HELP_TELNO",         this.ds_dept.getColumn(fRow, "TELNO"));
					
				}else{
					this.gfn_message("fail.정보검색.실패", "선택한부서의 담당자를");
					return false;
				}				
			}	
			
		// 협조부서가 null일 경우
		} else {
			
			obj.setColumn(e.row, "HELP_CHARGER_NM",    null);	//협조부서 담당자 초기화
			obj.setColumn(e.row, "HELP_CHARGER_EMPNO", null);
		}
	}else if(e.columnid == "DELETE_DE"){
		
		// 삭제 일자가 있을경우
		if(e.newvalue){
			
			this.div_work.CUS010105.T01.cmb_chrgDeptCode.set_readonly(true);			//담당부서
			this.div_work.CUS010105.T01.cmb_chrgDeptCode.set_cssclass("cmb_WF_Readonly");
			this.div_work.CUS010105.T01.cmb_helpDeptCode.set_readonly(true);			//협조부서
			this.div_work.CUS010105.T01.cmb_helpDeptCode.set_cssclass("cmb_WF_Readonly");
		
		// 삭제 일자가 없을경우
		}else{
			this.div_work.CUS010105.T01.cmb_chrgDeptCode.set_readonly(false);			//담당부서
			this.div_work.CUS010105.T01.cmb_chrgDeptCode.set_cssclass("");
		}
		
	}
}

/***********************************************************************
*  문자발송
*  - 저장시 발송은 JAVA에서 진행하며 해당 function은 '문자발송'버튼을 클릭했을때 작동
************************************************************************/
this.fn_sendSMS = function()
{
	this.ds_smsMsg.clearData();	
	
	var ds_temp = new Dataset;
	var cRow = this.ds_hmpgCstmrCvpl.rowposition;
	var recepNo = this.ds_hmpgCstmrCvpl.getColumn(cRow, "RCEPT_NO");
	
	var ds_temp = new Dataset;
	this.ds_smsMsg.clearData();
	var nRow = null;
	
	//주관부서
	if(this.comUtils.isNotNull(this.ds_hmpgCstmrCvpl.getColumn(cRow, "CHRG_DEPT_CODE"))){
		ds_temp.clearData();
		ds_temp = this.fn_getCusMinwonSmsMsg("MSG0000003",recepNo,1);
		
		// 수신자정보 설정
		var fRow = this.ds_dept.findRow("EMPNM", this.ds_hmpgCstmrCvpl.getColumn(cRow, "CHARGER_NM"));
		nRow = this.ds_smsMsg.addRow();
		this.ds_smsMsg.setColumn(nRow, "RECIPIENT_NUM"	, this.ds_dept.getColumn(fRow, "TELNO"));				// 수신자번호
		this.ds_smsMsg.setColumn(nRow, "CHANGE_WORD5"	, this.ds_hmpgCstmrCvpl.getColumn(cRow, "CHARGER_NM"));	// 수신자명
	}
	
	//협조부서
	if(this.comUtils.isNotNull(this.ds_hmpgCstmrCvpl.getColumn(cRow, "HELP_DEPT_CODE"))){
		ds_temp.clearData();
		ds_temp = this.fn_getCusMinwonSmsMsg("MSG0000003",recepNo,0);
		
		// 수신자정보 설정
		var fRow = this.ds_dept.findRow("EMPNM", this.ds_hmpgCstmrCvpl.getColumn(cRow, "HELP_CHARGER_NM"));
		nRow = this.ds_smsMsg.addRow();
		this.ds_smsMsg.setColumn(nRow, "RECIPIENT_NUM"	, this.ds_dept.getColumn(fRow, "TELNO"));						// 수신자번호
		this.ds_smsMsg.setColumn(nRow, "CHANGE_WORD5"	, this.ds_hmpgCstmrCvpl.getColumn(cRow, "HELP_CHARGER_NM"));	// 수신자명
	

		trace("HELP_DEPT_CODE : "+this.ds_smsMsg.saveXML());
	}
	
	// SMS 발송
	this.sms_MultiMessage(
		  this																//호출한 form객체
		, this.gfn_getDeptId()												//전송부서
		, this.ds_chrctrCnInfo.getColumn(0, "ANSWTEL")						//회신번호
		, '2'																//0:SMS , 2:MMS
		, ds_temp.getColumn(0, "MSSAGE_CN")									//전송메세지
		, this.ds_smsMsg													//수신자목록
		, ""																//전송희망시간 미입력(즉시발송)
		, ds_temp.getColumn(0,"MXTR_WRD1")									//동보단어1
		, ds_temp.getColumn(0,"MXTR_WRD2")									//동보단어2
		, ds_temp.getColumn(0,"MXTR_WRD3")									//동보단어3
		, ""																//기타
	);
}

