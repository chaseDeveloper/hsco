			var sController   	= "hsco/mis/cus/CUS010102/selectChargerAppnInfo.do";
			var sInDatasets   	= "input02=ds_cond01";
			var sOutDatasets  	= "ds_cargerAppnInfo2=output02";
		break;	
		
		//고객정보 조회
		case "selectHmpgCstmrInfo":
		    //검색조건
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0,"CSTMR_NM", this.div_work.CUS010105.T01.edt_nm.value)
