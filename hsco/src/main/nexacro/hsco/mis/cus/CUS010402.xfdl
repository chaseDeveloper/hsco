}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.CUS010402_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{ 	
	this.div_search.cal_bgnDe.setFocus(true);
	this.fn_search();
	
}
/**********************************************************************
* Form 오브젝트 초기화
***********************************************************************/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
	this.fn_misFormInit(this);
	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.div_search.cal_bgnDe.set_value(this.gfn_today().substring(0,4) + '0101');
	this.div_search.cal_endDe.set_value(this.gfn_today());	
	
	if(this.gfn_authcode("101") > -1){	//고객VOC담당
		this.ds_cond.setColumn(0, "AUTH_CODE", "101");
		this.div_search.div_dept.set_enable(true);
	}else{
		this.ds_cond.setColumn(0, "DEPT_CODE", this.gfn_getDeptId());
