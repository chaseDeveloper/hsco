***********************************************************************/ 
this.avg_intrt = function(row)
{
	var sum = 0;
		for(var i =0 ; i < this.ds_deptPrcSttus2.rowcount ; i++)
		{
			sum += this.ds_deptPrcSttus2.getColumn(i, "SUM");
		}
	if(sum != 0){
		var avge = this.ds_deptPrcSttus2.getColumn(row, "SUM")/sum * 100;
		avge = nexacro.round(avge,2);
	}else{
		var avge = 0;
	}
	
	avge = avge + '%'
	return avge;
}

