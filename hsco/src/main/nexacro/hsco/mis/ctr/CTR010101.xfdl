/*
 * 함수명  	: ds_bcncMastr_oncolumnchanged
 * 설명 		: 주민(사업자)번호변경시 변경시 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_bcncMastr_oncolumnchanged();
 */
this.ds_bcncMastr_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "BCNC_SE") {
		// validation check가 가변적으로 변경되므로 css또한 가변적으로 변경
		// mask set
		switch(e.newvalue) {
			case "C1": case "C9":
				// 사원의 경우에는 주민등록번호를 입력가능하도록 변경
				this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_mask("######-#######");
				this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_cssclass("");		// 사업자등록번호
				this.div_work.tab_bcncDtl.tabpage1.edt_rn.set_cssclass("");			// 주소
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_cssclass("");		// 전화번호
				this.div_work.tab_bcncDtl.tabpage1.cmb_smlpzSe.set_cssclass("");	// 중소기업여부
				break;
			default:
				this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_mask("###-##-#####");
				this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_cssclass("msk_WF_Essential");		// 사업자등록번호
				this.div_work.tab_bcncDtl.tabpage1.edt_rn.set_cssclass("edt_WF_Essential");			// 주소
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_cssclass("msk_WF_Essential");		// 전화번호
				this.div_work.tab_bcncDtl.tabpage1.cmb_smlpzSe.set_cssclass("cmb_WF_Essential");	// 중소기업여부
				break;
		}
	}
	
	/* 주민사업자번호*/
	if(e.columnid == "BIZRNO") {
		if(!this.comUtils.isNullEmpty(e.newvalue)) {
			var sBizrno = e.newvalue;
			sBizrno = this.comUtils.replaceAll(sBizrno, '-', '')
			if(sBizrno.length == 10) {
				if(!this.comUtils.isSaeopjaNo(sBizrno)) {
					this.gfn_message("info.형식.확인.요청", "사업자번호");
					obj.setColumn(e.row , e.columnid,"");
					this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.setSelect();
					obj.setColumn(e.row , "BIZRNO_CHANGE_AT", "0");
					return;
				}
			} else if(sBizrno.length == 13) {
				if(!this.comUtils.isJuminNo(sBizrno)) {
					this.gfn_message("info.형식.확인.요청", "주민번호");
					obj.setColumn(e.row , e.columnid,"");
					this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.setSelect();
					obj.setColumn(e.row , "BIZRNO_CHANGE_AT", "0");
					return;
				}
			} else {
				this.gfn_message("info.형식.확인.요청", "주민(사업자)번호");
				obj.setColumn(e.row , e.columnid,"");
				this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.setSelect();
				obj.setColumn(e.row , "BIZRNO_CHANGE_AT", "0");
				return;
			}
			// 주민(사업자)번호 변경여부
			obj.setColumn(e.row , "BIZRNO_CHANGE_AT", "1");
		}else {
			obj.setColumn(e.row , "BIZRNO_CHANGE_AT", "0");
		}
	}
	
	/* 대표자주민번호 */
	if(e.columnid == "RPRSNTV_INHBTNT_NO") {
		if(!this.comUtils.isNullEmpty(e.newvalue)) {
			var sInhbtntNo = e.newvalue;
			sInhbtntNo = this.comUtils.replaceAll(sInhbtntNo, '-', '')
			if(sInhbtntNo.length == 13) {
				if(!this.comUtils.isJuminNo(sInhbtntNo)) {
					this.gfn_message("info.형식.확인.요청", "대표자주민번호");
					obj.setColumn(e.row, e.columnid, "");
					this.div_work.tab_bcncDtl.tabpage1.msk_rprsntvInhbtntNo.setSelect();
				}
			} else {
				this.gfn_message("info.형식.확인.요청", "대표자주민번호");
				obj.setColumn(e.row, e.columnid, "");
				this.div_work.tab_bcncDtl.tabpage1.msk_rprsntvInhbtntNo.setSelect();
			}
		}
	}
	
	if(e.columnid == "TELNO"){
		this.fn_telNoSet_mask();
	}else if(e.columnid == "MBTLNUM"){
		this.fn_mbtlNoSet_mask();
	}else if(e.columnid == "FAXNO"){
		this.fn_faxNoSet_mask();
	}  
	
}


