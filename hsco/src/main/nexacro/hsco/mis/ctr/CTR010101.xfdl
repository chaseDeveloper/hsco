this.isNumber = function(s){
	s += ''; // 문자열로 변환
	s = s.replace(/^\s*|\s*$/g, ''); // 좌우 공백 제거
	if (s == '' || isNaN(s)) return false;
	return true;
}

this.fn_telNoSet_mask = function(obj:MaskEdit){
	var telNo = this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition, "TELNO");
	if(!this.comUtils.isNull(telNo) && telNo != ""){
		telNo = telNo.replace(/\-/g,'').trim().length;
	}
	
	if( this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition, "TELNO") ){
		switch(telNo){
			case 7 :
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_mask("@@@-@@@@");
				break;
			case 9 :
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_mask("@@-@@@-@@@@");
				break;
			case 10 :
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_mask("@@@-@@@-@@@@");
				break;
			case 11 :
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_mask("@@@-@@@@-@@@@");
				break;
			default:
				var mask = "";
				for(var i = 0 ; i < telNo ; i++){
					mask+="@";
				}
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_mask(mask);
				break;
		}
	} else {
		var mask = "";
		for(var i = 0 ; i < 11; i++){
			mask+="@";
		}
		this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_mask(mask);
	}
}


