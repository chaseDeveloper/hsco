 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";



/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
var saveRowKey = null;


/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.CTR010101_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}

this.CTR010101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	
	// 공통 코드
	var comboParams = [
		 ["ds_ctr013s", "CTR013", "Y", "A", "B"]	// 업체구분(검색조건)
		,["ds_ctr013",  "CTR013", "Y", "D", "B"]	// 업체구분
		,["ds_ctr019",  "CTR019", "Y", "D", "B"]	// 계약구분
	];
	
	var callback = function (svcId, errorCode, errorMsg) {
		
	
		// 지출담당자
		if(this.gfn_authGrpId("ACC_USER") != -1) {
			this.div_work.tab_bcncDtl.tabpage1.chk_excutCnrlExcpAt.set_enable(true);
		}else {
			this.div_work.tab_bcncDtl.tabpage1.chk_excutCnrlExcpAt.set_enable(false);
		}

		this.fn_search();
	};
	this.gfn_comboLoad (comboParams, callback);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	this.div_search.cmb_useAt_s.set_index(0);
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 거래처코드 마스터 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// clear Datasets
	this.ds_bcncMastrList.clearData();
	this.ds_bcncMastr.clearData();
	this.ds_cntrctList.clearData();
	this.ds_bcncAcnutnoList.clearData();
	
	// transaction
	this.fn_transaction("selectBcncmstList");
}

/*
 * 함수명  	: fn_selectBcncmst
 * 설명 		: 업체상세정보 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_selectBcncmst();
 */
this.fn_selectBcncmst = function()
{
	// clear Datasets
	this.ds_bcncMastr.clearData();
	this.ds_bcncAcnutnoList.clearData();
	
	// ds_cond 설정
	this.ds_cond.setColumn(0, "BCNC_CODE", this.ds_bcncMastrList.getColumn(this.ds_bcncMastrList.rowposition, "BCNC_CODE"));
	this.fn_transaction("selectBcncmst");
}

/*
 * 함수명  	: fn_selectCntrctList
 * 설명 		: 업체별거래실적 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_selectCntrctList();
 */
this.fn_selectCntrctList = function()
{
	// Clear Datasets
	this.ds_cntrctList.clearData();
	
	// ds_cond 설정
	this.ds_cond.setColumn(0, "BCNC_CODE", this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition ,"BCNC_CODE"));
	this.fn_transaction("selectCntrctList");
}

/*
 * 함수명  	: fn_selectBcncAcnutnoList
 * 설명 		: 거래처계좌정보 조회 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_selectBcncAcnutnoList();
 */
this.fn_selectBcncAcnutnoList = function()
{
	// clear Datasets
	this.ds_bcncAcnutnoList.clearData();
	
	// ds_cond 설정
	this.ds_cond.setColumn(0, "BCNC_CODE", this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition ,"BCNC_CODE" ));
	this.fn_transaction("selectBcncAcnutnoList");
}


/*
 * 함수명  	: fn_chkBizrno
 * 설명 		: 사업자등록번호 중복체크
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_chkBizrno();
 */
this.fn_chkBizrno = function()
{
	this.ds_chkBizrno.clearData();
	var rowType = this.ds_bcncMastr.getRowType(this.ds_bcncMastr.rowposition);
	this.ds_cond.setColumn(0, "BIZRNO_DTL", this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition ,"BIZRNO" ));
	this.ds_cond.setColumn(0, "BCNC_NM_DTL", this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition ,"BCNC_NM" ));
	if(rowType == Dataset.ROWTYPE_INSERT){
		this.ds_cond.setColumn(0, "CHK_FLAG", "Y");
	}else{
		this.ds_cond.setColumn(0, "CHK_FLAG", "N");
	}
	this.fn_transaction("selectChkBizrno");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	/* 거래처상세정보 또는 계좌정보가 변경된 경우에만 저장함 */
	if (!this.comUtils.isDatasetUpdated(this.ds_bcncMastr) && !this.comUtils.isDatasetUpdated(this.ds_bcncAcnutnoList)) {
		this.gfn_message("comm.데이터.변경.없음");
		return;
	}
	
	var bcncSe = this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition, "BCNC_SE");
	if(bcncSe == "C1" || bcncSe == "C9") {
		// TODO : 개인과 기타일 경우에는 주민등록번호 중복체크를 하지 않음
	} else {
		// 화면에서의 null 체크
		var bizrno = this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition ,"BIZRNO" )
		
		if (this.comUtils.isNullEmpty(bcncSe)) {
			this.gfn_message("comm.항목.입력.을", "거래처구분");
			return;
		}
		
		if (this.comUtils.isNullEmpty(bizrno)) {
			this.gfn_message("comm.항목.입력.을", "주민(사업자)번호");
			return;
		}
	}
	
	/* 사업자등록번호 중복체크 후 저장로직을 수행한다. */
	this.fn_chkBizrno();
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("strSvcId");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectBcncmstList": //거래처코드마스터 조회
			this.ds_bcncMastrList.clearData();
			var sController   	= "/hsco/mis/ctr/CTR010101/selectBcncmstList.do";
			var sInDatasets   	= "ds_cond=ds_cond";
			var sOutDatasets  	= "ds_bcncMastrList=ds_bcncMastrList ds_fnncInsttCode=ds_fnncInsttCode";
			break;
		case "selectBcncmst":   // 업체상세정보 조회
			var sController   	= "/hsco/mis/ctr/CTR010101/selectBcncmst.do";
			var sInDatasets   	= "ds_cond=ds_cond";
			var sOutDatasets  	= "ds_bcncMastr=ds_bcncMastr";
			break;
		
		case "selectBcncAcnutnoList":   // 거래처계좌정보 조회
			var sController   	= "/hsco/mis/ctr/CTR010101/selectBcncAcnutnoList.do";
			var sInDatasets   	= "ds_cond=ds_cond";
			var sOutDatasets  	= "ds_bcncAcnutnoList=ds_bcncAcnutnoList";
			break;
		
		case "selectCntrctList":   // 업체실적 조회
			var sController   	= "/hsco/mis/ctr/CTR010101/selectCntrctList.do";
			var sInDatasets   	= "ds_cond=ds_cond";
			var sOutDatasets  	= "ds_cntrctList=ds_cntrctList";
			break;
		
		case "selectChkBizrno":   //사업자등록번호 중복체크 
			var sController   	= "/hsco/mis/ctr/CTR010101/selectChkBizrno.do";
			var sInDatasets   	= "ds_cond=ds_cond";
			var sOutDatasets  	= "ds_chkBizrno=ds_chkBizrno";
			break;
		
		case "saveBcncmst":	    // 거래처 상세정보 저장
			var sController   	= "/hsco/mis/ctr/CTR010101/saveBcncmst.do";
			var sInDatasets   	= "ds_bcncMastr=ds_bcncMastr:U ds_bcncAcnutnoList=ds_bcncAcnutnoList:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	else {
		switch (strSvcId) {
			case "selectBcncmstList": 
				if (!this.comUtils.isNullEmpty(saveRowKey)) {
					var findRow = this.ds_bcncMastrList.findRow("BIZRNO", saveRowKey);
					if(findRow > -1) {
						saveRowKey = null;
						this.ds_bcncMastrList.set_rowposition(findRow, "BIZRNO");
						if(findRow > -1) {
							this.ds_bcncMastrList.set_rowposition(findRow);
						}
					}
				}
				break;
			case "selectBcncmst":
				// validation check가 가변적으로 변경되므로 css또한 가변적으로 변경
				this.fn_telNoSet_mask();
				this.fn_mbtlNoSet_mask();
				this.fn_faxNoSet_mask();
				
				var bcncSe = this.ds_bcncMastr.getColumn(0, "BCNC_SE");
				switch(bcncSe) {
					case "C1": case "C9":	// C1:개인, C9:기타
						this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_cssclass("");		// 사업자등록번호
						this.div_work.tab_bcncDtl.tabpage1.edt_rn.set_cssclass("");			// 주소
						this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_cssclass("");		// 전화번호
						this.div_work.tab_bcncDtl.tabpage1.cmb_smlpzSe.set_cssclass("");	// 중소기업여부
						break;
					default:
						this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_cssclass("msk_WF_Essential");		// 사업자등록번호
						this.div_work.tab_bcncDtl.tabpage1.edt_rn.set_cssclass("edt_WF_Essential");			// 주소
						this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_cssclass("msk_WF_Essential");		// 전화번호
						this.div_work.tab_bcncDtl.tabpage1.cmb_smlpzSe.set_cssclass("cmb_WF_Essential");	// 중소기업여부
						break;
				}
				
				// 팝업을 통한 입력을 반영목적
				var bmlRow = this.ds_bcncMastrList.rowposition;
				var bmRow  = this.ds_bcncMastr.rowposition;
				this.ds_bcncMastrList.setColumn(bmlRow,"BCNC_SE", this.ds_bcncMastr.getColumn(bmRow, "BCNC_SE"));
				this.ds_bcncMastrList.setColumn(bmlRow,"BCNC_NM", this.ds_bcncMastr.getColumn(bmRow, "BCNC_NM"));
				this.ds_bcncMastrList.setColumn(bmlRow,"BIZRNO",  this.ds_bcncMastr.getColumn(bmRow, "BIZRNO") );
				this.ds_bcncMastrList.applyChange();
				
				// (기본적으로 모두 막아야 하지만, 과거 데이터와 현재의 필수입력조건은 다르기 때문에) 제한적으로 입력제한 해제
				this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_enable(           !this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.value           );
// 				this.div_work.tab_bcncDtl.tabpage1.cmb_bcncSe.set_enable(           !this.div_work.tab_bcncDtl.tabpage1.cmb_bcncSe.value           );
// 				this.div_work.tab_bcncDtl.tabpage1.edt_bcncNm.set_enable(           !this.div_work.tab_bcncDtl.tabpage1.edt_bcncNm.value           );
// 				this.div_work.tab_bcncDtl.tabpage1.edt_rprsntv.set_enable(          !this.div_work.tab_bcncDtl.tabpage1.edt_rprsntv.value          );
// 				this.div_work.tab_bcncDtl.tabpage1.msk_rprsntvInhbtntNo.set_enable( !this.div_work.tab_bcncDtl.tabpage1.msk_rprsntvInhbtntNo.value );
// 				this.div_work.tab_bcncDtl.tabpage1.msk_jurirno.set_enable(          !this.div_work.tab_bcncDtl.tabpage1.msk_jurirno.value          );
// 				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_enable(            !this.div_work.tab_bcncDtl.tabpage1.msk_telno.value            );
// 				this.div_work.tab_bcncDtl.tabpage1.msk_mbtlno.set_enable(           !this.div_work.tab_bcncDtl.tabpage1.msk_mbtlno.value           );
				
				// 2018-01-16 필요하면 주석 풀어서 제한해야 함
				this.div_work.tab_bcncDtl.tabpage1.edt_bcncNm.set_enable(true);
				this.div_work.tab_bcncDtl.tabpage1.edt_rprsntv.set_enable(true);
				this.div_work.tab_bcncDtl.tabpage1.msk_rprsntvInhbtntNo.set_enable(true);
				this.div_work.tab_bcncDtl.tabpage1.msk_jurirno.set_enable(true);
				this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_enable(true);
				this.div_work.tab_bcncDtl.tabpage1.msk_mbtlno.set_enable(true);
				
				this.fn_selectBcncAcnutnoList(); // 거래처계좌정보 조회
				break;
			
			case "selectBcncAcnutnoList":  
				this.fn_selectCntrctList(); // 거래처 계약실적 조회
				break;
			
			case "saveBcncmst":	  
				this.fn_search();
				break;
			case "selectChkBizrno":
				// 사업자등록번호 중복체크(서버에서 중복건수 체크)
				if(this.ds_bcncMastr.getRowType(this.ds_bcncMastr.rowposition) == Dataset.ROWTYPE_INSERT) {
					var bcncSe = this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition, "BCNC_SE");
					if(bcncSe == "C1" || bcncSe == "C9") {
						// TODO : 개인과 기타일 경우에는 주민등록번호 중복체크를 하지 않음
					} else {
						// 서버에서의 중복 체크
// 						if(this.ds_chkBizrno.getColumn(this.ds_chkBizrno.rowposition ,"CNT") > 0) {
// 							this.gfn_message("info.항목.중복", "주민(사업자)번호");
// 							return;
// 						}
					}
				}
				
				// validation
				var bcncSe = this.ds_bcncMastr.getColumn(0, "BCNC_SE");
				switch(bcncSe) {
					case "C1": case "C9":	// C1:개인, C9:기타
						var findRow1 = this.ds_validation.findRow("colId", "TELNO");
						this.ds_validation.setColumn(findRow1, "notNull", "");
						var findRow2 = this.ds_validation.findRow("colId", "RN");
						this.ds_validation.setColumn(findRow2, "notNull", "");
						var findRow3 = this.ds_validation.findRow("colId", "SMLPZ_SE");
						this.ds_validation.setColumn(findRow3, "notNull", "");
						
						this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_cssclass("");		// 사업자등록번호
						this.div_work.tab_bcncDtl.tabpage1.edt_rn.set_cssclass("");			// 주소
						this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_cssclass("");		// 전화번호
						this.div_work.tab_bcncDtl.tabpage1.cmb_smlpzSe.set_cssclass("");	// 중소기업여부
						break;
					default:
						var findRow1 = this.ds_validation.findRow("colId", "TELNO");
						this.ds_validation.setColumn(findRow1, "notNull", "Y");
						var findRow2 = this.ds_validation.findRow("colId", "RN");
						this.ds_validation.setColumn(findRow2, "notNull", "Y");
						var findRow3 = this.ds_validation.findRow("colId", "SMLPZ_SE");
						this.ds_validation.setColumn(findRow3, "notNull", "Y");
						
						this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_cssclass("msk_WF_Essential");		// 사업자등록번호
						this.div_work.tab_bcncDtl.tabpage1.edt_rn.set_cssclass("edt_WF_Essential");			// 주소
						this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_cssclass("msk_WF_Essential");		// 전화번호
						this.div_work.tab_bcncDtl.tabpage1.cmb_smlpzSe.set_cssclass("cmb_WF_Essential");	// 중소기업여부
						break;
				}
				
				// validation check
				if(!this.gfn_checkValidation_NoUpdChk(this.ds_bcncMastr, this.ds_validation)) {
					return false;
				} 
				else if(!this.gfn_checkValidation_NoUpdChk(this.ds_bcncAcnutnoList, this.ds_validation)) {
					return false;
				}
				
				//사업자등록번호 또는 거래처명 중복시
				if(this.ds_chkBizrno.getColumn(this.ds_chkBizrno.rowposition ,"CNT") > 0) {
					var bizrno = this.comUtils.isNullThenEmpty(this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition, "BIZRNO"));
					var bcncNm = this.comUtils.isNullThenEmpty(this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition, "BCNC_NM"));
					var confmMsg = "입력하신 주민(사업자)번호("+bizrno+") 정보가 이미 거래처마스터에 등록되어 있습니다.";
					
					this.gfn_message("info.처리불가", confmMsg);
					return false;
					
// 					var confmMsg = "입력하신 주민(사업자)번호("+bizrno+") 또는\n거래처명("+bcncNm+")\n정보가 이미 거래처마스터에 등록되어 있습니다.\n저장";
// 					
// 					if(this.gfn_message("comm.행위여부", confmMsg)) {
// 						this.fn_transaction("saveBcncmst");
// 					}
				}else{ //정상데이터
					if(this.gfn_message("confirm.저장.여부")) {
						this.fn_transaction("saveBcncmst");
					}
				}
				
				var bizrno = this.ds_bcncMastr.getColumn(this.ds_bcncMastr.rowposition, "BIZRNO");
				saveRowKey = bizrno;
				
				break;
		}
	}
}



