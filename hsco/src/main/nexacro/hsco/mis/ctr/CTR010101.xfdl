/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	//신규시 업체상세정보 탭으로 이동 2016.07.04
	this.div_work.tab_bcncDtl.set_tabindex(0);
	
	// clear Dataset
	this.ds_bcncMastr.clearData();			// 업체상세정보
	this.ds_bcncAcnutnoList.clearData();	// 거래처 계좌정보
	this.ds_cntrctList.clearData();			// 거래실적
	
	// setting default Data
	var nRow = this.ds_bcncMastr.addRow();
	this.ds_bcncMastr.setColumn(nRow, "USE_AT",     "1");
	this.ds_bcncMastr.setColumn(nRow, "INPUT_DEPT", this.gfn_getDeptId());	// 접속자 부서
	
	/* 입력가능상태로 변경 */
	this.div_work.tab_bcncDtl.tabpage1.cmb_bcncSe.set_enable(true);
	this.div_work.tab_bcncDtl.tabpage1.msk_bizrno.set_enable(true);
	this.div_work.tab_bcncDtl.tabpage1.edt_bcncNm.set_enable(true);
	this.div_work.tab_bcncDtl.tabpage1.edt_rprsntv.set_enable(true);
	this.div_work.tab_bcncDtl.tabpage1.msk_rprsntvInhbtntNo.set_enable(true);
	this.div_work.tab_bcncDtl.tabpage1.msk_jurirno.set_enable(true);
	this.div_work.tab_bcncDtl.tabpage1.msk_telno.set_enable(true);
	this.div_work.tab_bcncDtl.tabpage1.msk_mbtlno.set_enable(true);
}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	/* 계약여부 체크 */
	var bmRow      = this.ds_bcncMastr.rowposition;
	var cntrctCnt  = this.ds_bcncMastr.getColumn(bmRow, "CNTRCT_CNT");
	var cntrctList = this.ds_bcncMastr.getColumn(bmRow, "CNTRCT_LIST");
	if(cntrctCnt > 0 ) {
		this.gfn_message("info.확인.진행가능", "현 거래처를 사용하는 계약건 <" + cntrctList + "> 삭제 ")
		return false;
	}
	
	if(this.gfn_message("confirm.삭제여부")) {		// 삭제하시겠습니까?
		var nRow = this.ds_bcncMastr.rowposition;
		this.ds_bcncMastr.deleteRow(nRow);
		
		this.fn_transaction("saveBcncmst");
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function()
{
	if (this.gfn_message("confirm.변경.취소.여부")) {
		this.ds_bcncMastr.reset();
		this.ds_bcncMastr.applyChange();
		this.ds_bcncAcnutnoList.reset();
