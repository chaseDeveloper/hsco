	// 공통코드
	var comboParams = [
	    ["ds_ctr022", "CTR022", "Y", "A", "B"]	// ds_ctr022 : 계약구분
	   ,["ds_ctr023", "CTR023", "Y", "A", "B"]	// ds_ctr023 : 계약종류
