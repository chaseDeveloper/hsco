 * return Type	: None
 * 사용법		: this.btn_demoDown_onclick();
 */
this.btn_demoDown_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_exportExcel(this.grd_demo, "grd_demo_001");	
}

]]></Script>
