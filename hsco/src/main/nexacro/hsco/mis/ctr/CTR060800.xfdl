    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : CTR060800
 * 화면명   : 계약통계현황
 * 화면설명 : 계약통계현황을 조회한다.
 * 작성일   : 2017.08.16
 * 작성자   : 김형태
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************
 */
 
 
/***********************************************************************
 * Script Include 
 ************************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";



/**********************************************************************
* 화면 변수 선언부
***********************************************************************/



/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.CTR060800_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// Form 초기화
	this.fn_init_form();

	// DataSet 초기화
	this.fn_init_dataset();
}

this.CTR060800_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.ds_cond_mastr.clearData();
	var nRow = this.ds_cond_mastr.addRow();
	this.ds_cond_mastr.setColumn(nRow, "CNTRCT_YEAR", this.comUtils.getClientDate("YYYY"));
	this.fn_search();
}



/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}



/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 계약통계현황 내역 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_allSelect.clearData();
	this.ds_businessSelect.clearData();
	this.ds_ongoingSelect.clearData();
	this.ds_constRuct.clearData();
	this.ds_constRuct2.clearData();
	this.ds_cmptSelect.clearData();
	this.ds_deptSelect.clearData();
	
	this.fn_transaction("select");
}



/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "select":
			var sController   	= "hsco/mis/ctr/CTR060800/CntDataStateList.do";
			var sInDatasets   	= "input1=ds_cond_mastr";
			var sOutDatasets  	= "ds_allSelect=output_all ds_businessSelect=output_bns ds_ongoingSelect=output_ong ds_cmptSelect=output_cmp ds_deptSelect=output_dept";
			break;
		
		case "selectDtl":
			var sController   	= "hsco/mis/ctr/CTR060800/businessSelectDetail.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_constRuct=output1 ds_constRuct2=output2";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0) {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch(strSvcId) {
			case "select":
				break;
			default:
				break;
		}
	}
}



/**********************************************************************
* non-Transaction 관련 컴포넌트 이벤트 처리 함수
***********************************************************************
*
* 1. Button 관련 이벤트 처리
* 
***********************************************************************/


/***********************************************************************
* Button 관련 이벤트 처리
************************************************************************/

/*
 * 함수명  	: btn_excel_down_onclick
 * 설명 		: 엑셀다운로드
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.btn_excel_down_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_exportExcel(this.tab.tabpages[this.tab.tabindex].grd, "Excel File Download");
}


