this.ds_businessSelect_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	var crRow      = this.ds_businessSelect.rowposition;
	var bsnsCode   = this.ds_businessSelect.getColumn(crRow, "BSNS_CODE");
	var cntrctYear = this.ds_cond_mastr.getColumn(0, "CNTRCT_YEAR");
	
