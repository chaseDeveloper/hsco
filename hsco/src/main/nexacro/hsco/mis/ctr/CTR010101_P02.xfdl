/*
 * 함수명  	: fn_popupInit
 * 설명 		: 팝업을 읽기전용으로 변환
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_popupInit(obj,e);
 */
this.fn_popupInit = function()
{
	// 모든 오브젝트에 대하여 readonly 이벤트 추가
	_fn_setButtonVisible = function(obj){
		var innerComp   = obj;
		var fRow       = 0;
		var compLength = obj.components.length;
		while(fRow < compLength ){
			innerComp = obj.components[fRow];
			switch(innerComp.toString()){
				case "[object Edit]":
					innerComp.set_cssclass("edt_WF_Readonly");
					innerComp.set_readonly(true);
					break;
				case "[object MaskEdit]":
					innerComp.set_cssclass("msk_WF_Readonly");
					innerComp.set_readonly(true);
					break;
				case "[object Spin]":
					innerComp.set_cssclass("spn_WF_Readonly");
					innerComp.set_readonly(true);
					break;
				case "[object Combo]":
					innerComp.set_cssclass("cmb_WF_Readonly");
					innerComp.set_readonly(true);
					break;
				case "[object Calendar]":
					innerComp.set_cssclass("cal_WF_Readonly");
					innerComp.set_readonly(true);
					break;
				case "[object TextArea]":
					innerComp.set_cssclass("txt_WF_Readonly");
					innerComp.set_readonly(true);
					break;
				case "[object CheckBox]":
					innerComp.set_enable(false);
					break;
				case "[object Button]":
					switch(innerComp.cssclass) {
						case "btn_WF_PopupClose": case "btn_WF_CRUD":
							break;
						default:
							innerComp.set_visible(false);
							break;
					}
					break;
				case "[object Form]":
				case "[object Div]":
				case "[object Tab]":
				case "[object Tabpage]":
					_fn_setButtonVisible(innerComp);
					break;
				case "[object Grid]":
					for(var cRow = 0; cRow < innerComp.getCellCount("Body"); cRow++) {
						innerComp.setCellProperty("Body", cRow, "edittype",   "none");
						innerComp.setCellProperty("Body", cRow, "expandshow", "hide");
					}
					break;
			}
			fRow++;
		}
	}
	_fn_setButtonVisible(this);
}

/*
 * 함수명  	: fn_popupInit
 * 설명 		: 팝업을 읽기전용으로 변환
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_popupInit(obj,e);
 */
this.fn_objInpCtrl = function(stObj, enableAt, arrEssntlObj, arrSkipObj)

{

	_fn_setButtonVisible = function(obj){

		var innerComp   = obj;

		var fRow       = 0;

		var compLength = obj.components.length;

		while(fRow < compLength ){

			innerComp = obj.components[fRow];

			

			var essntlAt = false;

			if(arrEssntlObj){

				for(var rRow = 0; rRow < arrEssntlObj.length; rRow++) {

					if(innerComp.name == arrEssntlObj[rRow]) {

						essntlAt = true;

						break;

					}

				}

			}

			

			var skipAt = false;

			if(arrSkipObj){

				for(var sRow = 0; sRow < arrSkipObj.length; sRow++) {

					if(innerComp.name == arrSkipObj[sRow]) {

						skipAt = true;

						break;

					}

				}

			}

			

			if(!skipAt) {

				switch(innerComp.toString()){

					case "[object Edit]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("edt_WF_Essential");

							innerComp.set_readonly(false);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_readonly(false);

						} else {

							innerComp.set_cssclass("edt_WF_Readonly");

							innerComp.set_readonly(true);

						}

						break;

					case "[object MaskEdit]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("msk_WF_Essential");

							innerComp.set_readonly(false);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_readonly(false);

						} else {

							innerComp.set_cssclass("msk_WF_Readonly");

							innerComp.set_readonly(true);

						}

						break;

					case "[object Spin]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("spn_WF_Readonly");

							innerComp.set_readonly(false);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_readonly(false);

						} else {

							innerComp.set_cssclass("spn_WF_Readonly");

							innerComp.set_readonly(true);

						}

						break;

					case "[object Combo]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("cmb_WF_Essential");

							innerComp.set_readonly(false);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_readonly(false);

						} else {

							innerComp.set_cssclass("cmb_WF_Readonly");

							innerComp.set_readonly(true);

						}

						break;

					case "[object Calendar]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("cal_WF_Essential");

							innerComp.set_readonly(false);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_readonly(false);

						} else {

							innerComp.set_cssclass("cal_WF_Readonly");

							innerComp.set_readonly(true);

						}

						break;

					case "[object TextArea]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("txt_WF_Essential");

							innerComp.set_readonly(false);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_readonly(false);

						} else {

							innerComp.set_cssclass("txt_WF_Readonly");

							innerComp.set_readonly(true);

						}

						break;

					case "[object CheckBox]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("chk_WF_Essential");

							innerComp.set_enable(true);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_enable(true);

						} else {

							innerComp.set_cssclass("");

							innerComp.set_enable(false);

						}

						break;

					case "[object Radio]":

						if(essntlAt && enableAt) {

							innerComp.set_cssclass("rdo_WF_Essential");

							innerComp.set_enable(true);

						} else if(enableAt) {

							innerComp.set_cssclass("");

							innerComp.set_enable(true);

						} else {

							innerComp.set_cssclass("");

							innerComp.set_enable(false);

						}

						break;

					case "[object Button]":

						if(enableAt) {

							if(innerComp.cssclass == "btn_WF_Search") {

								innerComp.set_visible(true);

							} else {

								innerComp.set_enable(true);

							}

						} else {

							if(innerComp.cssclass == "btn_WF_Search") {

								innerComp.set_visible(false);

							} else {

								innerComp.set_enable(false);

							}

						}

						break;

					case "[object Form]":

					case "[object Div]":

					case "[object Tab]":

					case "[object Tabpage]":

						_fn_setButtonVisible(innerComp);

						break;

					case "[object Grid]":

						if(!enableAt) {

							for(var cRow = 0; cRow < innerComp.getCellCount("Body"); cRow++) {

								innerComp.setCellProperty("Body", cRow, "edittype",   "none");

								innerComp.setCellProperty("Body", cRow, "expandshow", "hide");

							}

						}

						break;

				}

			}

			fRow++;

		}

	}

	_fn_setButtonVisible(stObj);

}
