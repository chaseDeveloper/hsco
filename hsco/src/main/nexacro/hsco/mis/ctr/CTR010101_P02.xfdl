/*
 * 함수명  	: grd_bcncAcnutnoList_oncellclick
 * 설명 		: 거래처 선택
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_bcncAcnutnoList_oncellclick();
 */
this.grd_bcncMastrList_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	// '선택'
	var cellProp = obj.getCellProperty("Body", e.cell, "displaytype");
	if(cellProp == "button") {
		var rowType = this.ds_bcncMastr.getRowType(0);	// detail
		if(rowType != Dataset.ROWTYPE_NORMAL) {
			this.gfn_message("info.항목.선택.알림", "수정하지 않은 행");
			return false;
		}
		
		// validation
		var bcncSe = this.ds_bcncMastr.getColumn(0, "BCNC_SE");
		switch(bcncSe) {
			case "C1": case "C9":	// C1:개인, C9:기타
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "BCNC_SE"))){
					this.gfn_message("comm.항목.입력.을", "거래처구분");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "USE_AT"))){
					this.gfn_message("comm.항목.입력.을", "사용여부");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "RPRSNTV"))){
					this.gfn_message("comm.항목.입력.을", "대표자");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "BCNC_NM"))){
					this.gfn_message("comm.항목.입력.을", "거래처명");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "BCNC_SE"))){
					this.gfn_message("comm.항목.입력.을", "거래처구분");
					return false;
				}
				break;
			default:
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "BCNC_SE"))){
					this.gfn_message("comm.항목.입력.을", "거래처구분");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "USE_AT"))){
					this.gfn_message("comm.항목.입력.을", "사용여부");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "RPRSNTV"))){
					this.gfn_message("comm.항목.입력.을", "대표자");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "BCNC_NM"))){
					this.gfn_message("comm.항목.입력.을", "거래처명");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "BCNC_SE"))){
					this.gfn_message("comm.항목.입력.을", "거래처구분");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "TELNO"))){
					this.gfn_message("comm.항목.입력.을", "전화번호");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "RN"))){
					this.gfn_message("comm.항목.입력.을", "도로명주소");
					return false;
				}
				if(this.comUtils.isNull(this.ds_bcncMastr.getColumn(0, "SMLPZ_SE"))){
					this.gfn_message("comm.항목.입력.을", "중소기업여부");
					return false;
				}
				break;
		}
		
		
		
		this.ds_rtn.clearData();
		this.ds_rtn.addRow();
		
		// 계좌 반환
		var findRow = this.ds_bcncAcnutnoList.findRow("MNBS_ACNUT_AT", '1');
		if(findRow > -1) {
			this.ds_rtn.copyRow(0, this.ds_bcncAcnutnoList, findRow);
		}
		
		// 거래처 반환
		this.ds_rtn.copyRow(0, this.ds_bcncMastr, 0);
		
		this.close(this.ds_rtn.saveXML());
	}
}


/*
 * 함수명  	: grd_bcncAcnutnoList_oncellclick
 * 설명 		: 거래처+계좌 선택
 *                
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_bcncAcnutnoList_oncellclick();
 */
this.grd_bcncAcnutnoList_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	// '선택'
	var cellProp = obj.getCellProperty("Body", e.cell, "displaytype");
	if(cellProp == "button") {
		var rowType  = this.ds_bcncAcnutnoList.getRowType(e.row);	// filter가 없으므로 grid와 1:1
		var forPopup = this.ds_bcncAcnutnoList.getColumn(e.row, "FOR_POPUP");
		if(rowType != Dataset.ROWTYPE_NORMAL && forPopup != "TRUE") {
			this.gfn_message("info.항목.선택.알림", "수정하지 않은 행");
			return false;
		}
		
		// gfn_message가 그냥 팝업이므로, 팝업메시지 호출중에 부모가 죽어(?) 버릴 경우 handle 오류가 발생 -> 그래서 이러한 번거로운 작업을 한번 더해줘야 함.
		_popupCallback = function() {
			this.ds_rtn.clearData();
			this.ds_rtn.addRow();
			this.ds_rtn.copyRow(0, this.ds_bcncMastr, 0);
			this.ds_rtn.copyRow(0, this.ds_bcncAcnutnoList, e.row);
			this.close(this.ds_rtn.saveXML());
		}
		
		if(forPopup == "TRUE") {
			this.gfn_message("info.항목.선택.알림", "선택하신 데이터는 계좌가 없어 임시 생성한 데이터입니다.\n이후에는 데이터를 등록하신 후, 계좌", null, null, _popupCallback);
			return false;
		} else {
			this.ds_rtn.clearData();
			this.ds_rtn.addRow();
			this.ds_rtn.copyRow(0, this.ds_bcncMastr, 0);
			this.ds_rtn.copyRow(0, this.ds_bcncAcnutnoList, e.row);
			this.close(this.ds_rtn.saveXML());
		}
	}
}



