{
    var comboParams = [
             ["ds_reqstTySe"    , "CTR035", "1", ""]       // 신청유형구분
            ,["ds_outsrcStleSe" , "CTR036", "1", ""]       // 도급형태구분
        ];

    this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
}

/*
 * 함수명      : fn_callbackAfter
 * 설명         : 공통 callback
 * params          : None
 * return Type     : None
 * 사용법        : this.fn_callbackAfter();
 */
