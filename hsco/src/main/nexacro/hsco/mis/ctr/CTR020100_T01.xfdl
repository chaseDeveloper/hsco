 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.fn_popupCallBack = function(strID, variant)
{
	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(variant);
	
	// 값이 존재하지 않을 경우에는 아무런 값을 덮어쓰지 않음
	if(!loadCnt) {
		return false;
	}
	
	switch(strID) {
		case "CTR020100_P01":
			break;
	}
}
