/*
 * 함수명        : fn_search
 * 설명            : 조회 이벤트 처리
 * params        : None
 * return Type    : None
 * 사용법        : this.fn_search();
 */
this.fn_search = function()
{
	if (Ex.util.isUpdated(this.ds_orderPlan_t1) || Ex.util.isUpdated(this.ds_orderPlan_t2) || Ex.util.isUpdated(this.ds_orderPlan_t3)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;    // 아니오
		}
    }
    
    //validation 체크
    if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
        return;
    }
  
	// clearData
	this.ds_orderPlanList.clearData();
	this.ds_orderPlan_t1.clearData();
	this.ds_orderPlan_t2.clearData();
	this.ds_orderPlan_t3.clearData();
   
	this.fn_transaction("selectOrderPlanList");	
}


/*
 * 함수명        : fn_search_budget1
 * 설명            : 발주별 예산 조회 이벤트 처리
 * params        : None
 * return Type    : None
 * 사용법        : this.fn_search_budget1();
 */
this.fn_search_budget1 = function()
{
	//validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	// clearData
	this.ds_orderBudget_t1.clearData();
	
	// parent data
	var pRow = this.ds_orderPlan_t1.rowposition;
	var orderPlanYear = this.ds_orderPlan_t1.getColumn(pRow, "ORDER_PLAN_YEAR");
	var orderPlanSn   = this.ds_orderPlan_t1.getColumn(pRow, "ORDER_PLAN_SN");
	
	// cond sub
	this.ds_cond_sub_t1.clearData();
	var nRow = this.ds_cond_sub_t1.addRow();
	this.ds_cond_sub_t1.setColumn(nRow, "ORDER_PLAN_YEAR", orderPlanYear);
	this.ds_cond_sub_t1.setColumn(nRow, "ORDER_PLAN_SN",   orderPlanSn  );
	
	// search
	this.fn_transaction("selectBudget1");
}


