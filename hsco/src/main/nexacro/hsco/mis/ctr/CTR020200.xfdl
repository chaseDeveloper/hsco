/*
 * 함수명        : fn_save
 * 설명            : 저장 이벤트 처리
 * params        : None
 * return Type    : None
 * 사용법        : this.fn_save();
 */
this.fn_save = function()
{
	if (!this.comUtils.isDatasetUpdated(this.ds_orderPlan_t1) &&
		!this.comUtils.isDatasetUpdated(this.ds_orderPlan_t2) &&
		!this.comUtils.isDatasetUpdated(this.ds_orderPlan_t3) &&
		!this.comUtils.isDatasetUpdated(this.ds_orderBudget_t1) &&
		!this.comUtils.isDatasetUpdated(this.ds_orderBudget_t2) &&
		!this.comUtils.isDatasetUpdated(this.ds_orderBudget_t3)
		) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	var tabIdx = this.tab_orderPlan.tabindex;
	if(tabIdx == 0) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	} else if(tabIdx == 1) {
		if( !this.gfn_checkValidation_NoUpdChk(this.ds_orderPlan_t1,   this.ds_validation) ||
			!this.gfn_checkValidation_NoUpdChk(this.ds_orderBudget_t1, this.ds_validation)		) {
			return false;
		}
		var pCnt = this.ds_orderPlan_t1.getRowCount();
		var rCnt = this.ds_orderBudget_t1.getRowCount();
		if(pCnt > 0 && rCnt < 1) {
			this.gfn_message("info.처리불가", "발주계획에 대한 예산내역이 존재하지 않습니다.\n예산내역을 입력하여 주십시오.");
			return false;
		}
		saveRow2 = this.ds_orderPlan_t1.rowposition;
	} else if(tabIdx == 2) {
		if( !this.gfn_checkValidation_NoUpdChk(this.ds_orderPlan_t2,   this.ds_validation) ||
			!this.gfn_checkValidation_NoUpdChk(this.ds_orderBudget_t2, this.ds_validation)		) {
			return false;
		}
		var pCnt = this.ds_orderPlan_t2.getRowCount();
		var rCnt = this.ds_orderBudget_t2.getRowCount();
		if(pCnt > 0 && rCnt < 1) {
			this.gfn_message("info.처리불가", "발주계획에 대한 예산내역이 존재하지 않습니다.\n예산내역을 입력하여 주십시오.");
			return false;
		}
		saveRow3 = this.ds_orderPlan_t2.rowposition;
	} else if(tabIdx == 3) {
		if( !this.gfn_checkValidation_NoUpdChk(this.ds_orderPlan_t3,   this.ds_validation) ||
			!this.gfn_checkValidation_NoUpdChk(this.ds_orderBudget_t3, this.ds_validation)		) {
			return false;
		}
		var pCnt = this.ds_orderPlan_t3.getRowCount();
		var rCnt = this.ds_orderBudget_t3.getRowCount();
		if(pCnt > 0 && rCnt < 1) {
			this.gfn_message("info.처리불가", "발주계획에 대한 예산내역이 존재하지 않습니다.\n예산내역을 입력하여 주십시오.");
			return false;
		}
		saveRow4 = this.ds_orderPlan_t3.rowposition;
	}

	if(!this.gfn_message("confirm.저장.여부")) {
		return false;
	}
	
	this.fn_transaction("saveOrderPlanList");
}


/*
 * 함수명        : fn_cancel
 * 설명            : 취소 이벤트 처리
 * params        : None
 * return Type    : None
 * 사용법        : this.fn_cancel ();
 */
this.fn_cancel= function()
{
	// 데이터셋 변경 체크
	if(Ex.util.isUpdated(this.ds_orderPlan_t1) || Ex.util.isUpdated(this.ds_orderPlan_t2) || Ex.util.isUpdated(this.ds_orderPlan_t3)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;
		}
	}

    if(!this.gfn_message("confirm.취소여부")) {
		return false;
    }
    
	this.ds_orderPlan_t1.reset();
	this.ds_orderPlan_t2.reset();
	this.ds_orderPlan_t3.reset();
	this.ds_orderPlan_t1.applyChange();
	this.ds_orderPlan_t2.applyChange();
	this.ds_orderPlan_t3.applyChange();
	
	this.ds_orderBudget_t1.reset();
	this.ds_orderBudget_t2.reset();
	this.ds_orderBudget_t3.reset();
	this.ds_orderBudget_t1.applyChange();
	this.ds_orderBudget_t2.applyChange();
	this.ds_orderBudget_t3.applyChange();
}


