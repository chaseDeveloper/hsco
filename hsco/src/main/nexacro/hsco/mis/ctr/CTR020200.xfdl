/*
 * 함수명      : fn_transaction
 * 설명         : Transaction 요청 처리 함수
 * params          : strSvcId : 서비스요청 ID
 * return Type     : None
 * 사용법        : this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
    switch (strSvcId) {
        case "selectOrderPlanList":
			var sController       = "hsco/mis/ctr/CTR020200/selectOrderPlanList.do";
			var sInDatasets       = "input1=ds_cond";
			var sOutDatasets      = "ds_orderPlanList=output4 ds_orderPlan_t1=output1 ds_orderPlan_t2=output2 ds_orderPlan_t3=output3";
			break;
        case "selectBudget1":
			var sController       = "hsco/mis/ctr/CTR020200/selectOrderBudget.do";
			var sInDatasets       = "input1=ds_cond_sub_t1";
			var sOutDatasets      = "ds_orderBudget_t1=output1";
			break;
		case "selectBudget2":
			var sController       = "hsco/mis/ctr/CTR020200/selectOrderBudget.do";
			var sInDatasets       = "input1=ds_cond_sub_t2";
			var sOutDatasets      = "ds_orderBudget_t2=output1";
			break;
		case "selectBudget3":
			var sController       = "hsco/mis/ctr/CTR020200/selectOrderBudget.do";
			var sInDatasets       = "input1=ds_cond_sub_t3";
			var sOutDatasets      = "ds_orderBudget_t3=output1";
			break;
        case "saveOrderPlanList":
			var sController      = "hsco/mis/ctr/CTR020200/saveOrderPlanList.do";
			var sInDatasets      = "input1=ds_orderPlan_t1:U input2=ds_orderPlan_t2:U input3=ds_orderPlan_t3:U";
			sInDatasets         += " input4=ds_orderBudget_t1:U input5=ds_orderBudget_t2:U input6=ds_orderBudget_t3:U";
			var sOutDatasets     = "";
			break;
    }
    Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

/*
 * 함수명      : fn_callBack
 * 설명         : Transaction Callback 처리
 * params          : strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type     : None
 * 사용법        :
*/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0'){
		switch (strSvcId) {
			case "selectOrderPlanList":
				var tabindex = this.tab_orderPlan.tabindex;
				if(tabindex == 1) {
					if(saveRow2 != -1) {
						this.ds_orderPlan_t1.set_rowposition(saveRow2);
						saveRow2 = -1;
					}
				} else if(tabindex == 2) {
					if(saveRow3 != -1) {
						this.ds_orderPlan_t2.set_rowposition(saveRow3);
						saveRow3 = -1;
					}
				} else if(tabindex == 3) {
					if(saveRow4 != -1) {
						this.ds_orderPlan_t3.set_rowposition(saveRow4);
						saveRow4 = -1;
					}
				}
				break;
			case "saveOrderPlanList":  
				this.fn_search();
				break;
		}
	}
}



