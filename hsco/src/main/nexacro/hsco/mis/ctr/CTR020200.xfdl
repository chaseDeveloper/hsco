this.CTR020200_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_init_form();
    this.fn_init_dataset();
    
	var comboParams = [
		  ["ds_prcureMthd"     , "CTR030", "Y", "S", "B"]       // 조달방식구분
		 ,["ds_unit"  	       , "CTR029", "Y", "S", "B"]       // 단위구분
		 ,["ds_ctr049"         , "CTR049", "Y", "S", "B"]       // 계약방법
		 ,["ds_com052"         , "COM052", "Y", "S", "B"]       // 승인여부
		 ,["ds_cntrctSe"       , "CTR022", "Y", "S", "B"]       // 계약구분
		 ,["ds_cntrctKnd"      , "CTR023", "Y", "S", "B"]       // 계약종류
		 ,["ds_cntrwkKnd"      , "CTR023", "Y", "S", "B"]       // 계약종류(공사)
		 ,["ds_servcKnd"       , "CTR023", "Y", "S", "B"]       // 계약종류(용역)
		 ,["ds_thngKnd"        , "CTR023", "Y", "S", "B"]       // 계약종류(물품)
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 부서 바인딩
		this.div_search.div_DeptComp.fn_setBind("ds_cond", "DEPT_CODE", "DEPT_NM");
		
		// 발주계획월 데이터셋 생성
		this.ds_orderPlanMt.clearData();
		for(var i = 1; i < 13; i++) {
			var nRow = this.ds_orderPlanMt.addRow();
			this.ds_orderPlanMt.setColumn(nRow, "CODE", i);
			this.ds_orderPlanMt.setColumn(nRow, "CODE_NM", i+"월");
		}
		
		// 검색조건 셋팅
		this.ds_cond.clearData();
		var nRow = this.ds_cond.addRow();
		var orderPlanYear = this.dateUtils.format(this.gfn_today(),"yyyy");
		if(orderPlanYear == '2017') {
			orderPlanYear = '2018'
		}
		this.ds_cond.setColumn(nRow, "ORDER_PLAN_YEAR", orderPlanYear);
		this.ds_cond.setColumn(nRow, "DEPT_CODE"      , this.gfn_getDeptId());
		this.ds_cond.setColumn(nRow, "DEPT_NM"        , this.gfn_getDeptName());
		
		if(this.gfn_authGrpId("CTR_ADMIN") != -1) {
			this.div_search.div_DeptComp.set_enable(true);
		}
		
		this.fn_search();
	};
    this.gfn_comboLoad(comboParams, callback);
}


