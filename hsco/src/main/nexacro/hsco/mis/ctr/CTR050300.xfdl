    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 공공구매목표관리
 * 02. 화면명   : CTR050300.xfdl
 * 03. 화면설명 : 공공구매목표율을 관리한다.
 * 04. 작성일   : 2020-04-28
 * 05. 작성자   : 권재만
 * 06. 수정이력 : 
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *    2020-04-28   정윤원   최초작성 
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include
 ************************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";


this.CTR050300_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	var today = this.dateUtils.today();
	
	var comboParams = [
	     ["ds_ctr022", "CTR022", "Y", "A", "B"]	// 계약구분
	    ,["ds_ctr030", "CTR030", "Y", "A", "B"]
	];
	var callback = function (svcId, errorCode, errorMsg) {
		this.fn_search();
	};
	this.gfn_comboLoad(comboParams, callback);
}

/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/
 
/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_data.clearData();
	
	// transaction
	this.fn_transaction("select");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// save
	if(this.gfn_message("confirm.저장.여부"))	{
		this.fn_transaction("save");
	}
}

 
/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "select":		// 조회
			var sController   	= "hsco/mis/ctr/CTR050300/select.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_data=output1";
			break;
		case "save":		// 저장
			var sController   	= "hsco/mis/ctr/CTR050300/save.do";
			var sInDatasets   	= "input1=ds_data:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 


/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0'){
		switch (strSvcId) {
			case "select":		// 조회
				break;
			case "save":		// 저장
				this.gfn_message("success.처리.성공"); 
				this.fn_search();	
				break;
		}
	}
}



/**********************************************************************
* Transaction 영역 끝
***********************************************************************/

