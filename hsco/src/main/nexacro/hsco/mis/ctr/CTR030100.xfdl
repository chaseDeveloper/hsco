 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0'){
		
		switch (strSvcId) {
			
			case "selectCntrct":	// 조회
				// sub Search
				this.fn_search_tp6();	// 첨부파일 조회(하위탭에서 제어 가능하도록 분리)
				this.fn_search_tab();	// 각 탭 조회
				break;
				
			case "selectTab":
				var cntrctSe = this.ds_cntrct.getColumn(0, "CNTRCT_SE");
				if(cntrctSe == '3') {
					this.tab_cntrct.tp11.set_enable(true);
				} else {
					this.tab_cntrct.tp11.set_enable(false);
				}
				break;
			case "saveCntrct":		// 저장
				this.fn_search();
				break;
			case "saveDeleteCntrct":
				// [탭02] 감독관
				this.ds_mangrReqstDtls.clearData();
				
				// [탭03] 배정예산
				this.ds_cntrctBudget.clearData();
				
				// [탭04] 입찰공고
				this.ds_bidDtls.clearData();
				
				// [탭05] 계약업체
				this.ds_entrpsAcctoCntrctDtls.clearData();
				this.ds_inorgCtnuCntrwk.clearData();
				
				// [탭06] 하도급업체 
				this.ds_entrpsAcctoSubcntrCnt.clearData();
				
				// [탭07] 보증
				this.ds_cntrctAssrncDtls.clearData();
				this.ds_flawAssrncDtls.clearData();
				this.ds_entrpsAcctoSldrtyAssr.clearData();
				
				// [탭08] 채권가압류
				this.ds_cditPrsszr.clearData();
				
				// [탭10] 계약변경
				this.ds_cntrctChange.clearData();
				this.ds_entrpsAcctoCntrctChan.clearData();
				this.ds_entrpsCntrctAssrncCha.clearData();
				
				// [탭11] 물품
				this.ds_cntrctThngDetail.clearData();
				
				// [탭12] 지체배상금
				this.ds_delayIndmnty.clearData();
				
				// [탭13] 설계자감리자(미확정채권양도)
				this.ds_unDcsnCditTrnsfr.clearData();
				this.ds_dsgnrSprvisor.clearData();
				break;
			default:
				break;
		}
	}
}


 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 계약번호 선택(검색조건)
	if(sPopupId == "CTR030100_P08") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
			var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
			var cntrcyYr = cntrctNo.substr(0,4);
			
			this.ds_cond.setColumn(0, "CNTRCT_YEAR", cntrcyYr);	// 계약년도
			this.ds_cond.setColumn(0, "CNTRCT_NO",   cntrctNo);	// 계약번호
			this.ds_cond.setColumn(0, "CNTRCT_NM",   cntrctNm);	// 계약명
			this.fn_search();
		}
	}
	
	// 의뢰번호 선택(계약대장)
	if(sPopupId == "CTR030100_P09") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var reqestNo = dsTemp.getColumn(0, "REQEST_NO");
			var reqestNm = dsTemp.getColumn(0, "REQEST_NM");
			this.ds_cntrct.setColumn(0, "REQEST_NO", reqestNo);	// 의뢰번호
			this.ds_cntrct.setColumn(0, "REQEST_NM", reqestNm);	// 의뢰명
		}
	}
	
	// 의뢰번호 선택(계약대장)
	if(sPopupId == "CTR030100_P08_UPPER_CNTRCT_NO") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
			var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
			this.ds_cntrct.setColumn(0, "UPPER_CNTRCT_NO",   cntrctNo);	// 계약번호
			this.ds_cntrct.setColumn(0, "UPPER_CNTRCT_NM",   cntrctNm);	// 계약명
		}
	}
	
	// 사업코드 선택(계약대장)
	if(sPopupId == "popBsns") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var bsnsCode = dsTemp.getColumn(0, "BSNS_CODE");
			var bsnsNm   = dsTemp.getColumn(0, "BSNS_NM");
			this.ds_cntrct.setColumn(0, "BSNS_CODE", bsnsCode);	// 사업코드
			this.ds_cntrct.setColumn(0, "BSNS_NM",   bsnsNm  );	// 사업명
		}
	}
}



