/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(계약대장)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// update check
	if (   !this.comUtils.isDatasetUpdated(this.ds_cntrct)					// 계약대장
	    && !this.comUtils.isDatasetUpdated(this.ds_mangrReqstDtls)			// [탭02] 감독관			: 감독관지정
	    && !this.comUtils.isDatasetUpdated(this.ds_cntrctBudget)			// [탭03] 배정예산			: 예산배정내역
	    && !this.comUtils.isDatasetUpdated(this.ds_bidDtls)					// [탭04] 입찰공고			: 입찰공고
	    && !this.comUtils.isDatasetUpdated(this.ds_entrpsAcctoCntrctDtls)	// [탭05] 계약업체			: 업체별계약내역
	    && !this.comUtils.isDatasetUpdated(this.ds_inorgCtnuCntrwk)			// [탭05] 계약업체			: 장기계속공사
	    && !this.comUtils.isDatasetUpdated(this.ds_entrpsAcctoSubcntrCnt)	// [탭06] 하도급업체		: 하도급업체
	    && !this.comUtils.isDatasetUpdated(this.ds_cntrctAssrncDtls)		// [탭07] 보증				: 계약보증
	    && !this.comUtils.isDatasetUpdated(this.ds_flawAssrncDtls)			// [탭07] 보증				: 하자보증
	    && !this.comUtils.isDatasetUpdated(this.ds_entrpsAcctoSldrtyAssr)	// [탭07] 보증				: 업체별연대보증인
	    && !this.comUtils.isDatasetUpdated(this.ds_cditPrsszr)				// [탭08] 채권가압류		: 채권가압류
	    && !this.comUtils.isDatasetUpdated(this.ds_cntrctThngDetail)		// [탭11] 물품				: 계약물품
	    && !this.comUtils.isDatasetUpdated(this.ds_delayIndmnty)			// [탭12] 지체배상금		: 지체배상금
	    && !this.comUtils.isDatasetUpdated(this.ds_dsgnrSprvisor)			// [탭13] 설계자감리자	: 설계자감리자
	   ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// 2020.03.11
	// 감독관 validation
	var mangrReqstDtlsCnt = this.ds_mangrReqstDtls.rowcount;
	// 계약업체 validation
	var entrpsAcctoCntrctCnt = this.ds_entrpsAcctoCntrctDtls.rowcount;
	
	
	// validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cntrct, this.ds_validation)) {
		// 계약대장
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_inorgCtnuCntrwk, this.ds_validation)) {
		// [탭01] 계약업무처리과정: 장기계속공사
		this.tab_cntrct.set_tabindex(0);
		return false;
	} else if((!this.gfn_checkValidation_NoUpdChk(this.ds_mangrReqstDtls, this.ds_validation)) || (mangrReqstDtlsCnt < 1)) {
		if(mangrReqstDtlsCnt < 1) {
			this.gfn_message("info.처리불가", "감독관 목록을\n입력하십시오.");
		}
		// [탭02] 감독관지정		: 감독관지정
		this.tab_cntrct.set_tabindex(1);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_cntrctBudget, this.ds_validation)) {
		// [탭03] 배정예산			: 예산배정내역
		this.tab_cntrct.set_tabindex(2);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_bidDtls, this.ds_validation)) {
		// [탭04] 입찰공고			: 입찰공고
		this.tab_cntrct.set_tabindex(3);
		return false;
	} else if((!this.gfn_checkValidation_NoUpdChk(this.ds_entrpsAcctoCntrctDtls, this.ds_validation)) || (entrpsAcctoCntrctCnt < 1)) {
		if(entrpsAcctoCntrctCnt < 1) {
			this.gfn_message("info.처리불가", "계약업체 목록을\n입력하십시오.");
		}
		if(this.infrmlCntrctAt == '1') {
			this.tab_cntrct.set_tabindex(3);
		}else {
			// [탭05] 계약업체			: 업체별계약내역
			this.tab_cntrct.set_tabindex(4);
		}
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_entrpsAcctoSubcntrCnt, this.ds_validation)) {
		// [탭06] 하도급업체 		: 하도급업체
		this.tab_cntrct.set_tabindex(5);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_cntrctAssrncDtls, this.ds_validation)) {
		// [탭07] 보증				: 계약보증
		this.tab_cntrct.set_tabindex(6);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_flawAssrncDtls, this.ds_validation)) {
		// [탭07] 보증				: 하자보증
		this.tab_cntrct.set_tabindex(6);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_entrpsAcctoSldrtyAssr, this.ds_validation)) {
		// [탭07] 보증				: 업체별연대보증인
		this.tab_cntrct.set_tabindex(6);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_cditPrsszr, this.ds_validation)) {
		// [탭08] 채권가압류		: 채권가압류
		this.tab_cntrct.set_tabindex(7);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_cntrctThngDetail, this.ds_validation)) {
		// [탭11] 물품				: 계약물품
		this.tab_cntrct.set_tabindex(10);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_delayIndmnty, this.ds_validation)) {
		// [탭12] 지체배상금		: 지체배상금
		this.tab_cntrct.set_tabindex(11);
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_dsgnrSprvisor, this.ds_validation)) {
		// [탭13] 기타				: 설계자감리자
		this.tab_cntrct.set_tabindex(12);
		return false;
	}
	
	// 계약업체 탭 : 지분율 합 100% 넘지 않게
	var qotaRtSum = this.ds_entrpsAcctoCntrctDtls.getSum("QOTA_RT");
	if(qotaRtSum > 100){
		this.gfn_message("errors.값확인.요청", "지분율의합이 100% 가|지분율을");
		this.tab_cntrct.set_tabindex(4);
		return;
	}
	
	// 계약업체 탭 : 업체별금액 합이 계약금액을 넘지 않게
	var cntrctAmount = this.ds_cntrct.getColumn(0, "CNTRCT_AMOUNT");
	var chkAmount = this.ds_entrpsAcctoCntrctDtls.getSum("CNTRCT_AMOUNT");
	if(chkAmount > cntrctAmount) {
		this.gfn_message("errors.값확인.요청", "업체별 계약금액의 합이 총 계약금액 이하가|계약금액을");
		this.tab_cntrct.set_tabindex(4);
		return;
	}
	
	// save
	if(this.gfn_message("confirm.저장.여부"))	{
		this.fn_transaction("saveCntrct");
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	switch (strSvcId) {
		case "selectCntrct":	// 계약대장 조회
			var sController   	= "/hsco/mis/ctr/CTR030100/selectCntrct.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cntrct=output1";
			break;
		case "selectTab":	// 각 탭 서브데이터 조회
			var bsnsCode = this.ds_cntrct.getColumn(0, "BSNS_CODE");
			var cntrctNo = this.ds_cntrct.getColumn(0, "CNTRCT_NO");
			var reqestNo = this.ds_cntrct.getColumn(0, "REQEST_NO");
			this.ds_cond_tab.clearData();
			var nRow = this.ds_cond_tab.addRow();
			this.ds_cond_tab.setColumn(nRow, "BSNS_CODE", bsnsCode);
			this.ds_cond_tab.setColumn(nRow, "CNTRCT_NO", cntrctNo);
			this.ds_cond_tab.setColumn(nRow, "REQEST_NO", reqestNo);
			
			var sController   	= "/hsco/mis/ctr/CTR030100/selectTab.do";
			var sInDatasets   	= "input1=ds_cond_tab";
			var sOutDatasets  	= "";
			// [탭01] 계약업무처리과정
			sOutDatasets  	   += " ds_cntrctJobProcessCrse=output1Tab1";
			// [탭02] 감독관
			sOutDatasets  	   += " ds_curMangr=output2Tab1";
			sOutDatasets  	   += " ds_mangrReqstDtls=output2Tab2";
			// [탭03] 배정예산
			sOutDatasets  	   += " ds_cntrctBudget=output3Tab1";
			// [탭04] 입찰공고
			sOutDatasets  	   += " ds_bidDtls=output4Tab1";
			// [탭05] 계약업체
			sOutDatasets  	   += " ds_entrpsAcctoCntrctDtls=output5Tab1";
			sOutDatasets  	   += " ds_inorgCtnuCntrwk=output5Tab2";
			// [탭06] 하도급업체 
			sOutDatasets  	   += " ds_entrpsAcctoSubcntrCnt=output6Tab1";
			// [탭07] 보증
			sOutDatasets  	   += " ds_cntrctAssrncDtls=output7Tab1";
			sOutDatasets  	   += " ds_flawAssrncDtls=output7Tab2";
			sOutDatasets  	   += " ds_entrpsAcctoSldrtyAssr=output7Tab3";
			// [탭08] 채권가압류
			sOutDatasets  	   += " ds_cditPrsszr=output8Tab1";
			// [탭10] 계약변경
			sOutDatasets  	   += " ds_cntrctChange=output10Tab1";
			sOutDatasets  	   += " ds_entrpsAcctoCntrctChan=output10Tab2";
			sOutDatasets  	   += " ds_entrpsCntrctAssrncCha=output10Tab3";
			// [탭11] 물품
			sOutDatasets  	   += " ds_cntrctThngDetail=output11Tab1";
			// [탭12] 지체배상금
			sOutDatasets  	   += " ds_delayIndmnty=output12Tab1";
			// [탭13] 설계자감리자
			sOutDatasets  	   += " ds_dsgnrSprvisor=output13Tab1";
			break;
		case "selectTp6":		// 첨부파일 조회
			var cRow   = this.ds_cntrct.rowposition;
			var fileSn = this.ds_cntrct.getColumn(cRow, "FILE_SN");
			
			this.ds_cond_subTp6.clearData();
			var nRow = this.ds_cond_subTp6.addRow();
			this.ds_cond_subTp6.setColumn(nRow, "FILE_SN", fileSn);
			
			var sController   	= "/hsco/mis/ctr/CTR030100/selectTp6.do";
			var sInDatasets   	= "input1=ds_cond_subTp6";
			var sOutDatasets  	= "ds_fileList=output6Tab1";
			break;
		case "saveCntrct":		// 저장
			var sController   	= "hsco/mis/ctr/CTR030100/saveCntrct.do";
			var sOutDatasets  	= "ds_cond=outputSrch";
			var sInDatasets   	= "inputSrch=ds_cond:A input1=ds_cntrct:U";
			// [탭02] 감독관
			sInDatasets  	   += " input2Tab1=ds_mangrReqstDtls:U";
			// [탭03] 배정예산
			sInDatasets  	   += " input3Tab1=ds_cntrctBudget:U";
			// [탭04] 입찰공고
			sInDatasets  	   += " input4Tab1=ds_bidDtls:U";
			// [탭05] 계약업체
			sInDatasets  	   += " input5Tab1=ds_entrpsAcctoCntrctDtls:U";
			sInDatasets  	   += " input5Tab2=ds_inorgCtnuCntrwk:U";
			// [탭06] 하도급업체 
			sInDatasets  	   += " input6Tab1=ds_entrpsAcctoSubcntrCnt:U";
			// [탭07] 보증
			sInDatasets  	   += " input7Tab1=ds_cntrctAssrncDtls:U";
			sInDatasets  	   += " input7Tab2=ds_flawAssrncDtls:U";
			sInDatasets  	   += " input7Tab3=ds_entrpsAcctoSldrtyAssr:U";
			// [탭08] 채권가압류
			sInDatasets  	   += " input8Tab1=ds_cditPrsszr:U";
			// [탭10] 계약변경
			sInDatasets  	   += " input10Tab1=ds_cntrctChange:U";
			sInDatasets  	   += " input10Tab2=ds_entrpsAcctoCntrctChan:U";
			sInDatasets  	   += " input10Tab3=ds_entrpsCntrctAssrncCha:U";
			// [탭11] 물품
			sInDatasets  	   += " input11Tab1=ds_cntrctThngDetail:U";
			// [탭12] 지체배상금
			sInDatasets  	   += " input12Tab1=ds_delayIndmnty:U";
			// [탭13] 설계자감리자
			sInDatasets  	   += " input13Tab1=ds_dsgnrSprvisor:U";
			break;
		case "saveDeleteCntrct":
			var sController   	= "hsco/mis/ctr/CTR030100/deleteCntrct.do";
			var sInDatasets   	= "input1=ds_cntrct:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}



