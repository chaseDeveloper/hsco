/*
 * 함수명  	: ds_cntrct_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(계약대장 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cntrct_oncolumnchanged(obj,e);
 */
this.ds_cntrct_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "CNTRCT_SE") {
		if(e.newvalue == '3') {
			this.tab_cntrct.tp11.set_enable(true);
		} else {
			this.ds_cntrctThngDetail.clearData();
			this.tab_cntrct.tp11.set_enable(false);
		}
	}
	
	// (계약대장) 낙찰율 계산
	if(e.columnid == "SCSBID_AMOUNT" || e.columnid == "PREARNGE_PC") {
		var scsbidAmount = e.columnid == "SCSBID_AMOUNT"       ? e.newvalue   : obj.getColumn(e.row, "SCSBID_AMOUNT");
		var prearngePc   = e.columnid == "PREARNGE_PC"         ? e.newvalue   : obj.getColumn(e.row, "PREARNGE_PC"  );
		scsbidAmount     = !this.comUtils.isNull(scsbidAmount) ? scsbidAmount : 0;	// 총 계약금액
		prearngePc       = !this.comUtils.isNull(prearngePc)   ? prearngePc   : 0;	// 예정가격
		
		var scsbidRt     = prearngePc == 0 ? 0 : nexacro.round(scsbidAmount / prearngePc * 100, 2);
		obj.setColumn(e.row, "SCSBID_RT", scsbidRt);
	}
	
	// (계약업체내역 Tab) 지분율에 따른 계약금액 계산
	if(e.columnid == "CNTRCT_AMOUNT") {
		var cntrctAmount = !this.comUtils.isNull(e.newvalue) ? e.newvalue : 0;		// 총 계약금액
		
		for(var fRow = 0; fRow < this.ds_entrpsAcctoCntrctDtls.getRowCount(); fRow++) {
			var qotaRt = this.ds_entrpsAcctoCntrctDtls.getColumn(fRow, "QOTA_RT");
			qotaRt     = !this.comUtils.isNull(qotaRt) ? qotaRt : 0;				// 지분율
			var cntrctAmount = nexacro.round(cntrctAmount * qotaRt / 100);			// 업체별_계약금액
			
			this.ds_entrpsAcctoCntrctDtls.setColumn(fRow, "CNTRCT_AMOUNT", cntrctAmount);
		}
	}
	
	if(e.columnid == "UPPER_CNTRCT_NO") {
		var newVal = e.newvalue;
		if(this.comUtils.isNull(newVal)) {
			obj.setColumn(e.row, "UPPER_CNTRCT_NM", "");
			obj.setColumn(e.row, "UPPER_CNTRCT_NO", "");
		} else {
			if(newVal.indexOf(' ') > -1) {
				obj.setColumn(e.row, "UPPER_CNTRCT_NM", "");
				obj.setColumn(e.row, "UPPER_CNTRCT_NO", "");
			}
		}
	}
}


/*
 * 함수명  	: ds_entrpsAcctoCntrctDtls_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(계약업체 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_entrpsAcctoCntrctDtls_oncolumnchanged(obj,e);
 */
this.ds_entrpsAcctoCntrctDtls_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	// 지분율
	if(e.columnid == "QOTA_RT") {
		var cntrctAmount = this.ds_cntrct.getColumn(0, "CNTRCT_AMOUNT");	// 총계약금액
		var qotaRt       = e.newvalue;										// 지분율
		
		cntrctAmount = !this.comUtils.isNull(cntrctAmount) ? cntrctAmount : 0;
		qotaRt       = !this.comUtils.isNull(qotaRt)       ? qotaRt       : 0;
		
		//계약업체내역 탭 : 지분율 합 100% 넘지 않게
		var qotaRtSum = obj.getSum(e.columnid);
		if(qotaRtSum > 100){
			this.gfn_message("errors.값확인.요청", "지분율의합이 100% 가|지분율을");
		}
		
		var cntrctAmount = nexacro.round(cntrctAmount * qotaRt / 100);
		obj.setColumn(e.row, "CNTRCT_AMOUNT", cntrctAmount);	// (업체별)계약금액
	}
	
	// 업체별 계약금액
	if(e.columnid == "CNTRCT_AMOUNT") {
		var cntrctAmount = this.ds_cntrct.getColumn(0, "CNTRCT_AMOUNT");
		var chkAmount = obj.getSum(e.columnid);
		
		if(chkAmount > cntrctAmount) {
			this.gfn_message("errors.값확인.요청", "업체별 계약금액의 합이 총 계약금액 이하가|계약금액을");
		}
	}
}


/*
 * 함수명  	: ds_cntrctThngDetail_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(계약물품 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_cntrctThngDetail_oncolumnchanged(obj,e);
 */
this.ds_cntrctThngDetail_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "QY" || e.columnid == "UNTPC") {
		var qy      = e.columnid == "QY"    ? e.newvalue : obj.getColumn(e.row, "QY"   );	// 수량
		var untPc   = e.columnid == "UNTPC" ? e.newvalue : obj.getColumn(e.row, "UNTPC");	// 단가
		qy    = !this.comUtils.isNull(qy)    ? qy    : 0;
		untPc = !this.comUtils.isNull(untPc) ? untPc : 0;
		
		var amount = nexacro.round(qy * untPc);
		obj.setColumn(e.row, "AMOUNT", amount);
	}
}


/*
 * 함수명  	: ds_curMangr_oncolumnchanged
 * 설명 		: Dataset 컬럼 변경 후 이벤트 처리(현행감독관 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_curMangr_oncolumnchanged(obj,e);
 */
this.ds_curMangr_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "CONFM_AT") {
		var confmerEmpno = this.gfn_getEmpNo();
		var confmerEmpnm = this.gfn_getUserName();
		
		if(e.newvalue == "1") {
			obj.setColumn(e.row, "CONFMER_EMPNO", confmerEmpno);
			obj.setColumn(e.row, "CONFMER_EMPNM", confmerEmpnm);
		} else {
			obj.setColumn(e.row, "CONFMER_EMPNO", '');
			obj.setColumn(e.row, "CONFMER_EMPNM", '');
		}
	}
}


/*
 * 함수명  	: fn_excelUpload
 * 설명 		: Excel Import 작업
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_excelUpload(obj,e);
 */
this.fn_excelUpload = function()
{
	var arg = {
		targetDs : this.ds_import,
		callBack : this.excelImport_callBack
	};	
	this.fn_importExcel(arg);
}


/*
 * 함수명  	: fn_importExcel
 * 설명 		: Excel Import 작업
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_importExcel(obj,e);
 */
this.fn_importExcel = function(arg)
{
	var importObj, objNm = "_common_excel_import";
	
	importObj = new nexacro.ExcelImportObject(objNm, this);
	this[objNm] = importObj;
	importObj.set_importtype(nexacro.ImportTypes.EXCEL);
	importObj.set_importtype(nexacro.ImportTypes.EXCEL);
	importObj.addEventHandler("onsuccess", this._gfn_excelImport_onsuccess, this);
	importObj.addEventHandler("onerror", this._gfn_excelImport_onerror, this);
	importObj.set_importurl(application.services["svcurl"].url + "XImport.do");
	
	importObj.user_callBack = null;
	
	var objTargetDs = null;

    if(arg.targetDs) {
		objTargetDs = arg.targetDs.name;
    }

    if (typeof arg.callBack == "function") {
		importObj.user_callBack = arg.callBack;
    }
	
    importObj.importData("", "[command=getsheetdata;output=outDs;]", "["+objTargetDs+"=outDs]","");
}


/*
 * 함수명  	: excelImport_callBack
 * 설명 		: Excel File Upload Callback
 * params  		: None
 * return Type 	: None
 * 사용법		: this.excelImport_callBack();
 */
this.excelImport_callBack = function()
{
	if(this.ds_import.rowcount > 0) {
		var uploadMode = 0;
		
		if(this.ds_import.findRow("Column0", "납품요구번호:") > -1) {			// 2번째 열부터 데이터를 끌어와서 계산
			uploadMode = '1';
		} else if(this.ds_import.findRow("Column1", "납품요구번호:") > -1) {	// 1번째 열부터 데이터를 끌어와서 계산
			uploadMode = '2';
		}
		
		if(uploadMode == '1') {
			var strDvyfgDemandNo = this.ds_import.getColumn(this.ds_import.findRow("Column0", "납품요구번호:"), "Column6");	// 납품요구번호(문자)
			var dvyfgDemandDe    = this.ds_import.getColumn(this.ds_import.findRow("Column0", "납품요구번호:"), "Column21");	// 납품요구일자
			
			// 물품정보를 가져오기 위해 시작행 및 종료행 설정
			var stRow    = this.ds_import.findRow("Column19", "납품장소");	// 납품장소가 존재하는 행
			var edRow    = -1;
			var stGrdRow = -1;		// 물품정보가 시작되는 행
			
			// 종료행 선택의 기본조건(순번 행이 더이상 존재하지 않는 행을 찾아야 함)
			for(var snRow = 0; snRow < this.ds_import.rowcount; snRow++) {
				// 물품정보 시작 위치
				if(this.ds_import.getColumn(snRow, "Column0") == "1") {
					stGrdRow = snRow;
				}
				// 물품정보 시작 위치 잡히면 처리
				if(stGrdRow > 0) {
					if(this.ds_import.getColumn(snRow, "Column15") == "수 량 합 계 :") {
						edRow = snRow;
						break;
					}
				}
			}
			
			if(edRow < 0) {
				edRow = this.ds_import.rowcount - 1;
			}
			
			var dvyfgDemandNo = strDvyfgDemandNo.replace(/-/g, "");	// 납품요구번호에서 숫자만 남김
			
			// 임시 dataset에 설정
			this.ds_tempCntrctThng.clearData();
			
			for(var cRow = stRow + 1; cRow < edRow; cRow += 2) {
				var nRow = this.ds_tempCntrctThng.addRow();
				var pRow = this.ds_cntrct.rowposition;
				var strUnitSe = this.ds_import.getColumn(cRow, "Column14");	// 단위
				var fRow      = this.ds_ctr029.findRow("CODE_NM", strUnitSe);
				var unitSe    = this.ds_ctr029.getColumn(fRow, "CODE");		// 단위구분
				
				this.ds_tempCntrctThng.setColumn(nRow, "CNTRCT_NO"      , this.ds_cntrct.getColumn(pRow, "CNTRCT_NO"));		// 의뢰번호
				this.ds_tempCntrctThng.setColumn(nRow, "THNG_CL_NO"     , this.ds_import.getColumn(cRow, "Column3"));		// 물품분류번호
				this.ds_tempCntrctThng.setColumn(nRow, "THNG_IDNTFC_NO" , this.ds_import.getColumn(cRow, "Column8"));		// 물품식별번호
				this.ds_tempCntrctThng.setColumn(nRow, "THNG_CL_NM"     , this.ds_import.getColumn(cRow, "Column10"));		// 물품분류명
				this.ds_tempCntrctThng.setColumn(nRow, "STNDRD_NM"      , this.ds_import.getColumn(cRow + 1, "Column10"));	// 규격명
				this.ds_tempCntrctThng.setColumn(nRow, "QY"             , this.ds_import.getColumn(cRow, "Column19"));		// 수량
				this.ds_tempCntrctThng.setColumn(nRow, "UNTPC"          , this.ds_import.getColumn(cRow, "Column16"));		// 단가
				this.ds_tempCntrctThng.setColumn(nRow, "UNIT_SE"        , unitSe);											// 단위구분
				this.ds_tempCntrctThng.setColumn(nRow, "AMOUNT"         , this.ds_import.getColumn(cRow + 1, "Column16"));	// 금액
				this.ds_tempCntrctThng.setColumn(nRow, "DVYFG_MTHD"     , "001");											// 납품방식(일괄납품으로 고정)
				this.ds_tempCntrctThng.setColumn(nRow, "DVYFG_CO"       , "1");												// 납품횟수(1회로 고정)
				this.ds_tempCntrctThng.setColumn(nRow, "DVYFG_PLACE"    , this.ds_import.getColumn(cRow + 1, "Column19"));	// 납품장소
				this.ds_tempCntrctThng.setColumn(nRow, "DVYFG_TMLMT_DE" , this.ds_import.getColumn(cRow, "Column23"));		// 납기일
				this.ds_tempCntrctThng.setColumn(nRow, "DVYFG_DEMAND_NO", dvyfgDemandNo);									// 납품요구번호
				this.ds_tempCntrctThng.setColumn(nRow, "DVYFG_DEMAND_DE", dvyfgDemandDe);									// 납품요구일자
			}
			
			// 임시 dataset에서 물품 dataset으로 복사
			this.ds_cntrctThngDetail.set_enableevent(false);
			for(var cpRow = 0; cpRow < this.ds_tempCntrctThng.rowcount; cpRow++) {
				var nRow = this.ds_cntrctThngDetail.addRow();
				this.ds_cntrctThngDetail.copyRow(nRow, this.ds_tempCntrctThng, cpRow);
			}
			this.ds_cntrctThngDetail.set_enableevent(true);
		}
	}
}
this.ds_mangrReqstDtls_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "CONFM_AT") {
		var confmerEmpno = this.gfn_getEmpNo();
		var confmerEmpnm = this.gfn_getUserName();

		if (e.newvalue == "1") {
			obj.setColumn(e.row, "CONFMER_EMPNO", confmerEmpno);
			obj.setColumn(e.row, "CONFMER_EMPNM", confmerEmpnm);
			obj.setColumn(e.row, "CONFM_DE", this.comUtils.getClientDate("YYYYMMDD"));
		}
		else {
			obj.setColumn(e.row, "CONFMER_EMPNO", '');
			obj.setColumn(e.row, "CONFMER_EMPNM", '');
			obj.setColumn(e.row, "CONFM_DE", '');
		}
	}
}
