	   ,["ds_ctr040", "CTR040", "Y", "D", "B"]	// ds_ctr040 : 도급방법
	   ,["ds_ctr042", "CTR042", "Y", "D", "B"]	// ds_ctr042 : 장기계약구분
	   ,["ds_ctr043", "CTR043", "Y", "D", "B"]	// ds_ctr043 : 진행구분
	   ,["ds_ctr044", "CTR044", "Y", "D", "B"]	// ds_ctr044 : 계약변경
	   ,["ds_ctr045", "CTR045", "Y", "D", "B"]	// ds_ctr045 : 계약업체변경
	   ,["ds_ctr046", "CTR046", "Y", "D", "B"]	// ds_ctr046 : 주관여부
	   ,["ds_ctr050", "CTR050", "Y", "D", "B"]	// ds_ctr050 : 계약접수
	   ,["ds_prj021", "PRJ021", "Y", "D", "B"]	// ds_prj021 : 사업공통코드(공종목적)
	];
