 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	/** 계약대장관리에 버튼을 통째로 빼버림(간이계약에서는 다이렉트 접근 / 현재화면에서는 개발자용 버튼으로 제어)  */
	var cntrctYear = this.ds_cond.getColumn(0, "CNTRCT_YEAR");
	if(this.comUtils.isNull(cntrctYear)) {
		this.gfn_message("comm.항목.입력.을", "'계약년도'")
		return -1;
	}
	
	// 검색조건 초기화
	this.ds_cond.setColumn(0, "CNTRCT_NO", "");
	this.ds_cond.setColumn(0, "CNTRCT_NM", "");
	
	// [탭01] 계약업무처리과정
	this.ds_cntrctJobProcessCrse.clearData();
	
	// [탭02] 감독관
	this.ds_mangrReqstDtls.clearData();
	
	// [탭03] 배정예산
	this.ds_cntrctBudget.clearData();
	
	// [탭04] 입찰공고
	this.ds_bidDtls.clearData();
	
	// [탭05] 계약업체
	this.ds_entrpsAcctoCntrctDtls.clearData();
	this.ds_inorgCtnuCntrwk.clearData();
	
	// [탭06] 하도급업체 
	this.ds_entrpsAcctoSubcntrCnt.clearData();
	
	// [탭07] 보증
	this.ds_cntrctAssrncDtls.clearData();
	this.ds_flawAssrncDtls.clearData();
	this.ds_entrpsAcctoSldrtyAssr.clearData();
	
	// [탭08] 채권가압류
	this.ds_cditPrsszr.clearData();
	
	// [탭10] 계약변경
	this.ds_cntrctChange.clearData();
	this.ds_entrpsAcctoCntrctChan.clearData();
	this.ds_entrpsCntrctAssrncCha.clearData();
	
	// [탭11] 물품
	this.ds_cntrctThngDetail.clearData();
	
	// [탭12] 지체배상금
	this.ds_delayIndmnty.clearData();
	
	// [탭13] 설계자감리자
	this.ds_dsgnrSprvisor.clearData();
	
	// 계약대장 초기화
	this.ds_cntrct.clearData();
	var nRow = this.ds_cntrct.addRow();
	this.ds_cntrct.setColumn(nRow, "CNTRCT_YEAR",      cntrctYear);
	this.ds_cntrct.setColumn(nRow, "INFRML_CNTRCT_AT", this.infrmlCntrctAt);
	
	return nRow;
}


