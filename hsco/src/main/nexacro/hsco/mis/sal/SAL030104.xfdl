		return;
	}
	
	if(!this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"CLOS_AT") ){
		if ( this.gfn_message("confirm.삭제여부")) { 
			var nRow = this.ds_mtDdcDtls.rowposition;
			this.ds_mtDdcDtls.deleteRow(nRow);
		}	
	} else if(this.div_search.cmb_pymntSe.value == "") { 
	    this.alert("지급구분을 선택하세요."); 
	} else {
	    this.alert("확정된 내역은 삭제할수 없습니다."); 	
	}  	 
}

