	/*
	this.ds_mtSalaryPymntList.setColumn(nRow, "PYMNT_YM",	this.ds_pymntCreate.getColumn("0", "PYMNT_YM"));
	this.ds_mtSalaryPymntList.setColumn(nRow, "SALARY_SE",	this.ds_pymntCreate.getColumn("0", "SALARY_SE"));
	this.ds_mtSalaryPymntList.setColumn(nRow, "PYMNT_SE",	this.ds_pymntCreate.getColumn("0", "PYMNT_SE"));
	this.ds_mtSalaryPymntList.setColumn(nRow, "PYMNT_DE",	this.ds_pymntCreate.getColumn("0", "PYMNT_DE"));
	*/
		
