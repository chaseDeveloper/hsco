var EMPNO;
var SN;

/***********************************************************************
 * onInit 영역
 ***********************************************************************/
this.SAL030104_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.div_search.div_pymntYm.fn_setBind("ds_cond00","PYMNT_YM");
	this.div_search.div_EMP.fn_setBind("ds_cond00","EMPNO","EMPNM");
	this.div_search.div_dept.fn_setBind("ds_cond00","DEPT_CODE","DEPT_NM");

	this.fn_init_form();
	this.fn_init_dataset();
	
}

//링크를 통해 들어온 정보를 가져오기
this.fn_getLinkParam = function()
{
	var pageUrl = "mis_sal::SAL030104.xfdl";
	this.fn_setParam(this.gfn_getLinkParam(pageUrl));
	
	//pageUrl 데이터 삭제
	var strExpr1 = "PAGE_URL=='" + pageUrl + "'";
	var findRow1 = application.gds_mdiParam.findRowExpr(strExpr1);
	application.gds_mdiParam.deleteRow(findRow1);
}

this.fn_setParam = function (sXml)
{
	if (this.comUtils.isNull(sXml)) {
		return;
	}

	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(sXml);
	if (loadCnt > 0) {
		PYMNT_YM = dsTemp.getColumn(0, "PYMNT_YM");
		EMPNO = dsTemp.getColumn(0, "EMPNO");
		SN = dsTemp.getColumn(0, "SN");
	}
}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.SAL030104_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_getLinkParam();
    this.fn_loadCombo();
    
	this.stc_close_at.set_visible(false);
	this.btn_usr_add.set_visible(false);
	this.btn_usr_del.set_visible(false);
	
    this.ds_cond00.clearData();
	this.ds_cond00.addRow();	    
    
    // 급여내역조회에서 들어왔을때
    if(PYMNT_YM != null && SN != null && EMPNO != null) {
		this.ds_cond00.setColumn(0, "PYMNT_YM", PYMNT_YM);    
		this.ds_cond00.setColumn(0, "SN"	  , SN);    
		this.ds_cond00.setColumn(0, "EMPNO"	  , EMPNO);
    }else {
		this.ds_cond00.setColumn(0,"PYMNT_YM",	this.dateUtils.format(this.gfn_today(),"yyyymm"));    
		this.ds_cond00.setColumn(0,"SN"		 ,	"1");    
		
		this.div_search.div_pymntYm.setFocus(true);
	}
	
	this.fn_search();
}

/***********************************************************************
 * 설명: 공통조회
 ************************************************************************/
this.fn_loadCombo = function()
{
	var comboParams = [["ds_rspofc", "HRM214", 1, ""],   // 직책
	                   ["ds_ofcps", "HRM003", 1, ""],    // 직위   HRM021->HRM003 으로 수정함. 
	                   ["ds_jssfc", "HRM001", 1, "전체"],    // 직종
