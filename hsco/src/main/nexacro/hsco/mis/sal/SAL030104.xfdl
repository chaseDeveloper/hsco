this.btn_usr_del_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if ( this.gfn_message("confirm.삭제여부")) {   
		var nRow = this.ds_mtSalaryPymntList.rowposition;
		this.ds_mtSalaryPymntList.deleteRow(nRow);
		
		this.ds_mtPymntDtls.clearData();
		this.ds_mtDdcDtls.clearData();
		this.ds_cond02.clearData();
		
		this.ds_cond02.addRow();
		this.ds_cond02.setColumn(0,"PYMNT_YM",	this.div_search.div_pymntYm.getValue());
		this.ds_cond02.setColumn(0,"SALARY_SE",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"SALARY_SE"));
		this.ds_cond02.setColumn(0,"PYMNT_DE",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"PYMNT_DE"));
		this.ds_cond02.setColumn(0,"EMPNO",		this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"EMPNO"));
		this.ds_cond02.setColumn(0,"PYMNT_SE",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"PYMNT_SE"));
		
		this.fn_transaction("selectMtPymntDtlsList");	
	}
}

