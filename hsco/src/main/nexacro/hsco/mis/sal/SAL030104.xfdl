}

/***********************************************************************
* 대상자조회처리
************************************************************************/
this.fn_search = function()
{
	this.ds_mtSalaryPymntList.clearData();
	this.ds_pymntCreate.clearData();
	
	this.ds_mtPymntDtls.clearData();
	this.ds_mtDdcDtls.clearData();
	
	// Validation 체크
	/*
	if(!this.gfn_checkValidation(this.ds_cond00, this.ds_validation)) {
		return;
	}
	*/
	
	this.fn_transaction("selectMtSalaryPymntList");
}

this.fn_insert = function()
{
	var parm = {
		 arg_0      : this
	};
	this.gfn_popup("Popup1", 400, 400, "", parm, "com::empno_P01.xfdl", "fn_callback3", true);
}

this.fn_callback3 = function(sPopupId, sReturn)
{	
	var tempDs = new Dataset();
	tempDs.loadXML(sReturn);
	if(tempDs.getRowCount() == 1) {
		if(this.confirm("선택한 사원의 급여계산을 진행하시겠습니까?")) {
			this.ds_cond00.setColumn(0, "EMPNO", tempDs.getColumn(0, "EMPNO"));
			this.ds_cond00.setColumn(0, "EMPNM", tempDs.getColumn(0, "EMPNM"));
			this.fn_transaction("salaryCalc");
		}
	}
}

/***********************************************************************
* 저장처리
************************************************************************/
this.fn_save = function()
{
	if(Ex.util.isUpdated(this.ds_mtPymntDtls) || Ex.util.isUpdated(this.ds_mtDdcDtls) || Ex.util.isUpdated(this.ds_mtSalaryPymntList)){	
		var bOK = this.confirm("저장하시겠습니까?");
		if(bOK == true) {
			this.fn_transaction("saveMtDtlsList");
		} else if(bOK == false) {
		    this.ds_mtPymntDtls.reset();
		    this.ds_mtDdcDtls.reset();
		    this.ds_mtSalaryPymntList.reset();
		}    
	 
	}else{
		alert("저장할 내역이 없습니다.");
		return;
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	    // 지급내역대상자
		case "selectMtSalaryPymntList": 
			var sController   	= "hsco/mis/sal/SAL030104/selectMtSalaryPymntList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_mtSalaryPymntList=output1 ds_pymntCreate=output9";
			break;		
			
		case "selectMtDtls":
			var sController   	= "hsco/mis/sal/SAL030104/selectMtDtls.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_mtPymntDtls=output1 ds_mtDdcDtls=output2 ds_pymntStdrCode=output3 ds_pymntStdrCode2=output4";
			break;
			
		// 내역 저장							
		case "saveMtDtlsList":	
			var sController   	= "hsco/mis/sal/SAL030104/mtDtlsCUD.do";	
			//var sInDatasets   	= "input4=ds_cond02:A input5=ds_mtPymntDtls:U input6=ds_mtDdcDtls:U input7=ds_mtSalaryPymntList:U";
			var sInDatasets   	= "input2=ds_mtPymntDtls:U input3=ds_mtDdcDtls:U";
			var sOutDatasets  	= "";
			break;

	    // 월급여조정 추가 대상자 조회
		case "selectMtSalaryPymnt2List": 
			var sController   	= "hsco/mis/sal/SAL030104/selectMtSalaryPymnt2List.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_mtSalaryPymntList00=output1";
			break;
			
	    // 개별급여계산
		case "salaryCalc": 
			var sController   	= "hsco/mis/sal/SAL030104/salaryCalc.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_cond00=output1";
			break;			
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	/*this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);*/
	if (nErrorCode != 0){
		       this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
		    // 지급내역대상자
			case "selectMtSalaryPymntList": 
				this.ds_mtSalaryPymntList.rowposition = 0;
				this.btn_visible_chk();
				break;	
			
			case "selectMtDtls":					
				this.setExprValue();				
				break;
				
			// 지급내역 저장	
			case "saveMtDtlsList":
				//this.salary_hap();
				this.gfn_message("success.처리.성공");
				this.fn_search();  // 대상자
				break;

		    // 월급여조정 추가 대상자 조회
			case "selectMtSalaryPymnt2List": 
				this.ds_mtSalaryPymntList.copyRow(this.ds_mtSalaryPymntList.rowposition, this.ds_mtSalaryPymntList00, 0);
				break;
				
			case "salaryCalc":
				trace("salaryCalc callback");
				trace(this.ds_cond00.saveXML());
				if(this.ds_cond00.getColumn(0, "CNT") == 1) {
					this.gfn_message("success.처리.성공");
					fn_search();
				} else {
					this.gfn_message("fail.실행.불가", this.ds_cond00.getColumn(0, "ERROR"));
				}
				
				break;
		}
	}
}

/***********************************************************************
* 지급대상자 클릭
************************************************************************/
this.ds_mtSalaryPymntList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow>-1){
		/*
		if(this.div_search.cmb_pymntSe.value && !this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"CLOS_AT")){
			this.grd_mtSalaryPymntList00.set_enable(true);
			this.grd_mtSalaryPymntList01.set_enable(true);
		} else {
			this.grd_mtSalaryPymntList00.set_enable(false);
			this.grd_mtSalaryPymntList01.set_enable(false);
		}
		*/
		if(Ex.util.isUpdated(this.ds_mtPymntDtls) || Ex.util.isUpdated(this.ds_mtDdcDtls)) {	
			this.fn_save()
		} else {
			this.ds_mtPymntDtls.clearData();
			this.ds_mtDdcDtls.clearData();
			this.ds_cond02.clearData();
			
			this.ds_cond02.addRow();
			this.ds_cond02.setColumn(0,"PYMNT_YM",	this.div_search.div_pymntYm.getValue());
			this.ds_cond02.setColumn(0,"SALARY_SE",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"SALARY_SE"));
			this.ds_cond02.setColumn(0,"PYMNT_DE",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"PYMNT_DE"));
			this.ds_cond02.setColumn(0,"EMPNO",		this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"EMPNO"));
			this.ds_cond02.setColumn(0,"PYMNT_SE",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"PYMNT_SE"));
			this.ds_cond02.setColumn(0,"SN",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"SN"));
			this.ds_cond02.setColumn(0,"JSSFC_SE",	this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"EMPL_SE"));
			
			//this.fn_transaction("selectMtPymntDtlsList");	
			this.fn_transaction("selectMtDtls");
		}
	} else {
		this.ds_mtPymntDtls.clearData();
		this.ds_mtDdcDtls.clearData();
	}

}

/***********************************************************************
* 합계액
************************************************************************/
this.salary_hap = function()
{
	// 지급합계
	var pymnt = 0;
	var Hrow_con = this.ds_mtPymntDtls.getRowCount();
	for(var i=0; i<= Hrow_con-1; i++){	
	      var pymnt_1 = this.ds_mtPymntDtls.getColumn(i,"PYMNT_AMOUNT")
	      pymnt = pymnt + pymnt_1;
	}
	this.MaskEdit01.set_value(pymnt); 
	
	// 공제합계
	var ddc = 0;
	var Hrow_con = this.ds_mtDdcDtls.getRowCount();
	for(var i=0; i<= Hrow_con-1; i++){	
	      var ddc_1 = this.ds_mtDdcDtls.getColumn(i,"DDC_AMOUNT")
	      ddc = ddc + ddc_1;
	}
	this.MaskEdit02.set_value(ddc); 
	
	var hap = 0;
	hap = pymnt - ddc;
	this.MaskEdit03.set_value(hap); 
	
}

