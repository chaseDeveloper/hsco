this.btn_visible_chk = function()
{
	if (this.ds_pymntCreate.getRowCount() < 1) {
		this.stc_close_at.set_visible(false);
		this.btn_usr_add.set_visible(false);
		this.btn_usr_del.set_visible(false);
	} else {
		this.btn_usr_add.set_visible(true);
		this.btn_usr_del.set_visible(true);
		
		if (this.ds_pymntCreate.getColumn(0, "DCSN_AT") == "1") {
			this.stc_close_at.set_visible(true);
			this.btn_usr_add.set_visible(false);
			this.btn_usr_del.set_visible(false);
		} else {
			this.stc_close_at.set_visible(false);
			this.btn_usr_add.set_visible(true);
			this.btn_usr_del.set_visible(true);	
		}
	}
}


