/***********************************************************************
* 그리드 사원조회
************************************************************************/
this.grd_mtSalaryPymntList_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(!this.ds_mtSalaryPymntList.getColumn(this.ds_mtSalaryPymntList.rowposition,"EMPNO")) {
		var nLeft = system.clientToScreenX(this, 10);
		var nTop  = system.clientToScreenY(this, 10);

		var newChild = new ChildFrame;
		newChild.init("Popup1"
					, "absolute", nLeft, nTop, 400, 400, null, null
					, "com::empno_P01.xfdl");

		newChild.set_openalign("center middle");
		newChild.set_dragmovetype("all");  
		newChild.set_showtitlebar(true);
		newChild.showModal(this.getOwnerFrame()
					, {}
					, this
					, "fn_callback2");
	}
}

/***********************************************************************************
* 설명	: 사원선택 팝업 콜백
***********************************************************************************/
this.fn_callback2 = function(sPopupId, sReturn)
{
	if(sPopupId == "Popup1")
	{
		var get_empno		= "";	// 사번
		var get_jssfc_se 	= "";	// 직책구분
		
		var get_pymnt_ym	= "";	// 지급년월
		var get_salary_se	= "";	// 급여구분(1: 정상, 2: 정산, 3: 시간외)
		var get_pymnt_se	= "";	// 지급구분(1: 1일자, 2: 20일자, 4: 계약직)
		var get_pymnt_de	= "";	// 지급일자
		
		var ll_rowcnt		= this.ds_mtSalaryPymntList.getRowCount();

		this.ds_empno.clearData();
		nRowCnt = this.ds_empno.loadXML(sReturn); 
		
		get_empno	 	= this.ds_empno.getColumn(0,"EMPNO");				// 사번
		get_jssfc_se	= this.ds_empno.getColumn(0,"JSSFC_SE");			// 직책구분 (000:임원, 001:업무직, 002:기술직, 003:기능직, 004:계약직
																			//            005: 기간제, 006: 인턴사원, 007: 전무이사, 008: 감사
		get_pymnt_ym	= this.ds_pymntCreate.getColumn("0", "PYMNT_YM");	// 지급년월
		get_salary_se	= this.ds_pymntCreate.getColumn("0", "SALARY_SE");	// 급여구분(1: 정상, 2: 정산, 3: 시간외)
		get_pymnt_se	= this.ds_pymntCreate.getColumn("0", "PYMNT_SE");	// 지급구분(1: 1일자, 2: 2일자, 4: 계약직)
		get_pymnt_de	= this.ds_pymntCreate.getColumn("0", "PYMNT_DE");	// 지급일자

		for (var i=0; i<ll_rowcnt-1; i++) {
			if ( get_empno == this.ds_mtSalaryPymntList.getColumn(i, "EMPNO")) {
				this.alert("이미 등록되 사용자 입니다.");
				return;
			}
		}
		
		if(get_empno) {

			if (get_pymnt_se  == "1" || get_pymnt_se  == "2" ) {
				if (get_jssfc_se == "000" || get_jssfc_se == "001" || get_jssfc_se == "002" || get_jssfc_se == "003" || get_jssfc_se == "007") {
					this.ds_cond00.setColumn(0,"EMPNO",		get_empno);
					this.ds_cond00.setColumn(0,"PYMNT_YM",	get_pymnt_ym);
					this.ds_cond00.setColumn(0,"SALARY_SE",	get_salary_se);
					this.ds_cond00.setColumn(0,"PYMNT_SE",	get_pymnt_se);
					this.ds_cond00.setColumn(0,"PYMNT_DE",	get_pymnt_de);

					this.fn_transaction("selectMtSalaryPymnt2List");
				} else {
					this.alert("등록 불가능한 대상자입니다.");
				}
			} else if (get_pymnt_se  == "4") {
				if (get_jssfc_se == "004") {
					this.ds_cond00.setColumn(0,"EMPNO",		get_empno);
					this.ds_cond00.setColumn(0,"PYMNT_YM",	get_pymnt_ym);
					this.ds_cond00.setColumn(0,"SALARY_SE",	get_salary_se);
					this.ds_cond00.setColumn(0,"PYMNT_SE",	get_pymnt_se);
					this.ds_cond00.setColumn(0,"PYMNT_DE",	get_pymnt_de);

					this.fn_transaction("selectMtSalaryPymnt2List");
				} else {
					this.alert("등록 불가능한 대상자입니다.");				
				}
			}
		}
		
	}
}



this.ds_mtDdcDtls_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "PYMNT_DDC_CODE") {
		if(e.newvalue == "S2010" || e.newvalue == "S2020" || e.newvalue == "S2150" || e.newvalue == "S2280"){
			this.gfn_message("comm.err", this.ds_pymntStdrCode2.getColumn(this.ds_pymntStdrCode2.findRow("CODE", e.newvalue), "CODE_NM")+"은(는) 신규로 추가 할 수 없습니다.");
			this.ds_mtDdcDtls.setColumn(e.row, "PYMNT_DDC_CODE", "");
		}
	}
}



this.setExprValue = function()
{
	var pymnt = this.grd_mtSalaryPymntList00.getCellValue(-2, 7);
	var ddc = this.grd_mtSalaryPymntList01.getCellValue(-2, 1)
	var real = pymnt - ddc;
	this.MaskEdit01.set_value(pymnt);
	this.MaskEdit02.set_value(ddc);
	this.MaskEdit03.set_value(real);
}


this.ds_mtPymntDtls_onvaluechanged = function(obj:Dataset,  e:nexacro.DSColChangeEventInfo)
{
	this.setExprValue();
	
}

this.ds_mtDdcDtls_onvaluechanged = function(obj:Dataset,  e:nexacro.DSColChangeEventInfo)
{
	this.setExprValue();
}
