 ************************************************************************/
this.fn_delete = function()
{	
	if ( this.gfn_message("confirm.삭제여부")) {  
		var nRow = this.DS_EXCESS_WORK.rowposition;
		this.DS_EXCESS_WORK.deleteRow(nRow);	
