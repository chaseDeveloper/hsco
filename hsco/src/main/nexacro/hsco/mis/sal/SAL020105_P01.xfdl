 * 설명       : 조회 이벤트 처리
 * params        : None
 * return Type    : None
 * 사용법      : this.fn_search();
 */
this.fn_search = function()
{
	// 저장, 파일 삭제, 업로드 버튼 보이기
	this.btn_popup_save.set_visible(true);
	this.div_qualfAllwncReqstInfo2.btn_delFiles.set_visible(true);
	this.div_qualfAllwncReqstInfo2.btn_uploadFiles.set_visible(true);
	
	// 생성
	if(popupSttus == "C") {
		this.div_qualfAllwncReqstInfo2.setOffsetTop("50");
	}
	
	// 조회 및 수정
	if(popupSttus == "R") {	
		this.div_qualfAllwncReqstInfo1.set_visible(true);
		
		// 승인여부가 승인, 반려일 때
		if(confmAt != 1) {
			// 저장, 파일 삭제, 업로드 버튼 숨기기
			this.btn_popup_save.set_visible(false);
			this.div_qualfAllwncReqstInfo2.btn_delFiles.set_visible(false);
			this.div_qualfAllwncReqstInfo2.btn_uploadFiles.set_visible(false);
		}
	}
	
	// 승인관리
	if(popupSttus == "M") {	
	
		this.div_qualfAllwncReqstInfo1.set_visible(true);
		
		// 이미 승인인 경우를 제외하고,
		if(confmAt != "2") {
			// 승인정보 활성화
			this.div_qualfAllwncReqstInfo1.cmb_salaryConfmAt.set_enable(true);
			this.div_qualfAllwncReqstInfo1.cal_salaryConfmDe.set_enable(false);	
			this.div_qualfAllwncReqstInfo1.edt_returnCn.set_enable(false);	
		}else {
			// 저장, 파일 삭제, 업로드 버튼 숨기기
			this.btn_popup_save.set_visible(false)
			this.div_qualfAllwncReqstInfo2.btn_delFiles.set_visible(true);
			this.div_qualfAllwncReqstInfo2.btn_uploadFiles.set_visible(false);
		}
	}
	
	// 가족수당신청정보(승인정보, 신청인정보) 조회
	this.fn_transaction("selectQualfAllwncReqstInfo");
	
}

/*
 * 함수명     : fn_insert
 * 설명       : 신규 생성 이벤트 처리
