/*
 * 함수명  	: btn_popup_save_onclick
 * 설명 		: 기술수당신청  팝업 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_popup_save_onclick();
 */
this.btn_popup_save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
    // 데이터셋 미변경 체크
    if (!this.comUtils.isDatasetUpdated(this.ds_qualfAllwncReqstInfo)) {
        this.gfn_message("comm.데이터.변경.없음")
        return false;
    }


	if(!this.gfn_checkValidation_NoUpdChk(this.ds_qualfAllwncReqstInfo, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		// 신규
		if(popupSttus == "C") {
			this.fn_transaction("saveQualfAllwncReqstInfo");

		// 변경
		}else{ 
			// 팝업 변경이면서 승인상태일때 confirm 날림
			if(popupSttus == "M" && this.ds_qualfAllwncReqstInfo.getColumn(0, "SALARY_CONFM_AT") == "2") {
				if(application.confirm("해당 신청정보를 승인하시면 인사정보에 반영됩니다.\n그래도 진행하시겠습니까?")) {
					this.fn_transaction("modifyQualfAllwncReqstInfo");
				}
			}else {
				this.fn_transaction("modifyQualfAllwncReqstInfo");
			}
		}
	}
}


/*
 * 함수명      : btn_popup_close
 * 설명        : 기술수당신청 팝업 닫기 이벤트 처리
