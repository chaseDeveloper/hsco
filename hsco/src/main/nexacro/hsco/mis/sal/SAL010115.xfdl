{
	LGV_COL_SORT = e.col; 
    this._gridSort(obj, e);
}

this.grd_indiComputDtls_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	LGV_COL_SORT = e.col; 
    this._gridSort(obj, e);
}]]></Script>
