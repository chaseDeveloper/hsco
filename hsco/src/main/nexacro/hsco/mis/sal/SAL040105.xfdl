    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 업무구분 : 퇴직금계산관리
 * 화면ID   : SAL040105
 * 화면명   : 퇴직금계산관리
 * 화면설명 : 퇴직금계산정보를 관리한다.
 * 작성일   : 2017.06.23
 * 작성자   : 이상명
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    
 ***********************************************************************/
 

/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.checkDs = [
	 this.ds_retireSalary
];


/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.SAL040105_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_misFormInit(this);
	this.gfn_initForm(this);	
	this.gfn_initCondDs(this, this.div_search);	
	
	this.div_search.div_pymntYm.setValue(this.dateUtils.format(this.gfn_today(),"yyyymm"));	
	this.div_search.div_pymntYm.setFocus(true);
	
	var comboParams = [			
		["ds_bankcode", "COM021", "1", ""],     // 은행코드 
		["ds_hrm001", "HRM001", "1", ""],      	// 직종		
		["ds_hrm002", "HRM002", "1", ""],      	// 직급
		["ds_hrm025", "HRM025", "1", "선택"], 	// 발령구분
		["ds_sal030", "SAL030", "1", "선택"]	// 퇴직사유
	];
	var callback = function() {
		this.fn_search();
	}
	this.gfn_comboLoad(comboParams, callback);
	
}


/***********************************************************************
* 조회처리
************************************************************************/
this.fn_search = function()
{
	var empno = "";		
	empno = this.div_search.div_EMP.GetValue("EMPNO");	
	this.ds_cond00.clearData();
	this.ds_cond00.addRow();	
	this.ds_cond00.setColumn(0,"RETIRE_YM",this.div_search.div_pymntYm.getValue());     
	this.ds_cond00.setColumn(0,"EMPNO",empno) 	
	
	// Validation 체크
	if(!this.gfn_checkValidation(this.ds_cond00, this.ds_validation)) {
		return;
	}	
	
	this.fn_transaction("selectRetireSalaryList");
}

/***********************************************************************
* 저장처리
************************************************************************/
this.fn_save = function()
{
	trace('fn_save');
    // Validation 체크
 	if(!this.gfn_checkValidation(this.ds_retireSalary, this.ds_validation)) {
		return;
 	}
    
    if(Ex.util.isUpdated(this.ds_retireSalary)) {
		if(this.gfn_message("confirm.저장.여부"))	{
			this.fn_transaction("saveRetireSalaryList");
		}
	} else {
	    alert("저장 할 내역이 없습니다.");	
	}		
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectRetireSalaryList": 
			var sController   	= "hsco/mis/sal/SAL040105/selectRetireSalaryList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_retireSalaryList=output1";
			break;	
		case "selectRetireSalary": 
			var sController   	= "hsco/mis/sal/SAL040105/selectRetireSalary.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_retireSalary=output1 ds_retireAvrgwage=output2 ds_midRetireSalary=output3";
			break;	
		case "saveRetireSalaryList":	
			var sController   	= "hsco/mis/sal/SAL040105/retireSalaryCUD.do";	
			var sInDatasets   	= "input8=ds_retireSalary:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/ 
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	/*this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);*/
	if (nErrorCode != 0){
		       this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	} else {		
		switch (strSvcId) {
			case "selectRetireSalaryList": 
				break;			
			case "selectRetireSalary": 
				break;
			case "saveRetireSalaryList":
			//    this.alert("저장이 완료되었습니다."); 
			    this.fn_search();
				break;
		}
	}
}

/***********************************************************************
* 행삭제
************************************************************************/
this.fn_delete = function()
{
    if ( this.gfn_message("confirm.삭제여부")) {  
	    this.ds_retireSalaryPymnt.deleteRow(0);	
	}    
}



this.ds_retireSalaryList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow < 0)  {
		return;
	}

	this.ds_cond01.clearData();
	this.ds_cond01.addRow();
	this.ds_cond01.setColumn(0, "EMPNO"		, obj.getColumn(e.newrow, "EMPNO"));
	this.ds_cond01.setColumn(0, "SEQ"		, obj.getColumn(e.newrow, "SEQ"));
	this.ds_cond01.setColumn(0, "RETIRE_SE"	, obj.getColumn(e.newrow, "RETIRE_SE"));
	
	this.ds_retireSalary.clearData();
	
	this.fn_transaction("selectRetireSalary");
	
}
