		case "selectYndExcclInputList": 
			var sController   	= "hsco/mis/sal/SAL050202/selectYndExcclInputList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_yndExcclcBsis=output1";
			break;	
		case "pdfuploadinsert": 
			var sController   	= "hsco/mis/sal/SAL050202/pdfuploadinsert.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "";
			break;				
		case "saveYndExcclInputList":	
			var sController   	= "hsco/mis/sal/SAL050202/YndExcclInputCUD.do";	
			var sInDatasets   	= "input2=ds_yndExcclcBsis:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

