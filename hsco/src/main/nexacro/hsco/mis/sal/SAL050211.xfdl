 * CALL TRANSACTION 영역
 **********************************************************************/ 
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	    case "selectYndExcclempList": 
			var sController   	= "hsco/mis/sal/SAL050202/selectYndExcclempList.do";
			var sInDatasets   	= "input2=ds_cond01";
			var sOutDatasets  	= "ds_emplist=output2";
