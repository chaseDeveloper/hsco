	this.ds_reqstSe.setColumn(nRow, "CODE"   , "2");
	this.ds_reqstSe.setColumn(nRow, "CODE_NM", "상실");
	nRow = this.ds_reqstSe.addRow();
	this.ds_reqstSe.setColumn(nRow, "CODE"   , "3");
	this.ds_reqstSe.setColumn(nRow, "CODE_NM", "변경");
	
	// 승인여부 초기값 셋팅
