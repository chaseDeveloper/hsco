	this.ds_confmSe.setColumn(nRow, "CODE"   , "2");
	this.ds_confmSe.setColumn(nRow, "CODE_NM", "승인");
	nRow = this.ds_confmSe.addRow();
	this.ds_confmSe.setColumn(nRow, "CODE"   , "3");
	this.ds_confmSe.setColumn(nRow, "CODE_NM", "반려");
	
