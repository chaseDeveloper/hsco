 ***********************************************************************/
this.SAL050202_P06_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();
	this.div_EMP.Empno_SetValue(empno);
}

/***********************************************************************
