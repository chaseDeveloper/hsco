/***********************************************************************
* 원천징수영수증 출력
************************************************************************/
this.btn_mtSalaryDtstmn_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_yndMediumEmpList.rowcount == 0) {
	     this.alert("출력할 자료가 없습니다."); 
	     return;
	}
	if(this.div_print.rad_printSe.value == "2"){
	     if(!this.div_print.edt_number.value) {
	          this.alert("시작번호를 입력하세요"); return;
	     } else if(!this.div_print.edt_number2.value) {
	          this.alert("마침번호를 입력하세요"); return;
	     }
	}
		
	var Strt1 = 0;
	var End1 = 0;
	
	if(this.div_print.rad_printSe.value == "2"){    
	    Strt1 = this.div_print.edt_number.value -1;
	    End1 = this.div_print.edt_number2.value ;
	    if(Strt1 > End1) {
	         this.alert("마지막번호를 다시 입력하세요");  return;
	    }
	}
	
	var EMP_NO2="";
	var EMP_NO3 = "'";
	for (var i=Strt1; i< End1; i++) {
	    var  EMP_NO1 = this.ds_yndMediumEmpList.getColumn(i,"EMPNO")
	    if(i == End1-1) {
	        EMP_NO2 = EMP_NO2 + EMP_NO3 + EMP_NO1 + EMP_NO3  ;  
	    } else if(i < End1){		   
		    EMP_NO2 = EMP_NO2 + EMP_NO3 + EMP_NO1 + EMP_NO3 + " , ";
		}    
	}	
		
	// 연말정산 결과정보
	 this.ds_cond00.clearData();	
	 this.ds_cond00.addRow();	
	 this.ds_cond00.setColumn(0,"EXCCLC_YEAR",this.div_search.spn_excclcYear.value);
	 this.ds_cond00.setColumn(0,"EXCCLC_SE",this.div_search.rad_excclcSe.value);
	 this.ds_cond00.setColumn(0,"EMPNO2",EMP_NO2);		
		 
	 var strSvcId       = "selectYndExcclcPrintList";   
	 var sController   	= "hsco/mis/sal/SAL050208/selectYndExcclcPrintList.do";
	 var sInDatasets   	= "input2=ds_cond00";
	 var sOutDatasets  	= "ds_yndExcclcDetail=output2 ds_cprInfo=output1";
	 var argument       = "";
	 var callbackfunc   = "";
	 var showprogress   = "";
	 var bAsync = false;
	 Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, argument, callbackfunc, showprogress, bAsync);  
	 
	var Hrow_con = this.ds_yndExcclcDetail.getRowCount();
	for(var i=0; i<= Hrow_con-1; i++){
	     
	     this.ds_yndExcclcHnDdc.clearData();
	     this.ds_yndExcclcDetail2.clearData();	
	     this.ds_yndExcclcDetail2.addRow();
	     this.ds_yndExcclcDetail2.copyRow(0,this.ds_yndExcclcDetail,i);  // 연말정산결과내역 복사
	    
	     this.ds_cond01.clearData();	
		 this.ds_cond01.addRow();	
		 this.ds_cond01.setColumn(0,"EXCCLC_YEAR",this.ds_yndExcclcDetail2.getColumn(0,"EXCCLC_YEAR"));
		 this.ds_cond01.setColumn(0,"EXCCLC_SE",this.ds_yndExcclcDetail2.getColumn(0,"EXCCLC_SE"));
		 this.ds_cond01.setColumn(0,"EMPNO",this.ds_yndExcclcDetail2.getColumn(0,"EMPNO"));	

