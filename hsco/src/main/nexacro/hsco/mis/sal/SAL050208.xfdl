this.div_print_rad_printSe_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	
	if(this.div_print.rad_printSe.value == "1"){
	     this.div_print.edt_number.set_enable(false);
	     this.div_print.edt_number2.set_enable(false);
