		       this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
} else {		
		switch (strSvcId) {
			case "selectYndMediumEmpList": 
			  	break;
			case "selectLaborIncomeDdcList": 				      
				break;			
		}
	}
}


