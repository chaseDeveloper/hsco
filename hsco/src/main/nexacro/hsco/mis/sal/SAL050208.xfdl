this.SAL050208_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init_form();
	this.fn_init_dataset();

}

/***********************************************************************
 * onLoad 영역
 ***********************************************************************/
this.SAL050208_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.div_search.spn_excclcYear.set_value( ((this.dateUtils.format(this.gfn_today(),"yyyy")*1) - 1) + "");
