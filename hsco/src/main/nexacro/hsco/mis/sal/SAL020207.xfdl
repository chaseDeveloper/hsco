 */
this.btn_clbMberSbscrbList_delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if (this.gfn_message("confirm.삭제여부")) {  
		var nRow = this.ds_clbMberSbscrbList.rowposition;
