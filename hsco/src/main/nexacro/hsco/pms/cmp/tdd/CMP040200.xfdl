/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "thingOwnerList":	//소유자 리스트를 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040200/thingOwnerList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "DS_TBCMP_THING_OWNER=output1";
			break;
		case "thingWtnncList": //기본 조서 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040200/thingWtnncList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_THING_WTNNC=output1";
			break;
		case "rwmnyPymntList": //당회청구예정액 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040200/rwmnyPymntList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_RWMNY_PYMNT=output1";
			break;
		case "thingPartcpntList": //관계인 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040200/thingPartcpntList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_THING_PARTCPNT=output1";
			break;
		case "tbcmpBillerChangeMemoU": //메모저장
			var sController   	= "hsco/pms/cmp/tdd/CMP040200/tbcmpBillerChangeMemoU.do";
			var sInDatasets   	= "input1=DS_TBCMP_BILLER_CHANGE_MEMO";
			var sOutDatasets  	= "";
			break;
		case "rwmnyPymntCUD": 	//저장
			var sController   	= "hsco/pms/cmp/tdd/CMP040200/rwmnyPymntCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_RWMNY_PYMNT:U"
							    + " input2=DS_TBCMP_THING_RWMNY:U"
							    + " input3=DS_TBCMP_BILLER_CHANGE:U"
							    + " input4=DS_TBCMP_THING_PARTCPNT:U"
							    + " input5=DS_TBCMP_THING_WTNNC:U";
			var sOutDatasets  	= "";
			break;
		case "thingPartcpntU": //관계인저장
			var sController   	= "hsco/pms/cmp/tdd/CMP040200/thingPartcpntU.do";
			var sInDatasets   	= "input1=DS_TBCMP_THING_PARTCPNT:U"
			var sOutDatasets  	= "";
			break;
