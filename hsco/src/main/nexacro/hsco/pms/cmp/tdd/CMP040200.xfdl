/*
 * 함수명  	: div_DETAIL00_btn_AddRow_onclick
 * 설명 		: 보상금 지급 행추가
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_DETAIL00_btn_AddRow_onclick = function(obj:Button,  e:nexacro.ClickEventInfo){
	var sumAmount = 0;
	var bCheck = false;
	var rqestSn = this.DS_TBCMP_RWMNY_PYMNT.getMax("RQEST_SN");
	rqestSn = rqestSn==null?1:rqestSn+1;
	
	var ds  = this.DS_TBCMP_THING_WTNNC; //물건조서
	var ds1 = this.DS_TBCMP_THING_RWMNY; //물건별보상 매핑테이블
	var nRow = 0;
	var rowcount = ds.rowcount;
	var thingSnList = "";
	var ownerSnList = "";
	
	for(var i=0; i<rowcount; i++){
		if(this.fn_boolean(ds.getColumn(i, "CHK")) == true){
			bCheck = true;
			//ds.setColumn(i, "CHK", '0');
			//ds.setColumn(i, "RQEST_SE", '1');
			//ds.setColumn(i, "RQEST_SN", rqestSn);
			var ownerRwmny = ds.getColumn(i, "OWNER_RWMNY");
			
			if(ownerRwmny != null){
				sumAmount += parseInt(ownerRwmny);
			}
			
			nRow = ds1.addRow();
			ds1.setColumn(nRow, "BSNS_AREA_CODE", ds.getColumn(i, "BSNS_AREA_CODE"));
			ds1.setColumn(nRow, "THING_SN", ds.getColumn(i, "THING_SN"));
			ds1.setColumn(nRow, "OWNER_SN", ds.getColumn(i, "OWNER_SN"));
			ds1.setColumn(nRow, "OWNER_NO", ds.getColumn(i, "OWNER_NO"));
			ds1.setColumn(nRow, "RQEST_SN", rqestSn);
			
			if(thingSnList == "") {
				thingSnList = ds.getColumn(i, "THING_SN");
				ownerSnList = ds.getColumn(i, "OWNER_SN");
			}
			else {
				  thingSnList = ds.getColumn(i, "THING_SN") + "," + thingSnList;
				  ownerSnList = ds.getColumn(i, "OWNER_SN") + "," + ownerSnList;				
			}
		}
	}
	
	if(bCheck == false){
		this.alert("추가 할수 있는 항목이 없습니다.");
		return;
	}
	
	var ownerNo = this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "OWNER_NO");
	var bsnsAreaCode = this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "BSNS_AREA_CODE");
	
	var nRow = this.DS_TBCMP_RWMNY_PYMNT.addRow();
	this.DS_TBCMP_RWMNY_PYMNT.set_enableevent(false);
	this.DS_TBCMP_RWMNY_PYMNT.setColumn(nRow, "BSNS_AREA_CODE", bsnsAreaCode);
	this.DS_TBCMP_RWMNY_PYMNT.setColumn(nRow, "OWNER_NO", ownerNo);
	this.DS_TBCMP_RWMNY_PYMNT.setColumn(nRow, "RQEST_SN", rqestSn);
	this.DS_TBCMP_RWMNY_PYMNT.setColumn(nRow, "RQEST_SE", "01");
	this.DS_TBCMP_RWMNY_PYMNT.setColumn(nRow, "RQEST_AMOUNT", sumAmount);
	this.DS_TBCMP_RWMNY_PYMNT.setColumn(nRow, "RQEST_DE", this.gfn_today());
	this.DS_TBCMP_RWMNY_PYMNT.setColumn(nRow, "PYMNT_PRARNDE", this.gfn_today()); 
