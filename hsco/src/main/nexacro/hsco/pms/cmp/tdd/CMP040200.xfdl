/*
 * 함수명  	: fn_sanction
 * 설명 		: 결제 버튼 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_sanction = function(){
	if(this.DS_TBCMP_RWMNY_PYMNT.getColumn(0,"SAVE_AT") == null){
		this.alert("청구되지 않았습니다.");
		return;
	}
	
	var sanValue = this.DS_TBCMP_RWMNY_PYMNT.getColumn(this.DS_TBCMP_RWMNY_PYMNT.rowposition, "LAST_SANCTN_STTUS");

	if(null != sanValue){
		if(!("S08" == sanValue || "S09" == sanValue)) {
			this.alert("fail.실행.불가", "결재 실행을");
			return;
		}
	}
	
	var gridObj = this.DS_TBCMP_RWMNY_PYMNT;			//그리드
	var gridSelectedRow = gridObj.rowposition;	//그리드 선택로우
	
	//보고서변환
	this.ds_cond02.copyRow(0, gridObj, gridSelectedRow);

	var sanctnId = "";
	var strSvcId = "innerSanctnUbiDataset";
	var reportTitle  = "보상금청구내역";
	var reportUri    = "/pms/cmp/CMP040200_R03.jrf";
	var saveFileName = "보상금청구내역";
	var sController   	= "/hsco/pms/cmp/tdd/CMP040200/rwmnyPymntPrintList.do";
	var sInDatasets   	= "input1=ds_cond02"; 
	var sOutDatasets  	= "DS_TBCMP_RWMNY_PYMNT_REPORT=output1 "
	                    + "ds_bsnsAreaNm=output2";
	
	this.gfn_makeReportTranInfo(sanctnId, strSvcId, sController, sInDatasets, sOutDatasets, reportTitle, reportUri, saveFileName);
	
	//내부결재요청
	//업무키 조합한다.
	var key = gridObj.getColumn(gridSelectedRow, "BSNS_AREA_CODE") + "^" 
			+ gridObj.getColumn(gridSelectedRow, "OWNER_NO") + "^" 
			+ gridObj.getColumn(gridSelectedRow, "RQEST_SN");

	this.ds_innerSanctnParam.clearData();
	this.ds_innerSanctnParam.addRow();
	this.ds_innerSanctnParam.setColumn(0, "INNER_SANCTN_MDL_ID", "CMP-100037");	/*내부결재모형ID*/
	this.ds_innerSanctnParam.setColumn(0, "SYS_SE_CODE", "PMS");				/*시스템구분 COM024*/
	this.ds_innerSanctnParam.setColumn(0, "JOB_SE", "CMP");						/*업무구분 COM025*/
	this.ds_innerSanctnParam.setColumn(0, "JOB_KEY", key);						/*시스템 업무별 UNIQUE KEY*/
	
	var fn_cb = function(sPopupId, sReturn){
		var sanctnResult = sReturn.split("^"); /*index 0: 응답코드, index 1 : 내부결재ID*/
		if("0000"==sanctnResult[0]){
			this.ds_sanction.clearData();
			var nRow = this.ds_sanction.addRow();
			this.ds_sanction.setColumn(nRow, "BSNS_AREA_CODE"  , gridObj.getColumn(gridSelectedRow, "BSNS_AREA_CODE"));
			this.ds_sanction.setColumn(nRow, "OWNER_NO"        , gridObj.getColumn(gridSelectedRow, "OWNER_NO"));
			this.ds_sanction.setColumn(nRow, "RQEST_SN"        , gridObj.getColumn(gridSelectedRow, "RQEST_SN"));
			this.ds_sanction.setColumn(nRow, "INNER_SANCTN_ID" , sanctnResult[1]);
			this.ds_sanction.setColumn(nRow, "JOB_KEY"         , key);
			
			this.fn_transaction("sanctnUpdate");
		}
		else {
			this.ds_sanction.clearData();
			var nRow = this.ds_sanction.addRow();
			this.ds_sanction.setColumn(nRow, "BSNS_AREA_CODE"  , gridObj.getColumn(gridSelectedRow, "BSNS_AREA_CODE"));
			this.ds_sanction.setColumn(nRow, "OWNER_NO"        , gridObj.getColumn(gridSelectedRow, "OWNER_NO"));
			this.ds_sanction.setColumn(nRow, "RQEST_SN"        , gridObj.getColumn(gridSelectedRow, "RQEST_SN"));
			this.ds_sanction.setColumn(nRow, "INNER_SANCTN_ID" , "");
			this.ds_sanction.setColumn(nRow, "JOB_KEY"         , "");
			
			this.fn_transaction("sanctnUpdate");
		}
	}
	
	this.ds_sanction.clearData();
	var nRow = this.ds_sanction.addRow();
	this.ds_sanction.setColumn(nRow, "BSNS_AREA_CODE"  , gridObj.getColumn(gridSelectedRow, "BSNS_AREA_CODE"));
	this.ds_sanction.setColumn(nRow, "OWNER_NO"        , gridObj.getColumn(gridSelectedRow, "OWNER_NO"));
	this.ds_sanction.setColumn(nRow, "RQEST_SN"        , gridObj.getColumn(gridSelectedRow, "RQEST_SN"));
	this.ds_sanction.setColumn(nRow, "INNER_SANCTN_NO" , "");
	this.ds_sanction.setColumn(nRow, "JOB_KEY"         , key);
	
	this.fn_transaction("sanctnUpdate");
	
	this.debug("sss : " + gridObj.saveXML());
	
	var result = this.gfn_openInnerSanctionPopup(this.ds_innerSanctnParam, fn_cb, gridObj.getColumn(gridSelectedRow, "REGIST_DT").substr(0,8));
}

