this.fn_summery_txt = function() {

	var rwmnyTot = this.DS_TBCMP_THING_WTNNC.getSum("OWNER_RWMNY");
	var rqestTot = this.DS_TBCMP_RWMNY_PYMNT.getSum("RQEST_AMOUNT");

	var npymntTot = 0;
	var npymnt = 0;
	var tmpTot = 0;

	for(var i=0; i<this.DS_TBCMP_RWMNY_PYMNT.rowcount; i++) {
		if("0" == this.DS_TBCMP_RWMNY_PYMNT.getColumn(i, "RWMNY_PYMNT_SE")) {
			npymnt = this.DS_TBCMP_RWMNY_PYMNT.getColumn(i, "RQEST_AMOUNT");
			npymntTot = Number(npymntTot) + Number(npymnt);
		}
	}

	tmpTot = Number(rwmnyTot) - Number(rqestTot);
	
	var calcA = this.comUtils.formatComma(rqestTot);
	var calcB = this.comUtils.formatComma(npymntTot);
	var calcC = this.comUtils.formatComma(tmpTot);

	var txt = "청구합계 : " 
	        + calcA  
	        + "     " 
	        + " 미지급잔액 : " 
	        + calcB 
	        + "     " 
	        + "미청구잔액 : " 
	        + calcC
	        +"";

	this.div_DETAIL02.tab_biller.tabpage1.sta_temp.set_text(txt);
}

