/*
 * 함수명  	: fn_save
 * 설명 		: 기본조서, 청구인변경, 등기이전 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function(){
	//업데이트 유무 체크
	if(this.fn_checkUpdate(this.DS_TBCMP_RWMNY_PYMNT) == false && //당회청구
	   this.fn_checkUpdate(this.DS_TBCMP_THING_PARTCPNT) == false && //관계인
	   this.fn_checkUpdate(this.DS_TBCMP_BILLER_CHANGE) == false){ //청구인변경
		this.alert("comm.데이터.변경.없음");
		return false;
	}
	
	
	if(this.confirm("S")){
		var rqestAmt = 0;
		var rwmnyAmt = 0;
		var tmpAmt = 0;
		var thingSnList = "";
		var ownerSnList = "";
		var arrThingSlList = "";		
		var arrOwnerSlList = "";		
		
		for(var i=0; i<this.DS_TBCMP_RWMNY_PYMNT.rowcount; i++) { // 청구내역
			if(Dataset.ROWTYPE_NORMAL != this.DS_TBCMP_RWMNY_PYMNT.getRowType(i)) { // 추가 또는 변경된 청구내역인 경우
				thingSnList = this.DS_TBCMP_RWMNY_PYMNT.getColumn(i, "THING_SN_LIST");
				ownerSnList = this.DS_TBCMP_RWMNY_PYMNT.getColumn(i, "OWNER_SN_LIST");
				if(null != thingSnList) {
					//당회청구예정금액 합산
					for(var j=0; j<this.DS_TBCMP_RWMNY_PYMNT.rowcount; j++) { // 청구내역
						if(thingSnList == this.DS_TBCMP_RWMNY_PYMNT.getColumn(j, "THING_SN_LIST") &&
						   ownerSnList == this.DS_TBCMP_RWMNY_PYMNT.getColumn(j, "OWNER_SN_LIST")) {
							tmpAmt = this.DS_TBCMP_RWMNY_PYMNT.getColumn(j, "RQEST_AMOUNT");
							rqestAmt = Number(rqestAmt) + Number(tmpAmt);
						}
					}
					
					arrThingSlList = thingSnList.split(","); 
					arrOwnerSlList = ownerSnList.split(","); 
					
					//  합산
					for(var k=0; k<arrThingSlList.length; k++) { // 선택된 물건 보상액 계산
						for(var j=0; j<this.DS_TBCMP_THING_WTNNC.rowcount; j++) {						    
							if(arrThingSlList[k] == this.DS_TBCMP_THING_WTNNC.getColumn(j, "THING_SN") && 
							   arrOwnerSlList[k] == this.DS_TBCMP_THING_WTNNC.getColumn(j, "OWNER_SN") ) {							   
								tmpAmt = this.DS_TBCMP_THING_WTNNC.getColumn(j, "OWNER_RWMNY");
								rwmnyAmt = Number(rwmnyAmt) + Number(tmpAmt);
							}
						}
					}

					//this.debug("보상금액 : " + rwmnyAmt + ", 청구금액" + rqestAmt);
					
					if(Number(rwmnyAmt) != Number(rqestAmt)) {
						this.alert("보상금액 "+this.comUtils.formatComma(rwmnyAmt)+" 과 청구금액 "+this.comUtils.formatComma(rqestAmt)+" 이 일치하지 않습니다.");
						return;
					}
					
					for(var k=0; k<arrThingSlList.length; k++) {
						for(var j=0; j<this.DS_TBCMP_THING_WTNNC.rowcount; j++) {
							if(arrThingSlList[k] == this.DS_TBCMP_THING_WTNNC.getColumn(j, "THING_SN")) {
								this.DS_TBCMP_THING_WTNNC.setColumn(j, "RQEST_SE", '1');
							}
						}
					}
				}
			}
		}
		
		this.DS_TBCMP_THING_OWNER.set_updatecontrol(false);
		var nRow = this.DS_TBCMP_THING_OWNER.rowposition;
		this.DS_TBCMP_THING_OWNER.setColumn(nRow, "BILLER_OWNER_NM", this.DS_TBCMP_BILLER_CHANGE.getColumn(0,"BILLER_OWNER_NM"));
		this.DS_TBCMP_THING_OWNER.setColumn(nRow, "BILLER_IHIDNUM", this.DS_TBCMP_BILLER_CHANGE.getColumn(0,"BILLER_IHIDNUM"));
		this.DS_TBCMP_THING_OWNER.setColumn(nRow, "BILLER_ADRES", this.DS_TBCMP_BILLER_CHANGE.getColumn(0,"BILLER_ADRES"));
		this.DS_TBCMP_THING_OWNER.setColumn(nRow, "BILLER_TELNO_1", this.DS_TBCMP_BILLER_CHANGE.getColumn(0,"BILLER_TELNO_1"));
		this.DS_TBCMP_THING_OWNER.setColumn(nRow, "BILLER_TELNO_2", this.DS_TBCMP_BILLER_CHANGE.getColumn(0,"BILLER_TELNO_2"));
		this.DS_TBCMP_THING_OWNER.setColumn(nRow, "SEIZR_AT", this.DS_TBCMP_BILLER_CHANGE.getColumn(0,"SEIZR_AT"));
		this.DS_TBCMP_THING_OWNER.setColumn(nRow, "RM", this.DS_TBCMP_BILLER_CHANGE.getColumn(0,"RM"));
		this.DS_TBCMP_THING_OWNER.setRowType(nRow, Dataset.ROWTYPE_NORMAL);
		this.DS_TBCMP_THING_OWNER.set_updatecontrol(true);

		this.fn_transaction("rwmnyPymntCUD");
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_cancel = function(){
	if(this.confirm("C")){
		this.DS_TBCMP_THING_WTNNC.reset();
		this.DS_TBCMP_THING_WTNNC.applyChange();
		this.DS_TBCMP_RWMNY_PYMNT.reset();
		this.DS_TBCMP_RWMNY_PYMNT.applyChange();
		this.DS_TBCMP_BILLER_CHANGE.reset();
		this.DS_TBCMP_BILLER_CHANGE.applyChange();
		this.DS_TBCMP_THING_PARTCPNT.reset();
		this.DS_TBCMP_THING_PARTCPNT.applyChange();
	}
}

/*
