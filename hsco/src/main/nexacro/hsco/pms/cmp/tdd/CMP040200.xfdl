/*
 * 함수명  	: div_DETAIL00_btn_DelRow_onclick
 * 설명 		: 보상급 지급 행삭제
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_DETAIL00_btn_DelRow_onclick = function(obj:Button,  e:nexacro.ClickEventInfo){
	if(this.DS_TBCMP_RWMNY_PYMNT.rowposition == -1){
		return;
	}
	
	if(this.confirm("D")){
		var ds = this.DS_TBCMP_RWMNY_PYMNT;
		var ds1 = this.DS_TBCMP_THING_RWMNY;
		var rowposition = ds.rowposition;
		var pymntDe = ds.getColumn(rowposition, "PYMNT_DE");
		var rqestSn = ds.getColumn(rowposition, "RQEST_SN");
		var innerSanctnAt = ds.getColumn(rowposition, "INNER_SANCTN_AT");
		var innerSanctnNm = ds.getColumn(rowposition, "INNER_SANCTN_NM");
		var thingSnList = ds.getColumn(rowposition, "THING_SN_LIST");

		if(innerSanctnAt == "S00" || innerSanctnAt == "S01" || innerSanctnAt == "S01"){
			this.alert("fail.삭제.불가", "결재상태가 " + innerSanctnNm + " 인 항목은");
			return;
		}

		if(pymntDe != null){
			this.alert("fail.삭제.불가", "보상금을 지급한 항목은");
			return;
		}

		ds.deleteRow(rowposition);
		
		for(var j=ds1.rowcount; j>=0; j--) {
			if(rqestSn == ds1.getColumn(j, "RQEST_SN")){
				ds1.deleteRow(j);
			}
		}
		
		for(var j=0; j<this.DS_TBCMP_THING_WTNNC.rowcount; j++){
			if(1 == this.DS_TBCMP_THING_WTNNC.getColumn(j, "CHK")) {
				this.DS_TBCMP_THING_WTNNC.setColumn(j, "CHK", 0);
				this.DS_TBCMP_THING_WTNNC.setColumn(j, "RQEST_SE", 0);
			}
		}
		
		
		if(null != thingSnList) {
			var arrThingSlList = thingSnList.split(","); 
			
			//  합산
			for(var k=0; k<arrThingSlList.length; k++) {
				for(var j=0; j<this.DS_TBCMP_THING_WTNNC.rowcount; j++) {
					if(arrThingSlList[k] == this.DS_TBCMP_THING_WTNNC.getColumn(j, "THING_SN")) {
						this.DS_TBCMP_THING_WTNNC.setColumn(j, "RQEST_SE", "0");
					}
				}
			}
		}
		
		if(ds.rowcount == 0) {
			this.div_DETAIL01.grd_DS_TBCMP_THING_WTNNC.setCellProperty("Head",0,"displaytype","checkbox");
			this.div_DETAIL01.grd_DS_TBCMP_THING_WTNNC.setCellProperty("Head",0,"edittype","checkbox");
			this.div_DETAIL01.grd_DS_TBCMP_THING_WTNNC.addEventHandler("onheadclick", this._fn_grdOnHeadChkClickHandler, this);
		}
		//this.DS_TBCMP_BILLER_CHANGE.clearData(); //청구인변경 초기화
	}
}

