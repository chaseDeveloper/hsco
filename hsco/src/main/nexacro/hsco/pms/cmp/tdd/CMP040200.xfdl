/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	//this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	
	var bSuccess = nErrorCode == 0?true:false;

	if (bSuccess == true){
		switch(strSvcId){
			case "thingWtnncList": //기본조서
				this.DS_TBCMP_THING_WTNNC.applyChange();
				this.fn_summery_txt();
				//this.fn_get_rwmnyPymntList();
				break;
			case "rwmnyPymntCUD": //전체저장
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				//this.fn_search();
				this.div_WORK_grd_DS_TBCMP_THING_OWNER_onselectchanged(null,null);
				break;
			case "rwmnyPymntU": //내부결재저장
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.DS_TBCMP_RWMNY_PYMNT.clearData();
				this.fn_transaction("rwmnyPymntList");
				break;
			case "thingPartcpntU": //관계인변경
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.fn_get_thingPartcpntList(this.DS_TBCMP_THING_OWNER.rowposition);
				break;
			case "tbcmpBillerChangeMemoU": //메모저장
				this.div_memo.set_visible(false);
				break;
			case "rwmnyPymntList": //청구내역 조회
				this.fn_summery_txt();
				break;
			case "prtinDs":
				break;
		}
	}
	else {
		this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
}

