this.fn_print1 = function(){
	var vOpts = new this.cf_Opts();
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setReportPath("/pms/cmp/CMP040400_R01.jrf");
	vOpts.setSaveFilename("편입 토지 및 물건 내역");
