this.DS_TBCMP_OFLDC_INFO_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	//수정중인 데이터가 있는지 체크
	//행추가로 인한 로우 체인지가 발생한 경우 행추가한 행의 업데이트 여부를 스킵한다.
	var rowposition = this.DS_TBCMP_OFLDC_INFO.rowposition;
	
	if(this.fn_checkUpdate(this.DS_TBCMP_OFLDC_INFO, e.newrow, false) ||
	   this.fn_checkUpdate(this.DS_TBCMP_OFLDC_TRGTER)){
		if(this.confirm("confirm.변경.취소.여부") == true){
			if(this.DS_TBCMP_OFLDC_INFO.getRowType(rowposition) == 2) {			 
				this.DS_TBCMP_OFLDC_INFO.deleteRow(rowposition);
			}
			else {
				this.DS_TBCMP_OFLDC_INFO.reset();
			}
			
			return true;
		}else{
			return false;
		}
	}
	
	return true;
}

