 * 함수명  	: div_search_cmb_BSNS_AREA_CODE_onitemchanged
 * 설명 		: 사업지구의 내용 변경시 소재지 콤보 업데이트
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_search_cmb_BSNS_AREA_CODE_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.fn_get_locplcCode(this.DS_LOCPLC_CODE, "전체", {bsnsAreaCode:this.div_search.cmb_BSNS_AREA_CODE.value});
}

/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_search = function(){
	if(this.gfn_checkValidation(this.div_search, this.ds_validation) == false) {
		return;
	}

	this.DS_TBCMP_OFLDC_INFO.clearData();
	this.DS_TBCMP_OFLDC_TRGTER.clearData();
	this.DS_TBCMP_THING_OWNER.clearData();
	this.DS_TBCMP_DSCSS_DOC.clearData();
	
	this.ds_dscssOwnerList.bSearch = false;
	this.ds_thingOwnerList.bSearch = false;
	
	this.fn_divToDS(this.ds_cond00, this.div_search);
	
	this.ds_dscssOwnerList.bSearch = true;
	this.fn_transaction("tbcmpOfldcInfoList");		

	this.ds_thingOwnerList.bSearch = true;
	this.fn_transaction("tbcmpThingOwnerList");
}

/*
 * 함수명  	: fn_insert
 * 설명 		: 신규
 * params  		: None
 * return Type 	: None
 * 사용법		:
 */
this.fn_insert = function(){

	if(this.gfn_checkValidation(this.div_search, this.ds_validation) == false) {
		return;
	}

	if(this.div_WORK.tab.tabindex == 0){
		var nRow = this.DS_TBCMP_OFLDC_INFO.addRow();
		this.DS_TBCMP_OFLDC_INFO.setColumn(nRow, "BSNS_AREA_CODE", this.div_search.cmb_BSNS_AREA_CODE.value); 
		this.DS_TBCMP_OFLDC_INFO.setColumn(nRow, "OPERTN_DE", this.gfn_today()); 
	}else{

		var rowposition = this.DS_TBCMP_THING_OWNER.rowposition;
		if(rowposition > -1){
			var nRow = this.DS_TBCMP_DSCSS_DOC.addRow();
			this.DS_TBCMP_DSCSS_DOC.setColumn(nRow, "BSNS_AREA_CODE", this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "BSNS_AREA_CODE"));
			this.DS_TBCMP_DSCSS_DOC.setColumn(nRow, "OWNER_NO", this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "OWNER_NO")); 
			this.DS_TBCMP_DSCSS_DOC.setColumn(nRow, "DSCSS_DE", this.gfn_today()); 
			this.DS_TBCMP_DSCSS_DOC.setColumn(nRow, "ACEPTNC_ADJDC_ODR", 0); 
		}
	}
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		:
 */
this.fn_save = function(){


	if(this.div_WORK.tab.tabindex == 0){
		var bUptTbcmpOfldcInfo = this.comUtils.isDatasetUpdated(this.DS_TBCMP_OFLDC_INFO);
		var bUptTbcmpOfldcTrgter = this.comUtils.isDatasetUpdated(this.DS_TBCMP_OFLDC_TRGTER);

		if(bUptTbcmpOfldcInfo == false && bUptTbcmpOfldcTrgter == false){
			this.alert("comm.데이터.변경.없음");
			return;
		}
		
		if(this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_OFLDC_INFO, this.ds_validation) == false) {
			return;
		}

		if(this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_OFLDC_TRGTER, this.ds_validation) == false) {
			return;
		}

		if(this.confirm("s") ==  true){
			this.fn_transaction("tbcmpOfldcInfoCUD");
		}
	}
	else {
		var bUptTbcmpDscssDoc = this.comUtils.isDatasetUpdated(this.DS_TBCMP_DSCSS_DOC);

		if(bUptTbcmpDscssDoc == false){
			this.alert("comm.데이터.변경.없음");
			return;
		}
		
		if(this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_DSCSS_DOC, this.ds_validation) == false) {
			return;
		}

		if(this.confirm("s") ==  true){
			this.fn_transaction("tbcmpDscssDocCUD");
		}
	}
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제
 * params  		: None
 * return Type 	: None
 * 사용법		:
 */
this.fn_delete = function(){
	if(this.confirm("D")){
		if(this.div_WORK.tab.tabindex == 0){
			if(this.DS_TBCMP_OFLDC_TRGTER.rowcount > 0 ){
				this.alert("하위 자료가 존재합니다.");
				return;
			}
			this.DS_TBCMP_OFLDC_INFO.deleteRow(this.DS_TBCMP_OFLDC_INFO.rowposition);
		}
		else{
			this.DS_TBCMP_DSCSS_DOC.deleteRow(this.DS_TBCMP_DSCSS_DOC.rowposition);
		}
	}
}

/*
 * 함수명  	: fn_cancel
