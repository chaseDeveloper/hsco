/*
 * 함수명  	: fn_set_ofldc
 * 설명 		: 공문대상자 팝업 콜백함수
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_set_ofldc = function(ds){
	var rowposition = this.DS_TBCMP_OFLDC_INFO.rowposition;
	
	if(ds.rowcount > 0){
		for(var i=0; i<ds.rowcount; i++){
			if(ds.getColumn(i, "CHK") == '1'){
			
				var nFindRow = this.DS_TBCMP_OFLDC_TRGTER.findRow( "OWNER_NO", ds.getColumn(i,"OWNER_NO"));
				
				if(nFindRow != -1){
					continue;
				}
				
				var nRow = this.DS_TBCMP_OFLDC_TRGTER.addRow();
				this.DS_TBCMP_OFLDC_TRGTER.setColumn(nRow,"OWNER_NO" , ds.getColumn(i,"OWNER_NO"));
				this.DS_TBCMP_OFLDC_TRGTER.setColumn(nRow,"OWNER_NM" , ds.getColumn(i,"OWNER_NM"));
				this.DS_TBCMP_OFLDC_TRGTER.setColumn(nRow,"ADRES"    , ds.getColumn(i,"ADRES"));
				this.DS_TBCMP_OFLDC_TRGTER.setColumn(nRow,"TELNO_1"  , ds.getColumn(i,"TELNO_1"));
				this.DS_TBCMP_OFLDC_TRGTER.setColumn(nRow,"BSNS_AREA_CODE",this.DS_TBCMP_OFLDC_INFO.getColumn(rowposition, "BSNS_AREA_CODE"));
				this.DS_TBCMP_OFLDC_TRGTER.setColumn(nRow,"OPERTN_DE",     this.DS_TBCMP_OFLDC_INFO.getColumn(rowposition, "OPERTN_DE"));
				this.DS_TBCMP_OFLDC_TRGTER.setColumn(nRow,"OPERTN_SN",     this.DS_TBCMP_OFLDC_INFO.getColumn(rowposition, "OPERTN_SN"));
			}
		}
	}
	
	return true;
}

