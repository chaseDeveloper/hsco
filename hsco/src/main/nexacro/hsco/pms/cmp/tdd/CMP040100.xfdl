			var sInDatasets   	= "input1=DS_TBCMP_OFLDC_INFO:U"
			                    + " input2=DS_TBCMP_OFLDC_TRGTER:U";
			var sOutDatasets  	= "";
			break;
		case "tbcmpDscssDocCUD":		//협의 목록 CUD(신규)
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/tbcmpDscssDocCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_DSCSS_DOC:U";
			var sOutDatasets  	= "";
			break;
		case "tbcmpDscssDocExcelC":		//협의 목록 엑셀등록(신규)
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/tbcmpDscssDocExcelC.do";
			var sInDatasets   	= "input1=ds_excel:U";
			var sOutDatasets  	= "";
			break;
		case "tbcmpOfldcTrgterCUD":		//공문대상자 등록(신규)
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/tbcmpOfldcTrgterCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_OFLDC_TRGTER:U";
			var sOutDatasets  	= "";
			break;
		case "dscssCrcmstncsList": 		//공문 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/dscssCrcmstncsList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_dscssCrcmstncsList=output1";
			break;
			
		case "dscssCrcmstncsWithOwnerList": //공문 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/dscssCrcmstncsWithOwnerList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_dscssCrcmstncsWithOwnerList=output1";
			bSync = false;
			break;
		case "thingOwnerList": 			//사업지구별 소유자 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/thingOwnerList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_thingOwnerList=output1";
			break;
			
		case "dscssDocList": 			//협의내역 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/dscssDocList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_dscssDocList=output1";
			break;
			
		case "dscssDocCUD": 			//협의내역 리스트 CUD
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/dscssDocCUD.do";
			var sInDatasets   	= "input1=ds_dscssDocList:U";
			var sOutDatasets  	= "";
			break;
	}

	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	var bSuccess = nErrorCode == 0?true:false;
	
	if(bSuccess == true){
		switch(strSvcId){
			case "tbcmpOfldcInfoCUD":   //공문리스트
			case "tbcmpDscssDocExcelC": //협의목록 엑셀등록
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.fn_search();
				break;
			case "tbcmpOfldcTrgterCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.div_WORK_tab_tabpage1_grd_dscssCrcmstncsList_onselectchanged(null, null);
				break;
			case "tbcmpDscssDocCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				break;
			case "tbcmpOfldcInfoList":
			case "tbcmpOfldcTrgterList":
			case "tbcmpThingOwnerList":
			case "tbcmpDscssDocList":
				break;
		}
	}
}



