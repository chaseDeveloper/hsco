/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.CMP040100_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();//공통 폼 초기화
	this.fn_init_dataset();//Dataset 초기화
}

this.CMP040100_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_load_combo();
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_load_combo
 * 설명 		: 모든 코드 리스트 검색
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_get_codeList();
 */
this.fn_load_combo = function(){
	this.fn_add_comboHeader(this, this.div_search.cmb_BSNS_AREA_CODE, "선택");
	this.fn_get_userBsnsArea(this.DS_BSNS_AREA_CODE, "선택");				//사업지구
	this.fn_get_cmmCode(this.ds_opertnMth, "선택", {grpCode:"CMP017"});		//시행방법
	this.fn_get_cmmCode(this.ds_opertnSe, "선택", {grpCode:"CMP018"});		//시행구분
}

