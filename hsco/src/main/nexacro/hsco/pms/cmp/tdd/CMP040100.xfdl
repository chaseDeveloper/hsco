 */
this.div_WORK_tab_tabpage2_grd_thingOwnerList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	this.DS_TBCMP_DSCSS_DOC.clearData();
	
	//소재지, 감정평가 법인 조회
	var bsnsAreaCode = this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "BSNS_AREA_CODE");
	var ownerNo      = this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "OWNER_NO");
	this.ds_cond00.setColumn(0,"BSNS_AREA_CODE",bsnsAreaCode);
	this.ds_cond00.setColumn(0,"OWNER_NO",ownerNo);

	this.fn_transaction("tbcmpDscssDocList");
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "tbcmpOfldcInfoList":		// 공문목록조회(신규)
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/tbcmpOfldcInfoList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "DS_TBCMP_OFLDC_INFO=output1";
			break;
		case "tbcmpOfldcTrgterList":	// 공문대상자 조회(신규)
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/tbcmpOfldcTrgterList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "DS_TBCMP_OFLDC_TRGTER=output1";
			break;			
		case "tbcmpThingOwnerList":		// 소유자목록 조회(신규)
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/tbcmpThingOwnerList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "DS_TBCMP_THING_OWNER=output1";
			break;			
		case "tbcmpDscssDocList":		// 협의목록 조회(신규)
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/tbcmpDscssDocList.do";
