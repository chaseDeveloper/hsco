this.DS_TBCMP_ALTRTV_ACQS_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if("CHK" == e.columnid) {
		if(Dataset.ROWTYPE_NORMAL == obj.getRowType(e.row)) {
			obj.set_updatecontrol(false);
		}
	}
}

this.DS_TBCMP_ALTRTV_ACQS_onvaluechanged = function(obj:Dataset,  e:nexacro.DSColChangeEventInfo)
{
	if(-1 == e.col) {
		
		if(0 == e.row ) {
			obj.set_updatecontrol(true);
		}
	}
	else {
		if("CHK" == e.columnid) {
			obj.set_updatecontrol(true);
		}	
	}
}

this.div_DETAIL_grd_DS_TBCMP_ALTRTV_ACQS_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.DS_TBCMP_ALTRTV_ACQS.set_updatecontrol(false);
}

/* 매핑테이블 조회 */
this.DS_TBCMP_ACEPTNC_CNFRMN_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_cond03.clearData();
	var nRow = this.ds_cond03.addRow();
	var ds = this.DS_TBCMP_ACEPTNC_CNFRMN;
	var ds1 = this.DS_TBCMP_ALTRTV_ACQS;
	for(var i = 0 ; i < ds1.rowcount ; i++){
		ds1.setColumn( i , "CHK" , null);
	}

	this.ds_cond03.setColumn( nRow , "BSNS_AREA_CODE" , ds.getColumn( ds.rowposition , "BSNS_AREA_CODE"));
	this.ds_cond03.setColumn( nRow , "OWNER_NO" , ds.getColumn( ds.rowposition , "OWNER_NO"));
	this.ds_cond03.setColumn( nRow , "DOC_ISSU_NO" , ds.getColumn( ds.rowposition , "DOC_ISSU_NO"));
	this.fn_transaction("aceptncThingList");
}

/* 매핑테이블 CRUD 팝업 호출 */
this.div_DETAIL_grd_DS_TBCMP_ACEPTNC_CNFRMN_oncelldblclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		arg_1 : this.ds_cond03,
	};
	
	this.gfn_popup("Pop1", 800, 800, "대체취득 물건 내역 선택", parm, "pms_cmp_tdd::CMP040801_P01.xfdl" , "pop_callback");					
}

 /*
 * 함수명  	: fn_PopupCallback
 * 설명 		: Popup Transaction Callback 처리
 * params  		: sPopupId    : 팝법 ID
 *                sReturn  : 
 * return Type 	: None
 * 사용법		: 
 */
this.fn_PopupCallback = function(sPopupId, sReturn)
{
	if(sPopupId == "Pop1") {
		this.DS_TBCMP_ACEPTNC_CNFRMN_onrowposchanged();
	}
}
