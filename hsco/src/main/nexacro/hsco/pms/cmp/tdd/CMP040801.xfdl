/*
 * 함수명  	: DS_TBCMP_ACEPTNC_CNFRMN_oncolumnchanged
 * 설명 		: 확인서 발급내역 데이터 유효 확인후
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.DS_TBCMP_ACEPTNC_CNFRMN_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{

	if(e.columnid == "USGAMT_NOW" || e.columnid == "ISSU_PRPOS_SE"){

		if(this.comUtils.isNullEmpty(obj.getColumn( obj.rowposition , "USGAMT_NOW")) == true || obj.getColumn( obj.rowposition , "USGAMT_NOW") == 0) return;
		
		trace("USGAMT_NOW =" + obj.getColumn( obj.rowposition , "USGAMT_NOW"));
	
		var totalLadClfrnm = this.fn_get_sum(this.DS_TBCMP_THING_WTNNC, "RWMNY", "THING_CL_KIND", "L");		//토지의 금액 총 합
		var totalHouseClfrnm = this.fn_get_sum(this.DS_TBCMP_THING_WTNNC, "RWMNY", "THING_CL_KIND", "J");	//그 외 지장물의 금액 총 합
		var usgamtBefore = this.fn_get_sum(this.DS_TBCMP_ACEPTNC_CNFRMN, "USGAMT_NOW");						//금회 사용액 총 합
		var blce = totalLadClfrnm + totalHouseClfrnm - usgamtBefore;										//잔액 = 주택 + 지장물 - 금회 사용액 총 합
		var issuSe = obj.getColumn(e.row, "ISSU_PRPOS_SE");
		
		if(issuSe == "2") { //사실확인용
			obj.setColumn(e.row, "USGAMT_BEFORE", 0);
			obj.setColumn(e.row, "USGAMT_NOW", 0);
			obj.setColumn(e.row, "BLCE", 0);
		}
		else {
			if(blce < 0){
				this.alert("기사용액이 총 액보다 클 수 없습니다.");
				obj.setColumn(e.row, "USGAMT_NOW", 0);
			}else{
				//obj.setColumn(e.row, "USGAMT_BEFORE", usgamtBefore);				
				obj.setColumn(e.row, "USGAMT_NOW", this.DS_TBCMP_ACEPTNC_CNFRMN.getColumn(this.DS_TBCMP_ACEPTNC_CNFRMN.rowposition , "USGAMT_NOW"));
				obj.setColumn(e.row, "BLCE", blce);
			}
		}		
	}
}

