/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "thingOwnerList":		//소유자 리스트를 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/thingOwnerList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "DS_TBCMP_THING_OWNER=output1";
			break;
		
		case "altrtvAcqsCUD": 		//대체취득 물건내역 저장
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/altrtvAcqsCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_ALTRTV_ACQS:U";
			var sOutDatasets  	= "";
			break;
		
		case "aceptncCnfrmnCUD": 	//확인서 발급 내역 저장
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/aceptncCnfrmnCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_ACEPTNC_CNFRMN:U";
			var sOutDatasets  	= "";
			break;
				
		case "thingWtnncList": 		//수용내역리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/thingWtnncList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_THING_WTNNC=output1";
			break;
	
		case "altrtvAcqsList": 		//대체취득 물건내역 리스트
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/altrtvAcqsList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_ALTRTV_ACQS=output1";
			bSync = false;
			break;
		
		case "aceptncCnfrmnList": 	//확인서 발급내역 리스트
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/aceptncCnfrmnList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_ACEPTNC_CNFRMN=output1";
			break;
			
		case "ownerThingList": 		//기본 조서 리스트 조회
			var sController   	= "hsco/pms/cmp/rad/CMP030400/ownerThingList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_ownerThingList=output1";
			bSync = false;
			break;
			
		case "rewardBsnsDstrcList": //보상사업지구 정보 리스트
			var sController   	= "hsco/pms/cmp/CMP010100/rewardBsnsDstrcList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_tbcmpRewardBsnsDstrc=output1";
			bSync = false;
			break;
			
		case "aceptncThingList": //대체취득내역 매핑 리스트
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/aceptncThingList.do";
			var sInDatasets   	= "input1=ds_cond03";
			var sOutDatasets  	= "DS_TBCMP_ACEPTNC_THING=output1";
			bSync = false;
			break;			
			
		case "aceptncCnfrmnAllCUD": 	//확인서 발급 내역 저장
			var sController   	= "hsco/pms/cmp/tdd/CMP040801/aceptncCnfrmnAllCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_ACEPTNC_CNFRMN:U"
								+ " input2=DS_TBCMP_ALTRTV_ACQS:U";
			var sOutDatasets  	= "";
			break;
	}

	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	
	var bSuccess = nErrorCode == 0?true:false;
	if (bSuccess == true){
		switch(strSvcId){
			case "thingWtnncList":
				this.fn_clearAll_ds(2);
				this.fn_get_altrtvAcqsList();
				this.fn_get_aceptncCnfrmnList();
				break;
			case "altrtvAcqsCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.fn_get_altrtvAcqsList();
				break;
			case "aceptncCnfrmnCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.fn_get_aceptncCnfrmnList();
				break;
			case "aceptncCnfrmnAllCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.fn_get_altrtvAcqsList();
				this.fn_get_aceptncCnfrmnList();
				break;
			case "aceptncThingList":		
				ds = this.DS_TBCMP_ACEPTNC_THING;
				ds1 = this.DS_TBCMP_ALTRTV_ACQS;
				for(var i = 0 ; i < ds.rowcount ; i++){
					if(ds1.findRow( "ACQS_SN" , ds.getColumn( i , "ACQS_SN")) != -1 ){
						ds1.setColumn(ds1.findRow( "ACQS_SN" , ds.getColumn( i , "ACQS_SN")) , "CHK" , 1);
					} 
				}						
				break;
				
		}
	}
}


this.div_DETAIL_btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.DS_TBCMP_ACEPTNC_CNFRMN.rowposition == -1){
		this.alert("선택된 확인서 발급내역이 없습니다.");
		return;
	}
	
	if(this.fn_checkUpdate(this.DS_TBCMP_ALTRTV_ACQS)) {
		this.alert("info.처리불가","[대체취득 물건 내역] 리스트에 변경내역이 있습니다.\n저장 후 실행하시기 바랍니다.");
		return;
	}
	
	var acqsSn = "-1";
	var tmpId = "";
	
	for(var i=0; i<this.DS_TBCMP_ALTRTV_ACQS.rowcount; i++) {
		if(this.DS_TBCMP_ALTRTV_ACQS.getColumn(i, "CHK") == '1') {
			tmpId = this.DS_TBCMP_ALTRTV_ACQS.getColumn(i, "ACQS_SN");
			if("-1" == acqsSn) {
				acqsSn = tmpId;
			}
			else {
				acqsSn = acqsSn + "," + tmpId;
			}
		}
	}
	
	if("-1" == acqsSn) {
		acqsSn = "";
	}
	
	var nm = this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "NM");
	var addr = this.DS_TBCMP_THING_OWNER.getColumn(this.DS_TBCMP_THING_OWNER.rowposition, "ADRES");
	
	// 레포트 파라메타 셋팅
	this.ds_condPrint01.clearData();
	this.ds_condPrint01.addRow();
	this.ds_condPrint01.copyRow(0, this.DS_TBCMP_THING_OWNER, this.DS_TBCMP_THING_OWNER.rowposition);
	this.ds_condPrint01.setColumn(0, "NM", encodeURIComponent(nm));
	this.ds_condPrint01.setColumn(0, "ADRES", encodeURIComponent(addr));
	this.ds_condPrint01.setColumn(0, "DOC_ISSU_NO", this.DS_TBCMP_ACEPTNC_CNFRMN.getColumn(this.DS_TBCMP_ACEPTNC_CNFRMN.rowposition, "DOC_ISSU_NO"));
	this.ds_condPrint01.setColumn(0, "ACQS_SN", acqsSn);
	
	this.gfn_ReportPopup(this, "PMS/CMP/CMP040801_R01.crf", this.ds_condPrint01);
}

