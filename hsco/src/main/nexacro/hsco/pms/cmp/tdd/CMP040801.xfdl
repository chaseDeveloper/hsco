    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : CMP040801
 * 화면명   : 수용확인서발급
 * 화면설명 : 수용확인서발급
 * 작성일   : 2015.07.21
 * 작성자   : 지효정
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::cmpUtilInclude.xjs";

this.arrDS = null;
this.FEATURE_ONE_BUTTON = true; //상단 버튼하나로 저장루틴 수행시에 true
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.frm_CMP040801_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_init();//공통 폼 초기화
	this.fn_init_form();//폼 초기화
	this.fn_init_dataset();//Dataset 초기화
	this.fn_load_combo();
	//this.fn_set_arrValidationObj();
}

/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function(){
	if(this.FEATURE_ONE_BUTTON == false){
		this.div_DETAIL.btn_save.set_visible(true);
		this.div_DETAIL.btn_save00.set_visible(true);
	}
	/*******************************************************************************
	*프로세스에 맞게 데이이터셋을 순서대로 배열에 삽입
	********************************************************************************/
	this.fn_setAll_ds();
	
	this.div_DETAIL.grd_DS_TBCMP_ALTRTV_ACQS.setCellProperty("Head", 0, "value", "0");
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	//조회, 종료시에 수정여부 체크할 데이터셋 지정
	this.checkDs.push(this.DS_TBCMP_ALTRTV_ACQS);
	this.checkDs.push(this.DS_TBCMP_ACEPTNC_CNFRMN);
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_get_codeList
 * 설명 		: 모든 코드 리스트 검색
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_get_codeList();
 */
this.fn_load_combo = function(){
	this.fn_add_comboHeader(this, this.div_search.cmb_BSNS_AREA_CODE, "선택");
	this.fn_add_comboHeader(this, this.div_search.cmb_LOCPLC_CODE, "전체");
	this.fn_get_userBsnsArea(this.DS_BSNS_AREA_CODE, "선택");				//사업지구

	this.fn_get_lndcgrCode(this.DS_TBCMP_LNDCGR_CODE);					//지목 코드 
	this.fn_get_cmmCode(this.DS_THING_CL, "", {grpCode:"CMP001"} ); 	//물건분류
	this.fn_get_cmmCode(this.DS_UNIT, "", {grpCode:"CMP002"} ); 		//단위
	this.fn_get_cmmCode(this.DS_ALTRTV_ACQS_SE, "", {grpCode:"CMP006"} ); 	//취득구분
	
	this.fn_get_cmmCode(this.DS_CNFRMN_ISSU_SE, "", {grpCode:"CMP004"} ); 	//확인서발급
	this.fn_get_cmmCode(this.DS_ISSU_PRPOS_SE, "", {grpCode:"CMP005"} ); 	//발급용도
}

/*
 * 함수명  	: div_search_cmb_BSNS_AREA_CODE_onitemchanged
 * 설명 		: 사업지구의 내용 변경시 소재지 콤보 업데이트
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_search_cmb_BSNS_AREA_CODE_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.fn_get_locplcCode(this.DS_LOCPLC_CODE, "전체", {bsnsAreaCode:this.div_search.cmb_BSNS_AREA_CODE.value});
}

/*
 * 함수명  	: fn_search
 * 설명 		: 사용자 리스트
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}

	this.fn_clearAll_ds();
	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.fn_transaction("thingOwnerList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_save = function(){
	//업데이트 유무 체크
	if(this.fn_checkUpdate(this.DS_TBCMP_ALTRTV_ACQS) == false &&
	   this.fn_checkUpdate(this.DS_TBCMP_ACEPTNC_CNFRMN) == false){
		this.alert("comm.데이터.변경.없음");
		return false;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_ALTRTV_ACQS, this.ds_validation)) {
		return;
	}
	
	if(!this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_ACEPTNC_CNFRMN, this.ds_validation)) {
		return;
	}
	
	if(this.confirm("S")){
		this.fn_transaction("aceptncCnfrmnAllCUD");	
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
