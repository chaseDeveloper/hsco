	this.div_detail_tab_rewardBsnsDstrc_tabpage1_edt_edBsnsAreaCode_onchanged();
	this.ds_cond02.setColumn(0,0,bsnsType);
	this.fn_transaction("businessDistrictCode");
}

/*
 * 함수명  	: div_detail_tab_rewardBsnsDstrc_tabpage2_btn_save_onclick
 * 설명 		: 소재지 저장
 * params  		: Non
 * return Type 	: None
 * 사용법		: 
 */

this.div_detail_tab_rewardBsnsDstrc_tabpage2_btn_save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.gfn_checkValidation(this.ds_tbcmpLocplcInfo, this.ds_validation) == false) {
		return;
	}
	
	if(this.confirm("S")){
		this.fn_transaction("locplcInfoCUD");
	}
}

/*
 * 함수명  	: div_detail_tab_rewardBsnsDstrc_tabpage2_btn_insert_onclick
 * 설명 		: 소재지 추가
 * params  		: Non
 * return Type 	: None
 * 사용법		: 
 */
this.div_detail_tab_rewardBsnsDstrc_tabpage2_btn_insert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rowposition = this.ds_tbcmpRewardBsnsDstrc.rowposition;
	if(rowposition > -1){
		var bsnsAreaCode = this.ds_tbcmpRewardBsnsDstrc.getColumn(this.ds_tbcmpRewardBsnsDstrc.rowposition, "BSNS_AREA_CODE");
		var nRow = this.ds_tbcmpLocplcInfo.addRow();
		var max  = this.ds_tbcmpLocplcInfo.getMax("LOCPLC_CODE");
		var smax = this.ds_tbcmpLocplcInfo.getMax("SORT_ORDR");
		max  = Number(this.nvl(max, 0)) + 1;
		smax = Number(this.nvl(smax, 0)) + 1;
		//max = max < 10 ? '0' + max : max;
		
		max = (max+"").padLeft(2, "0");
		
		this.ds_tbcmpLocplcInfo.setColumn(nRow,"BSNS_AREA_CODE",bsnsAreaCode);
		this.ds_tbcmpLocplcInfo.setColumn(nRow,"LOCPLC_CODE",max);
		this.ds_tbcmpLocplcInfo.setColumn(nRow,"SORT_ORDR",smax);
		this.ds_tbcmpLocplcInfo.setColumn(nRow,"LOCPLC_CNT",'0');
	}
}

