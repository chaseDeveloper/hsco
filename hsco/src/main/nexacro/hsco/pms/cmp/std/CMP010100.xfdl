    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID		: 	CMP010100
 * 화면명		: 	보상사업지구관리
 * 화면설명	: 	보상사업지구를 관리한다.
 * 작성일		: 	2015.07.20
 * 작성자		: 	변승우
 * 수정이력 	: 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 *
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::cmpUtilInclude.xjs";

this.FEATURE_ONE_BUTTON = true; //상단 버튼하나로 저장루틴 수행시에 true

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.CMP010100_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_form();
	this.fn_init_dataset();		
}
  
this.CMP010100_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_search();
}
 
/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/ 
 
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */ 
this.fn_init_form = function(){
	if(this.FEATURE_ONE_BUTTON == false){
		this.div_detail.tab_rewardBsnsDstrc.tabpage1.btn_save.set_visible(true);
		this.div_detail.tab_rewardBsnsDstrc.tabpage2.btn_save.set_visible(true);
		this.div_detail.tab_rewardBsnsDstrc.tabpage3.btn_save.set_visible(true);
	}
	
	this.fn_load_combo();
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function(){
	//조회, 종료시에 수정여부 체크할 데이터셋 지정
	this.checkDs.push(this.ds_tbcmpRewardBsnsDstrc);
	this.checkDs.push(this.ds_tbcmpLocplcInfo);
	this.checkDs.push(this.ds_bsnsbyPrprapr);
	
	this.gfn_initCondDs(this.div_search);
}

/*
 * 함수명  	: fn_load_combo
 * 설명 		: 콤보등 코드성 데이터 로드
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_load_combo = function(){
	this.fn_transaction("seAccnutAcntCodeList");
	//this.fn_get_cmmCode(this.ds_bsnsType, "", {grpCode:"PRJ012"});		//사업유형
	this.fn_get_userBsnsArea(this.DS_BSNS_AREA_CODE, "전체");			//사업지구
}

/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	// Clear Datasets
	this.ds_tbcmpRewardBsnsDstrc.clearData();
	this.ds_tbcmpLocplcInfo.clearData();
	this.ds_bsnsbyPrprapr.clearData();
	
	// 사원번호설정
	this.ds_cond00.setColumn(0,"EMPNO",this.gfn_getEmpNo());
	
	this.ds_cond00.setColumn(0,"BSNS_AREA_CODE",this.div_search.cmb_BSNS_AREA_CODE.value);
	this.fn_transaction("rewardBsnsDstrcList");
}

/*
 * 함수명  	: fn_insert
 * 설명 		: 신규
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function(){
	this.ds_tbcmpLocplcInfo.clearData();
	this.ds_bsnsbyPrprapr.clearData();
	var nRow = this.ds_tbcmpRewardBsnsDstrc.addRow();
	this.ds_tbcmpRewardBsnsDstrc.setColumn(nRow, "USE_AT", '1');
	this.ds_tbcmpRewardBsnsDstrc.setColumn(nRow, "BSNM", "화성도시공사");
	this.div_detail.tab_rewardBsnsDstrc.set_tabindex(0);//상세정보 탭으로 이동

	if(this.div_detail.tab_rewardBsnsDstrc.tabpage1.chk_selfInput.isChecked()) {
		this.div_detail.tab_rewardBsnsDstrc.tabpage1.chk_selfInput.set_value(false);
	}
	
	this.div_detail_tab_rewardBsnsDstrc_tabpage1_chk_selfInput_onchanged();
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_delete = function(){
	if(this.confirm("D") == true){
		var rowcount = this.ds_bsnsbyPrprapr.rowcount;
		
		//감정평가 결과가 존재하면 삭제 할 수 없도록...
		for(var i=0; i<rowcount; i++){
			if(Number(this.ds_bsnsbyPrprapr.getColumn(i, "APASMT_CNT")) != 0){
				this.alert("감정평가 결과가 있는 감정평가법인은 삭제 할 수 없습니다.");
				return
			}
		}
		
		this.ds_tbcmpLocplcInfo.cancel();
		this.ds_bsnsbyPrprapr.cancel();
		this.ds_tbcmpRewardBsnsDstrc.deleteRow(this.ds_tbcmpRewardBsnsDstrc.rowposition);
		this.div_detail.tab_rewardBsnsDstrc.set_tabindex(0);
		this.div_work_grd_rewardBsnsDstrcList_onselectchanged();
	}
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_save = function(){
	//업데이트 유무 체크
	if(this.fn_checkUpdate(this.ds_tbcmpRewardBsnsDstrc) == false &&
	   this.fn_checkUpdate(this.ds_tbcmpLocplcInfo) == false &&
	   this.fn_checkUpdate(this.ds_bsnsbyPrprapr) == false){
		this.alert("comm.데이터.변경.없음");
		return false;
	}

	//사업지구 유효성 체크(업데이트 유무 체크는 스킵)
	if(this.gfn_checkValidation_NoUpdChk(this.ds_tbcmpRewardBsnsDstrc, this.ds_validation) == false) {
		return false;
	}
	
	//소재지 유효성 체크(업데이트 유무 체크는 스킵)
	if(this.gfn_checkValidation_NoUpdChk(this.ds_tbcmpLocplcInfo, this.ds_validation) == false) {
		return false;
	}
	
	//감정평가법인 유효성 체크(업데이트 유무 체크는 스킵)
	if(this.gfn_checkValidation_NoUpdChk(this.ds_bsnsbyPrprapr, this.ds_validation) == false) {
		return false;
	}
	
	var bsnsAreaCode = this.ds_tbcmpRewardBsnsDstrc.getColumn(this.ds_tbcmpRewardBsnsDstrc.rowposition, "BSNS_AREA_CODE")
	
	//소재지에 사업지역코드 설정
	for(var i=0; i<this.ds_tbcmpLocplcInfo.rowcount; i++){
		this.ds_tbcmpLocplcInfo.setColumn(i,"BSNS_AREA_CODE",bsnsAreaCode);
	}
	
	//감정평가사에 사업지역코드 설정
	for(var i=0; i<this.ds_bsnsbyPrprapr.rowcount; i++){
		this.ds_bsnsbyPrprapr.setColumn(i,"BSNS_AREA_CODE",bsnsAreaCode);
	}
	
	if(this.confirm("S")){
		this.fn_transaction("rewardBsnsDstrcAllCUD");
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_cancel = function(){
	if (this.confirm("C") == true) {
		this.ds_tbcmpRewardBsnsDstrc.cancel();
		this.ds_tbcmpLocplcInfo.cancel();
		this.ds_bsnsbyPrprapr.cancel();
	}
}

/*
 * 함수명  	: fn_close
 * 설명 		: 종료 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_close();
 */ 
this.fn_close = function(){
	this.close();
}
 
/*
 * 함수명  	: div_work_grd_rewardBsnsDstrcList_onselectchanged
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: 
 */
this.div_work_grd_rewardBsnsDstrcList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo){		
	//소재지, 감정평가 법인 데이터셋 초기화
	this.ds_tbcmpLocplcInfo.clearData();
	this.ds_bsnsbyPrprapr.clearData();
	this.fn_bsnsChange("A");
	
	//소재지, 감정평가 법인 조회
	var bsnsAreaCode = this.ds_tbcmpRewardBsnsDstrc.getColumn(this.ds_tbcmpRewardBsnsDstrc.rowposition, "BSNS_AREA_CODE");
	if(bsnsAreaCode != null){
		this.ds_cond00.setColumn(0,"BSNS_AREA_CODE",bsnsAreaCode);
		this.fn_transaction("locplcInfoList");
		this.fn_transaction("bsnsbyPrpraprList");
	}
}

this.ds_tbcmpRewardBsnsDstrc_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	//수정중인 데이터가 있는지 체크
	//행추가로 인한 로우 체인지가 발생한 경우 행추가한 행의 업데이트 여부를 스킵한다.
	var rowposition = this.ds_tbcmpRewardBsnsDstrc.rowposition;
	
	if(this.fn_checkUpdate(this.ds_tbcmpRewardBsnsDstrc, e.newrow, false) ||
	   this.fn_checkUpdate(this.ds_tbcmpLocplcInfo) ||
	   this.fn_checkUpdate(this.ds_bsnsbyPrprapr)){
		if(this.confirm("confirm.변경.취소.여부") == true){
			if(this.ds_tbcmpRewardBsnsDstrc.getRowType(rowposition) == 2) {			 
				this.ds_tbcmpRewardBsnsDstrc.deleteRow(rowposition);
			}
			else {
				this.ds_tbcmpRewardBsnsDstrc.reset();
			}
			
			return true;
		}else{
			return false;
		}
	}
	
	return true;
}

/*
 * 함수명  	: div_detail_tab_rewardBsnsDstrc_tabpage1_btn_save_onclick
 * 설명 		: 보상사업지구 상세내역 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_detail_tab_rewardBsnsDstrc_tabpage1_btn_save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.gfn_checkValidation(this.ds_tbcmpRewardBsnsDstrc, this.ds_validation) == false) {
		return;
	}
	
	if(this.confirm("S")){
		this.fn_transaction("rewardBsnsDstrcCUD");
	}
}

/*
 * 함수명  	: div_detail_tab_rewardBsnsDstrc_tabpage1_cmb_bsnsType_onitemchanged
 * 설명 		: 보상사업지구 사업유형 콤보 선택
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_detail_tab_rewardBsnsDstrc_tabpage1_cmb_bsnsType_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var bsnsType = this.div_detail.tab_rewardBsnsDstrc.tabpage1.cmb_bsnsType.value;
