/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	switch (strSvcId) {
		
		case "rewardBsnsDstrcList": 		//보상사업지구 정보 리스트
			var sController   	= "hsco/pms/cmp/CMP010100/rewardBsnsDstrcList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_tbcmpRewardBsnsDstrc=output1";
			break;
		
		case "locplcInfoList":		//소재지 정보 리스트
			var sController   	= "hsco/pms/cmp/CMP010100/locplcInfoList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_tbcmpLocplcInfo=output1";
			break;
		
		case "bsnsbyPrpraprList":	//감정평가법인 정보 리스트
			var sController   	= "hsco/pms/cmp/CMP010100/bsnsbyPrpraprList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_bsnsbyPrprapr=output1";
			break;
		
		case "rewardBsnsDstrcAllCUD":	//보상사업지구 CUD
			var sController   	= "hsco/pms/cmp/CMP010100/rewardBsnsDstrcAllCUD.do";
			var sInDatasets   	= "input1=ds_tbcmpRewardBsnsDstrc:U"
							    + " input2=ds_tbcmpLocplcInfo:U"
							    + " input3=ds_bsnsbyPrprapr:U";
			var sOutDatasets  	= "";
			break; 
			
		case "rewardBsnsDstrcCUD":	//보상사업지구 CUD
			var sController   	= "hsco/pms/cmp/CMP010100/rewardBsnsDstrcCUD.do";
			var sInDatasets   	= "input1=ds_tbcmpRewardBsnsDstrc:U";
			var sOutDatasets  	= "";
			break; 
			
		case "locplcInfoCUD":		//소재지 CUD
			var sController   	= "hsco/pms/cmp/CMP010100/locplcInfoCUD.do";
			var sInDatasets   	= "input1=ds_tbcmpLocplcInfo:U";
			var sOutDatasets  	= "";
			break;
			
		case "bsnsbyPrpraprCUD":	//평가법인 CUD
			var sController   	= "hsco/pms/cmp/CMP010100/bsnsbyPrpraprCUD.do";
			var sInDatasets   	= "input1=ds_bsnsbyPrprapr:U";
			var sOutDatasets  	= "";
			break;

		case "businessDistrictCode":	//사업지구 채번
			var sController   	= "hsco/pms/cmp/CMP010100/businessDistrictCode.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_bsnsCode=output1";
			break;
		case "seAccnutAcntCodeList":	// 구분회계코드
			var sController   	= "hsco/mis/acc/ACC060102/seAccnutAcntTyManageList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_bsnsType=output4";
			break;
	}	
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	
	switch (strSvcId) {
		
		case "rewardBsnsDstrcList": 		//보상사업지구 정보 리스트
			this.div_work_grd_rewardBsnsDstrcList_onselectchanged();
			break;

		case "locplcInfoList":		//소재지 정보 리스트
			break;
		
		case "bsnsbyPrpraprList":	//감정평가법인 정보 리스트
			break;
		
		case "rewardBsnsDstrcAllCUD":	//보상사업지구 CUD
			this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
			this.fn_search();
			break; 
			
		case "rewardBsnsDstrcCUD":	//보상사업지구 CUD
			break; 
			
		case "locplcInfoCUD":		//소재지 CUD
			this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
			this.fn_transaction("locplcInfoList");
			break;
			
		case "bsnsbyPrpraprCUD":	//평가법인 CUD
			this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
			this.fn_transaction("bsnsbyPrpraprList");
			break;

		case "businessDistrictCode":	//사업지구 채번
			this.div_detail.tab_rewardBsnsDstrc.tabpage1.edt_edBsnsAreaCode.set_value(this.ds_bsnsCode.getColumn(0,0));
			break;
		case "seAccnutAcntCodeList":
			break;
	}
}

