    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : CMP050100_P01
 * 화면명   : 협의요청 및 내역등록
 * 화면설명 : 협의요청 및 내역등록
 * 작성일   : 2016.02.15
 * 작성자   : 지효정
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::cmpUtilInclude.xjs";

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.CMP050100_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();//공통 폼 초기화
	this.fn_init_dataset();//Dataset 초기화
}

this.CMP050100_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_load_combo();
	
	this.ds_dscssDocList.clearData();
	this.ds_cond00.setColumn(0, "BSNS_AREA_CODE", this.parent.bsnsAreaCode);
	this.ds_cond00.setColumn(0, "OWNER_NO", this.parent.ownerNo);
	this.fn_transaction("dscssDocList"); 
	this.fn_transaction("ownerDetail");
}

this.fn_init_dataset = function()
{
	this.ds_cond00.addRow();
	this.ds_cond01.addRow();
}

/*
 * 함수명  	: fn_get_codeList
 * 설명 		: 모든 코드 리스트 검색
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_get_codeList();
 */
this.fn_load_combo = function(){
	this.fn_get_cmmCode(this.ds_slngItyy, "선택", {grpCode:"CMP009"} ); 	//날인불가사유
}

this.btn_Confirm_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var opener = this.opener; //Runtime 작동시 부모 폼
	if(opener[this.parent.callback] == null){
		opener = this.opener.div_workFrame.div_main; //브라우저 작동시 부모 폼
	}
	
	opener[this.parent.callback]();
	this.close();
}

this.btn_Insert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_dscssDocList.addRow();
	this.ds_dscssDocList.setColumn(nRow, "BSNS_AREA_CODE", this.parent.bsnsAreaCode); 
	this.ds_dscssDocList.setColumn(nRow, "OWNER_NO", this.parent.ownerNo); 
	this.ds_dscssDocList.setColumn(nRow, "DSCSS_DE", this.gfn_today()); 
	this.ds_dscssDocList.setColumn(nRow, "ACEPTNC_ADJDC_ODR", 1); 
	this.ds_dscssDocList.setColumn(nRow, "APPLC_AT", '1'); 
}

this.btn_Save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.confirm("s") ==  true){
		this.fn_transaction("dscssDocCUD"); 
	}
}

this.btn_Close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}

this.div_WORK_grd_dscssDocList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	this.ds_cond01.setColumn(0, "BSNS_AREA_CODE", this.ds_dscssDocList.getColumn(this.ds_dscssDocList.rowposition, "BSNS_AREA_CODE"));
	this.ds_cond01.setColumn(0, "REWARD_ODR", this.ds_dscssDocList.getColumn(this.ds_dscssDocList.rowposition, "ACEPTNC_ADJDC_ODR"));
	this.fn_transaction("aceptncAdjdcDetail");
}

this.ds_dscssDocList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "ACEPTNC_ADJDC_ODR"){
		this.ds_cond01.setColumn(0, "BSNS_AREA_CODE", this.ds_dscssDocList.getColumn(this.ds_dscssDocList.rowposition, "BSNS_AREA_CODE"));
		this.ds_cond01.setColumn(0, "REWARD_ODR", this.ds_dscssDocList.getColumn(this.ds_dscssDocList.rowposition, "ACEPTNC_ADJDC_ODR"));
		this.fn_transaction("aceptncAdjdcDetail");
	}
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "dscssDocList": 			//협의내역 리스트 조회
			var sController   	= "hsco/pms/cmp/tdd/CMP040100/dscssDocList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_dscssDocList=output1";
			break;
			
		case "dscssDocCUD": 			//협의내역 리스트 CUD
			var sController   	= "hsco/pms/cmp/CMP050100/dscssDocCUD.do";
			var sInDatasets   	= "input1=ds_dscssDocList:U"
								+ " input2=ds_owner:U"
								+ " input3=ds_aceptncAdjdc:U";
			var sOutDatasets  	= "";
			break;
			
		case "ownerDetail": 			//소유자 날인불가사유 상세
			var sController   	= "hsco/pms/cmp/CMP050100/ownerDetail.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_owner=output1";
			break;
			
		case "aceptncAdjdcDetail": 			//소유자 날인불가사유 상세
			var sController   	= "hsco/pms/cmp/CMP050100/aceptncAdjdcDetail.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_aceptncAdjdc=output1";
			break;
	}

	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	
	var bSuccess = nErrorCode == 0?true:false;
	
	if(bSuccess == true){
		switch(strSvcId){
			case "ownerDetail":
				if(this.ds_owner.rowcount > 0){
					this.ds_owner.set_rowposition(0);
				}
				break;
				
			case "aceptncAdjdcDetail":
				if(this.ds_aceptncAdjdc.rowcount > 0){
					this.ds_aceptncAdjdc.set_rowposition(0);
				}else{
					this.ds_aceptncAdjdc.addRow();
					this.ds_aceptncAdjdc.setColumn(0, "BSNS_AREA_CODE", this.ds_dscssDocList.getColumn(this.ds_dscssDocList.rowposition, "BSNS_AREA_CODE"));
					this.ds_aceptncAdjdc.setColumn(0, "REWARD_ODR", this.ds_dscssDocList.getColumn(this.ds_dscssDocList.rowposition, "ACEPTNC_ADJDC_ODR"));
					this.ds_aceptncAdjdc.set_rowposition(0);
				}
		}
	}
}
