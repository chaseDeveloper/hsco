/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	var bSuccess = nErrorCode == 0?true:false;
	
	if(bSuccess) {
		switch (strSvcId) {
			case "thingOwnerExcelList":
				this.gfn_exportExcel(this.div_WORK.grd_OWNER_EXCEL_LIST, "CMP020200_OWNER");
				break;
			case "thingPartcpntExcelList":
				this.gfn_exportExcel(this.div_WORK.grd_PARTCPNT_EXCEL_LIST, "CMP050100_PARTCPNT");
				break;
			case "thingCl2":

				this.div_search.div_THING_CL.fn_create(this.ds_thingCl2, false);
				var bChk = true;
				
				var arrTmp = ['14','15','21','22','23','25','28','30'];
				for(var i=0; i<this.div_search.div_THING_CL.components.length; i++) {
					bChk = true;

					for(var j=0; j< arrTmp.length; j++) {
						if("chk_"+arrTmp[j] == this.div_search.div_THING_CL.components.get_item(i).id) {
							bChk = false;
							continue;
						}
					}
	
					if(bChk) {
						this.div_search.div_THING_CL.components.get_item(i).set_value(true);
						bChk = false;
					}
				}
				
				break;
		}
	}

	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
}

this.div_WORK_btn_excelDownload_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}

	this.fn_divToDS(this.ds_cond00, this.div_search);
	
	if("1" == this.div_WORK.rdo_OWNER_SE.value) {
		this.fn_transaction("thingOwnerExcelList");
	}
	else {
		this.fn_transaction("thingPartcpntExcelList");
	}
}

this.div_WORK_btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var arrOwnerNo = this.fn_checkValue(this.div_WORK.grd_thingOwnerList, "OWNER_NO");
	
	if(0 == this.ds_thingOwnerList.rowcount || arrOwnerNo.length == 0) {
		this.alert("선택된 소유자가 없습니다.");
		return false;
	}
	
	var arrThingCl = "-1";
	var tmpId = "";
	
	for(var i=0; i<this.div_search.div_THING_CL.components.length; i++) {
		if(this.div_search.div_THING_CL.components.get_item(i).value) {
			tmpId = this.div_search.div_THING_CL.components.get_item(i).id.replace("chk_","");
			if("-1" == arrThingCl) {
				arrThingCl = tmpId;
			}
			else {
				arrThingCl = arrThingCl + "," + tmpId;
			}
		}
	}

	if("-1" == arrThingCl) {
		this.alert("선택된 물건분류가 없습니다.");
		return false;
	}
	
	var bsnsAreaCode    = this.div_search.cmb_bsnsAreaCode.value;
	
	this.gfn_popup("CMP050100_P03"
		   , 550
		   , 532
		   , "보고서출력"
		   , {bsnsAreaCode:bsnsAreaCode,ownerNoList:arrOwnerNo,arrThingClList:arrThingCl}
		   , "pms_cmp_aad::CMP050100_P03.xfdl");	

}

