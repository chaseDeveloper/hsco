/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::cmpUtilInclude.xjs";
this.CMP050100_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();	
}

this.CMP050100_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_init_form();
	this.fn_load_combo();
}

/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */ 
this.fn_init_form = function()
{
	//this.div_search.spn_aceptncAdjdcOdr.set_min(0);
	/*******************************************************************************
	**검색 영역 물건분류 멀티 체크박스 초기화
	********************************************************************************/
	var div_thingCl = this.div_search.div_THING_CL;
	div_thingCl.fn_init(3);
	div_thingCl.fn_set_DSColumn("CODE", "CODE_NM");
	div_thingCl.fn_set_checkAll(this.div_search.chk_ALL);
}

/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	//조회, 종료시에 수정여부 체크할 데이터셋
	this.checkDs.push(this.ds_thingWtnncList);
	
	this.gfn_initCondDs(this, this.div_search);		
}

/*
 * 함수명  	: fn_get_codeList
 * 설명 		: 모든 코드 리스트 검색
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_get_codeList();
 */
this.fn_load_combo = function(){
	this.fn_add_comboHeader(this, this.div_search.cmb_bsnsAreaCode, "선택"); 
	this.fn_add_comboHeader(this, this.div_search.cmb_locplcCode, "전체");
	this.fn_get_userBsnsArea(this.ds_bsnsAreaCode, "선택");				//사업지구
	this.fn_get_lndcgrCode(this.ds_tbcmpLndcgrCode);				//지목 코드 
	this.fn_get_cmmCode(this.ds_thingCl, "", {grpCode:"CMP001"} ); 	//물건분류
	this.fn_get_cmmCode(this.ds_slngItyy, "선택", {grpCode:"CMP009"} ); 	//날인불가사유
	
	this.fn_get_thingClChkBox2(this.ds_thingCl2, this.div_search.div_THING_CL, {grpCode:"CMP001"}, true);	//물건분류 (멀티 체크박스)
}

/*
 * 함수명  	: div_search_cmb_BSNS_AREA_CODE_onitemchanged
 * 설명 		: 사업지구콤보 선택시 소재지 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_search_cmb_BSNS_AREA_CODE_onitemchanged();
 */
this.div_search_cmb_bsnsAreaCode_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var bsnsAreaCode = this.div_search.cmb_bsnsAreaCode.value;
	this.fn_get_locplcCode(this.ds_locplcCode, "전체", {bsnsAreaCode:bsnsAreaCode});	//소재지
	this.fn_get_aceptncAdjdcOdr(this.ds_aceptncAdjdcOdr , "전체", {bsnsAreaCode:bsnsAreaCode}); 
}

/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}
	
	this.ds_thingOwnerList.clearData();
	this.ds_thingWtnncList.clearData();
	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.fn_transaction("thingOwnerList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_save = function(){
	if(this.confirm("S") == true){
		this.fn_transaction("thingWtnncCUD");
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_cancel = function(){
	this.ds_thingWtnncList.cancel();
}

/*
 * 함수명  	: div_WORK_grd_thingOwnerList_onselectchanged
 * 설명 		: 소유자 목록 로우 체인지
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_grd_thingOwnerList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	if(e != null) {
		if(e.col == 0) {
			if(this.ds_thingWtnncList.rowcount > 0) {
				return;
			}
		}
		else {
			for(var i=0; i<this.ds_thingOwnerList.rowcount; i++) {
				if("1" == this.ds_thingOwnerList.getColumn(i, "CHK")) {
					this.ds_thingOwnerList.setColumn(i, "CHK",0);
				}
			}
		}
	}

	this.ds_thingWtnncList.clearData();
	this.ds_dscssDocList.clearData();
	
	this.fn_divToDS(this.ds_cond00, this.div_search);
	var ownerNo = this.ds_thingOwnerList.getColumn(e.row, "OWNER_NO");
	this.ds_cond00.setColumn(0, "ownerNo", ownerNo);
	//this.ds_cond00.setColumn(0, "applcAt", "1");
	
	this.fn_transaction("thingWtnncList");
	this.fn_transaction("dscssDocList");
}

/*
 * 함수명  	: div_WORK_btn_rgst_onclick
 * 설명 		: 개별등록 버튼
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_btn_rgst_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{

}

/*
 * 함수명  	: fn_reload_dscssDoc
 * 설명 		: 개별등록 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_reload_dscssDoc = function(){
	this.ds_dscssDocList.clearData();
	this.fn_transaction("dscssDocList");
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "thingOwnerList": 		//소유자 리스트 조회
			var sController   	= "hsco/pms/cmp/CMP050100/thingOwnerList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_thingOwnerList=output1";
			break;
			
		case "thingWtnncList": 		//기본 조서 리스트 조회
			var sController   	= "hsco/pms/cmp/CMP050100/thingWtnncList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_thingWtnncList=output1";
			break;
			
		case "thingWtnncCUD": 		//기본 조서 CUD
			var sController   	= "hsco/pms/cmp/CMP050100/thingWtnncCUD.do";
