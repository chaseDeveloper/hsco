								+ " input5=DS_TBCMP_PARTCPNT_CHANGE:U"
								+ " input6=ds_thingWtnncPhoto:U";
			var sOutDatasets  	= "";
			break;
			
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0?true:false;
	var nowType = "1";
	
	if (bSuccess == true){
		switch(strSvcId){
			case "thingWtnncList":
				if(retRow != 0) {
					this.DS_TBCMP_THING_WTNNC.set_rowposition(retRow);
				}
				break;
			case "thingOwnerList":
				this.div_DETAIL00_tab_owner_tabpage1_grd_owner_onselectchanged();
				break;
				
			case "thingWtnncCUD":
			case "thingWtnncAllCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				this.fn_search();
				break;
				
			case "thingOwnerCUD":
			case "thingOwnerChangeCUD":
			case "thingPartcpntCUD":
			case "thingPartcpntChangeCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				//this.fn_thingOwnerList(this.DS_TBCMP_THING_WTNNC.rowposition);
				break;
			case "thingWtnncPhotoD":
				if(0 == this.ds_thingWtnncPhoto.getRowCount()) {
					retRow  = this.DS_TBCMP_THING_WTNNC.rowposition;
					nowType = this.DS_TBCMP_THING_WTNNC.getRowType(retRow);
					this.DS_TBCMP_THING_WTNNC.setColumn(retRow, "FILE_SN","");
					
					if(nowType == Dataset.ROWTYPE_NORMAL) {
						this.DS_TBCMP_THING_WTNNC.set_updatecontrol(false);
						var dd = this.DS_TBCMP_THING_WTNNC.setRowType(retRow, Dataset.ROWTYPE_NORMAL);
						this.DS_TBCMP_THING_WTNNC.set_updatecontrol(true);
						this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
					}
				}
				break;
		}
	}
	else {
		this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
}

