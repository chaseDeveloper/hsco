/*
 * 함수명  	: div_DETAIL00_btn_insert_onclick
 * 설명 		: 소유자 추가
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_DETAIL00_btn_insert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//소유자 
	if(this.div_DETAIL00.tab_owner.tabindex == 0){
		//부모가 될 데이터셋(기본 조서)이 선택되어 있는지 체크
		if(this.DS_TBCMP_THING_WTNNC.rowposition != -1){
			//기본 조서 테이블에서 기본키로 쓰일 키 값을 복사한다.
			var rowposition = this.DS_TBCMP_THING_WTNNC.rowposition;
			var bsnsAreaCode = this.DS_TBCMP_THING_WTNNC.getColumn(rowposition, "BSNS_AREA_CODE");
			var thingSn = this.DS_TBCMP_THING_WTNNC.getColumn(rowposition, "THING_SN");
			var totAr = this.DS_TBCMP_THING_WTNNC.getColumn(rowposition, "AR_INCRPR");
			
			var nRow = this.DS_TBCMP_THING_OWNER.addRow();
			this.DS_TBCMP_THING_OWNER.setColumn(nRow, "BSNS_AREA_CODE", bsnsAreaCode);
			this.DS_TBCMP_THING_OWNER.setColumn(nRow, "THING_SN", thingSn);
			//첫번째 행일경우 지분율을 최대로 잡는다.
			//두번째 이상일경우 자동으로 지분율을 계산해서 등록해준다.
			if(nRow == 0){
				//this.DS_TBCMP_THING_OWNER.setColumn(nRow, "POSESN_QOTA", totAr + '/' +  totAr);
				this.DS_TBCMP_THING_OWNER.setColumn(nRow, "POSESN_QOTA", totAr + '/' + totAr);
				this.DS_TBCMP_THING_OWNER.setColumn(nRow, "INPUT_TME", "1");	
			}else{
				var sumAr = this.fn_get_sum(this.DS_TBCMP_THING_OWNER, "AR");
				var sumQota = this.fn_get_sum(this.DS_TBCMP_THING_OWNER, "POSESN_QOTA");
				//총 면적수량이 1000이고 남은 면적수량이 200일경우
				//아래의 두가지 포맷중에 선택해서 사용하면 됨..
				// '200/1000' 으로 표시
				this.DS_TBCMP_THING_OWNER.setColumn(nRow, "POSESN_QOTA", totAr - sumAr + '/' +  totAr);	
				// '1/5' 로 표시
				//this.DS_TBCMP_THING_OWNER.setColumn(nRow, "POSESN_QOTA", this.fn_perToFractional(100 - sumQota));
				this.DS_TBCMP_THING_OWNER.setColumn(nRow, "INPUT_TME", this.DS_TBCMP_THING_OWNER.getColumn(nRow-1,"INPUT_TME"));	
			}
		}
	}
}

