/*
 * 함수명  	: DS_TBCMP_THING_WTNNC_canrowposchange
 * 설명 		: 기본조서 로우 체인지 전에 데이터 유효성 체크
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.DS_TBCMP_THING_WTNNC_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{	
	
	//수정중인 데이터가 있는지 체크
	//행추가로 인한 로우 체인지가 발생한 경우 행추가한 행의 업데이트 여부를 스킵한다.
	if(//this.fn_checkUpdate(this.DS_TBCMP_THING_WTNNC, e.newrow, false) ||
	   this.fn_checkUpdate(this.DS_TBCMP_THING_OWNER) ||
	   this.fn_checkUpdate(this.DS_TBCMP_OWNER_CHANGE) ||
	   this.fn_checkUpdate(this.DS_TBCMP_THING_PARTCPNT) ||
	   this.fn_checkUpdate(this.DS_TBCMP_PARTCPNT_CHANGE) ||
	   this.fn_checkUpdate(this.ds_thingWtnncPhoto)){
		if(this.confirm("confirm.변경.취소.여부") == true){
			//소유자, 관계인 초기화
			this.DS_TBCMP_THING_OWNER.reset();
			this.DS_TBCMP_THING_PARTCPNT.reset();
			return true;
		}else{
			return false;
		}
	}
	
	return true;
}

/*
 * 함수명  	: DS_TBCMP_THING_WTNNC_oncolumnchanged
 * 설명 		: 기본조서 컬럼 체인지 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.DS_TBCMP_THING_WTNNC_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var wtnncRowpos= this.DS_TBCMP_THING_WTNNC.rowposition;
	var codeNm = "";
	var etc1 = null;
	var findRow = -1;
	
	if(e.columnid == 'THING_CL'){			//물건분류
		etc1 = null;
		findRow = this.DS_THING_CL.findRow("CODE", e.newvalue);
		
		etc1= this.DS_THING_CL.getColumn(findRow, "ETC1");
		codeNm = this.DS_THING_CL.getColumn(findRow, "CODE_NM");	
		
		this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "THING_NM", codeNm);			//물건종류에 물건분류명 지정
		
		//토지인 경우
		if(etc1 == 'L'){	
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "LAD_SE", "1"); 			//토지구분을 사유지로 지정
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "REMNDR_LAD_SE", "1");		//잔여지구분을 일반으로 지정
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "RGIST_TRGET_AT", "1");	//등기대상을 'Y'로 지정
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "UNIT", "01");				//단위를 ㎡로 지정
		}
		//토지가 아닌경우
		else{				
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "LAD_SE", ""); 			//토지구분을 사유지로 지정
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "REMNDR_LAD_SE", "");		//잔여지구분을 일반으로 지정
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "RGIST_TRGET_AT", "0");	//등기대상을 'N' 로 지정
		}
		
		//실농비인 경우
		if(e.newvalue == '06'){
			this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "STRCT_STNDRD", codeNm);	//물건종류에 물건분류명 지정
		}
	}else if(e.columnid == "LNDCGR_LGSTR"){		//지목_공부
		findRow = this.DS_TBCMP_LNDCGR_CODE.findRow("CODE", e.newvalue);
		codeNm = this.DS_TBCMP_LNDCGR_CODE.getColumn(findRow, "CODE_NM");
		this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "LNDCGR_INCRPR", e.newvalue);
		this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "STRCT_STNDRD", codeNm);		//구조및 규격에 지목_공부와 동일값 지정
		this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "NOW_USE_STTUS", codeNm);		//현실적인 이용현황에 지목_공부와 동일값 지정
	}else if(e.columnid == "AR_LGSTR"){			//면적_지적
		this.DS_TBCMP_THING_WTNNC.setColumn(wtnncRowpos, "AR_INCRPR", e.newvalue);		//면적_편입에 면적_지적과 동일값 지정
	}
}
