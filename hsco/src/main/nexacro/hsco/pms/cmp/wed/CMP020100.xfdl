/**********************************************************************
 * TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "thingWtnncList": 			//기본 조서 리스트 조회
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingWtnncList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "DS_TBCMP_THING_WTNNC=output1";
			break;
			
		case "thingWtnncCUD": 			//기본조서 저장, 수정, 삭제
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingWtnncCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_THING_WTNNC:U"
			var sOutDatasets  	= "";
			break;
			
		case "thingOwnerList":			//소유자 리스트를 조회
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingOwnerList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_THING_OWNER=output1";
			break;
			
		case "thingOwnerCUD": 			//소유자 저장, 수정, 삭제
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingOwnerCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_THING_OWNER:U"
			var sOutDatasets  	= "DS_TBCMP_THING_OWNER=output1";
			break;
		
		case "thingOwnerChangeList":	//소유자 변경 리스트를 조회
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingOwnerChangeList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_OWNER_CHANGE=output1";
			break;

		case "thingOwnerChangeCUD": 	//소유자 변경 저장, 수정, 삭제
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingOwnerChangeCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_OWNER_CHANGE:U"
			var sOutDatasets  	= "";
			break;
		
		case "thingPartcpntList": 		//관계인 리스트 조회
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingPartcpntList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_THING_PARTCPNT=output1";
			break;
			
		case "thingPartcpntCUD": 		//관계인변경 저장, 수정, 삭제
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingPartcpntCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_THING_PARTCPNT:U"
			var sOutDatasets  	= "DS_TBCMP_THING_PARTCPNT=output1";
			break;
			
		case "thingPartcpntChangeList"://관계인 변경 리스트 조회
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingPartcpntChangeList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "DS_TBCMP_PARTCPNT_CHANGE=output1";
			break;
			
		case "thingPartcpntChangeCUD": 	//관계인변경 저장, 수정, 삭제
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingPartcpntChangeCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_PARTCPNT_CHANGE:U"
			var sOutDatasets  	= "";
			break;
			
		case "thingWtnncPhotoList":		//첨부파일 리스트 조회
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingWtnncPhotoList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_thingWtnncPhoto=output1";
			break;
			
		case "thingWtnncPhotoC": 		//첨부파일 저장
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingWtnncPhotoC.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_thingWtnncPhoto=output1";
			break;
			
		case "thingWtnncPhotoD": 		//첨부파일 삭제
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingWtnncPhotoD.do";
