/**
 * 파일업로드 파일 선택 이벤트 핸들러
 * @param fileFullName 선택 전체 파일명
 * @param type 실행구분(ExtFileUpload/ExtFileDownload)
 * @param fileFullName 선택 파일명
 * @return void
 */
this.fn_onFileSelected = function(type, fileFullName){

	this.ds_cond02.clearData();
	if(type == this.FILE_MANAGER.TYPE_UPLOADER){
		var isNmArray = this.pmsUtil.isArray(fileFullName);
		var rowposition = this.DS_TBCMP_THING_WTNNC.rowposition;
		var bsnsAreaCode = this.DS_TBCMP_THING_WTNNC.getColumn(rowposition, "BSNS_AREA_CODE");
		var thingSn = this.DS_TBCMP_THING_WTNNC.getColumn(rowposition, "THING_SN");
		var fileSn = this.nvl(this.DS_TBCMP_THING_WTNNC.getColumn(rowposition, "FILE_SN"),"");

		if(isNmArray){
			var selFileLeng = fileFullName.length;
			for(var i = 0; i < selFileLeng; i++){
				var nRow = this.ds_cond02.addRow();
				this.ds_cond02.setColumn(nRow, "BSNS_AREA_CODE", bsnsAreaCode);
				this.ds_cond02.setColumn(nRow, "THING_SN", thingSn);
				this.ds_cond02.setColumn(nRow, "FILE_SN", fileSn);
				this.ds_cond02.setColumn(nRow, "SYS_SE_CODE" , this.SYS_SE); 
				this.ds_cond02.setColumn(nRow, "JOB_SE_CODE", this.JOB_SE);
			}	

			this.fn_saveFiles();
		}
		else {
			var nRow = this.ds_cond02.addRow();
			this.ds_cond02.setColumn(nRow, "BSNS_AREA_CODE", bsnsAreaCode);
			this.ds_cond02.setColumn(nRow, "THING_SN", thingSn);
			this.ds_cond02.setColumn(nRow, "FILE_SN", fileSn);
			this.ds_cond02.setColumn(nRow, "SYS_SE_CODE" , this.SYS_SE); 
			this.ds_cond02.setColumn(nRow, "JOB_SE_CODE", this.JOB_SE);
			this.fn_saveFiles();
		}
	}
}

/**
 * 파일업로드 파일 업로드 성공 이벤트 핸들러
 * @param fileFullName 선택 전체 파일명
 * @param type 실행구분(ExtFileUpload/ExtFileDownload)
 * @param e 발생이벤트
 * @return void
 */
this.fn_onFileSuccess = function(type, e){
	var fileSn = "";
	if(type == this.FILE_MANAGER.TYPE_UPLOADER){
		retRow  = this.DS_TBCMP_THING_WTNNC.rowposition;
		nowType = this.DS_TBCMP_THING_WTNNC.getRowType(retRow);
		fileSn  = this.ds_thingWtnncPhoto.getColumn(0,"FILE_SN");
		
		this.DS_TBCMP_THING_WTNNC.setColumn(retRow, "FILE_SN",fileSn);
		
		if(nowType == Dataset.ROWTYPE_NORMAL) {
			this.DS_TBCMP_THING_WTNNC.set_updatecontrol(false);
			var dd = this.DS_TBCMP_THING_WTNNC.setRowType(retRow, Dataset.ROWTYPE_NORMAL);
			this.DS_TBCMP_THING_WTNNC.set_updatecontrol(true);
			this.alert("성공적으로 처리되었습니다.");
		}
	}else if(type == this.FILE_MANAGER.TYPE_DOWNLOADER){
		this.gfn_message("success.파일.다운로드.성공");
	}
}

