/*
 * 함수명  	: DS_TBCMP_THING_OWNER_oncolumnchanged
 * 설명 		: 소유자 컬럽 값 체인지, 지분, 면적 값 자동 계산
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.DS_TBCMP_THING_OWNER_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var totAr = this.DS_TBCMP_THING_WTNNC.getColumn(this.DS_TBCMP_THING_WTNNC.rowposition, "AR_INCRPR");
	if(e.columnid == "POSESN_QOTA"){
		if(e.newvalue.indexOf('/') == -1 || isNaN(e.newvalue.split('/')[0]) == true || isNaN(e.newvalue.split('/')[1]) == true){
			this.alert("입력값이 숫자/숫자 포맷이어야 합니다.");
			this.DS_TBCMP_THING_OWNER.setColumn(this.DS_TBCMP_THING_OWNER.rowposition, "POSESN_QOTA", "");
			return;
		}
		if(this.fn_get_sum(obj, "POSESN_QOTA") > 100){
			this.alert("지분의 합은 100%를 넘을 수 없습니다.")
			obj.setColumn(obj.rowposition, "POSESN_QOTA", e.oldvalue);
			return;
		}
		obj.setColumn(obj.rowposition, "AR", totAr * this.fn_fractionalToPer(e.newvalue) * 0.01);
	}else if(e.columnid == "AR"){
		var sumAr = this.fn_get_sum(obj, "AR");
		var sumQota = this.fn_get_sum(obj, "POSESN_QOTA");
		if(sumAr > totAr){
			this.alert("면적의 합은 " + totAr + "을 넘을 수 없습니다.")
			obj.setColumn(obj.rowposition, "AR", e.oldvalue);
			return;
		}
		//총 면적수량이 1000이고 남은 면적수량이 200일경우
		//아래의 두가지 포맷중에 선택해서 사용하면 됨..
		// '200/1000' 으로 표시
		obj.setColumn(obj.rowposition, "POSESN_QOTA", e.newvalue + '/' +  totAr);	
		// '1/5' 로 표시
		//obj.setColumn(obj.rowposition, "POSESN_QOTA",  this.fn_perToFractional(100 - sumQota));	
	}
}

