		case "thingWtnncAllCUD": 		//기본조서 저장, 수정, 삭제
			var sController   	= "hsco/pms/cmp/wed/CMP020100/thingWtnncAllCUD.do";
			var sInDatasets   	= "input1=DS_TBCMP_THING_WTNNC:U"
								+ " input2=DS_TBCMP_THING_OWNER:U"
