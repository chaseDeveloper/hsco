/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	// 검색영역 Validation 체크
	if(this.gfn_checkValidation(this.div_search, this.ds_validation) == false) {
		return;
	}
	
	this.div_DETAIL02.imv_imageView.set_image("");
	this.ds_thingWtnncPhoto.clearData();
	this.fn_clearAll_ds();

	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.fn_transaction("thingWtnncList");
}

/*
 * 함수명  	: fn_insert
 * 설명 		: 신규
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_insert = function(){
	//신규입력시에 검색조건의 사업지구와 보상차수, 소재지를 가져온다.
	//신규입력시의 검색조건의 필수 입력값은 사업지구와 보상차수.
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation02)) {
		return;
	}
	
	var rewardOdr = this.div_search.cmb_REWARD_ODR.value;
	
	if(rewardOdr == "") {
		rewardOdr = "1";
	}
	
	//보상차수,소재지,지번
	
	var nRow = this.DS_TBCMP_THING_WTNNC.addRow();
	var locplcCode = "";
	var lnmSe      = "";
	var lnm        = "";
	var slno       = "";

	if(nRow > 0) {
		rewardOdr  = this.DS_TBCMP_THING_WTNNC.getColumn(nRow -1, "REWARD_ODR");
		locplcCode = this.DS_TBCMP_THING_WTNNC.getColumn(nRow -1, "LOCPLC_CODE");
		lnmSe      = this.DS_TBCMP_THING_WTNNC.getColumn(nRow -1, "LNM_SE");
		lnm        = this.DS_TBCMP_THING_WTNNC.getColumn(nRow -1, "LNM");
		slno       = this.DS_TBCMP_THING_WTNNC.getColumn(nRow -1, "SLNO");
	}
	
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "BSNS_AREA_CODE", this.div_search.cmb_BSNS_AREA_CODE.value);
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "LOCPLC_CODE", this.div_search.cmb_LOCPLC_CODE.value);
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "REWARD_ODR", rewardOdr);
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "RGIST_TRGET_AT", "0");
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "APASMT_CNT", "0");
	
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "LOCPLC_CODE", locplcCode);	
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "LNM_SE", lnmSe);	
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "LNM", lnm);	
	this.DS_TBCMP_THING_WTNNC.setColumn(nRow, "SLNO", slno);	
	
	this.DS_TBCMP_THING_WTNNC.newRowPosition = nRow;
	//소유자, 관계인 초기화
	//this.fn_clearAll_ds(1);
	this.DS_TBCMP_THING_OWNER.clearData();
	this.DS_TBCMP_THING_PARTCPNT.clearData();
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_delete = function(){
	if(this.confirm("D") == true){
		if(0 != this.DS_TBCMP_THING_WTNNC.getColumn(this.DS_TBCMP_THING_WTNNC.rowposition,"APASMT_CNT")) {
			this.alert("fail.삭제.불가", "감정평가 등록된 물건은");
			return;
		}
	
		//this.DS_TBCMP_THING_WTNNC.cancel();
		//this.DS_TBCMP_THING_OWNER.cancel();
		//this.DS_TBCMP_THING_PARTCPNT.cancel();
		//this.ds_thingWtnncPhoto.cancel();		
		this.DS_TBCMP_THING_WTNNC.deleteRow(this.DS_TBCMP_THING_WTNNC.rowposition);
		this.div_DETAIL00.tab_owner.set_tabindex(0);
		this.div_DETAIL01.tab_relation.set_tabindex(0);
		this.div_WORK_grd_thingWtnnc_onselectchanged();
	}
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_save = function(){
	
	//업데이트 유무 체크
	if(this.fn_checkUpdate(this.DS_TBCMP_THING_WTNNC) == false &&
	   this.fn_checkUpdate(this.DS_TBCMP_THING_OWNER) == false &&
	   this.fn_checkUpdate(this.DS_TBCMP_OWNER_CHANGE) == false &&
	   this.fn_checkUpdate(this.DS_TBCMP_THING_PARTCPNT) == false &&
	   this.fn_checkUpdate(this.DS_TBCMP_PARTCPNT_CHANGE) == false &&
	   this.fn_checkUpdate(this.ds_thingWtnncPhoto) == false){
		this.alert("comm.데이터.변경.없음");
		return false;
	}
	
	//기본조서 유효성 체크(업데이트 유무 체크는 스킵)
	if(this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_THING_WTNNC, this.ds_validation) == false) {
		return false;
	}
	
	//소유자 유효성 체크(업데이트 유무 체크는 스킵)
	if(this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_THING_OWNER, this.ds_validation) == false) {
		return false;
	}
	
	//관계인 유효성 체크(업데이트 유무 체크는 스킵)
	if(this.gfn_checkValidation_NoUpdChk(this.DS_TBCMP_THING_PARTCPNT, this.ds_validation) == false) {
		return false;
	}
	
	if(this.confirm("S")){
		//저장후에 저장한 rowposition에 포커스를 주기 위해
		//this.DS_TBCMP_THING_WTNNC.backupPosition = this.DS_TBCMP_THING_WTNNC.rowposition;
		retRow = this.DS_TBCMP_THING_WTNNC.rowposition;
		
		this.fn_transaction("thingWtnncAllCUD");
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_cancel = function(){
	if (this.confirm("C") == true) {
