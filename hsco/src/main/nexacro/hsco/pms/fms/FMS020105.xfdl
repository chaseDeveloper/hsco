 */
this.Grid00_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	var parm = {
		 arg_0      : this
	};
	this.gfn_popup('ASS010400_P02', 500, 500, '주소 선택', parm, "mis_ass::ASS010400_P02.xfdl", "fn_popupCallBack");
}
