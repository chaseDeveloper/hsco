/*
 * 함수명  	: btn_FCLTS_SE_SN_onclick
 * 설명 		: 시설물구분 팝업 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_FCLTS_SE_SN_onclick(obj,e);
 */
this.btn_FCLTS_SE_SN_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.parent.parent.ds_reprsntFcltsManage.getRowCount() == 0) {
		this.gfn_message("info.진행가능", "시설정보 '추가' 혹은 '조회'");
		return false;
	}
	
	var parm = {
		 arg_0      : this
	};
	this.gfn_popup("FMS010101_P01", 620, 200, "시설물구분 선택", parm, "pms_fms::FMS010101_P01.xfdl", "fn_popupCallBack", true);
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: 팝업 콜백 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_popupCallBack(sPopupId, sReturn);
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "FMS010101_P01") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var fcltsSeSn = dsTemp.getColumn(0, "SN");
			var kndNm     = dsTemp.getColumn(0, "KND_NM");
			var asortNm   = dsTemp.getColumn(0, "ASORT_NM");
			
			var nRow = this.parent.parent.ds_reprsntFcltsManage.rowposition;
			this.parent.parent.ds_reprsntFcltsManage.setColumn(nRow, "FCLTS_SE_SN", fcltsSeSn);
			this.parent.parent.ds_reprsntFcltsManage.setColumn(nRow, "KND_NM",      kndNm    );
			this.parent.parent.ds_reprsntFcltsManage.setColumn(nRow, "ASORT_NM",    asortNm  );
		}
	}
}


/*
 * 함수명  	: btn_upload1_onclick
 * 설명 		: 전경사진 추가 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_upload1_onclick(obj,e);
 */
this.btn_upload1_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.parent.parent.ds_reprsntFcltsManage.getRowCount() == 0) {
		this.gfn_message("info.진행가능", "시설정보 '추가' 혹은 '조회'");
		return false;
	}
	
	if(this.parent.parent.ds_fileList1.getRowCount() > 0) {
		alert("현재 업로드된 사진을 삭제하신 후 진행하여 주시기 바랍니다.");
		return false;
	}
	
	this.fn_autoFileSn(
		 "PMS"										// 대분류   (SYS_SE)
		,"FMS"										// 업무구분 (JOB_SE)
		, this.parent.parent.File_UpDownManager		// fileManager 오브젝트
		, this.parent.parent.ds_fileList1			// fileObject
		, this.parent.parent.ds_reprsntFcltsManage	// fileSn을 불러오거나 반영해야 하는 컬럼
		, "TBFMS_REPRSNT_FCLTS_MANAGE"				// fileSn을 저장할 테이블
		, "PHOTO1_FILE_SN"							// fileSn 컬럼명이 다를 경우를 대비
		, ["SN"]									// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
	);
}


