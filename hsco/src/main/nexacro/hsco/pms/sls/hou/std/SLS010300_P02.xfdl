			+'PAY_TMLMT_SB=Column32,'				// 납부기한_확장계약_기본
			+'PAY_AMT_SB_S9=Column33,'				// 납부금액_확장잔금_기본
			+'PAY_TMLMT_SB_S9=Column34,'			// 납부기한_확장잔금_기본
			+'PAY_AMT_SC=Column35,'					// 납부금액_확장계약_마이너스
