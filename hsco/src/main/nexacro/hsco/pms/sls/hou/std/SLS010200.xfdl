    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : SLS010100
 * 화면명   : 주택정보
 * 화면설명 : 주택정보를 관리한다.
 * 작성일   : 2015.07.25
 * 작성자   : 최승락
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    2015.00.00   홍길동   fn_xxx() 보완 수정
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "pms_lib::pmsUtilInclude.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
// 화면 전역에서 사용할 Form 변수를 선언한다.
// TODO


/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SLS010200_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();             // 공통 폼 초기화
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화
}

this.SLS010200_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
    // 공통코드처리
	this.fn_loadCombo();
}


/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
}

/*
 * 함수명  	    : initDataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

/***********************************************************************
 * 공통 코드 조회
 ***********************************************************************/
/*
 * 함수명  	    : fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function()
{
	// 공통코드 목록 조회
 	var comCodeParam = [
 		  {OUT_PUT:"ds_houseDrc",  GRP_CODE:"HOU002", USE_AT:"1"} // 주택타입방향
 		, {OUT_PUT:"ds_houseStle", GRP_CODE:"HOU003", USE_AT:"1"} // 주택타입형태
 		, {OUT_PUT:"ds_payCode",   GRP_CODE:"HOU0A1", USE_AT:"1"} // 주택납부코드 		
 		
 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);


	// 주택코드 목록 조회
	var paramMap = {};
	paramMap["USE_AT"] = "1";
	paramMap["HOUSE_SE"] = "S1,S2,S3,S4,S5,S9,R1,R2,R3,R9";	

	this.pmsUtil.getHouseCode(paramMap, "ds_houseCodeSh");
	this.pmsUtil.getHouseCode(paramMap, "ds_houseCode");

}


/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	    : fn_search
 * 설명 		: 주택기본정보  조회 이벤트 처리
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	// Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}

	// Clear Datasets
	this.pmsUtil.clearDataSet([this.ds_houseTypeList]);

	// 주택목록조회
	this.fn_transaction("selectHouseTypeList");
}

 /*
 * 함수명  	    : fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{

	// Clear Datasets
	this.pmsUtil.clearDataSet([this.ds_houseTybyPayList, this.ds_houseTybyPayOptnList, this.ds_houseTybyPayDscntList]);
		
	if (this.ds_houseTypeList.rowcount > 0 ) {
		var nRow = this.ds_houseTypeList.addRow();
		this.ds_houseTypeList.setColumn(nRow, "HOUSE_CODE", this.ds_cond00.getColumn(0,"HOUSE_CODE"));		
	} else {
		this.alert("조회된 자료가 없습니다.");
		return;
	}
}

/*
 * 함수명  	    : fn_save
 * 설명 		: 주택기본정보 저장 이벤트 처리
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{

	if (this.ds_houseTypeList.rowcount > 0) {
		// Validation 체크
		if(!this.gfn_checkValidation(this.ds_houseTypeList, this.ds_validation00)) {
			return;
		}
		
		if(this.confirm("저장하시겠습니까?"))	{
			this.fn_transaction("saveHouseTypeList");
		}
	}
		
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
/*
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
	
		// 주택타입정보 조회	
		case "selectHouseTypeList":
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/selectHouseTypeList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_houseTypeList=output1";			
			break;

		// 주택타입정보 저장
		case "saveHouseTypeList":
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/saveHouseTypeList.do";
			var sInDatasets   	= "input1=ds_houseTypeList:U";
			var sOutDatasets  	= "";
			break;

		// 납부정보 조회(tabpage1)
		case "selectHouseTybyPayPayList":
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/selectHouseTybyPayPayList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_houseTybyPayList=output1";			
			break;
			
		// 납부정보 저장(tabpage1)
		case "saveHouseTybyPayPayList":
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/saveHouseTybyPayList.do";
			var sInDatasets   	= "input1=ds_houseTybyPayList:U";
			var sOutDatasets  	= "";
			break;
			
		// 옵션정보 조회(tabpage2)
		case "selectHouseTybyPayOptnList":
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/selectHouseTybyPayOptnList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_houseTybyPayOptnList=output1";			
			break;
			
		// 옵션정보 저장(tabpage2)
		case "saveHouseTybyPayOptnList": 
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/saveHouseTybyPayList.do";
			var sInDatasets   	= "input1=ds_houseTybyPayOptnList:U";
			var sOutDatasets  	= "";
			break;
			
		// 할인정보 조회(tabpage3)
		case "selectHouseTybyPayDscntList":
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/selectHouseTybyPayDscntList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_houseTybyPayDscntList=output1";
			break;

		// 할인정보 저장(tabpage3)
		case "saveHouseTybyPayDscntList":
			var sController   	= "hsco/pms/sls/hou/std/SLS010200/saveHouseTybyPayList.do";
			var sInDatasets   	= "input1=ds_houseTybyPayDscntList:U";
			var sOutDatasets  	= "";
			break;
			
		default :
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/ 
 /*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{		
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		switch(strSvcId){
		
			// 주택타입정보 조회
			case "selectHouseTypeList" :
				if(this.ds_houseTypeList.rowcount > 0 ){
					this.Tab00.set_enable(true);					
					// 탭조회
					this.Tab00_onchanged();
				}      
				break;

			// 주택타입정보 저장
			case "saveHouseTypeList" :
				this.fn_search();
				break;
				
			// 납부정보 조회(tabpage1)
			case "selectHouseTybyPayPayList" :
				this.ds_payCode.filter("CODE == 'S0'|| CODE== 'S1'|| CODE== 'S2'|| CODE== 'S4'|| CODE== 'S5'|| CODE== 'S9'|| CODE== 'R0'|| CODE== 'R1'|| CODE== 'R2'|| CODE== 'R3'|| CODE== 'R4'|| CODE== 'R5'|| CODE== 'R9'");
				break;
				
			// 납부정보 저장(tabpage1)
			case "saveHouseTybyPayPayList" :
				this.Tab00_onchanged();
				break;
				
			// 옵션정보 조회(tabpage2)
			case "selectHouseTybyPayOptnList" :
				this.ds_payCode.filter("CODE == 'SB'|| CODE== 'SC'|| CODE== 'SD'");
				break;
				
			// 옵션정보 저장(tabpage2)
			case "saveHouseTybyPayOptnList" :
				this.Tab00_onchanged();
				break;
				
			// 할인정보 조회(tabpage3)
			case "selectHouseTybyPayDscntList" :
				this.ds_payCode.filter("CODE == 'D1'|| CODE== 'D2'|| CODE== 'D3'");
				break;
				
			// 할인정보 저장(tabpage3)
			case "saveHouseTybyPayDscntList" :
				this.Tab00_onchanged();
				break;
				
			// 주택코드 - 조회영역
			case "houseCodeSh" :
				comboOption = {
					type: this.pmsUtil.PROMPT_SELECT,
					noNullText: "선택"
				}
				this.pmsUtil.setComboPrompt(this.div_search.cmb_houseCodeSh, comboOption);
				break;
				
			default :
				break;
		}
		
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}	
}


/**********************************************************************
 * Tab00 처리
 **********************************************************************/ 

// 탭 정보 조회 호출
this.ds_houseTypeList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.Tab00_onchanged();
}

// 탭 밴경 전 체크
this.Tab00_canchange = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	if (this.div_search.edt_houseCode == "") {
		this.alert("주택타입정보 조회 후 이동 가능합니다.");
		return false;
	}
}

// 탭 변경 체크
this.Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	if (this.ds_houseTypeList.rowcount > 0 ) {
	
		// 조회조건
		var houseCode = this.ds_houseTypeList.getColumn(this.ds_houseTypeList.rowposition, "HOUSE_CODE");
		var houseTyCode = this.ds_houseTypeList.getColumn(this.ds_houseTypeList.rowposition, "HOUSE_TY_CODE");
		
		if (this.comUtils.isNull(houseCode) || this.comUtils.isNull(houseTyCode)) {
			return;
		}
		
		// Clear Datasets
		this.pmsUtil.clearDataSet([this.ds_cond01]);		

		this.ds_cond01.setColumn(0, "HOUSE_CODE", houseCode);
		this.ds_cond01.setColumn(0, "HOUSE_TY_CODE", houseTyCode);
		
		// 탭별 조회
		if (this.Tab00.tabindex == 0) {
							
			// Clear Datasets
			this.pmsUtil.clearDataSet([this.ds_houseTybyPayList]);

			// 납부정보 조회
			this.fn_transaction("selectHouseTybyPayPayList");
						
		} else if (this.Tab00.tabindex == 1) {
					
			// Clear Datasets
