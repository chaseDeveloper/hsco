/**********************************************************************
 * 납부비율 변화에 따른 납부금액 계산
 **********************************************************************/
this.ds_houseTybyPayList_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo){

	if ( e.columnid == "PAY_RATE" ) {
		var suplyAmount= this.ds_houseTypeList.getColumn(0, "SUPLY_AMOUNT"); // 공급금액
		var nRatio = 0;  // 납부비율
		var nAmt = 0;    // 납부금액 * 납부비율
		var nSum = 0;    // 합계
		var nS09Idx = 0; // 잔금 index	
		
		if(suplyAmount > 0){ //공급금액이 존재할때만 계산
			for(var iRow=0; iRow < this.ds_houseTybyPayList.rowcount; iRow++){

				// 잔금 index
				if (this.ds_houseTybyPayList.getColumn(iRow, "PAY_CODE") == "S9") {
					nS09Idx = iRow;
				}
				
				nRatio = this.ds_houseTybyPayList.getColumn(iRow, "PAY_RATE"); 
				
				// 비율이 0보다 큰경우에만 계산
				if (Number(nRatio) >= 0) {
					nAmt = (Math.floor((Number(suplyAmount) * (Number(nRatio) * 0.01)) / 10000)) * 10000; //만단위 절사
					nSum = nSum + parseInt(nAmt);
					this.ds_houseTybyPayList.setColumn(iRow, "PAYAMT", parseInt(nAmt));
				}
			}
			
			// 남는금액은 잔금에 합산함.
			if (nSum < Number(suplyAmount)) {
				var curPayamt = Number(this.ds_houseTybyPayList.getColumn(nS09Idx, "PAYAMT"));
				var rstPayamt = Number(suplyAmount) - nSum;
				var sumPayamt = curPayamt + rstPayamt;
				
				this.ds_houseTybyPayList.setColumn(nS09Idx, "PAYAMT", sumPayamt);					
			}
		} else {
			this.alert("납부금액이 없습니다.");
			return;
		}
	}
}


