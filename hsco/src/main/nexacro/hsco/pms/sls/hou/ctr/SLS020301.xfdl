/*
 * 함수명  	    : fn_tabpage_insert 
 * 설명 		: 주택 압류 ,대출 리스트 행 추가
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_tabpage_insert();
 */
this.fn_tabpage_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow          = this.ds_hcntrctrList.rowposition;  //주택계약자 리스트 선택된 목록 RowPositon
	var nAddRowIndex  = 0                               ;  //추가된 행 RowPosition
	var nTabIndex     = this.Tab00.tabindex             ;  //현재 선택된 TabIndex
	var strCntrCtrNo                                    ;  //주택계약자 계약번호                       

	if(nRow < 0){                                 //선택된 주택계약자 리스트가 없는 경우
		this.gfn_message("info.항목.선택.알림","주택계약 정보");
	    return false;
	}
	
	strCntrCtrNo  = this.ds_hcntrctrList.getColumn(nRow, "CNTRCTR_NO");  //선택된 주택계약자 계약번호 가져오기

	switch (nTabIndex) {
		case 0:     //주택 압류 리스트
		    nAddRowIndex = this.ds_hcntrCtrSeizrList.addRow();
		    this.ds_hcntrCtrSeizrList.setColumn(nAddRowIndex, "CNTRCTR_NO", strCntrCtrNo); //주택계약자 계약번호 입력
		    this.ds_hcntrCtrSeizrList.setColumn(nAddRowIndex, "LON_REGIST", "0"         ); //대출정보 등록해제
			break;
		case 1:     //주택 대출 리스트
   		    nAddRowIndex = this.ds_hcntrCtrLonList.addRow();
		    this.ds_hcntrCtrLonList.setColumn(nAddRowIndex, "CNTRCTR_NO", strCntrCtrNo  ); //주택계약자 계약번호 입력
			break;
	}

	
}

/*
 * 함수명  	    : fn_tabpage_save 
 * 설명 		: 주택 압류,대출 리스트 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_tabpage_save();
 */
this.fn_tabpage_save = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nTabIndex     = this.Tab00.tabindex             ;  //현재 선택된 TabIndex
	
	if(this.confirm("저장하시겠습니까?"))	{

	    switch(nTabIndex){
	        case 0:
	            this.fn_transaction("saveHcntrCtrSeizrList");  //압류리스트 저장
	            break;
	        case 1:
	            this.fn_transaction("saveHcntrCtrLonList")  ;  //대출리스트 저장
	            break;
	            
	    } // END SWITCH
		       
	} // END IF
	
}


