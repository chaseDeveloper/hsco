//탭 이동후 이벤트 처리
this.Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
    var nRow = this.ds_hcntrctrList.rowposition;  //주택계약자 리스트 선택된 목록 RowPositon
    
	if (nRow >=  0) {
		this.fn_tab_search();	
