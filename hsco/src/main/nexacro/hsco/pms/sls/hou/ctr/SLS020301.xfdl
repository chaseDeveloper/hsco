// 주택계약 리스트 데이터셋 Rowposition 변경시 이벤트 처리
this.ds_hcntrctrList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
    var nRow = e.newrow;
    if(nRow < 0 ){
        this.gfn_message("info.항목.선택.알림","주택계약 정보");
        return false;
    }
    
	//(2) tabpage1, tabpage2 압류/대출 정보리스트 상세 출력
	this.fn_tab_search();   //주택계약 압류 정보 출력 함수 호출
	
}

/***********************************************************************
 * 사용자 정의 함수
 ***********************************************************************/ 
/*
 * 함수명  	    : fn_tab_search 
 * 설명 		: 탭 조회-탭 변경시 상세 내용  조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_tab_search();
 */
this.fn_tab_search = function()
{
   
	var nRow           = this.ds_hcntrctrList.rowposition                    ;  //선택된 Row index
    var strCntrCtrNo   = this.ds_hcntrctrList.getColumn(nRow,"CNTRCTR_NO")   ;  //현재 선택된 Row의 주택계약번호
    var nTabIndex      = this.Tab00.tabindex                                 ;  //현택 Tab Index번호
    
	this.Tab00.set_enable(true);
	
    //주택계약 리스트 
    var strCntrCtrNo   = this.ds_hcntrctrList.getColumn(nRow , "CNTRCTR_NO");  //주택계약번호
    var strDong        = this.ds_hcntrctrList.getColumn(nRow , "DONG"      );  //주택 동
    var strHo          = this.ds_hcntrctrList.getColumn(nRow , "HO"        );  //호
    
	//검색 항목 데이터셋 세팅
	this.ds_cond01.setColumn(0 ,"CNTRCTR_NO"  , strCntrCtrNo);  // 주택계약번호
	this.ds_cond01.setColumn(0 ,"DONG"        , strDong     );  // 동
	this.ds_cond01.setColumn(0 ,"HO"          , strHo       );  // 호

	switch(nTabIndex){                                      //탭 Index 체크
		case 0:
		   this.ds_hcntrCtrSeizrList.clearData()         ;  //압류 리스트 데이터셋 클리어
		   this.fn_transaction("selectHcntrCtrSeizrList");  //압류정보 리스트 호출
		   break;
		case 1:
		   this.ds_hcntrCtrLonList.clearData()           ;  //대출 리스트 데이터셋 클리어
		   this.fn_transaction("selectHcntrCtrLonList")  ;  //대출정보 리스트 호출
		   break;
	}
		
}
 
//탭 이동전 이벤트 처리
