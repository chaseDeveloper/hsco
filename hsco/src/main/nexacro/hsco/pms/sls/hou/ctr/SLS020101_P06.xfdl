/**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/
/*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){

		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId){
			// 분양약정 조회
			case "selectlcntrctrPayPopList" :
				break;
				
			// 분양약정 조회
			case "saveLcntrctrPayPopList" :
				this.alert("정상적으로 저장되었습니다");
				this.fn_search();
				break;
				
			// 보증금 조회
			case "selectLcntrctrDepositPayPopList" :
				break;
				
			// 보증금 조회
			case "saveHcntrctrDepositPayPopList" :
				this.alert("정상적으로 저장되었습니다");
				this.fn_search();
				break;

				
			// 임대약정 조회
			case "selectLcntrctrRentPayPopList" :
				break;
				
			// 임대약정 저장
			case "saveHcntrctrRentPayPopList" :
				this.alert("정상적으로 저장되었습니다");
				this.fn_search();
				break;
				
			// 분양약정 납부금액계산 및 적용
			case "selectLttotInspyIntrInfo" :			
				this.btn_calc_lttot_onclick_callback();
				break;
				
			// 임대약정 납부금액계산 및 적용
			case "selectRentInspyIntrInfo" :			
				this.btn_calc_rent_onclick_callback();
