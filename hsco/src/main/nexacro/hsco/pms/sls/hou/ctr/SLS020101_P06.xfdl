    var dong = this.ds_cond00.getColumn(0, "DONG");
    var ho = this.ds_cond00.getColumn(0, "HO");
    var houseTyCode = this.ds_cond00.getColumn(0, "HOUSE_TY_CODE");
	var cstmrNm = this.ds_cond00.getColumn(0, "CNTRCTR_NM");
	var rentQualfSe = this.ds_cond00.getColumn(0, "RENT_QUALF_SE");
