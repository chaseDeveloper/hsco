this.fn_loadCombo = function()
{
	// 공통코드 목록 조회
 	var comCodeParam = [
 		  {OUT_PUT:"ds_payCode", GRP_CODE:"HOU0A1", USE_AT:"1"} // 납부코드
 		, {OUT_PUT:"ds_paymthd", GRP_CODE:"HOU0A7", USE_AT:"1"} // 납부방법
 		, {OUT_PUT:"ds_paySe",   GRP_CODE:"HOU0A8", USE_AT:"1"} // 납부구분
 		, {OUT_PUT:"ds_paySn",   GRP_CODE:"HOU0A9", USE_AT:"1"} // 납부순번 	

 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
 	
	// 주택코드 목록 조회
	var paramMap = {};
	paramMap["USE_AT"] = "";
	this.pmsUtil.getLadCodeList(paramMap, "ds_landCode", "fn_loadCombo_callback");
}

