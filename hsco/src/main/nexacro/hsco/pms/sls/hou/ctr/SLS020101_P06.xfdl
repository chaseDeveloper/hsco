				return;
			}	
		}
		*/

		// 분양약정 저장
		if (this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("saveLcntrctrPayPopList");		
		}
	} else if(this.Tab00.tabindex== 1) { //보증금
		// 검색영역 Validation 체크
		if(!this.gfn_checkValidation(this.ds_lcntrctrDepositPopList, this.ds_validation01)) {
			return;
		}		
		
		// 분양금<>등록금액 불일치 시 저장 불가
		/*
		var paymentSum = this.ds_lcntrctrDepositPopList.getSum('PAYAMT');     //납입금액 총 SUM
		var suplyAmout = this.ds_PayTotlAmt.getColumn(0, "SUPLY_AMOUNT"); //주택 타입 분양금(당초금액)
		if(this.ds_lcntrctrDepositPopList.rowcount > 0){
			if(Number(paymentSum) != Number(suplyAmout)){
				this.alert("등록된 납부금액들 합이 분양금과 일치하지 않습니다");
				return;
			}	
		}
		*/
		// 보증금 저장
		if (this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("saveHcntrctrDepositPayPopList");
		}
	} else if(this.Tab00.tabindex== 2) { //임대료
		// 검색영역 Validation 체크
		if(!this.gfn_checkValidation(this.ds_lcntrctrRentPayPopList, this.ds_validation02)) {
			return;
		}		
		
		// 임대료 저장
		if (this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("saveHcntrctrRentPayPopList");
		}
	}	
}


// 삭제
this.fn_delete = function()
{
	// Dataset 선택
	var targetDS = new Dataset();
		if (this.Tab00.tabindex == 0) {
		targetDS = this.ds_lcntrctrPayPopList;
