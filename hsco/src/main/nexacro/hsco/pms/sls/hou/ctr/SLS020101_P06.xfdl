// 임대약정 - 납부금액계산 및 적용 콜백
this.btn_calc_rent_onclick_callback = function()
{
	var nCurRow = this.ds_lcntrctrRentPayPopList.findRow("CHK", "1");
	var nNewRow = this.ds_lcntrctrRentPayPopList.getRowCount() - 1;
	
	// 분납이자 계산(분납이자가 반환되지 않는다)
	var payamt = this.ds_InspyIntrInfo.getColumn(0, "PAYAMT");
	var curPayamt = this.ds_lcntrctrRentPayPopList.getColumn(nCurRow, "PAYAMT");	
	var newPayamt = this.ds_lcntrctrRentPayPopList.getColumn(nNewRow, "PAYAMT");

	var inspayIntr = Number(curPayamt) - Number(payamt); //분납이자

	// 조회 금액이 0일때 처리
	cal_payamt = payamt == "0" ? curPayamt : payamt;
	cal_inspayIntr = payamt == "0" ? "0" : inspayIntr;

	this.ds_lcntrctrRentPayPopList.setColumn(nCurRow, "PAYAMT", cal_payamt);
	this.ds_lcntrctrRentPayPopList.setColumn(nCurRow, "ARRRG_AMOUNT", cal_inspayIntr);
	
	// ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^
	// 납부합계, 미수금액 계산 (여기에 몇개의 항목이 더 와야 한다) 메소드로 빼는게 맞다. 수정한다.
	// ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^	
	this.ds_lcntrctrRentPayPopList.setColumn(nCurRow, "PAYAMT_SM", curPayamt);
	this.ds_lcntrctrRentPayPopList.setColumn(nCurRow, "UNCLAMT", curPayamt);
	
	this.ds_lcntrctrRentPayPopList.setColumn(nNewRow, "PAYAMT_SM", newPayamt);
	this.ds_lcntrctrRentPayPopList.setColumn(nNewRow, "UNCLAMT", newPayamt);	
	
}

// 임대약정 - 수납금액 변경 처리
this.Tab00_tabpage2_mse_rcivAmount_onchanged = function(obj:MaskEdit, e:nexacro.ChangeEventInfo)
{
	var nCurRow = this.ds_lcntrctrRentPayPopList.findRow("CHK", "1");
	var nNewRow = this.ds_lcntrctrRentPayPopList.getRowCount() - 1;
	
	// 분납금액, 납부금액 계산
	if ( (gPayAmt -  e.postvalue) >= 0) {	
		this.ds_lcntrctrRentPayPopList.setColumn(nCurRow, "PAYAMT", e.postvalue);
		this.ds_lcntrctrRentPayPopList.setColumn(nNewRow, "PAYAMT", (gPayAmt - e.postvalue));
	} else {		
		this.Tab00.tabpage2.mse_rcivAmount.set_value(0);
		this.alert("입력금액이 납부금액을 초과 하였습니다.");
		return;
	}
}

