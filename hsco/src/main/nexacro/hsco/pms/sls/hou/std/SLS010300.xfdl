this.fn_currowExists = function(){

	
    var nRow         = this.ds_houseDongHoList.rowposition;
	var strExists_yn = this.ds_houseDongHoList.getColumn(nRow,"EXISTS_YN");

    var nRow2    =  this.ds_houseTybyPayList.rowposition;
    var nRowtype = this.ds_houseTybyPayList.getRowType(nRow2);
    
    //수정불가
    var result = "";
    if(strExists_yn == 'Y' && nRowtype  != '2'){
        result = "N";
    }
    else {
        result = "Y";
    }
   
   return result;
    
}

