/* 데이터셋 Rowposition 변경시 이벤트 처리 */
this.ds_houseDongHoList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	/* 선택된 주택 코드 목록에 해당하는 주택타입 콤보 호출 */
	var nRow           = this.ds_houseDongHoList.rowposition;
    
    /* 선택된 데이터가 없거나, 조회 항목이 없는 경우 */
    if(nRow < 0 ){
      //  this.gfn_message("info.선택데이터.없음.선택.요청");
        return;
    }
    
    /*주택코드별 납부코드 콤보 리스트*/
    var strHouseCode = this.ds_houseDongHoList.getColumn(nRow, "HOUSE_CODE"    ); //주택코드
    var strHouseTy   =  this.ds_houseDongHoList.getColumn(nRow, "HOUSE_TY_CODE"); //주택타입코드
    //this.fn_fill_code(this.ds_payCode, "/hsco/pms/sls/hou/std/SLS010300/comboHouseTyByPayList.do", "", {HOUSE_CODE:strHouseCode},"fn_callbackcmbPayList");
    
    this.fn_cmbHouseTyByPayCodeList(); //주택타입별 납부코드 콤보 리스트 호출
    
    //this.ds_payCode.filter(" HOUSE_TY_CODE == '84BSW-0101' "); //주택코드별 납부코드 목록
    //this.ds_payCode.filter(" HOUSE_TY_CODE == '" + strHouseTy +"'"); //주택코드별 납부코드 목록

    
    var strRowtype   = this.ds_houseDongHoList.getRowType(nRow           ); //로우타입
    var strExists    = this.ds_houseDongHoList.getColumn(nRow,"EXISTS_YN"); //계약정보에서 사용여부(Y:사용중,N:사용안함)
    
    /*납부정보에서 사용중, 신규행이 아닌경우 수정불가*/
    if(strExists == 'Y'){
        this.div_houseDongHoInfo.set_enable(false);
    }else{ 
        this.div_houseDongHoInfo.set_enable(true);
    }

    /* 납부정보,옵션정보,할인정보 데이터셋 클리어 */
	this.ds_houseTybyPayList.clearData()     ; // 납부정보 데이터셋
	this.ds_houseTybyPayOptnList.clearData() ; // 옵션정보 데이터셋
	this.ds_houseTybyPayDscntList.clearData(); // 할인정보 데이터셋

    /* 동호 행 추가시 하단 탭 활성화/비활성화 */
	if(strRowtype == '2'){
         this.TAB_ENABLE = false;          // 하단 탭 버튼 비활성화
	}else{
        this.TAB_ENABLE = true;            // 하단 탭 버튼 활성화
	    this.fn_house_dongho_ty_by_info(); // (2) 주택타입 상세 정보 호출
	    this.fn_tab_search();              // (3) 탭 정보 조회
    }
    
}


