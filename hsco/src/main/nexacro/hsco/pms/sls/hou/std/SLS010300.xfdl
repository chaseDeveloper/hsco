	}
	
}

/*
 * 함수명  	    : fn_tabpage_insert
 * 설명 		: 해당탭 행추가 (추가버튼 클릭시)
 * params  		: obj:Button,  e:nexacro.ClickEventInfo
 * return Type 	: None
 * 사용법		: this.fn_tabpage_insert();
 */
this.fn_tabpage_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.TAB_ENABLE == false){
	  return;
	}
	
	var nDatasetCount = this.ds_houseDongHoList.rowposition;  //주택동호정보 목록 카운트
	 
	if(nDatasetCount < 0 ){
	    this.gfn_message("info.선택데이터.없음.선택.요청");
	    return ;
	}
	
	var nRow          = 0;
	var nAddRowIndex  = 0;
		
	var nRow           = this.ds_houseDongHoList.rowposition;  //선택된 주택동호 정보 목록 index 번호
	
	var strHouseCode   = this.ds_houseDongHoList.getColumn(nRow, "HOUSE_CODE")   ; //주택코드
	var strHouseTyCode = this.ds_houseDongHoList.getColumn(nRow, "HOUSE_TY_CODE"); //주택타입코드
	var strDong        = this.ds_houseDongHoList.getColumn(nRow, "DONG")         ; //동
	var strHo          = this.ds_houseDongHoList.getColumn(nRow, "HO")        ; //호
	
	//납부정보		
	if (this.Tab00.tabindex == 0) {
	
	    nAddRowIndex = this.ds_houseTybyPayList.addRow();  //추가된 행 index no
	    
	    this.ds_houseTybyPayList.setColumn(nAddRowIndex, "CHK"           , '0'            );
		this.ds_houseTybyPayList.setColumn(nAddRowIndex, "HOUSE_CODE"    , strHouseCode   );
		this.ds_houseTybyPayList.setColumn(nAddRowIndex, "HOUSE_TY_CODE" , strHouseTyCode );
		this.ds_houseTybyPayList.setColumn(nAddRowIndex, "DONG"          , strDong        );
		this.ds_houseTybyPayList.setColumn(nAddRowIndex, "HO"            , strHo          );
		this.ds_houseTybyPayList.setColumn(nAddRowIndex, "OPTN_IEM_AT"   , '0'            );
		this.ds_houseTybyPayList.setColumn(nAddRowIndex, "ENGG_SE"       , '01'           );
		
		 
	//옵션정보
	} else if (this.Tab00.tabindex == 1) {
		
        nAddRowIndex = this.ds_houseTybyPayOptnList.addRow();  //추가된 행 index no

        this.ds_houseTybyPayOptnList.setColumn(nAddRowIndex, "CHK"           , '0'            );
		this.ds_houseTybyPayOptnList.setColumn(nAddRowIndex, "HOUSE_CODE"    , strHouseCode   );
	    this.ds_houseTybyPayOptnList.setColumn(nAddRowIndex, "HOUSE_TY_CODE" , strHouseTyCode );
	    this.ds_houseTybyPayOptnList.setColumn(nAddRowIndex, "DONG"          , strDong        );
	    this.ds_houseTybyPayOptnList.setColumn(nAddRowIndex, "HO"            , strHo          );
	    this.ds_houseTybyPayOptnList.setColumn(nAddRowIndex, "OPTN_IEM_AT"   , '1'            );
	    this.ds_houseTybyPayOptnList.setColumn(nAddRowIndex, "ENGG_SE"       , '02'           );
			
	//할인정보
	} else if (this.Tab00.tabindex == 2) {
		
		nAddRowIndex = this.ds_houseTybyPayDscntList.addRow();  //추가된 행 index no
		
		this.ds_houseTybyPayDscntList.setColumn(nAddRowIndex, "CHK"           , '0'            );
		this.ds_houseTybyPayDscntList.setColumn(nAddRowIndex, "HOUSE_CODE"    , strHouseCode   );
	    this.ds_houseTybyPayDscntList.setColumn(nAddRowIndex, "HOUSE_TY_CODE" , strHouseTyCode );
	    this.ds_houseTybyPayDscntList.setColumn(nAddRowIndex, "DONG"          , strDong        );
	    this.ds_houseTybyPayDscntList.setColumn(nAddRowIndex, "HO"            , strHo          );
	    this.ds_houseTybyPayDscntList.setColumn(nAddRowIndex, "OPTN_IEM_AT"   , '0'            );
	    this.ds_houseTybyPayDscntList.setColumn(nAddRowIndex, "ENGG_SE"       , '03'           );
		
	}
	
	
}


