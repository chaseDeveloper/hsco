 /**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/ 
 /*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
    var bSuccess = nErrorCode == 0 ? true : false;
	
	//성공,에러메시지 출력 
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);

	if (bSuccess){
			
        var comboOption = null;
       		
		switch (strSvcId) {
		    
		    case "cmbHouseCodeList" : 	// 주택코드  콤보  -- SvcId  ds_ 를 제외한 명(OutDataset_ID)
				comboOption = {
					type: this.pmsUtil.PROMPT_SELECT,  //PROMPT_SELECT(선택),PROMPT_ALL
					//noNullText: "코드없음"  //목록이 없는 경우 출력 텍스트
				}
				this.pmsUtil.setComboPrompt(this.div_search.cmb_houseCodeCmb, comboOption, true);	// 조회영역
				break;
			
			// 타입 콤보 로드
			case "tyAeaListCmb"  :
				/*
				comboOption = {
					type: this.pmsUtil.PROMPT_SELECT, //PROMPT_SELECT(선택),PROMPT_ALL
					noNullText: "코드없음" //목록이 없는 경우 출력 텍스트
				}
				this.pmsUtil.setComboPrompt(this.div_search.cmb_houseSeSh01, comboOption, true);		// 조회영역
				*/
				break;
				
			case "comboHouseTyCodeList" :
				break;
		 
			case "selectHouseDongHoList"        : // 주택동호 정보 목록
				  break;	
				  
			case "comboHouseTyByPayList"        : // 주택 타입별 납부코드 콤보 리스트
				  break;	  
				  
			case "selectHouseDongHoTyByInfo"    : // 주택동호별 타입 상세 정보
		        break;    	
		        
		    case "saveHouseDongHoList"          : // 주택 동호 정보 저장	
		     
		        this.fn_search();                 // 주택동호 정보 조회
		     //   this.fn_setposition(this.ds_houseDongHoList);
		     
		        this.ds_houseDongHoList.setLastRowposition(); //저장후에는 마지막 로우에 포커스가 가도록
		        
		        break;
		        
		    case "selectHouseTybyPayList"       : // 납부정보 조회(tapage1)	
		        break;    
		        
		    case "selectHouseTybyPayOptnList"   : // 옵션정보 조회(tabpage2)
				break;
				
			case "selectHouseTybyPayDscntList"  : // 할인정보 조회(tabpage3)
				break;    
				
			case "saveHouseTybyPayList"         : // 납부정보 저장 (tapage1)	
			    this.Tab00_onchanged();
		        break;    
		        
		    case "saveHouseTybyPayOptnList"     : // 옵션정보 저장(tabpage2)
		        this.Tab00_onchanged();           //Tab 변경 이벤트 호출
				break;
				
			case "saveHouseTybyPayDscntList"    : // 할인정보 저장(tabpage3)
			    this.Tab00_onchanged();           //Tab 변경 이벤트 호출 
				break;    
					
			default :
					break;		
		}
		
        
    }else{
	   // this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}	
		
	
}

 
