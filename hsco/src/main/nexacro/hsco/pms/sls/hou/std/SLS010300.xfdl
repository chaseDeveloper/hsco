 /***********************************************************************
 * 사용자 정의 함수
 ***********************************************************************/ 
/*
 * 함수명  	    : fn_tab_search
 * 설명 		: 탭변경시 상세정보 내용 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_tab_search();
 */
this.fn_tab_search = function()
{

	var nRow      = this.ds_houseDongHoList.rowposition; //현재 선택된 RowPosition
    
	if(nRow < 0 ){
	   return;
	}
	
	var nRowType  = this.ds_houseDongHoList.getRowType(nRow); 
	var strHouseSe = this.ds_houseDongHoList.getColumn(nRow, "HOUSE_SE"); //주택구분(1:분양,2:임대,3:분양전환)

	
	//동호정보의 추가된 행이면 리턴 시킴 납부,옵션,할인 정보 등록 할수 없음
	if(nRowType == '2'){
	    return;
	}
	
	if (this.Tab00.tabindex == 0) {
	    if(strHouseSe == '1'){ //분양-납부정보
	        this.ds_payCode.filter(" CODE=='S0' || CODE=='S1' || CODE=='S2' || CODE=='S3' || CODE=='S4' || CODE=='S5' || CODE=='S6' || CODE =='S9' || CODE=='SE'  ");
	    }else if(strHouseSe == '2'){ //임대-납부정보
	        this.ds_payCode.filter(" CODE=='R0' || CODE=='R1' || CODE=='R2' || CODE=='R3' || CODE=='R4' || CODE=='R5' || CODE=='R9' || CODE=='RP' ");
	    }else if(strHouseSe == '3'){ //분양전환
	        this.ds_payCode.filter(" CODE=='S0' || CODE=='S1' || CODE=='S2' || CODE=='S3' || CODE=='S4' || CODE=='S5' || CODE=='S6' || CODE =='S9' || CODE=='SE' || CODE=='R0' || CODE=='R1' || CODE=='R2' || CODE=='R3' || CODE=='R4' || CODE=='R5' || CODE=='R9' || CODE=='RP' ");
	    }
	
		// 납부정보 데이터셋 클리어
		this.ds_houseTybyPayList.clearData();
		
		// 조회조건 세팅
		this.ds_cond01.setColumn(0, "HOUSE_CODE"    ,this.ds_houseDongHoList.getColumn(nRow, "HOUSE_CODE")   ); //주택코드
		this.ds_cond01.setColumn(0, "DONG"          ,this.ds_houseDongHoList.getColumn(nRow, "DONG")         ); //동
		this.ds_cond01.setColumn(0, "HO"            ,this.ds_houseDongHoList.getColumn(nRow, "HO")           ); //호
		this.ds_cond01.setColumn(0, "HOUSE_TY_CODE" ,this.ds_houseDongHoList.getColumn(nRow, "HOUSE_TY_CODE")); //주택타입코드
			
		// 납부정보 조회
		this.fn_transaction("selectHouseTybyPayList");
		
	} else if (this.Tab00.tabindex == 1) {
       // this.alert(this.Tab00.tabindex);
        //납부코드 필터링
        this.ds_payCodeSh.filter(" CODE=='SB' || CODE=='SC' || CODE=='SD' ");
          
		// 옵션정보 데이터셋 클리어
		this.ds_houseTybyPayOptnList.clearData();
		
		// 조회조건
		this.ds_cond02.setColumn(0, "HOUSE_CODE"    ,this.ds_houseDongHoList.getColumn(nRow, "HOUSE_CODE")   ); //주택코드
		this.ds_cond02.setColumn(0, "DONG"          ,this.ds_houseDongHoList.getColumn(nRow, "DONG")         ); //동
		this.ds_cond02.setColumn(0, "HO"            ,this.ds_houseDongHoList.getColumn(nRow, "HO")           ); //호
		this.ds_cond02.setColumn(0, "HOUSE_TY_CODE" ,this.ds_houseDongHoList.getColumn(nRow, "HOUSE_TY_CODE")); //주택타입코드
		
		// 옵션정보 조회
		this.fn_transaction("selectHouseTybyPayOptnList");
		
	} else if (this.Tab00.tabindex == 2) {
	    //this.alert(this.Tab00.tabindex);
	    //납부코드 필터링
        this.ds_payCode.filter(" CODE=='D1' || CODE=='D2' || CODE=='D3' ");
        
		// 할인정보 데이터셋 클리어
		this.ds_houseTybyPayDscntList.clearData();
		
		// 조회조건
		this.ds_cond03.setColumn(0, "HOUSE_CODE"    ,this.ds_houseDongHoList.getColumn(nRow, "HOUSE_CODE")   ); //주택코드
		this.ds_cond03.setColumn(0, "DONG"          ,this.ds_houseDongHoList.getColumn(nRow, "DONG")         ); //동
		this.ds_cond03.setColumn(0, "HO"            ,this.ds_houseDongHoList.getColumn(nRow, "HO")           ); //호
