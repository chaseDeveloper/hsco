 		    var sController   	= "hsco/pms/sls/hou/std/SLS010300/comboHouseTyCodeList.do";
	        var sInDatasets   	= "input1=ds_cond00";
	        var sOutDatasets  	= "ds_houseTyCmb=output1";
		    break;
		
		// 주택 동호 리스트
