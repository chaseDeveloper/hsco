 * @param e 발생이벤트
 * @return void
 */
this.fn_onFileSuccess = function(type, e){

	if(type == this.FILE_MANAGER.TYPE_UPLOADER){
		 this.alert("처리완료");
		 	this.fn_transaction("selectSlaryList");	
		//this.fn_setSelFileInfoTitle();	// 파일 타이틀 설정
	}
	
