/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SLS010100_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();             // 공통 폼 초기화
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화
}


this.SLS010100_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{		
	// Combo에 사용되는 Dataset Load
	this.fn_loadCombo();		
	// 사용여부 (Default : 사용)
	this.div_search.rdo_useAt.set_index(1);
	//조회 2016.06.21
	this.fn_search();

}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	    : initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}


/*
/*
 * 함수명  	    : fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);	// 조건에 사용되는 Dataset 초기화
}

/***********************************************************************
 * 공통 코드 조회
 ***********************************************************************/
/*
 * 함수명  	    : fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function()
{
	// 공통코드 목록 조회
 	var comCodeParam = [
 		  {OUT_PUT:"ds_houseSeSh", GRP_CODE:"HOU0A2", USE_AT:"1"} // 계약구분
 		, {OUT_PUT:"ds_intrtSe",   GRP_CODE:"HOU015", USE_AT:"1"} // 용도구분
 		, {OUT_PUT:"ds_lttotCnvrsSe",   GRP_CODE:"HOU013", USE_AT:"1"} // 분양전환구분
 		
 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
 		
	// 사업코드 목록 조회
	/*
	var bsnsParamMap = {};
	bsnsParamMap["USE_AT"] = "";	
	this.pmsUtil.getBsnsCodeList(bsnsParamMap, "ds_bsnsCode");
	*/
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: callback 처리 후에 더 필요한 작업이 있을 경우, 작성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
 */
this.fn_loadCombo_callback = function ()
{
	// 주택구분 분양 설정
	this.ds_intrtSe.filter("CODE.substr(0,1)=='I'");
	this.ds_houseSeSh.filter("CODE.substr(0,1)!='R'");
	this.ds_houseSe.filter("CODE.substr(0,1)!='R'");
	
	this.div_top.cmb_lttotCnvrsSe.set_index(0);
}

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	    : fn_search
 * 설명 		: 주택기본정보  조회 이벤트 처리
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{

	// Clear Datasets
	this.pmsUtil.clearDataSet([this.ds_houseList]);
		
	// 주택목록조회
	this.fn_transaction("selectHouseList");
}

 /*
 * 함수명  	    : fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{

	// 사용여부 (Default : 사용)
	this.div_search.rdo_useAt.set_index(1);
	
	if (this.ds_houseList.rowcount > 0 ) {
		var nRow = this.ds_houseList.addRow();
		this.ds_houseList.setColumn(nRow, "HOUSE_SE", this.ds_cond00.getColumn(0,"HOUSE_SE"));		
	} 
	else {
		this.alert("조회된 자료가 없습니다.");
		return;
	}
}

/*
 * 함수명  	    : fn_save
 * 설명 		: 주택기본정보 저장 이벤트 처리
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{

	// Validation 체크
	if(this.ds_houseList.rowcount > 0) {
	
		// Validation 체크
		if(this.comUtils.isDatasetUpdated(this.ds_houseList) == true)		
		{
			if(!this.gfn_checkValidation(this.ds_houseList, this.ds_validation00)) {
				return;
			}
		}		
	
		// Validation 체크
		if(this.comUtils.isDatasetUpdated(this.ds_intrtList) == true)		
		{		
			if(!this.gfn_checkValidation(this.ds_intrtList, this.ds_validation00)) {
				return;
			}	
		}
	} else {
		this.alert("조회된 자료가 없습니다.");
		return;
	}

	// 저장확인
	if(this.confirm("저장하시겠습니까?"))	{
		this.fn_transaction("saveHouseList");
	}
}


/*
 * 함수명  	    : fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	if(this.gfn_message("confirm.삭제여부"))	{	
		var nRow = this.ds_houseList.rowposition;
		this.ds_houseList.deleteRow(nRow);	
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
/*
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectHouseList": // 주택기본정보 조회
			var sController   	= "hsco/pms/sls/hou/std/SLS010100/selectHouseList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_houseList=output1";			
			break;
		case "saveHouseList":	// 저장
			var sController   	= "hsco/pms/sls/hou/std/SLS010100/save.do";
			var s  = "input1=ds_houseList:U"
				  + " input2=ds_intrtList:U";			
			var sInDatasets   	= s;
			var sOutDatasets  	= "";
			break;
			
		default :
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


/**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/ 
 /*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{		
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId){
		
			// 공통코드 목록 조회
			case "selectCommonCode" :
			
				this.ds_houseSe.copyData(this.ds_houseSeSh);
			
				// 조회영역 - 토지구분
				targetCombo = this.div_search.cmb_houseSeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";

				
				// 분양전환구분
				//targetCombo = this.div_top.cmb_lttotCnvrsSe;
				//comboOption = {};
				//comboOption["type"] = this.pmsUtil.PROMPT_SELECT;
				//comboOption["noNullText"] = "코드없음";

				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
								
				this.fn_loadCombo_callback();

				break;

			// 사용자코드 - 사업코드
			case "houseCode" :			
				break;	

			// 사용자코드 - 사업코드
			case "bsnsCode" :
				break;

			// 주택기본정보 조회
			case "selectHouseList":
				if(this.ds_houseList.rowcount > 0 ){
					this.Tab00.set_enable(true);
				}      
				break;


			// 주택기본정보 저장
			case "saveHouseList":
				this.fn_search();
				break;

			default :
				break;
		}
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
		
	}	
}


