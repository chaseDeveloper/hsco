    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : SLS020500
 * 화면명   : 입주대기자관리
 * 화면설명 : 입주대기자를 관리한다.
 * 작성일   : 2015.10.10
 * 작성자   : 정윤원
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    2015.00.00   홍길동   fn_xxx() 보완 수정
 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::pmsUtilInclude.xjs"; 

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.HOUSE_SE = "S2";
////this.HOUSE_SE = "S1,S2,S3,S4,S5,S9,R1,R2,R3,R9";

//this.HOUSE_SE = "S2,S4";  //임대(S2,S4),임차권양도
//this.HOUSE_SE = "S1,S3,S5,S9";  //임대(S2,S4),임차권양도
//this.dNowDate = this.gfn_dateInfo("NOW_DATE");  //현재 날짜


/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
//form init 
this.SLS020500_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{   
    //그리드 정렬
	this.fn_init(this);
	
	// Form 초기화
	this.fn_init_form();
	
	// DataSet 초기화
	this.fn_init_dataset();
	
}

this.SLS020500_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	 // 공통콤보 코드처리
	this.fn_loadCombo();
	
	//주택코드 콤보리스트(사용자 콤보 처리)
	this.fn_loadComboNonCodeTbl();
	
	//this.div_search.cal_fromDate.set_value(this.dNowDate ); //현재 서버 년월일 세팅(시작일)
	//this.div_search.cal_toDate.set_value(this.dNowDate )  ; //현재 서버 년월일 세팅(종료일)
	
	// 문자발송 내용
	this.fn_fill_code(this.ds_smsCn,
					  "/hsco/cmm/ums/mssageManageList.do", 
					  "",
					  {CODE : "SLS006"},		// 문자코드 
					  "");
	
}


/***********************************************************************
 * 공통 코드 조회
 ***********************************************************************/
/*
 * 함수명  	    : fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 * 1. param_1   : 반환받을 DATASET 명 		(예 : ds_comCode00)
 * 2. param_2	: 그룹코드					(예 : COM007)
 * 3. param_3	: 사용여부		    		(예 : 1(사용), ""(사용안함))
 * 4. param_4	: 콤보박스INDEX(0) 표시		(예 : "전체", "선택", " "(공백), ""(첫번째데이터부터시작) )
 */
this.fn_loadCombo = function ()
{
    //처리구분,자격구분  콤보
	var comboParams = [
			              ["ds_qualfSeCmb"    ,"HOU004" ,"1" ,"선택"    ]   //입주자격(분양,임대)
			             ,["ds_processSeCmb"  ,"HOU005" ,"1" ,"선택"    ]   //입주대기자 처리구분
			             ,["ds_relationCmb"   ,"HRM013" ,"1" ,"선택"    ]   //입주대기자 처리구분
		              ];
		              
    this.gfn_comboLoad(comboParams, this.fn_callbackAfter);		              

}


/* 사용자 콤보처리
 * 함수명  	: fn_loadComboNonCodeTbl
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: strDs - Data를 담아올 dataset 명
 *                obj - Click된 Combo Component
 *                ※ 위 Argument가 사용되면 함수는 해당되는 건만 찾아서 데이터를 가져온다.
 *                   전체 Combo의 데이터를 가져오려고 한다면, 생략하면 된다. 예 : this.fn_loadComboNonCodeTbl();
 * return Type 	: None
 * 사용법		: this.fn_loadComboNonCodeTbl();
 */
this.fn_loadComboNonCodeTbl = function (strDs, obj)
{

	// [BEGIN] 주택코드 목록
	// 전달값 구성
	var paramMap = {};
	paramMap["HOUSE_SE"] = this.HOUSE_SE;	// 주택구분(판매)
	paramMap["USE_AT"  ] = "1"          ;   // 사용여부 : 0(미사용), 1(사용)
	
	this.pmsUtil.getHouseCode(paramMap, "ds_houseCodeListCmb");

	
}


/*
 * 함수명  	    : fn_callbackAfter
 * 설명 		: combo에 대한 callback 함수 이후에 처리되어야 할 코딩 기술
 * params  		: svcId - Transaction Service ID.
 * ※ 반드시, 데이터를 담아 올 dataset명(예 : "ds_comCode03")을 사용해야 한다.
 * return Type 	: None
