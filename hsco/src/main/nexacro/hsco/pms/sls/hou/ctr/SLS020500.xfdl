this.fn_insert = function()
{          
                           
   var strHouseCode   = this.div_search.cmb_houseCodeSh.value;  //주택코드 선택된 코드값
   var strHouseCodeNm = this.div_search.cmb_houseCodeSh.text ;  //주택코드 선택된 텍스트

	this.ds_houseMvnWaitrFamilyList.clearData();
  
    if(this.comUtils.isNullEmpty(strHouseCode) == true){
    	this.gfn_message("info.선택데이터.없음.선택.요청");
          return; 
    }

    this.ds_houseMvnWaitrList.set_enableevent(false) ;
    var nRow = this.ds_houseMvnWaitrList.addRow();  // 행추가
    this.ds_houseMvnWaitrList.set_enableevent(true) ;
  
    this.ds_houseMvnWaitrList.setColumn(nRow, "HOUSE_CODE"   , strHouseCode                   ); //주택코드
    this.ds_houseMvnWaitrList.setColumn(nRow, "HOUSE_CODE_NM", strHouseCodeNm                 ); //주택코드명
