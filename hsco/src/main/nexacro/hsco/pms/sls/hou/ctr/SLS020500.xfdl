/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/  
/*
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{

    var bSync = true;
    
	switch (strSvcId) {
		
		// 주택입주 대기자 리스트 조회
		case "selectHouseMvnWaitrList": 
 		    var sController   	= "hsco/pms/sls/hou/ctr/SLS020500/selectHouseMvnWaitrList.do";
	        var sInDatasets   	= "input1=ds_cond00";
	        var sOutDatasets  	= "ds_houseMvnWaitrList=output1";
	        bSync = false;
		    break;     
		
		
		//주택 입주 대기자 리스트 저장 (입력,수정,삭제)
		case "saveHouseMvnWaitrList": 
 		     var sController   	= "hsco/pms/sls/hou/ctr/SLS020500/saveHouseMvnWaitrList.do";
	        var sInDatasets   	= "input1=ds_houseMvnWaitrList:U";
	        var sOutDatasets  	= "";
	         break;       

		//주택 입주 대기자 리스트 저장 (입력,수정,삭제)
		case "saveHouseMvnFamilyList": 
 		     var sController   	= "hsco/pms/sls/hou/ctr/SLS020500/saveHouseMvnFamilyList.do";
	        var sInDatasets   	= "input1=ds_houseMvnWaitrFamilyList:U";
	        var sOutDatasets  	= "";
	         break;       
        
        // 주택입주 대기자 가족 리스트 조회
		case "selectCstmrFamilyList": 
 		    var sController   	= "hsco/pms/sls/hou/ctr/SLS020500/selectCstmrFamilyList.do";
	        var sInDatasets   	= "input1=ds_cond01";
	        var sOutDatasets  	= "ds_houseMvnWaitrFamilyList=output1";
		    break;
        
        // 주택입주 대기자 계약통지일자 업데이트
		case "updateNticeDeList": 
 		    var sController   	= "hsco/pms/sls/hou/ctr/SLS020500/updateNticeDeList.do";
	        var sInDatasets   	= "input1=ds_houseMvnWaitrListReport:U";
	        var sOutDatasets  	= "";
		    break;       
   

   
   
    //    	this.pmsUtil.dumper(this.ds_cond02); //전체 출력
		
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
	//Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}
 
 

 /**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/ 
 /*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	
	//성공,에러메시지 출력 
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	
	if (bSuccess){
			
        var comboOption = null;
       		
		switch (strSvcId) {
		    //주택코드 콤보
			case "houseCodeListCmb"              :
				comboOption = {
					type: this.pmsUtil.PROMPT_ALL, //PROMPT_SELECT(선택),PROMPT_ALL
					noNullText: "코드없음" //목록이 없는 경우 출력 텍스트
				}
				this.pmsUtil.setComboPrompt(this.div_search.cmb_houseCodeSh, comboOption, true);  // 조회영역
				
				break;
			 
			// 평형(평수) 콤보 로드
			case "aeaListCmb"                    :
				comboOption = {
					type: this.pmsUtil.PROMPT_ALL, //PROMPT_SELECT(선택),PROMPT_ALL
					noNullText: "코드없음" //목록이 없는 경우 출력 텍스트
				}
				this.pmsUtil.setComboPrompt(this.div_search.cmb_houseAea, comboOption, true);	  // 조회영역
				break;
				
			// 타입 콤보 로드
			case "tyAeaListCmb"                   :
				comboOption = {
					type: this.pmsUtil.PROMPT_ALL, //PROMPT_SELECT(선택),PROMPT_ALL
					noNullText: "코드없음" //목록이 없는 경우 출력 텍스트
				}
				this.pmsUtil.setComboPrompt(this.Div00.cmb_houseTyAea, comboOption, true);		  // 조회영역
				break;
						  
			case "selectHouseMvnWaitrList"        :                                               // 주택 입주대기자 조회 리스트
				 
				 
				if(this.ds_houseMvnWaitrList.getColumn(this.ds_houseMvnWaitrList.rowposition, "HOUSE_CSTMR_NO") != undefined){ 
				    var edt_adres06 = this.ds_houseMvnWaitrList.getColumn(0, "IHIDNUM");
					this.Div00.edt_adres06.set_tooltiptext(edt_adres06.substr(0,6)+ '-' + edt_adres06.substr(6));
				}	
				 				
				break;
				
			case "saveHouseMvnWaitrList"          :                                               // 주택 입주대기자 저장후, 조회
				this.fn_search();                                                      
				this.ds_houseMvnWaitrList.setLastRowposition(); //저장후에는 마지막 로우 포커스로 이동
				break;    
				
			case "selectCstmrFamilyList"          :                                               // 주택 입주대기자 가족 리스트 조회
				break;    
				
			case "updateNticeDeList"              :                                               // 주택 입주대기자 계약통지일자 업데이트
				this.fn_search();
				this.ds_houseMvnWaitrList.setLastRowposition(); //저장후에는 마지막 로우 포커스로 이동
				break;  
	
		}
		
	}else{
	   // this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}	
	
} 

