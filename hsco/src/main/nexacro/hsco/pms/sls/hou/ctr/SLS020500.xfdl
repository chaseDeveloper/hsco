//------------------------------
// 문자발송 Button Clicked 
//------------------------------
this.fn_sendSMS = function()
{
	var chkRowCnt = this.ds_houseMvnWaitrList.getRowCount();
	
    if ( chkRowCnt <= 0 ){
        this.alert("전송대상을 선택하세요.");
        return false;   
    }
	
	var callCnt = 0;
	this.ds_sms.clearData();

	/*
    [화성도시공사:계약안내]안녕하십니까? %고객명%고객님 화성도시공사입니다.  
    신청하신 공공임대 아파트 계약안내문이 %안내문자발송일%에 등기우편으로 발송되었습니다.  계약일은 %계약일%입니다.
	*/
	for (var i = 0 ; i < this.ds_houseMvnWaitrList.rowcount ; i++) {
	
		if (this.ds_houseMvnWaitrList.getColumn(i, "CHK") == '1') {
		
		    var TODAY = this.gfn_today();
		    var APDAY = this.ds_headerInfo.getColumn(0, "APPLY_DE");
		    
		    var nRow = this.ds_sms.addRow();	
			// 수신자정보 설정
			this.ds_sms.setColumn(nRow, "MBTLNO"	  , this.ds_houseMvnWaitrList.getColumn(i, "MBTLNO"));	// 수신자번호
			this.ds_sms.setColumn(nRow, "NM"	      , this.ds_houseMvnWaitrList.getColumn(i, "CSTMR_NM"));		// 수신자명	
					
            this.ds_sms.setColumn(nRow, "CHANGE_WORD0", this.ds_houseMvnWaitrList.getColumn(i, "CSTMR_NM")); // 고객명 
            this.ds_sms.setColumn(nRow, "CHANGE_WORD1", APDAY.substring(0,4) + "년 " + APDAY.substring(4,6) + "월 " + APDAY.substring(6,8) + "일");
            this.ds_sms.setColumn(nRow, "CHANGE_WORD2", TODAY.substring(0,4) + "년 " + TODAY.substring(4,6) + "월 " + TODAY.substring(6,8) + "일");
            
            this.ds_sms.setColumn(nRow, "CHANGE_WORD3", "");                            // 
            this.ds_sms.setColumn(nRow, "CHANGE_WORD4", "");                                   // 
            this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_0", "1");  // 대기자 : 1 , 계약자 : 2
            this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_2", this.ds_houseMvnWaitrList.getColumn(i, "HOUSE_CODE"));  // 주택코드
            this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_4", this.ds_houseMvnWaitrList.getColumn(i, "HOUSE_SN"));  //
		
			callCnt = callCnt + 1;
		}
	}

    if ( callCnt > 0 ) {
    
		var CONTENT = this.ds_smsCn.getColumn(0, "MSSAGE_CN") || '';  // 메시지 내용
		this.gfn_popup("COM010700_P01.xfdl"
				   , 800
				   , 500
				   , "SMS발송"
				   , {CONTENT : CONTENT, 
				      JOB_SE : "SLS006", 
                      MXTR_WRD0 : "고객명",
                      MXTR_WRD1 : "계약일",
                      MXTR_WRD2 : "안내문발송일",
                      MXTR_WRD3 : "",
                      MXTR_WRD4 : "",                     				      
				      ds : this.ds_sms}
				   , "com::COM010700_P01.xfdl"
				   , "sms_callback");
    }else{
		return false;
    }
}

