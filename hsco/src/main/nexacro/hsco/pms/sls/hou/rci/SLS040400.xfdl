// 계정구분 itemChanged
//--------------------------------
this.div_work_div_inputType05_cmb_trvctAcntSe_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.div_work.div_inputType05.ACNT_CODE.set_value("");
