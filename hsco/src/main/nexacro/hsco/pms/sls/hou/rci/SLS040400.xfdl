	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{

	var bSync = true;

	switch (strSvcId) {
	
		//수입조정일계표
		case "selectIncmeMdatDailyList":
			var sController   	= "hsco/pms/sls/hou/rci/SLS040400/selectIncmeMdatDailyList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_incmeMdatDailyList=output1";
			break;	
			
		//월별징수현황
		case "selectMonthlyLevList":
			var sController   	= "hsco/pms/sls/hou/rci/SLS040400/selectMonthlyLevList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_monthlyLevList=output1";
			break;		
				
		//자금수입	기록부
		case "selectCptalIncmeList":
			var sController   	= "hsco/pms/sls/hou/rci/SLS040400/selectCptalIncmeList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cptalIncmeList=output1";
			break;
			
		//수입예산정리부
		case "selectIncmeBdgArngList":
			var sController   	= "hsco/pms/sls/hou/rci/SLS040400/selectIncmeBdgArngList.do";	
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_incmeBdgArngList=output1";
			break;
			
		//지구별 수입예산정리부_월계표
		case "selectAreaIncmeBdgArngList1": 
			var sController   	= "hsco/pms/sls/hou/rci/SLS040400/selectAreaIncmeBdgArngList1.do";		
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_areaIncmeBdgArngList1=output1";
			break;
			
		//지구별 수입예산정리부_집계표
		case "selectAreaIncmeBdgArngList2": 
			var sController   	= "hsco/pms/sls/hou/rci/SLS040400/selectAreaIncmeBdgArngList2.do";		
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_areaIncmeBdgArngList2=output1";
			break;						
			
		//내부결재 번호 조회
		case "searchDecsn": 
			var sController   	= "hsco/pms/com/COM000000/selectDecsn.do";		
			var sInDatasets   	= "input1=ds_cond99";
			var sOutDatasets  	= "ds_sanction=output1";
			bSync = false; //동기방식으로 처리
			break;			
			
		//내부결재 상태조회
		case "sanknKnd": 
 		    var sController   	= "hsco/pms/sls/hou/ctr/SLS020201/SANCTNKND.do";
	        var sInDatasets   	= "input1=ds_cond05";
	        var sOutDatasets  	= "ds_sanctnknd=output1";
	        bSync = false; //동기방식으로 처리
		    break;      

		//내부결재 번호 등록
		case "insertDecsn": 
			var sController   	= "hsco/pms/com/COM000000/insertDecsn.do";		
			var sInDatasets   	= "input1=ds_cond99";
			var sOutDatasets  	= "";
			break;					
	}

	//Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{

	if (nErrorCode != 0){

		switch (strSvcId) {
		
			case "selectIncmeMdatDailyList": //수입조정일계표
				break;	
				
			case "selectMonthlyLevList": //월별징수현황
				break;					
		
			case "selectCptalIncmeList": // 자금수입기록부
				break;
				
			case "selectIncmeBdgArngList": // 수입예산정리부
				break;	
							
			case "selectAreaIncmeBdgArngList1": // 지구별 수입예산정리부_ 월계표
				break;				

			case "selectAreaIncmeBdgArngList2": // 지구별 수입예산정리부_집계표
				break;				
										
		}
	} else {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch (strSvcId) {
		
			case "selectIncmeMdatDailyList": //수입조정일계표
				if(this.ds_incmeMdatDailyList.rowcount <= 0 ){
					this.alert("출력대상자료가 부존재합니다!");
					return false;				
				}else {
					this.fn_print_report();
				}
				break;
				
			case "selectMonthlyLevList": //월별징수현황
				if(this.ds_monthlyLevList.rowcount <= 0 ){
					this.alert("출력대상자료가 부존재합니다!");
					return false;				
				}else {
					this.fn_print_report();
				}
				break;	
								
								
				
			case "selectCptalIncmeList": // 자금수입기록부
				if(this.ds_cptalIncmeList.rowcount <= 0 ){
					this.alert("출력대상자료가 부존재합니다!");
					return false;				
				}else {
					this.fn_print_report();
				}
				break;
				
			case "selectIncmeBdgArngList": // 수입예산정리부
				if(this.ds_incmeBdgArngList.rowcount <= 0 ){
					this.alert("출력대상자료가 부존재합니다!");
					return false;				
				}else {
					this.fn_print_report();
				}
				break;
								
			case "selectAreaIncmeBdgArngList1": // 지구별 수입예산정리부_월계표
				if(this.ds_areaIncmeBdgArngList1.rowcount <= 0 ){
					this.alert("출력대상자료가 없습니다.");
					return false;				
				}
				else{
					this.fn_print_report();
				}
				break;
				
			case "selectAreaIncmeBdgArngList2": // 지구별 수입예산정리부_집계표
				if(this.ds_areaIncmeBdgArngList2.rowcount <= 0 ){
					this.alert("출력대상자료가 없습니다.");
					return false;				
				}
				else{
					this.fn_print_report();
				}
				break;								
				
			case "searchDecsn": // 내부결재 번호조회
				// 상태조회
				this.ds_cond05.clearData;
				this.ds_sanctnknd.clearData();
				this.ds_cond05.addRow();
				this.ds_cond05.setColumn( 0  , "INNER_SANCTN_ID" , this.ds_sanction.getColumn( this.ds_sanction.rowposition , "INNER_SANCTN_NO"));
				this.ds_cond05.setColumn( 0  , "JOB_SE" , "SLS");
				this.fn_transaction("sanknKnd"); 	
				break;				
								
		}
	}
	// 오류 및 성공 메시지를 화면에 보여준다.
	this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
}

