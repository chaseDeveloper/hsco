//---------------------------------
this.div_search_cmb_inputType_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{

	this.ds_cond.clearData();
	this.ds_cond.addRow();	
	this.ds_cond.setColumn(0, 'APPLC_MT', this.gfn_today().substr(0, 6));	
	this.fn_setToday(); 	
	
	sInputType = obj.value;  //Mod
		
	for(var i=1; i<=5; i++){
		if(i == parseInt(sInputType)){
			this.div_work['div_inputType0'+i].set_visible(true);
			this.div_work['div_inputType0'+i].setOffsetTop(6);
			this.div_work['div_inputType0'+i].setOffsetLeft(0);
		}
		else{
			this.div_work['div_inputType0'+i].set_visible(false);
		}
	}
	this.div_work.div_inputType05.cmb_trvctAcntSe.set_index(0);
}


