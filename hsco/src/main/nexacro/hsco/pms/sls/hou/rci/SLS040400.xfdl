{
	var sAcntSe = this.div_work.div_inputType05.cmb_trvctAcntSe.value;	

	if( this.comUtils.isNullEmpty(sAcntSe) || sAcntSe == "") {
		this.alert("계정구분이 선택되지 않았습니다");
	
	} else {	
		// 예산계정과목 팝업 호출						
		this.ds_cond02.setColumn(0,"YEAR",      this.ds_cond.getColumn(0, "APPLC_MT").substr(0,4)); // 예산년도
		this.ds_cond02.setColumn(0,"DEPT_CODE", this.gfn_getDeptId());
		this.ds_cond02.setColumn(0,"DEPT_NM",   this.gfn_getDeptName());		
		this.ds_cond02.setColumn(0,"BSNS_SE",   this.div_work.div_inputType05.cmb_trvctAcntSe.value.substr(0,1));   // 사업구분
		this.ds_cond02.setColumn(0,"BALC_SE",   this.div_work.div_inputType05.cmb_trvctAcntSe.value.substr(1,1));   // 수지구분
		  
		var parm = {
			  arg_0      : this
			 ,searchDs   : this.ds_cond02.saveXML()       // 검색조건(YEAR, BSNS_SE, BALC_SE, DEPT_CODE 컬럼만 이용)
			 ,strObj     : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE" // 수정불가능 처리할 오브젝트 명(ex : "edt_YEAR,cbo_BSNS_SE,rdo_BALC_SE,div_srchDept")
			 ,year_S     : null            // 예산년도(srchDs보다 우선순위를 가짐)
			 ,bsnsSe_S   : null            // 사업구분(srchDs보다 우선순위를 가짐)
			 ,balcSe_S   : null            // 수지구분(srchDs보다 우선순위를 가짐)
			 ,deptCode_S : null            // 부서코드(srchDs보다 우선순위를 가짐)
			 ,acntPopup  : false            // (true : 예산계정코드선택팝업, false : 지출단위선택팝업)
		};
		this.gfn_popup("mnthngCptalList", 900, 635, "예산선택팝업", parm, "com::bdgAcntPop.xfdl", "fn_PopupCallback");
	}			
}

//--------------------------------
// PopUp Callback
//--------------------------------
this.fn_PopupCallback = function(sPopupId, sReturn)
{
    var dsRtn = new Dataset();
    nRowCnt = dsRtn.loadXML(sReturn);
    
	if(nRowCnt){
				
		if(sPopupId == "mnthngCptalList") {
			var acntCode      = dsRtn.getColumn(0, "BUDGET_ACNT_CODE");
			var acntNm        = dsRtn.getColumn(0, "BUDGET_ACNT_NM");
			var expndtrUnitCd = dsRtn.getColumn(0, "EXPNDTR_UNIT_CODE");
			var expndtrUnitNm = dsRtn.getColumn(0, "EXPNDTR_UNIT_NM");		

			this.div_work.div_inputType05.ACNT_CODE.set_value(acntCode);
			this.div_work.div_inputType05.ACNT_NM.set_value(acntNm);
			this.div_work.div_inputType05.EXP_CODE.set_value(expndtrUnitCd);
			this.div_work.div_inputType05.EXP_NM.set_value(expndtrUnitNm);
		}		
	}
}

