			var sFileName = "월별징수현황";
			var sArg = "수 입 징 수 현 황";
			vOpts.setLocalDs(["ds_monthlyLevList", this.ds_monthlyLevList]);
			break
			
		case "3": // 자금수입기록부
			var sPath = "/pms/sls/SLS040400_R02.jrf";
