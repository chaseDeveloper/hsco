 * 설명 		: 콤보 onItemChange 이벤트 처리(아이템 선택)
 * params  		: obj 	: 이벤트 발생 콤보 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <Combo onitemchanged = "fn_comboOnItemChangeHandler">...</Combo>
 */
this.fn_comboOnItemChangeHandler = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var comName = obj.name;
	var comValue = e ? e.postvalue : obj.value;
	var comIndex = e ? e.postindex : obj.index;
	var isNullValue = this.comUtils.isNullEmpty(comValue) ? true : false;

	var paramMap = {};
	switch(comName){
	
		// 주택코드 콤보 변경
		case "cmb_houseCode" :
		
			if(!isNullValue){
				paramMap["HOUSE_CODE"] = comValue;
				this.pmsUtil.getHouseAea(paramMap, "ds_houseAeaList");
			}
			break;
			
		default :
			break;
	}
}

/*
 * 함수명  	: fn_checkboxOnClickHandler
 * 설명 		: 체크박스 onClick 이벤트 처리(아이템 선택)
 * params  		: obj 	: 이벤트 발생 콤보 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <CheckBox onclick = "fn_checkboxOnClickHandler">...</CheckBox>
 */
this.fn_checkboxOnClickHandler = function(obj:CheckBox,  e:nexacro.ClickEventInfo)
{
	var chkName = obj.name;
	var chkValue = obj.value;
	
	switch(chkName){
		
		// 납부코드 전체
		case "chk_payAll" :
			var setCheckValue = chkValue == "1" ? true : false;
			var div_payCodeList = this.DIV_PAYCODE; // 체크박스 표시 Div
			div_payCodeList.fn_checkAll(setCheckValue);	// true/false
			break;
			
		// 납부코드 선납
		case "chk_prePay" :
			var div_search = obj.parent;	// 조회영역 DIV
			
			// 선납의 경우 납부구분을 선납으로 설정하고 선택불가로 처리(AS-IS)
			var cmbEnabled = chkValue == "1" ? false : true;
			var cmbValue = chkValue == "1" ? chkValue : "";
			div_search.cmb_paySe.set_value(cmbValue);
			div_search.cmb_paySe.set_enable(cmbEnabled);
			break;
		
		default :
			break;
	}
}

/*
 * 함수명  	: fn_payCodeListOnClickHandler
 * 설명 		: 동적 체크박스 선택/해제 SELECT BOX (전체선택,전체해제) 
 * params  		: obj 	: 이벤트 발생 콤보 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: 
 */
this.fn_payCodeListOnClickHandler = function(obj:CheckBox, e:nexacro.ClickEventInfo){
	
	var div_payCodeList = obj.parent;						// 체크박스 표시 Div
	var isPayCodeAllCheck = div_payCodeList.fn_isAllCheck();
	
	//trace(isPayCodeAllCheck);
	
	var setCheckValue = isPayCodeAllCheck ? "1" : "0";
	var searchDiv = this.TAB_LTTOT_SEARCH_DIV;
	searchDiv.chk_payAll.set_value(setCheckValue);
}

