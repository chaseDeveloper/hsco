 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "lib::comInclude.xjs";	// 시스템 공통 라이브러리 Include

// 업무에 필요한 공통 라이브러리 파일을 추가할 수 있다.
include "pms_lib::pmsUtil.xjs";	// PMS 업무공통 라이브러리

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
// 화면 전역에서 사용할 Form 변수를 선언한다.
//this.HOUSE_SE = "S1,S2,S3,S4,S5,S9,R1,R2,R3,R9";	// 주택구분 - 분양, 임대 모두 포함
this.DEFAULT_DATE_FROM = "20000101";
this.DEFAULT_DATE_TO = this.gfn_today();
this.DEFAULT_DATA_SE = 0;
this.DEFAULT_YEAR_CNT_FM = 0;
this.DEFAULT_YEAR_CNT_TO = 8;
this.TAB_LTTOT_SEARCH_DIV = this.tab_master.tpg_lttot.div_search02;			// 분양금,보증금내역 TAB 조회영역
this.TAB_RENT_SEARCH_DIV = this.tab_master.tpg_rent.div_search03;			// 임대료내역 TAB 조회영역
this.DIV_PAYCODE = this.tab_master.tpg_lttot.div_search02.div_payCodeList;  // 체크박스 표시 Div

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SLS040305_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화	
}

this.SLS040305_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// 기본화면 정보 설정
	this.fn_setDefaultInfo();
	
	// 기초정보 조회
	this.fn_init();
}

/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	// 시스템 공통으로 Form을 Initialize 한다.
	this.gfn_initForm(this);
	
	this.fn_checkListCreate();		// 체크박스 동적생성
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

/**
 * 화면구성 기초정보 조회
 */
this.fn_init = function(){

	// 기초정보 조회
	this.fn_transaction("selectInitInfo");
}

/*
 * 함수명  	: fn_search
 * 설명 		: 이율관리 대상목록  조회 이벤트 처리
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){

	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}
	
	var tranStrSvcId = null;
	
	// 선택 탭 조회조건 확인
	var selTabIndex = this.tab_master.tabindex;
	switch(selTabIndex){
		
		// 분양금,보증금 내역
		case 0 :
		
			// 선택 납부코드 설정
			var selPayCode = this.DIV_PAYCODE.fn_get_valueStr();
			this.ds_cond00.setColumn(0, "PAY_CODE", selPayCode);
		
			if(!this.gfn_checkValidation(this.TAB_LTTOT_SEARCH_DIV, this.ds_validation)) {
				return;
			}
			
			// 계약세대 선택값 재 설정 계약자상태코드(HOU0A3) : 01(정상계약), 02(정상해지)
			// 02(정상해지)의 경우 null 값으로 변경
			var inCntrctrSttusCode = this.ds_cond00.getColumn(0, "CNTRCTR_STTUS_CODE");
			if(inCntrctrSttusCode == "02"){
				this.ds_cond00.setColumn(0, "CNTRCTR_STTUS_CODE", null);
			}
			
			// Clear Datasets
			this.pmsUtil.clearDataSet([this.ds_lttotGtnList]);	// 분양금,보증금내역 목록
			
			tranStrSvcId = "selectLttotGtnList";
			break;
			
		// 임대료 내역
		case 1 :
		
			if(!this.gfn_checkValidation(this.TAB_RENT_SEARCH_DIV, this.ds_validation)) {
				return;
			}
			
			// Clear Datasets
			this.pmsUtil.clearDataSet([this.ds_rentList]);	// 임대료내역 목록
			
			tranStrSvcId = "selectRentList";
			break;
			
		default :
			break;
	}
	
	if(tranStrSvcId == null){
		return ;
	}
	
	this.fn_transaction(tranStrSvcId);
}

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	var sController = null;
	var oInDatasets = null;
	var sOutDatasets = null;
	switch (strSvcId) {

		// 화면기초정보 조회 - 주택코드, 납부구분코드, 납부코드
		case "selectInitInfo" :
		
			trace("tabindex =" + this.tab_master.tabindex);
			// 요청값
			if(this.tab_master.tabindex == 0){
				var HOUSE_SE = "S1,S2,S3,S4";	// 주택구분 - 분양
			} else {
				var HOUSE_SE = "S2,S4";	// 주택구분 - 임대
			}
			var reqParam = {};
			reqParam["HOUSE_SE"] = HOUSE_SE;	// 주택구분
			reqParam["USE_AT"] = "1";				// 사용여부(0:미사용, 1:사용)
			//reqParam["LTTOT_CNVRS_SE"] = "02";	// 분양전환구분 : 02(분양전환) - 미 기입시 쿼리시 02로 세팅
		
			sController = "hsco/pms/sls/hou/rci/SLS040305/selectInitInfo.do";
			oInDatasets = reqParam;
			sOutDatasets = "ds_houseCodeList=houseCode ds_paySeCodeList=paySeCode ds_payCodeList=payCode";
			break;
			
		// 주택별 수납현황(분양금,보증금내역) 조회
		case "selectLttotGtnList" :
		
			sController = "hsco/pms/sls/hou/rci/SLS040305/selectLttotGtnList.do";
			oInDatasets = "input1=ds_cond00";
			sOutDatasets = "ds_lttotGtnList=output1";
			break;
			
		// 주택별 수납현황(임대료내역) 조회
		case "selectRentList" :
		
			sController = "hsco/pms/sls/hou/rci/SLS040305/selectRentList.do";
			oInDatasets = "input1=ds_cond00";
			sOutDatasets = "ds_rentList=output1";
			break;
		
		default :
			break;
	}

	if(sController == null) return;
	
	var sInDataType = typeof(oInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, reqParam, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, oInDatasets, sOutDatasets, null, null, null, bSync);
	}
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var targetCombo = null;
		var comboOption = null;
		switch(strSvcId){
		
			// 화면기초정보 로드
			case "selectInitInfo" :

				// 조회영역-주택코드
				targetCombo = this.div_search.cmb_houseCode;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_SELECT;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				
				// Tab조회영역-납부구분
				targetCombo = this.TAB_LTTOT_SEARCH_DIV["cmb_paySe"];
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				
				// 납부코드 동적 생성
				var div_payCodeList = this.DIV_PAYCODE;  		// 체크박스 표시 Div
				div_payCodeList.fn_create(this.ds_payCodeList); // 체크박스 생성
				
				// 전체 선택 처리
				div_payCodeList.fn_checkAll(true);
				this.TAB_LTTOT_SEARCH_DIV.chk_payAll.set_value(true);
				
				break;
				

				
			// 주택코드별 평형 목록 조회
			case "houseAeaList" :
			
				targetCombo = this.div_search.cmb_aeaCode;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				break;
				
			// 주택별 수납현황(분양금,보증금내역) 조회
			case "selectLttotGtnList" :
				// TODO
				break;
				
			// 주택별 수납현황(임대료내역) 조회
			case "selectRentList" :
				// TOD
				break;
				
			default :
				break;
		}
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}
