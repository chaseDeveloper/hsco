		// 결과파일 생성
		case "btn_createFile" :
			this.fn_downloadAtcngReqstFile();	// 결과파일 다운로드
			break;
			
		default :
			break;
	}
}

/*
 * 함수명  	: fn_comboOnItemChangeHandler
 * 설명 		: 콤보 onItemChange 이벤트 처리(아이템 선택)
 * params  		: obj 	: 이벤트 발생 콤보 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <Combo onitemchanged = "fn_comboOnItemChangeHandler">...</Combo>
 */
this.fn_comboOnItemChangeHandler = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var comName = obj.name;
	var comValue = e ? e.postvalue : obj.value;
	var comIndex = e ? e.postindex : obj.index;
	var isNullValue = this.comUtils.isNullEmpty(comValue) ? true : false;

	var paramMap = {};
	switch(comName){

		// 주택코드 콤보 변경
		case "cmb_houseCode" :
			
			// Clear Datasets
			this.pmsUtil.clearDataSet([this.ds_houseDongList, this.ds_houseHoList]);	// 동, 호 목록
			
			if(!isNullValue){
				paramMap["HOUSE_CODE"] = comValue;
				this.pmsUtil.getHouseDong(paramMap, "ds_houseDongList");
				this.pmsUtil.getHouseHo(paramMap, "ds_houseHoList");
			}
			break;
			
		// 동 콤보 변경
		case "cmb_dongCode" :
		
			var selHouseMap = this.pmsUtil.getComboSelectedItem(this.div_search.cmb_houseCode);
			paramMap["HOUSE_CODE"] = selHouseMap["HOUSE_CODE"];
			paramMap["DONG"] = comValue;
			this.pmsUtil.getHouseHo(paramMap, "ds_houseHoList");
			break;

		default :
			break;
	}
}

/*
 * 함수명  	: fn_calendarOnChangedHandler
 * 설명 		: 캘린터 onchanged 이벤트 처리
 * params  		: obj 	: 이벤트 발생 콤보 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <Calendar onchanged="fn_calendarOnChangedHandler">...</Calendar>
 */
this.fn_calendarOnChangedHandler = function(obj:Calendar, e:nexacro.ChangeEventInfo)
{

	var calName = obj.name;
	var calValue = obj.value;
	var oParent = obj.parent;
	
	var isNullValue = this.comUtils.isNullEmpty(calValue);
	
	switch(calName){
		
		// 해지일자
		case "cal_trmnatDe" :
			
			// 해지일자 입력 시 상태구분을 해지(3)로 고정, 상태구분 사용불가 처리
			var reqstSeEnabled = isNullValue ? true : false;
			var selReqstSeValue = isNullValue ? "" : "3";
			var cmbReqstSe = oParent["cmb_reqstSe"];
			cmbReqstSe.set_value(selReqstSeValue);
			cmbReqstSe.set_enable(reqstSeEnabled);
			break;
		
		default :
			break;
	}
}

