				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				break;
			
			// 주택코드/동별 호 목록 조회
			case "houseHoList" :
			
				targetCombo = this.tab_master.tpg_detail.div_search02.cmb_hoCode;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				break;

			// 신청/해지 결과 목록 조회
			case "selectAtcngReqstList" :

				this.fn_setSelFileInfoTitle();	// 초기화
				
				break;
				
			// 상세정보 조회
			case "selectAtcngReqstDetList" :
				// TODO
				break;
				
			// 신청/해지 처리건수 조회
			case "selectAtcngProcessCnt" :
				
				// 조회된 신청/해지 처리건수에 따라 처리
				this.fn_checkProcessCnt();
				
				break;
				
			// 신청/해지 적용 처리
			case "saveAtcngRqestApplyInfo" :
			
				// 재 조회
				this.fn_search();
				break;
				
			default :
				break;
		}
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}
