	this.ds_cond01.setColumn(0, "ACNUTNO", ACNUTNO);
	
	this.tab_master.set_tabindex(1);
	
	this.fn_search();
}

//엔터 시 상세정보에서 조회
this.tab_master_tpg_reqst_grd_reqst_onkeyup = function(obj:Grid, e:nexacro.KeyEventInfo)
{
	if(e.keycode == "13"){
		var BANK_CODE = this.ds_reqstList.getColumn(this.ds_reqstList.rowposition, "BANK_CODE");
		var ACNUTNO = this.ds_reqstList.getColumn(this.ds_reqstList.rowposition, "ACNUTNO");
		
		this.ds_cond01.setColumn(0, "BANK_CODE", BANK_CODE);
		this.ds_cond01.setColumn(0, "ACNUTNO", ACNUTNO);
		
		this.tab_master.set_tabindex(1);
		
		this.fn_search();
	}
}
