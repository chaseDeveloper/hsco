/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	var sController = null;
	var oInDatasets = null;
	var sOutDatasets = null;
	switch (strSvcId) {
	
		// 화면기초정보 조회 - 주택코드, 은행코드
		case "selectInitInfo" :
		
			// 요청값
			var reqParam = {};
			reqParam["HOUSE_SE"] = this.HOUSE_SE;	// 주택구분
			reqParam["USE_AT"] = "1";				// 사용여부(0:미사용, 1:사용)
			//reqParam["LTTOT_CNVRS_SE"] = "02";	// 분양전환구분 : 02(분양전환) - 미 기입시 쿼리시 02로 세팅
		
			sController = "hsco/pms/sls/hou/rci/SLS040201/selectInitInfo.do";
			oInDatasets = reqParam;
			sOutDatasets = "ds_houseCodeList=houseCode ds_bankCodeList=bankCode";
			break;
		
		// 신청/해지 결과 목록 조회
		case "selectAtcngReqstList" :

			sController = "hsco/pms/sls/hou/rci/SLS040201/selectAtcngReqstList.do";
			oInDatasets = "input1=ds_cond00";
			sOutDatasets = "ds_reqstList=reqstList ds_reqstSumInfo=reqstSumInfo";
			break;
			
		// 상세정보 조회
		case "selectAtcngReqstDetList" :

			sController = "hsco/pms/sls/hou/rci/SLS040201/selectAtcngReqstDetList.do";
			oInDatasets = "input1=ds_cond01";
			sOutDatasets = "ds_reqstDetList=reqstDetList";
			break;
			
		// 신청/해지 처리 건수 조회
		case "selectAtcngProcessCnt" :
			
			sController = "hsco/pms/sls/hou/rci/SLS040201/selectAtcngProcessCnt.do";
			oInDatasets = "input1=ds_cond00";
			sOutDatasets = "ds_reqstProcessCnt=reqstCntInfo";
			break;
			
		// 신청/해지 적용 처리
		case "saveAtcngRqestApplyInfo" :
			
			sController = "hsco/pms/sls/hou/rci/SLS040201/saveAtcngRqestApplyInfo.do";
			oInDatasets = "input1=ds_reqstList";
			sOutDatasets = "";
			break;
		
		default :
			break;
	}

	if(sController == null) return;
	
	var sInDataType = typeof(oInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, reqParam, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, oInDatasets, sOutDatasets, null, null, null, bSync);
	}
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
