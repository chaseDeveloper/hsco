	this.ds_cond01.setColumn(0, "DONG", this.div_search.cmb_dongHoSh.value);  // 선택 동
	this.ds_cond01.setColumn(0, "HO"  , this.div_search.cmb_hoSh.value);  // 선택 호
	this.ds_cond01.setColumn(0, "BGNDE"  , this.div_search.cal_bgnDeSubls.value);  // 선택 호	
	this.ds_cond01.setColumn(0, "ENDDE"  , this.div_search.cal_endDeSubls.value);  // 선택 호	
	this.ds_cond01.setColumn(0, "CNTRCTR_NM"  , this.div_search.edt_nm.value);  // 선택 호	
	trace("#########SLS020308############");
	trace(this.ds_cond01.saveXML());
	trace("#########SLS020308#############");	
		
	this.fn_transaction("houseSublsList");
	
}

/*
