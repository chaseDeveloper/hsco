/*
 * 함수명  	: fn_insert
 * 설명 		: 신규.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function(){
	
	var nRow = this.ds_hcntrctrList.rowposition;  //계약자 리스트 선택된 로우

    if(nRow < 0){
    	this.gfn_message("info.선택데이터.없음.선택.요청");
    	//this.alert("선택된 주택계약자 정보가 없습니다.");
        return false; 
    }
    
	
	if(this.ds_houseSublsList.rowposition > -1){
		var sublsSn = this.ds_houseSublsList.getColumn(this.ds_houseSublsList.rowposition, "SUBLS_SN");
		if(this.comUtils.isNullEmpty(sublsSn) == true){
			this.alert("전대정보는 한건만 추가할 수 있습니다.");
			return;
		}
	}
	
	var cntrctrNo = this.ds_hcntrctrList.getColumn(this.ds_hcntrctrList.rowposition, "CNTRCTR_NO");
	var nRow      = this.ds_houseSublsList.addRow();
	
	/* 초기값 세팅 */
	this.ds_houseSublsList.setColumn(nRow, "CNTRCTR_NO",cntrctrNo       );  //계약번호
	this.ds_houseSublsList.setColumn(nRow, "BGNDE"     ,this.gfn_today());  //전대기간.시작일
	this.ds_houseSublsList.setColumn(nRow, "ENDDE"     ,this.gfn_today());  //전대기간.종료일
    this.ds_houseSublsList.setColumn(nRow, "REGIST_DT" ,this.gfn_today());  //등록일
	
}

/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */ 
this.fn_delete = function()
{
    if(this.ds_houseSublsList.rowcount <= 0){
        return;
    }
     
    if(this.confirm("삭제하시겠습니까?"))	{
        this.fn_checkItemDelete(this.grd_houseSublsList); //그리드 삭제  
    } 
    
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */ 
this.fn_cancel = function(){
	
	if(this.ds_houseSublsList.rowcount <= 0){
        return;
    }
    
	if(this.confirm("작업을 취소 하시겠습니까?")){
	   this.ds_houseSublsList.cancel();
	}
}

/*
 * 함수명  	: fn_comboOnItemChangeHandler
 * 설명 		: 콤보 리스트 변경시 발생 이벤트
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */ 
this.fn_comboOnItemChangeHandler = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var comName = obj.name;
	var comValue = e ? e.postvalue : obj.value;
	var comIndex = e ? e.postindex : obj.index;

	var paramMap = {};
	switch(comName){
			
		// 조회조건 - 주택 콤보
		case "cmb_houseCode" :   //이벤트발생 컴포넌트
			paramMap["HOUSE_CODE"] = comValue;
			this.pmsUtil.getHouseDong(paramMap, "ds_houseDongList");  //연동될 다음 컴포넌트
			break;
			
		case "cmb_dong" :
			paramMap["HOUSE_CODE"] = this.ds_houseDongList.getColumn(comIndex, "HOUSE_CODE" );
			paramMap["DONG"] = comValue;
			this.pmsUtil.getHouseHo(paramMap, "ds_houseHoList");
			break;
			
		default :
			break;
	}
	
}

