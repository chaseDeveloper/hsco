this.div_work_btn_sms_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var chkRowCnt = this.ds_imdeAmtList.getRowCount();
	
	if ( chkRowCnt <= 0 ){
	    this.alert("전송대상을 선택하세요.");
		return false;	
	} 
	
	var callCnt = 0;
	this.ds_sms.clearData();
	
	/* 문자코드 [SLS002:체납안내], 고객명, 체납종류, 체납금액, 체납기준일, 체납대상
	   [화성도시공사:체납안내]안녕하십니까? %CHANGEWORD1%고객님 화성도시공사입니다. 
	   %CHANGEWORD1%고객님께서는 %CHANGEWORD5% %CHANGEWORD2%이 미납중이오니 빠른시일내에 납부하시기 바랍니다. 
	   ( %CHANGEWORD4% 기준 )
	*/	
	for (var i = 0 ; i < this.ds_imdeAmtList.rowcount ; i++) {
	
		if (this.ds_imdeAmtList.getColumn(i, "CHK") == '1') {
		
		    var nRow = this.ds_sms.addRow();	
			// 수신자정보 설정
			this.ds_sms.setColumn(nRow, "MBTLNO"   , this.ds_imdeAmtList.getColumn(i, "MBTLNO"));	// 수신자번호
			this.ds_sms.setColumn(nRow, "NM"	   , this.ds_imdeAmtList.getColumn(i, "CSTMR_NM"));	// 수신자명			
            this.ds_sms.setColumn(nRow, "CHANGE_WORD0", this.ds_imdeAmtList.getColumn(i, "CSTMR_NM")); // 고객명  
            this.ds_sms.setColumn(nRow, "CHANGE_WORD1", this.comUtils.formatComma(this.ds_imdeAmtList.getColumn(i, "TOT_AMT"))+"원");                                    
            this.ds_sms.setColumn(nRow, "CHANGE_WORD2", ""); // 체납금액
            this.ds_sms.setColumn(nRow, "CHANGE_WORD3", this.gfn_today());                            // 체납기준일
            this.ds_sms.setColumn(nRow, "CHANGE_WORD4", "임대료");                                   // 체납대상
            this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_0" , "2");  // 대기자 : 1 , 계약자 : 2
            this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_1" , this.ds_imdeAmtList.getColumn(i, "CNTRCTR_NO"));  // 고객 번호
            this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_2" , this.ds_imdeAmtList.getColumn(i, "HOUSE_CODE"));  //
			
			callCnt = callCnt + 1;
		}
	}

    if ( callCnt > 0 ) {
    
		var CONTENT = this.ds_smsCn.getColumn(0, "MSSAGE_CN") || '';  // 메시지 내용
		this.gfn_popup("COM010700_P01.xfdl"
				   , 800
				   , 500
				   , "SMS발송"
				   , {CONTENT : CONTENT,
  					  MXTR_WRD0 : "고객명",
					  MXTR_WRD1 : "체납종류",
					  MXTR_WRD2 : "체납금액",
					  MXTR_WRD3 : "체납기준일",
					  MXTR_WRD4 : "체납대상",				      
				      JOB_SE : "SLS002", 
				      ds : this.ds_sms}
				   , "com::COM010700_P01.xfdl"
				   , "sms_callback");
    }else{
		return false;
    }	
}



this.div_work_Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	var tabid = this.div_work.Tab00.tabpages[this.div_work.Tab00.tabindex].name; // eg. tabpage1
	
	switch(tabid)
	{
		case 'tabpage2':		// 임대료
			this.div_work.btn_infoPrint.set_visible(true);
			this.div_work.btn_sms.set_visible(true);
			this.div_work.Tab00.tabpage2.Div01.edt_Month_St.set_value(1); 		 // 체납개월 시작
			this.div_work.Tab00.tabpage2.Div01.edt_Month_Ed.set_value(999); 	 // 체납개월 종료
			this.div_work.Tab00.tabpage2.Div01.edt_Amt_St.set_value(1); 		 // 체납금액 시작
			this.div_work.Tab00.tabpage2.Div01.edt_Amt_Ed.set_value(9999999999); // 체납금액 종료
			break;
			
		case 'tabpage1':		// 분양금
			this.div_work.btn_infoPrint.set_visible(false);
			this.div_work.btn_sms.set_visible(false);
			break;
			
		case 'tabpage4':		// 보증금
			this.div_work.btn_infoPrint.set_visible(false);
			this.div_work.btn_sms.set_visible(false);
			break;

		case 'tabpage3':		// 융자금
			this.div_work.btn_infoPrint.set_visible(false);
			this.div_work.btn_sms.set_visible(false);
			break;
	}	
