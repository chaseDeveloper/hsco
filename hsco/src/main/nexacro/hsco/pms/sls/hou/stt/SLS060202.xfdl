/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "boonyangAmtList": //분양금 체납 내역 조회
			var sController   	= "/hsco/pms/sls/hou/stt/SLS060202/boonyangAmtList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_boonyangAmtList=output1";
			break;
			
		case "bojeungAmtList": //보증금 체납 내역 조회
			var sController   	= "/hsco/pms/sls/hou/stt/SLS060202/bojeungAmtList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_bojeungAmtList=output1";
			break;			
			
		case "imdeAmtList": 	//임대료 체납 내역 조회
			var sController   	= "/hsco/pms/sls/hou/stt/SLS060202/imdeAmtList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_imdeAmtList=output1";
			break;
			
		case "yoongjaAmtList": //융자금 체납 내역 조회
			var sController   	= "/hsco/pms/sls/hou/stt/SLS060202/yoongjaAmtList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_yoongjaAmtList=output1";
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


