	//this.div_work.Tab00.tabpage2.cal_stDe.set_value(today);
	//this.div_work.Tab00.tabpage2.cal_stdrDe.set_value(today);
	//this.div_work.Tab00.tabpage2.cal_payTmlmt.set_value(this.fn_last_yyyymmdd(today));
}

/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function(){
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	
	this.div_search.rdo_ShAt.set_value("1");
	this.div_search.cal_stPayTmlmt.set_value("19000101");
	this.div_search.cal_edPayTmlmt.set_value(this.gfn_today());			 // 현재
	this.div_search.cal_Rciv_De.set_value(this.gfn_today()); 			 // 1년전
	this.div_search.cal_Arrrg_Dt.set_value(this.gfn_today());			 // 현재
	this.div_work.Tab00.tabpage2.Div01.edt_Month_St.set_value(1); 		 // 체납개월 시작
	this.div_work.Tab00.tabpage2.Div01.edt_Month_Ed.set_value(999); 	 // 체납개월 종료
	this.div_work.Tab00.tabpage2.Div01.edt_Amt_St.set_value(1); 		 // 체납금액 시작
	this.div_work.Tab00.tabpage2.Div01.edt_Amt_Ed.set_value(9999999999); // 체납금액 종료
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){

	var tabid = this.div_work.Tab00.tabpages[this.div_work.Tab00.tabindex].name; // eg. tabpage1
	// 검색영역 Validation 체크
	//if(this.gfn_checkValidation(this.div_search, this.ds_validation) == false) {
	//	return;
	//}
	//this.fn_divToDS(this.ds_cond00, this.div_search);
	switch(tabid)
	{
		case 'tabpage1':		// 분양금
			this.fn_dataSet('tabpage1');
			this.fn_transaction("boonyangAmtList"); 
			//this.search_btl_set();
			break;
		case 'tabpage2':		// 임대료
			//this.search_btl_set();
			this.fn_dataSet('tabpage2');
			this.fn_transaction("imdeAmtList"); 
			break;
		case 'tabpage3':		// 융자금
			//this.search_btl_set();
			this.fn_dataSet('tabpage3');
			this.fn_transaction("yoongjaAmtList"); 			
			break;			
		case 'tabpage4':		// 보증금	
			this.fn_dataSet('tabpage4');
			this.fn_transaction("bojeungAmtList"); 				
 			break;
	}	
}

	/*
	 * 함수명  	: fn_dataSet
	 * 설명 		: 목록 조회 조회 이벤트 처리
	 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
	 * params  		: None
	 * return Type 	: None
	 * 사용법		: this.fn_dataSet();
	 */	
	this.fn_dataSet = function(strDs)
	{	
	
	    var searchAt = this.div_search.rdo_ShAt.value; //합산(1),상세(0)구분
		var houseCode = this.div_search.cmb_houseCodeSh.value; //주택코드
		var dong = this.div_search.edt_dong.value;	//동
		var ho  = this.div_search.edt_ho.value;		//호
		var stPayTmlmt = this.div_search.cal_stPayTmlmt.value;  // 납부기한일 시작	
		var edPayTmlmt = this.div_search.cal_edPayTmlmt.value;	//납부기한일 종료
		var Rciv_De = this.div_search.cal_Rciv_De.value;	//수납기준일
		var cal_Arrrg_Dt = this.div_search.cal_Arrrg_Dt.value;	//이자생성일
		var Month_St = this.div_work.Tab00.tabpage2.Div01.edt_Month_St.value;	//체납개월 시작
		var Month_Ed = this.div_work.Tab00.tabpage2.Div01.edt_Month_Ed.value;	//체납개월 종료
		var Amt_St = this.div_work.Tab00.tabpage2.Div01.edt_Amt_St.value;	//체납금액 시작
		var Amt_Ed = this.div_work.Tab00.tabpage2.Div01.edt_Amt_Ed.value;	//체납금액 종료


		this.ds_cond00.clearData();
		var nRow = this.ds_cond00.addRow();
		
		this.ds_cond00.setColumn(0,"SEARCH_AT",searchAt); //합산(1),상세(0)구분		
		this.ds_cond00.setColumn(0,"HOUSE_CODE",houseCode); //주택코드
		this.ds_cond00.setColumn(0,"RCIV_DE",Rciv_De);  //수납기준일
		this.ds_cond00.setColumn(0,"ARRRG_DT",cal_Arrrg_Dt);  //이자생성일
		this.ds_cond00.setColumn(0,"DONG",dong); //동
		this.ds_cond00.setColumn(0,"HO",ho);  //호
		this.ds_cond00.setColumn(0,"PAY_TMLMT_ST", stPayTmlmt);   // 납부기한일 시작
		this.ds_cond00.setColumn(0,"PAY_TMLMT_ED", edPayTmlmt);   //납부기한일 종료

 		if (strDs == "tabpage2"){
