    this.tpg_00.tabpage2.Div01.msk_rat2.set_value(nBaseIntrt   ); //위약금이율-설정
    this.tpg_00.tabpage2.Div01.msk_amt.set_value(nResrveAmount  ); //유보금-설정
    this.tpg_00.tabpage2.Div01.chk_01.set_value(nRntchrgApplcAt); //임대료적용여부-설정
    this.tpg_00.tabpage2.Div01.chk_02.set_value(nPenlTyApplcAt ); //위약금적용여부-설정
    this.tpg_00.tabpage2.Div01.chk_03.set_value(nIndmnTyApplcAt); //배상금 적용여부-설정    

    
    /*이벤트 설정 */
    this.tpg_00.tabpage2.Div01.msk_rat1.set_enableevent(true)   ; //이자이율-설정
    this.tpg_00.tabpage2.Div01.msk_rat2.set_enableevent(true)   ; //위약금이율-설정
    this.tpg_00.tabpage2.Div01.msk_amt.set_enableevent(true)     ; //유보금-설정
    this.tpg_00.tabpage2.Div01.chk_01.set_enableevent(true)     ; //임대료적용여부-설정
