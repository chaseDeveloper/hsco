  if(rowType != '2'){
      return;
  }
  
  if(!this.gfn_checkValidation(this.Div08, this.ds_validation)) {
    	return;
  }

  if(this.confirm("저장하시겠습니까?")){
  
      /* 주택계약자해약 */
