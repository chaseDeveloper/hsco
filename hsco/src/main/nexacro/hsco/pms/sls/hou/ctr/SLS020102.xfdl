this.fn_houseCancelListInfo = function(){

    /*신규등록인경우, 행추가, 기본값 세팅*/
    if(this.ds_hcntrctrCancelList.rowcount <= 0){ //신규인경우

        var nRow    = this.ds_hcntrctrCancelList.addRow(); //주택계약 해약정보 행 추가
        var nGetRow = this.ds_hcntrctrList.rowposition;    //선택된 계약자정보 인덱스      
     
        /*기본 정보 세팅*/
        this.ds_hcntrctrCancelList.setColumn(0, "CNTRCTR_NO", this.ds_hcntrctrList.getColumn(nGetRow, "CNTRCTR_NO")); //주택계약번호   
        this.ds_hcntrctrCancelList.setColumn(0, "HOUSE_CODE", this.ds_hcntrctrList.getColumn(nGetRow, "HOUSE_CODE")); //주택코드
        this.ds_hcntrctrCancelList.setColumn(0, "DONG"      , this.ds_hcntrctrList.getColumn(nGetRow, "DONG"      )); //동
        this.ds_hcntrctrCancelList.setColumn(0, "HO"        , this.ds_hcntrctrList.getColumn(nGetRow, "HO"        )); //호
        
        /*압류정보 세팅 */
        this.ds_hcntrctrCancelList.setColumn(0, "TOT_AMOUNT", this.ds_hcntrctrSeizrInfo.getColumn(0, "TOT_AMOUNT" )); //압류금액
        this.ds_hcntrctrCancelList.setColumn(0, "CRDTR_NM"  , this.ds_hcntrctrSeizrInfo.getColumn(0, "CRDTR_NM"   )); //채권자명
        
        /*접수,해약일자세팅*/
        this.Div08.cal_cancelDe.set_enableevent(false);   //해약일자 이벤트 해제
        
        this.ds_hcntrctrCancelList.setColumn(0, "CANCL_DE"  , this.gfn_dateInfo("NOW_DATE")                        ); //해약일자(현재날짜)
        this.Div08.cal_receptDe.set_value(this.gfn_dateInfo("NOW_DATE") )                                           ; //접수일자(현재날짜)
        
        this.Div08.cal_cancelDe.set_enableevent(true);    //해약일자 이벤트 실행
        
        this.Div08.cmb_cancelResn.set_index(0);
        this.Div08.cmb_bank.set_index(0);
    }
   
   
    /*임대시작일,임대종료일 세팅 */
    this.ds_hcntrctrCancelList.setColumn(0, "RENT_BGNDE", this.ds_hcntrctrList.getColumn(nGetRow, "RENT_BGNDE")); //임대시작일
    this.ds_hcntrctrCancelList.setColumn(0, "RENT_ENDDE", this.ds_hcntrctrList.getColumn(nGetRow, "RENT_ENDDE")); //임대종료일
    
   
    //분양인경우 Tabpage1,임대인경우 Tabpage2 선택
    if( STR_HOUSE_TYPE_SE == '01'){        
        
        if(this.tpg_00.tabindex == 0){
            this.tpg_00_onchanged(this.tpg_00); //탭 변경 이벤트 강제발생
        }
        else{
            this.tpg_00.set_tabindex(0)       ; //분양-S        
        }    
    }
    else{        
        
        if(this.tpg_00.tabindex == 1){
            this.tpg_00_onchanged(this.tpg_00); //탭 변경 이벤트 강제발생
        }
        else{
            this.tpg_00.set_tabindex(1)       ; //임대-R
        }            
    }
    
    //해약정보 없을때 상세정보 잠금 2016.06.22
	var chk = this.ds_hcntrctrCancelList.getColumn(this.ds_hcntrctrCancelList.rowposition,"CHK");
	if(chk == '1'){	
		this.Div08.set_enable(true);
	}else{
		this.Div08.set_enable(false);
	}
}
 
//탭 변경전
this.tpg_00_canchange = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	//주택구분	
	var chk = this.ds_houseRentCancelList.getColumn(this.ds_houseRentCancelList.rowposition,"CHK");
	//alert("chk = " + chk + "  STR_HOUSE_TYPE_SE = " + STR_HOUSE_TYPE_SE + "   e.preindex = " + e.preindex  + "  e.postindex = " + e.postindex);
	//탭
	if (STR_HOUSE_TYPE_SE == ""){
		return false;		
	}
	else if (chk != "0" && e.postindex == 2){
		return false;		
	}
	else if (STR_HOUSE_TYPE_SE != "" && e.postindex == 2){//시설점검
		return true;		
	}
	else if (STR_HOUSE_TYPE_SE == "01" && this.tpg_00.tabindex == 0){//분양
		return false;		
	}
	else if (STR_HOUSE_TYPE_SE == "01" && e.preindex == 2 && e.postindex == 1){//분양 시설점검에서 -> 임대 
		return false;		
	}
	else if (STR_HOUSE_TYPE_SE == "02" && this.tpg_00.tabindex == 1){//임대
		return true;
	}
	else if (STR_HOUSE_TYPE_SE == "02" && e.preindex == 1 && e.postindex == 2){//임대 임대에서 -> 시설점검
		return true;
	}
	else if (STR_HOUSE_TYPE_SE == "02" && e.preindex == 2 && e.postindex == 0){//임대 시설점검에서 -> 분양 
		return false;		
	}
}

