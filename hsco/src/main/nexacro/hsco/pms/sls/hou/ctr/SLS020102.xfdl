}

/*
 * 함수명  	    : fn_search
 * 설명 		: 주택해약 정보 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
 this.fn_search = function()
{
    
  	// Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
    	return;
	}
	
	this.ds_hcntrctrList.set_enableevent(false);  //데이터셋 이벤트 해제
	
	this.ds_hcntrctrList.clearData();
   
    this.ds_hcntrctrCancelList.clearData();
    this.ds_hcntrctrCancelReturnAmtList.clearData();
    
    this.ds_houseRentCancelList.clearData();
    this.ds_houseRentCancelReturnAmtList.clearData();
    this.ds_houseRentPayList.clearData();   
    
    this.ds_hcntrctrList.set_enableevent(true);   //데이터셋 이벤트 설정    
    
    var sChk = this.div_search.chk_cntrctrSttusCode.value;  //해약자만 검색 체크박스(01:계약자/false, 02:해약자/true)
     
	// 조회조건 추가 설정
	//this.ds_cond00.setColumn(0, "HOUSE_SE", this.div_search.cmb_houseGbn.value );   //주택구분 (임대,주택)
    this.ds_cond00.setColumn(0, "HOUSE_CODE", this.div_search.cmb_houseCode.value ) ; //주택코드
    
    //2016.03.28 동/호정보 없을때 오류 방지
    if(this.ds_cmbDongHoList.rowcount > 0){
		var dongHoMap = this.pmsUtil.getComboSelectedItem(this.div_search.cmb_dongHo);
		if( dongHoMap["DONG"] != null ){
			this.ds_cond00.setColumn(0, "DONG", dongHoMap["DONG"] );  // 선택 동
		}
		else{
			this.ds_cond00.setColumn(0, "DONG", "" );  // 선택 동
		}
		if( dongHoMap["HO"] != null ){
			this.ds_cond00.setColumn(0, "HO"  , dongHoMap["HO"] );  // 선택 호
		}
		else{
			this.ds_cond00.setColumn(0, "HO"  , "" );  // 선택 호
		}
    }
    
    this.ds_cond00.setColumn(0, "AEA"                ,this.div_search.cmb_tyAea.value); //평형(평수) 
    this.ds_cond00.setColumn(0, "CSTMR_NM"           ,this.div_search.edt_nm.value   ); //이름
   
    this.ds_cond00.setColumn(0, "CNTRCTR_STTUS_CODE" , sChk);  // 계약상태코드(01:계약자,02:해약자)
    this.ds_cond00.setColumn(0, "CNTRCTR_STTUS_AT"   , "Y" );  // 순수 해약(계약)정보만 조회
    
    //------ 기존 소스 -------------------------------//
    //this.ds_cond00.setColumn(0, "CNTRCTR_STTUS_CODE" , "01");  // 계약상태코드
	//this.ds_cond00.setColumn(0, "CNTRCTR_STTUS_AT"   , "Y" );  // 순수 해약(계약)정보만 조회
	//this.ds_cond00.setColumn(0, "RENT_LTTOT_CNVRS_AT", "Y" );  // 임대/분양 전환일자있는사람만(Y:  , N: )
	
    //this.pmsUtil.dumper(this.ds_cond00);
	//------ 기존 소스 -------------------------------//
	
	// 주택계약 리스트 조회
	this.pmsUtil.getHcntrctrList("ds_cond00", "ds_hcntrctrList");
	
}

