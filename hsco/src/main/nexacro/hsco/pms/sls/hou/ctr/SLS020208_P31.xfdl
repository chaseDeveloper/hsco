/**
 * 함수명  	: fn_onFileSelected 이벤트 핸들러
 * 설명 		: 업로드 파일선택 첨부파일선택시 콜백 함수
 * params  		: fileFullName 선택 전체 파일명, type 실행구분(ExtFileUpload/ExtFileDownload)
 * return Type 	: None
 * 사용법		: this.fn_onFileSelected();
 */
this.fn_onFileSelected = function(type, fileFullName) {
//alert("type = " + type +"    fileFullName = " + fileFullName );
	if(type == this.FILE_MANAGER.TYPE_UPLOADER) {
		var isNmArray = this.comUtils.isArray(fileFullName);
		
		if(isNmArray) {
			// TODO - 본 화면은 단건 처리이므로 해당 없음.				
		} 
		else{
			if(typeof(fileFullName) == "boolean" && !fileFullName){
				// 선택파일이 조건에 맞지않아 제외되어 건수가 없을 경우
				// 선택행의 FILE_SN을 삭제한다.
				this.ds_houseSublsList.setColumn(this.ds_houseSublsList.rowposition, "FILE_SN", "");	// 첨부된 파일 데이터 셋 입력 취소 처리
				this.ds_houseSublsList.setColumn(this.ds_houseSublsList.rowposition, "CTRTC_FILE", "");
			}
			else{
				this.ds_houseSublsList.setColumn(this.ds_houseSublsList.rowposition, "ORGINL_FILE_NM", fileFullName);	// 첨부된 파일 데이터 셋 입력 처리
				
				this.fn_setAtchFileView();		// 첨부파일 영역 처리
			}
		}
	}
}


/**
 * 함수명  	: fn_downloadAtchFile
 * 설명 		: 첨부파일 보기(다운로드)
 * params  		: null
 * return Type 	: None
 * 사용법		: this.fn_downloadAtchFile();
 */
this.fn_downloadAtchFile = function(){
	var fileSn = this.ds_houseSublsList.getColumn(this.ds_houseSublsList.rowposition, "FILE_SN");			// 파일일련번호(파일경로)
	var fileOrder = this.ds_houseSublsList.getColumn(this.ds_houseSublsList.rowposition, "FILE_ORDR");
	var fileName = this.ds_houseSublsList.getColumn(this.ds_houseSublsList.rowposition, "ORGINL_FILE_NM");	// 실제 파일명
	
	this.debug("fileSn :" + fileSn);
	this.debug("fileOrder :" + fileOrder);
	this.debug("fileName :" +fileName);
	
	this.FILE_MANAGER.download(fileSn, fileOrder, fileName);
}

/**
 * 파일업로드 선택 파일 취소
 * @param 
 * @return void
 */
this.fn_cancelAtchFile = function(){
	
	var nCurRow = this.ds_houseSublsList.rowposition;	
	var useAddFileSn = this.FILE_MANAGER.USE_ADD_FILE_SN;
	var cancelField = useAddFileSn ? "FILE_SN" : "ORGINL_FILE_NM";
	var selFileNm = this.ds_houseSublsList.getColumn(nCurRow, cancelField);
	var result = this.FILE_MANAGER.cancelFiles(selFileNm);
	
	if(result){
		this.ds_houseSublsList.setColumn(nCurRow, "ORGINL_FILE_NM", "");		// 표기용 파일명 정보 초기화
		
		if(useAddFileSn){
			this.ds_houseSublsList.setColumn(nCurRow, "FILE_SN", "");	// 처리용 파일일련번호 정보 초기화
			this.ds_houseSublsList.setColumn(nCurRow, "CTRTC_FILE", "");	// 처리용 파일일련번호 정보 초기화
		}
		
		this.fn_setAtchFileView();		// 첨부파일 영역 처리
	}
}

/**
 * 함수명  	: fn_deleteAtchFile
