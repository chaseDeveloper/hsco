 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */ 
this.fn_delete = function()
{
	if(this.ds_houseSublsList.rowcount <= 0){
        this.alert("삭제할 정보가 없습니다");
        return;
    }

	if(this.confirm("전대내역을 모두 삭제하시겠습니까?")){
	   this.fn_transaction("deleteAllSublsList");
	}
}

/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */ 
this.fn_cancel = function(){
	
	if(this.ds_houseSublsList.rowcount <= 0){
        return;
    }
    
	if(this.confirm("작업을 취소 하시겠습니까?")){
	   this.ds_houseSublsList.cancel();
	}
}

