 */
this.fn_init_form = function()
{
	//this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search00);	// 조건에 사용되는 Dataset 초기화
}


/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function()
{

	// 공통코드 목록 조회
 	var comCodeParam = [
 		  {OUT_PUT:"ds_paySn", GRP_CODE:"HOU0A9", USE_AT:"1"} // 분납순번
 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
 	
	// 주택코드 목록 조회
	var paramMap = {};	
	paramMap["USE_AT"] = "1";
	paramMap["HOUSE_SE"] = "S1,S2,S3,S4";
	this.pmsUtil.getHouseCode(paramMap, "ds_houseCodeSh" , "fn_set");	
 	
}

this.fn_set = function(){
	var ds = this.ds_houseCodeSh;
	ds.insertRow(0);
	ds.setColumn(0, "HOUSE_CODE", "%");
	ds.setColumn(0, "HOUSE_NM", "전체");
	ds.setColumn(0, "ALL_HOUSE_NM", "전체");
	ds.set_rowposition(0);
	this.div_search00.cmb_houseCodeSh.set_index(0);	
}

// 탭페이지 변경
this.Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{

}

/*
 * 함수명  	    : fn_search
 * 설명 		: 계약목록 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function() 
{
	
		// 검색영역 Validation 체크
		if(!this.gfn_checkValidation(this.div_search00, this.ds_validation00)) {
			return;
		}
		
		// Clear Datasets
		this.pmsUtil.clearDataSet([this.ds_houseLttotPayList]);	
	    
		// 분양금납부대상자 조회
		this.fn_transaction("selectHouseLttotPayList");

}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
 /*
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	
	    // 분양금납부대상자 조회
		case "selectHouseLttotPayList" :
			var sController   	= "hsco/pms/sls/hou/rqe/SLS030201/selectHouseLttotPayList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_houseLttotPayList=output1";			
			break;
			
	    // 분양금납부대상자 저장(조정파일생성)
		case "saveHouseLttotPayList" :
			var sController   	= "hsco/pms/sls/hou/rqe/SLS030201/saveHouseLttotPayList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "";
			break;
			
	    // 임대료납부대상자 조회
		case "selectHouseRentPayList" :
			var sController   	= "hsco/pms/sls/hou/rqe/SLS030201/selectHouseRentPayList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_houseRentPayList=output1";			
			break;
			
	    // 임대료납부대상자 저장(체납파일생성)
		case "saveNpymHouseRentPayList" :
			var sController   	= "hsco/pms/sls/hou/rqe/SLS030201/saveNpymHouseRentPayList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "";
			break;
			
	    // 임대료납부대상자 조정파일 생성여부 체크
		case "selectMdatFileCreatYn" :
			var sController   	= "hsco/pms/sls/hou/rqe/SLS030201/selectMdatFileCreatYn.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_mdatFileCreatYn=output1";			
			break;
			
	    // 임대료납부대상자 저장(조정파일생성)
		case "saveMdaHouseRentPayList" :
			var sController   	= "hsco/pms/sls/hou/rqe/SLS030201/saveMdaHouseRentPayList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "";
			break;

	    // 임대료전월비교 조회
		case "selectHouseRentPayLsmthCmpr" :
			var sController   	= "hsco/pms/sls/hou/rqe/SLS030201/selectHouseRentPayLsmthCmpr.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_houseRentPayLsmthCmpr=output1";			
			break;
			
		// 납부서 일괄 출력			
		case "selectBillPrintListHouseALL":	
				var sController   	= "hsco/pms/sls/com/SLS000000/selectBillPrintListHouseALL.do";
				var sInDatasets   	= "input1=ds_cond_report";
				var sOutDatasets  	= "ds_billPrintList=output1";
				break;			

		// 프로시저(임대료 , 지원금 납부요청 프로시저)
		case "selectSlaryList6": 
				//this.debug("start selectSlaryList6");
				var sController   	= "hsco/pms/sls/com/SLS000000/P7.do";
				var s  = "input1=ds_mt_rntchrg02:U"
				       + " input2=ds_mt_rntchrg01:U"
				       + " input3=ds_mt_rntchrg03:U";
				var sInDatasets   	=  s;
				var sOutDatasets  	= "";
				break;	

		default :
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/ 
/*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId){		
		
			// 공통코드 목록 조회
			case "selectCommonCode" :
				break;
				
			// 분양금납부대상자 조회
			case "selectHouseLttotPayList" :
//				if(this.ds_houseLttotPayList.rowcount > 0 ){
// 					this.ds_houseLttotPayList.set_rowposition(nRowPos);
// 					nRowPos = 0;
//				}
				break;
				
			// 분양금납부대상자 저장(조정파일생성)
			case "saveHouseLttotPayList" :
				this.fn_search();
				this.alert("분양금 조정파일생성이 완료 되었습니다.");				
				break;
				
			// 임대료납부대상자 조회
			case "selectHouseRentPayList" :
				if(this.ds_houseRentPayList.rowcount > 0 ){
					this.ds_houseRentPayList.set_rowposition(nRowPos);
					nRowPos = 0;
				}			
				break;
				
			// 임대료납부대상자 저장(체납파일생성)
			case "saveNpymHouseRentPayList" :
				this.alert("체납파일생성이 완료 되었습니다.");
				break;
							
			// 임대료납부대상자 조정파일 생성여부 체크
			case "selectMdatFileCreatYn" :
				this.Tab00_tabpage2_btn_mdatFileCreat_onclick_callBack();
				break;
				
			// 임대료납부대상자 저장(조정파일생성)
			case "saveMdaHouseRentPayList" :
				this.fn_search();
				this.alert("임대료 조정파일생성이 완료 되었습니다.");				 
				break;
				
			// 임대료전월비교 조회
			case "selectHouseRentPayLsmthCmpr" :
				break;
				
				
			case "selectSlaryList6" :
				this.alert("납부요청 완료");			
				break;

			case "selectBillPrintListHouseALL" :
				this.fn_billPrint();
				break;



			default :
				break;
		}
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}

