/* 납부서 출력 */
this.btn_mdatFileCreat00_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_houseLttotPayList.rowcount == 0){
		this.alert("출력할 내역이 없습니다.");
		return;
	}

	var ds = this.ds_houseCodeSh;
	var houseCode = this.div_search00.cmb_houseCodeSh.value;
    var v_house_se = ds.getColumn(ds.findRow("HOUSE_CODE" , houseCode)  , "HOUSE_SE");     
    if(v_house_se=="S1" || v_house_se=="S3" ){
      var v_bj_yn = '';
    }else{
      var v_bj_yn = 'B';
    }
    
	this.ds_cond_report.clearData();
	this.ds_cond_report.addRow();
	
	var cntrctrNo = new Array();	
	for(var i = 0 ; i < this.ds_houseLttotPayList.rowcount ; i++){
		if(this.ds_houseLttotPayList.getColumn( i , "CHK") == "1"){
			cntrctrNo.push(this.ds_houseLttotPayList.getColumn(i , "CNTRCTR_NO"));	// 계약자번호		
		}	
	}	
	this.ds_cond_report.addRow();
	this.ds_cond_report.setColumn( 0 , "CNTRCTR_NO" , cntrctrNo);
	this.ds_cond_report.setColumn( 0 , "TR_DATE" , this.cal_mdatDe.value);
	this.ds_cond_report.setColumn( 0 , "BJ_YN" , v_bj_yn);

	this.fn_transaction("selectBillPrintListHouseALL");	
}

