 ***********************************************************************/
this.SLS030201_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();             // 공통 폼 초기화
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화	
}


this.SLS030201_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	// Combo에 사용되는 Dataset Load
	this.fn_loadCombo();
	
	// 조회조건 초기값 설정(div_search00)

	this.ds_cond00.setColumn(0, "PAY_TMLMT", this.dateUtils.getLastDay(this.gfn_today(),"s"));
	this.ds_cond00.setColumn(0, "MDAT_DE", this.gfn_today());
	
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: initForm
