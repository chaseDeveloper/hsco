 */

 
 /***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "pms_lib::pmsUtilInclude.xjs";


/***********************************************************************
 * 부모 Form에서 파라미터로 넘겨야 할 값
 * callback : function()
 ***********************************************************************/
this.callback = null; 	//부모폼에서 정의한 콜백 함수
var gPayAmt = 0;		//분납계산용 납부금액
var gPayTmlmt = null; 	//분납계산용 납부기한일

this.SYS_SE = "PMS";						// 시스템 구분
/*this.JOB_SE = "SLS";						// 업무 구분*/
this.JOB_SE = "HOU";						// 업무 구분
this.FILE_MANAGER = null;					// 파일 업/다운로더


this.SLS020208_P11_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();//공통 폼 초기화
	this.fn_init_form();//폼 초기화
	this.fn_init_dataset();//Dataset 초기화
}


this.SLS020208_P11_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// 파일 업/다운로더 설정
	var fileConfig = {
		onChange: this.fn_onFileSelected,
		onSuccess : this.fn_onFileSuccess,
		onError : this.fn_onFileError
	};
	this.FILE_MANAGER = new this.fileManager(this, this.SYS_SE, this.JOB_SE);
	this.FILE_MANAGER.init(fileConfig);
	
	//압류/대출에 맞는 Tab 보여주기
	var tabindex = this.parent.tabindex;	
	this.tab_onselected(tabindex);
	

	// 콤보로드
	this.fn_loadCombo();

}

/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function(){
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);		
}


 /***********************************************************************
 * 공통코드
 ***********************************************************************/
/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function()
{
	// 공통코드 목록 조회
 	var comCodeParam = [
 		  {OUT_PUT:"ds_seizeCodeSh", GRP_CODE:"HOU006", USE_AT:"1"} // 압류구분
 		, {OUT_PUT:"ds_lonSeCode",   GRP_CODE:"HOU0A5", USE_AT:"1"} // 대출구분
 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
 	
	// 토지코드 목록 조회
	var paramMap = {};
	paramMap["USE_AT"] = "";
	this.pmsUtil.getLadCodeList(paramMap, "ds_landCode", "fn_loadCombo_callback");
}

// 공통코드 콜백
this.fn_loadCombo_callback = function() 
{
	
}

// 압류 or 대출 구분 조회
this.tab_onselected = function(tabindex) 
{
	if (tabindex == 0) { //압류
		this.Tab00.set_tabindex(tabindex);
		this.Tab00.removeTabpage(1);
		this.pmsUtil.clearDataSet([this.ds_seizeList]);
		this.ds_cond00.copyData(this.parent.arg_1);
		this.fn_transaction("selectSeizeOne");
	} else if (tabindex == 1) { //대출
		this.Tab00.set_tabindex(tabindex);
		this.Tab00.removeTabpage(0);
		this.pmsUtil.clearDataSet([this.ds_lonList]);	
		this.ds_cond01.copyData(this.parent.arg_1);
		this.fn_transaction("selectLonOne");	
	} 
}

// 조회
this.fn_search = function() 
{
	if (this.Tab00.tabindex == 0) {
		// 압류정보 조회
		this.pmsUtil.clearDataSet([this.ds_seizeList]);
		this.fn_transaction("selectSeizeOne");
		
	} else if (this.Tab00.tabindex == 1) {
		// 대출정보 조회
		this.pmsUtil.clearDataSet([this.ds_lonList]);	
		this.fn_transaction("selectLonOne");	
	} 
}


// 저장
this.fn_save = function()
{
	if (this.Tab00.tabindex == 0) {
		// 압류정보 저장
		if (this.gfn_message("confirm.저장.여부")) {

			if(this.ds_seizeList.getColumn(this.ds_seizeList.rowposition, "CNTRCTR_NO") == null){
				this.ds_seizeList.setColumn(this.ds_seizeList.rowposition, "CNTRCTR_NO", this.ds_cond00.getColumn(0, "CNTRCTR_NO"));
			}
			if(this.Tab00.tabpage1.chkLonInfoAdd.isChecked()){
				this.ds_seizeList.setColumn(this.ds_seizeList.rowposition, "LON_INFO_ADD", "Y"); //대출정보등록
			}
			if(this.Tab00.tabpage1.chkLonInfoDel.isChecked()){
				if(this.ds_seizeList.getColumn(this.ds_seizeList.rowposition, "RELIS_DE") == null){
					this.alert("대출정보를 해제하실 경우, 해제일은 필수입니다");
					return;
				}
				this.ds_seizeList.setColumn(this.ds_seizeList.rowposition, "LON_INFO_DEL", "Y"); //대출정보해제
			}
			this.fn_transaction("saveSeizeOne");		
		}
	} else if(this.Tab00.tabindex== 1) {
		// 대출정보 저장
		if (this.gfn_message("confirm.저장.여부")) {
		
			if(this.ds_lonList.getColumn(this.ds_lonList.rowposition, "CNTRCTR_NO") == null){
				this.ds_lonList.setColumn(this.ds_lonList.rowposition, "CNTRCTR_NO", this.ds_cond01.getColumn(0, "CNTRCTR_NO"));
			}
		
			if(this.ds_lonList.getColumn(this.ds_lonList.rowposition, "CNTRCTR_NO") == null){
				this.ds_lonList.setColumn(this.ds_lonList.rowposition, "CNTRCTR_NO", this.ds_cond01.getColumn(0, "CNTRCTR_NO"));
			}
			this.fn_transaction("saveLonOne");
		}
	}	
}


// 삭제
this.fn_delete = function()
{
	// Dataset 선택
	var targetDS = new Dataset();
	if (this.Tab00.tabindex == 0) {
		targetDS = this.ds_lcntrctrPayPopList;
	} else if (this.Tab00.tabindex == 1) {
		targetDS = this.ds_lcntrctrRentPayPopList;
	}
	
	// 삭제
	if (targetDS.rowcount > 0) {
		// 선택자료 가져오기
		var option = {};
		option["viewRow"] = "expr:CHK == '1'"
		var checkList = this.pmsUtil.datasetToObject(targetDS, option, true);
		var checkListLeng = checkList == null ? 0 : checkList.length;

		// 선택자료 여부 체크
		if ( checkListLeng == 0 ) {
			this.alert("선택된 자료가없습니다.");
			return;
		}
		
		// 삭제
		this.gfn_delMultiRow(targetDS);
	}
	
}



/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// 압류 조회
		case "selectSeizeOne" : 
			var sController   	= "hsco/pms/sls/hou/ctr/SLS020208/selectSeizeOne.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_seizeList=output1";
			break;
			
		// 압류 저장
		case "saveSeizeOne" : 
			var sController   	= "hsco/pms/sls/hou/ctr/SLS020208/saveSeizeOne.do";
			var sInDatasets   	= "input1=ds_seizeList:U";
			var sOutDatasets  	= "ds_outSeize=output1";
			break;
			
		// 대출 조회
		case "selectLonOne" : 
			var sController   	= "hsco/pms/sls/hou/ctr/SLS020208/selectLonOne.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_lonList=output1";
			break;
			
		// 대출 저장
		case "saveLonOne" : 
			var sController   	= "hsco/pms/sls/hou/ctr/SLS020208/saveLonOne.do";
			var sInDatasets   	= "input1=ds_lonList:U";
			var sOutDatasets  	= "ds_outLon=output1";
			break;	
		
		// 파일조회
		case "selectSlaryList2":
			trace("######################")
			trace(this.ds_cond.saveXML());
			trace("######################")
				
			var sController   	= "hsco/pms/sls/lad/std/SLS070102/getMultiFileList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  ;
			if(this.Tab00.tabindex == 0){
				sOutDatasets = "ds_rpairs_file=multifileList";
			}else{
				sOutDatasets = "ds_rpairs_file2=multifileList";
			}
			break;				
			
		// 파일 일련번호 조회
		case "getFileSn" :
