/**
 * 파일 삭제
 * @param 
 * @return void
 */
this.fn_deleteAtchFile = function(){
	this.ds_file_delete.clearData();
	var checkListLeng = 0;
	var confirmMsg = "선택한 이미지를 삭제하시겠습니까?";
	if(confirm(confirmMsg)){
			this.ds_file_delete.clearData();
			
			if(this.Tab00.tabindex == 0){ //압류
				// 선택자료 가져오기
				for(var i = 0 ; i < this.ds_rpairs_file.rowcount ; i++){
					if(this.ds_rpairs_file.getColumn( i , "CHK") == '1'){
						var nRow = this.ds_file_delete.addRow();

						this.ds_file_delete.setColumn( nRow , "FILE_SN" , this.ds_rpairs_file.getColumn( i , "FILE_SN"));
						this.ds_file_delete.setColumn( nRow , "FILE_ORDR" , this.ds_rpairs_file.getColumn( i , "FILE_ORDR"));	
						this.ds_file_delete.setColumn( nRow , "CNTRCTR_NO" , this.ds_seizeList.getColumn(this.ds_seizeList.rowposition , "CNTRCTR_NO"));
						this.ds_file_delete.setColumn( nRow , "SEIZR_SN" , this.ds_seizeList.getColumn( 0 , "SEIZR_SN"));
						checkListLeng += 1;
					}
				}
			}else if(this.Tab00.tabindex == 1){//대출
				// 선택자료 가져오기
				for(var i = 0 ; i < this.ds_rpairs_file2.rowcount ; i++){
					if(this.ds_rpairs_file2.getColumn( i , "CHK") == '1'){
						var nRow = this.ds_file_delete.addRow();
						this.ds_file_delete.setColumn( nRow , "FILE_SN" , this.ds_rpairs_file2.getColumn( i , "FILE_SN"));
						this.ds_file_delete.setColumn( nRow , "FILE_ORDR" , this.ds_rpairs_file2.getColumn( i , "FILE_ORDR"));	
						this.ds_file_delete.setColumn( nRow , "CNTRCTR_NO" , this.ds_lonList.getColumn(this.ds_lonList.rowposition , "CNTRCTR_NO"));
						this.ds_file_delete.setColumn( nRow , "SEIZR_SN" , this.ds_lonList.getColumn( 0 , "SEIZR_SN"));
						checkListLeng += 1;
					}
				}
			}

			// 선택자료 여부 체크
			if (checkListLeng == 0 ) {
				this.alert("선택된 자료가없습니다.");
				return;
			}

			this.fn_transaction("deleteFile");		// 첨부파일 삭제;
	}
}

