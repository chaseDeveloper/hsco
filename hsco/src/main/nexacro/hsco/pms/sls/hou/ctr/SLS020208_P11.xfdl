			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fileSn=output_fsn";
			break;
			
		// 파일 저장
		case "saveSlaryList2":	
			var sController   	= "hsco/pms/sls/hou/ctr/SLS020208/atchCUD.do";	
			var sInDatasets 	= "input1=ds_cond02";                  
			var sOutDatasets  	= "";
			break;
			
		// 파일 선택 파일 삭제	
		case "deleteFile" :			
		    var sController = "hsco/pms/sls/hou/ctr/SLS020208/atchDelete.do";
			var sInDatasets = "input1=ds_file_delete";
			var sOutDatasets = "";
			break;		

			
		default :
			break;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


/**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/
/*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
	
		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId){
			// 압류 조회
			case "selectSeizeOne" :
				this.selectFileList();
				break;
				
			// 압류 저장
			case "saveSeizeOne" :
				this.ds_cond02.setColumn(this.ds_cond00.rowposition, "CNTRCTR_NO", this.ds_cond00.getColumn(0, "CNTRCTR_NO"));
				this.ds_cond02.setColumn(this.ds_outSeize.rowposition, "SEIZR_SN", this.ds_outSeize.getColumn(0, "SEIZR_SN"));
				this.fn_save2();
				break;
				
			// 대출 조회
			case "selectLonOne" :
				this.selectFileList();
				break;
				
			// 대출 저장
			case "saveLonOne" :
				this.ds_cond02.setColumn(this.ds_cond01.rowposition, "CNTRCTR_NO", this.ds_cond01.getColumn(0, "CNTRCTR_NO"));
				this.ds_cond02.setColumn(this.ds_outLon.rowposition, "LON_SN", this.ds_outLon.getColumn(0, "LON_SN"));
				this.fn_save2();
				break;
				
			// 파일 저장
			case "saveSlaryList2":	
				this.fn_search();
				break;
				
			// 파일 선택 파일 삭제		
			case "deleteFile" :		
				this.fn_search();
				break;		
			
			default :
				break;
		}
	}
}


