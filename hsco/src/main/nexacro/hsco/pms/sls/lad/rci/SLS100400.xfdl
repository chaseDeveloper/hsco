    {
      if(application.confirm("변경중인데이타가 존재합니다. 진행하시겠습니까?")){
		  
	  }else{
	     return;
	  }
    }
	this.ds_decsnList.clearData();
	this.ds_cond00.clearData();
	var nRow = this.ds_cond00.addRow();
