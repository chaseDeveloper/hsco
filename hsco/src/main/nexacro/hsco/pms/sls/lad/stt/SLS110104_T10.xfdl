 * 사용법		: <Edit onkeyup="fn_editOnKeyUpHandler">...</Dataset>
 */
this.fn_editOnKeyUpHandler = function(obj:Edit, e:nexacro.KeyEventInfo)
{
	var objName = obj.name;
	var keyCode = e.keycode;
	switch(objName){
		// 성명
		case "edt_cntrctrNm" :
			if(keyCode == 13){
				this.PARENT_FORM.fn_search();
			}
			break;
		default :
			break;
	}
}

