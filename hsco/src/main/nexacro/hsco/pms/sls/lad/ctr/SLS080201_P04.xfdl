 /***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "pms_lib::pmsUtilInclude.xjs";


/***********************************************************************
 * 부모 Form에서 파라미터로 넘겨야 할 값
 * callback : function()
 ***********************************************************************/
this.callback = null; 	//부모폼에서 정의한 콜백 함수
var gPayAmt = 0;		//분납계산용 납부금액
var gPayTmlmt = null; 	//분납계산용 납부기한일


this.SLS080201_P04_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();//공통 폼 초기화
	this.fn_init_form();//폼 초기화
	this.fn_init_dataset();//Dataset 초기화
}


this.SLS080201_P04_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// 부모전달 파라미터
	this.callback = this.parent.callback;
			
	this.ds_cond00.setColumn(0, "CNTRCTR_NO", this.parent.cntrctrNo);
	this.ds_cond00.setColumn(0, "CSTMR_NM", this.parent.cstmrNm);	
	this.ds_cond00.setColumn(0, "LAD_CODE", this.parent.ladCode);
	this.ds_cond00.setColumn(0, "BLCK", this.parent.blck);
	this.ds_cond00.setColumn(0, "LOT", this.parent.lot);
	this.ds_cond00.setColumn(0, "LOT_TY", this.parent.lotTy);
	this.ds_cond00.setColumn(0, "PRPOS_SE", this.parent.prposSe);
	this.ds_cond00.setColumn(0, "SUPLY_SE", this.parent.suplySe);
	this.ds_cond00.setColumn(0, "LTTOT_RENT_SE", 'S');
	this.ds_cond00.setColumn(0, "PAY_NO", '%');
	
	this.debug("TELNO = " + this.parent.telno);
	this.debug("MBTLNO = " + this.parent.mbtlno);
	
	this.ds_cond00.setColumn(0, "TELNO", this.parent.telno);
	this.ds_cond00.setColumn(0, "MBTLNO", this.parent.mbtlno);
		
	
	// 콤보로드
	this.fn_loadCombo();
	
	
}

/*
 * 함수명  	    : fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function(){
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}

/*
 * 함수명  	    : fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);		
}


 /***********************************************************************
 * 공통코드
 ***********************************************************************/
/*
 * 함수명  	    : fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function()
{
	// 공통코드 목록 조회
 	var comCodeParam = [
 		  {OUT_PUT:"ds_payCode", GRP_CODE:"LAD0A1", USE_AT:"1"} // 납부코드
 		, {OUT_PUT:"ds_prposSe", GRP_CODE:"LAD003", USE_AT:"1"} // 용도구분
 		, {OUT_PUT:"ds_suplySe", GRP_CODE:"LAD004", USE_AT:"1"} // 공급구분
 		, {OUT_PUT:"ds_paymthd", GRP_CODE:"LAD0A7", USE_AT:"1"} // 납부방법
 		, {OUT_PUT:"ds_paySe",   GRP_CODE:"LAD0A8", USE_AT:"1"} // 납부구분
 		, {OUT_PUT:"ds_paySn",   GRP_CODE:"LAD0A9", USE_AT:"1"} // 납부순번 	
 
 		
 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
 	
	// 토지코드 목록 조회
	var paramMap = {};
	paramMap["USE_AT"] = "";
	this.pmsUtil.getLadCodeList(paramMap, "ds_landCode", "fn_loadCombo_callback");
}

// 공통코드 콜백
this.fn_loadCombo_callback = function() 
{
	
	this.fn_search();
}



// 조회
this.fn_search = function() 
{
	this.pmsUtil.clearDataSet([this.ds_PayTotlAmt]);
	this.fn_transaction("selectLcntrctrPayTotlAmt");	
		
	if (this.Tab00.tabindex == 0) {
	
		// 분양약정 조회
		this.ds_cond00.setColumn(0, "LTTOT_RENT_SE", "S");
		this.ds_cond00.setColumn(0, "PAY_CODE", null);
		this.fn_transaction("selectLcntrctrPayPopList");
		
	} else if (this.Tab00.tabindex == 1) {
	
		// 임대약정 조회
		this.ds_cond00.setColumn(0, "LTTOT_RENT_SE", "R");
		this.ds_cond00.setColumn(0, "PAY_CODE", null);
		this.fn_transaction("selectLcntrctrRentPayPopList");	
	
	} else if (this.Tab00.tabindex == 2) {
	
		// 위약금 조회
		this.ds_cond00.setColumn(0, "LTTOT_RENT_SE", null);
		this.ds_cond00.setColumn(0, "PAY_CODE", "HC");
		this.fn_transaction("selectLcntrctrPayPopList");	
	}	
}


// 저장
this.fn_save = function()
{
	if (this.Tab00.tabindex == 0) {
		// 검색영역 Validation 체크
		if(!this.gfn_checkValidation(this.ds_lcntrctrPayPopList, this.ds_validation00)) {
			return;
		}
		
		// 분양금<>등록금액 불일치 시 저장 불가
		var paymentSum = this.ds_lcntrctrPayPopList.getSum('PAYAMT');   //납입금액 총 SUM
		var bgnnAmout = this.ds_PayTotlAmt.getColumn(0, "BGNN_AMOUNT"); //분양금(당초금액)
		if(Number(paymentSum) != Number(bgnnAmout)){
			this.alert("등록된 납부금액들 합이 분양금과 일치하지 않습니다");
			return;
		}
		
					
		// 분양약정 저장
		if (this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("saveLcntrctrPayPopList");		
		}
	} else if(this.Tab00.tabindex== 1) {
		// 검색영역 Validation 체크
		if(!this.gfn_checkValidation(this.ds_lcntrctrRentPayPopList, this.ds_validation01)) {
			return;
		}		
		
		// 임대약정 저장
		if (this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("saveLcntrctrRentPayPopList");
		}
	} else if(this.Tab00.tabindex== 2) {
	
		// 분양약정 저장
		if (this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("saveLcntrctrPayPopList");		
		}
	
	}
}


// 삭제
this.fn_delete = function()
{
	// Dataset 선택
	var targetDS = new Dataset();
	if (this.Tab00.tabindex == 0) {
		targetDS = this.ds_lcntrctrPayPopList;
	} else if (this.Tab00.tabindex == 1) {
		targetDS = this.ds_lcntrctrRentPayPopList;
	}
	
	// 삭제
	if (targetDS.rowcount > 0) {
		// 선택자료 가져오기
		var option = {};
		option["viewRow"] = "expr:CHK == '1'"
		var checkList = this.pmsUtil.datasetToObject(targetDS, option, true);
		var checkListLeng = checkList == null ? 0 : checkList.length;

		// 선택자료 여부 체크
		if ( checkListLeng == 0 ) {
			this.alert("선택된 자료가없습니다.");
			return;
		}
		
		// 삭제
		this.gfn_delMultiRow(targetDS);
	}
	
}



