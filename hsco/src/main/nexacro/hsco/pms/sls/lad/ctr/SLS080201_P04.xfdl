/*
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// 분양약정 조회
		case "selectLcntrctrPayPopList" : 
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080201/selectLcntrctrPayPopList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_lcntrctrPayPopList=output1";
			break;
			
		// 분양약정 저장
		case "saveLcntrctrPayPopList" : 
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080201/saveLcntrctrPayPopList.do";
			var sInDatasets   	= "input1=ds_lcntrctrPayPopList:U";
			var sOutDatasets  	= "";
			break;
			
		// 임대약정 조회
		case "selectLcntrctrRentPayPopList" : 
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080201/selectLcntrctrRentPayPopList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_lcntrctrRentPayPopList=output1";
			break;
			
		// 분양약정 저장
		case "saveLcntrctrRentPayPopList" : 
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080201/saveLcntrctrRentPayPopList.do";
			var sInDatasets   	= "input1=ds_lcntrctrRentPayPopList:U";
			var sOutDatasets  	= "";
			break;
			
		// 분양약정 납부금액계산 및 적용
		case "selectLttotInspyIntrInfo" : 
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080201/selectLttotInspyIntrInfo.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_InspyIntrInfo=output1";
			break;
			
		// 임대약정 납부금액계산 및 적용
		case "selectRentInspyIntrInfo" : 
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080201/selectRentInspyIntrInfo.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_InspyIntrInfo=output1";
			break;
			
		//당초금액 조회
		case "selectLcntrctrPayTotlAmt" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080201/selectLadDetailInfoForBgnnAmount.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_PayTotlAmt=output1";
			break;

		default :
			break;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


/**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/
/*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){

		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId){
			// 분양약정 조회
			case "selectlcntrctrPayPopList" :
				break;
				
			// 분양약정 조회
			case "saveLcntrctrPayPopList" :
				this.fn_search();
				break;
				
			// 임대약정 조회
			case "selectLcntrctrRentPayPopList" :
				break;
				
			// 임대약정 저장
			case "saveLcntrctrRentPayPopList" :
				this.fn_search();
				break;
				
			// 분양약정 납부금액계산 및 적용
			case "selectLttotInspyIntrInfo" :			
				this.btn_calc_lttot_onclick_callback();
				break;
				
			// 임대약정 납부금액계산 및 적용
			case "selectRentInspyIntrInfo" :			
				this.btn_calc_rent_onclick_callback();
				break;
				
			// 분양금 조회
			case "selectLcntrctrPayTotlAmt" :
				break;

			default :
				break;
		}
	}
}


