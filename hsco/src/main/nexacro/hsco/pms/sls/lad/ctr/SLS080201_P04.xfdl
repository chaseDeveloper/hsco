// 분양약정 납부금액계산 및 적용
this.Tab00_tabpage1_btn_calc_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// isUpdate 여부 체크후 진행하게 로직을 추가한다.
	
	if (this.Tab00.tabindex == 0) {
		var nCheckIdx = this.ds_lcntrctrPayPopList.findRow("CHK", "1");	
		
		var cntrctrNo = this.ds_lcntrctrPayPopList.getColumn(nCheckIdx, "CNTRCTR_NO");	//계약자번호
		var ladCode = this.ds_cond00.getColumn(0, "LAD_CODE")							//토지코드
		var toDay = this.ds_lcntrctrPayPopList.getColumn(nCheckIdx, "PAY_TMLMT");		//납부예정일	
		var payamt = this.Tab00.tabpage1.mse_rcivAmount.value;
		
		this.ds_cond01.setColumn(0, "SE", "A");
		this.ds_cond01.setColumn(0, "LAD_CODE", ladCode);
		this.ds_cond01.setColumn(0, "TO_DAY", toDay);
		this.ds_cond01.setColumn(0, "PAY_TMLMT", gPayTmlmt);
		this.ds_cond01.setColumn(0, "PAYAMT", payamt);
		this.ds_cond01.setColumn(0, "CNTRCTR_NO", cntrctrNo);
		this.ds_cond01.setColumn(0, "CAL_SE", "0");
		
		// Validation 체크
		if (this.comUtils.isNull(payamt)) {
			this.Tab00.tabpage1.mse_rcivAmount.setFocus(true);	
			this.alert("수납금액을 입력하십시오.");
			return;
		}
			
		// Grid Validation 체크
		if(!this.gfn_checkValidation(this.ds_lcntrctrPayPopList, this.ds_validation02)) {
			return;
		}
		
		this.fn_transaction("selectLttotInspyIntrInfo");
	}
}


// 납부금액계산 및 적용 콜백
this.btn_calc_lttot_onclick_callback = function()
{
	var nCurRow = this.ds_lcntrctrPayPopList.findRow("CHK", "1");
	var nNewRow = this.ds_lcntrctrPayPopList.getRowCount() - 1;
	
	// 분납이자 계산(분납이자가 반환되지 않는다)
	var payamt = this.ds_InspyIntrInfo.getColumn(0, "PAYAMT");
	var curPayamt = this.ds_lcntrctrPayPopList.getColumn(nCurRow, "PAYAMT");	
	var newPayamt = this.ds_lcntrctrPayPopList.getColumn(nNewRow, "PAYAMT");

	var inspayIntr = Number(curPayamt) - Number(payamt); //분납이자

	// 조회 금액이 0일때 처리
