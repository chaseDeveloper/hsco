				this.ds_cond01.setColumn( nRow , "LAD_CODE" , this.div_search.cmb_ladCodeSh.value);
				this.ds_cond01.setColumn( nRow , "BLCK" , this.div_search.edt_blck.value);
				this.ds_cond01.setColumn( nRow , "LOT" , this.div_search.edt_lot.value);
				this.ds_cond01.setColumn( nRow , "LOT_TY" , this.div_search.edt_lotTy.value);
				
				//this.ds_cond01.setColumn( nRow , "APPLC_DE1" , this.div_search.cmb_applc_de1.value);
				//this.ds_cond01.setColumn( nRow , "APPLC_DE2" , this.div_search.cmb_applc_de2.value);
				this.fn_transaction("PrepayList");	
			} 
			else 
		    {
				alert(" 사용할 수 없는 토지코드 입니다. ");
			}			
			break;
	}				
}       
                                                                
                                                            
/*
 * 함수명  	: fn_save
 * 설명 		: 계약자 자장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
// 	if(!this.gfn_checkValidation(this.grd_applcnt, this.ds_validation)) {
// 		return;
// 	}	

	if(this.confirm("저장하시겠습니까?"))	{
		var tabid = this.Div00.Tab00.tabpages[this.Div00.Tab00.tabindex].name; // eg. tabpage1
		switch(tabid)
		{
			case 'tabpage1':			
			for( var i = 0 ; i < this.ds_lad_detail_info.rowcount ; i++) {				
				if (this.ds_lad_detail_info.getColumn(i, "CHK") != 1) continue;									
				trace("면적/단가 정산 저장 !!!");
				this.ds_lad_detail_info.setColumn( i , "IN_FG" , 'UP_02');
				this.ds_lad_detail_info.setColumn( i , "PAY_TMLMT" , this.Div00.Tab00.tabpage1.div_search.Calendar01.value); 
				this.ds_lad_detail_info.setColumn( i , "MDAT_DE" , this.Div00.Tab00.tabpage1.div_search.Calendar00.value);						
			}
			this.fn_transaction("saveSlaryList1");				
			break;
			
			case 'tabpage3':
			this.fn_transaction("updatePrepayList");	
			break;
		}
	}
}	

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	var sController = null;
	var oInDatasets = null;
	var sOutDatasets = null;
	switch (strSvcId) {

		// 토지코드별 조회
		case "selectLadDetailList" :
		
			sController = "hsco/pms/sls/lad/std/SLS070103/ladDetailList.do";
			oInDatasets = "input1=ds_cond00";
			sOutDatasets = "ds_lad_detail_info=output1";
			break;

		case "PrepayList" :
		
			sController = "hsco/pms/sls/lad/std/SLS070103/PrepayList.do";
			oInDatasets = "input1=ds_cond01";
			sOutDatasets = "ds_lcntrctr_pay=output1";
			break;

		case "ApplcDeList" :
		
			sController = "hsco/pms/sls/lad/std/SLS070103/ApplcDeList.do";
			oInDatasets = "input1=ds_cond01";
			sOutDatasets = "ds_applc_de=output1";
			break;
			
		case "IntrtList" :
		
			sController = "hsco/pms/sls/lad/std/SLS070103/IntrtList.do";
			oInDatasets = "input1=ds_cond01";
			sOutDatasets = "ds_intrt=output1";
			break;			

		case "saveSlaryList1" :
		
			sController = "hsco/pms/sls/lad/std/SLS070103/saveSlaryList.do";
			oInDatasets = "input1=ds_lad_detail_info:U";
			sOutDatasets = "";
			break;						
			
		case "saveSlaryList2" :
		
			sController = "hsco/pms/sls/lad/std/SLS070103/saveSlaryList2.do";
			oInDatasets = "input1=ds_lcntrctr_pay:U";
			sOutDatasets = "";
			break;
									
		case "updatePrepayList" :		
			sController = "hsco/pms/sls/lad/std/SLS070103/updatePrepayList.do";
			oInDatasets = "input1=ds_lcntrctr_pay:U";
			sOutDatasets = "";
			break;						
		default :
			break;
			
	}

	if(sController == null) return;
	
	var sInDataType = typeof(oInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, reqParam, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, oInDatasets, sOutDatasets, null, null, null, bSync);
	}

}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var resultLeng = 0;
		var schItem = null;
		switch(strSvcId){
		
			// 공통코드 목록 조회
			case "saveSlaryList" :
				this.fn_search();
				break;
				
			case "ApplcDeList" :
				this.div_search.cmb_applc_de1.set_index(0);
				this.div_search.cmb_applc_de2.set_index(1);
				break;				
				
			case "IntrtList" :
				this.fn_transaction("PrepayList");
				break;		

			case "saveSlaryList2" :
				this.fn_transaction("PrepayList");
				break;	
			case "updatePrepayList" :
				alert("저장완료");
				break;
												
							
		}
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}
