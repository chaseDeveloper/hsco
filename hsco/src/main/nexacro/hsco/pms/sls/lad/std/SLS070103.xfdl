this.Div00_Tab00_tabpage3_Grid02_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
  if(e.col == 0) {
	var cntrctr_no = this.ds_lcntrctr_pay.getColumn(e.row, "CNTRCTR_NO");
	var chk = this.ds_lcntrctr_pay.getColumn(e.row, "CHK");
    for( var i = 0 ; i < this.ds_lcntrctr_pay.rowcount ; i++) {
		if (this.ds_lcntrctr_pay.getColumn(i, "CNTRCTR_NO") == cntrctr_no)
			this.ds_lcntrctr_pay.setColumn( i , "CHK" , chk);
    }
  }
}


this.Div00_Tab00_tabpage1_div_search_Button02_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var UNTPC = this.Div00.Tab00.tabpage1.div_search.edt_dong01.value;
	var detailRow = this.ds_lad_detail_info.findRow("CHK", "1");

	if(detailRow > -1){
		for( var i = 0 ; i < this.ds_lad_detail_info.rowcount ; i++)
		{		
			if(this.ds_lad_detail_info.getColumn( i , "CHK") == 1)
			{
				var DCSN_AR = this.ds_lad_detail_info.getColumn( i , "DCSN_AR");
				var DCSN_AMOUNT = UNTPC * DCSN_AR;
				this.ds_lad_detail_info.setColumn( i , "DCSN_UNTPC" , UNTPC);
				this.ds_lad_detail_info.setColumn( i , "DCSN_AMOUNT" , DCSN_AMOUNT);		
				this.ds_lad_detail_info.setColumn( i , "IN_FG" , 'UP_02');		
			}	
		}			
	}else{
		this.alert("일괄적용 항목을 선택해 주세요.");
	}

}

/*
 * 함수명  	: Div00_btn_apply_onclick
 * 설명 		: 선수금적용 계산
 * params  		: 
 * return Type 	: None
 * 사용법		: this.Div00_Tab00_tabpage3_div_search_Button124_onclick();
 */	

this.Div00_btn_apply_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	for( var i = 0 ; i < this.ds_lcntrctr_pay.rowcount ; i++)
	{
		if(this.ds_lcntrctr_pay.getColumn( i , "CHK") == 1)
		{
			var dc_rate_l4 = this.ds_lcntrctr_pay.getColumn(i, "DC_RATE_L4") / 100.;
			var payAmt = this.ds_lcntrctr_pay.getColumn(i, "PAYAMT");
			var dayCnt = this.ds_lcntrctr_pay.getColumn(i, "PREPAY_DAYCNT");
			var amt = payAmt * dc_rate_l4 * dayCnt / 365.
			
			this.ds_lcntrctr_pay.setColumn( i , "PREPAY_EXCCLC_AMOUNT" , nexacro.round(amt, -1));		
		}	
	}		
}

