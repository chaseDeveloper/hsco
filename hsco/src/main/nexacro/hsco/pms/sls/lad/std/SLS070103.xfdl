*=========================================*/
this.fn_search = function() 
{
	// 검색영역 Validation 체크
// 	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
// 		return;
// 	}
	var tabid = this.Div00.Tab00.tabpages[this.Div00.Tab00.tabindex].name; // eg. tabpage1
	
	switch(tabid)
	{
		case 'tabpage1':		// 면적 정산
			this.search_btl_set();
			break;
		case 'tabpage2':		// 금액 정산
			this.search_btl_set();
			break;
		case 'tabpage3':		// 선수금 정산	
		    if(this.div_search.cmb_ladCodeSh.value == '024')
		    {
				this.ds_cond01.clearData();
