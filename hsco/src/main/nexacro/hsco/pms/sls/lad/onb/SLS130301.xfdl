/*
 * 함수명  	: fn_snInit
 * 설명 		: 순번초기화
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_snInit = function(ds,col){
	var trow = ds.getRowCount();

	for(var i = 0 ; i < trow ; i++)
	{
	   ds.setColumn(i,col,i + 1);
	}	
}

