	if(this.ds_plnmRgst.isUpdated() == true)
	{	
	   if(this.gfn_checkValidation(this.ds_plnmRgst, this.ds_validation) == false){  
		  return;
	   }	   
	}
		
	if( !this.ds_plnmRgst.isUpdated() && !this.ds_onbAtchFile.isUpdated())
	{
		this.gfn_message("comm.내역.변경.없음");	    
		return;
	}

	 if(this.gfn_message("comm.행위여부","저장"))
	 { 	     
		  if (this.ds_onbAtchFile.isUpdated()) {			  	      
			  var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/savePlnmRgst.do"; 
			  var sInDatasets   	= "input1=ds_plnmRgst:U input3=ds_onbAtchFile:U";
			  var sOutDatasets  	= "";			  	      
			  this.FILE_MANAGER.upload(sController, sInDatasets, sOutDatasets);	      
		  } else {	      
			  this.fn_transaction("savePlnmRgst");
		  }
	 }else{
		return;
	 }    
}


