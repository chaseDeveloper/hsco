// 공매정보 신규
this.fn_insert_1 = function()
{
	var rowCnt    = this.ds_pbctRgst.getRowCount();
	var trnsId    = this.ds_trnsId.getColumn(0,"TRNS_ID");
	var orgPlmnNo = this.ds_trnsId.getColumn(0,"ORG_PLNM_NO");
	var PBCT_SEQ  = "";
	var PBCT_DGR  = "";
	
	if (this.ds_pbctRgst.rowcount > 0) {
	    PBCT_SEQ  = this.ds_pbctRgst.getColumn(this.ds_pbctRgst.rowposition, "PBCT_SEQ");
	    PBCT_DGR  = this.ds_pbctRgst.getColumn(this.ds_pbctRgst.rowposition, "PBCT_DGR");
	}

	// 공매정보
	if (this.ds_pbctRgst.rowcount < 1) {
		this.ds_pbctRgst.clearData();
		var curow = this.ds_pbctRgst.addRow();
		this.ds_pbctRgst.setColumn(curow,"TRNS_ID",trnsId);                 //전송ID
		this.ds_pbctRgst.setColumn(curow,"STND_LNK_ORG_NO",STND_LNK_ORG_NO);//연계기관번호
		this.ds_pbctRgst.setColumn(curow,"ORG_PLNM_NO",orgPlmnNo);          //기관공고번호
		this.ds_pbctRgst.setColumn(curow,"PBCT_YR",year);                   //공매년도
    }
	//집행권한
	if (this.ds_exctAuthRgst.rowcount < 1) {
		this.ds_exctAuthRgst.clearData();
		var curow1 = this.ds_exctAuthRgst.addRow();
		this.ds_exctAuthRgst.setColumn(curow1,"TRNS_ID",trnsId);                  //전송ID
		this.ds_exctAuthRgst.setColumn(curow1,"STND_LNK_ORG_NO",STND_LNK_ORG_NO); //연계기관번호
		this.ds_exctAuthRgst.setColumn(curow1,"PBCT_YR",year);                    //공매년도
		this.ds_exctAuthRgst.setColumn(curow1,"PBCT_SEQ",PBCT_SEQ); 
		this.ds_exctAuthRgst.setColumn(curow1,"PBCT_DGR",PBCT_DGR); 
		this.ds_exctAuthRgst.setColumn(curow1,"AUTH_CD_1","0001");              //권한
		this.ds_exctAuthRgst.setColumn(curow1,"AUTH_CD_2","0002");              //권한
		this.ds_exctAuthRgst.setColumn(curow1,"AUTH_CD_3","0003");              //권한
	}
	//입찰조건
    if (this.ds_pbctCdtnRgst.rowcount < 1) {	
		this.ds_pbctCdtnRgst.clearData();
		var curow2 = this.ds_pbctCdtnRgst.addRow();
		this.ds_pbctCdtnRgst.setColumn(curow2,"TRNS_ID",trnsId);                    //전송ID
		this.ds_pbctCdtnRgst.setColumn(curow2,"STND_LNK_ORG_NO",STND_LNK_ORG_NO);   //연계기관번호
		this.ds_pbctCdtnRgst.setColumn(curow2,"PBCT_YR",year);
		this.ds_pbctCdtnRgst.setColumn(curow2,"PBCT_SEQ",PBCT_SEQ);
		this.ds_pbctCdtnRgst.setColumn(curow2,"PBCT_DGR",PBCT_DGR);
	}
}

// 제약조건 신규
this.fn_insert_2 = function()
{
	if(this.ds_bidPtctCdtnRgst.getRowCount() > 0) return;

    // 경쟁방식코드
    var CPTN_MTD_CD  = this.ds_plnmRgst.getColumn(this.ds_plnmRgst.rowposition, "CPTN_MTD_CD");
