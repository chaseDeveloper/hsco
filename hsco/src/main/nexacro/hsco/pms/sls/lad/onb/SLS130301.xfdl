// 유효성검사 버튼
this.Div00_btn_chkValidation_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
  var idx = this.Div00.Tab00.tabindex;
  var ret = false;
  switch (idx) 
  {
	   case 0 : // 공고정보
		   ret = this.fn_save_0_validation();  
	   break;
	   case 1 : // 공매정보
		   ret = this.fn_save_1_validation();  
	   break;
	   case 2 : // 제약조건
		   ret = this.fn_save_2_validation();  
	   break;
	   case 3 : // 물건정보
		   ret = this.fn_save_3_validation();  
	   break;
  }
  if (ret)
    this.alert("유효성검사 완료");	
}

