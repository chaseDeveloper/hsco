		this.ds_bidPtctCdtnRgst.setColumn(curow,"STND_LNK_ORG_NO",STND_LNK_ORG_NO);	//연계기관번호
		this.ds_bidPtctCdtnRgst.setColumn(curow,"BID_MNMT_NO",bidMnmtNo);          	//입찰관리번호
    } else {
        this.alert("입찰참가조건은 공고등록정보의 경쟁방식이 제한경쟁(0002) 또는 지명경쟁(0003) 인 경우 등록가능합니다.");
