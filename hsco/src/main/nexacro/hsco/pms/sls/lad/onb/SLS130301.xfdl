 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {		
		
	    case "selectPlnmRgstTransId":	// 최종전송 Id , 최종공고번호
	        bSync = false;
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectPlnmRgstTransId.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_trnsId=output1";
			break; 
	   case "selectPlnmRgstList":	   // 공고목록 조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectPlnmRgstList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_plnmRgstList=output1";
			break; 
	   case "selectPlnmRgst":	   // 공고정보 조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectPlnmRgst.do"; 
			var sInDatasets   	= "input1=ds_trnsId";
			var sOutDatasets  	= "ds_plnmRgst=output1 ds_onbAtchFile=output2";
			break; 
	   case "selectPbctRgst":	   // 공매정보 조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectPbctRgst.do"; 
			var sInDatasets   	= "input1=ds_trnsId";
			var sOutDatasets  	= "ds_pbctRgst=output1";
			break; 
	   case "selectPbctRgstList":  // 공매목록 조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectPbctRgstList.do"; 
			var sInDatasets   	= "input1=ds_plnmRgst";
			var sOutDatasets  	= "ds_pbctRgstList=output1";
			break; 
	   
	   case "selectExctAuthRgst":	   // 집행권한 / 입찰조건  조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectExctAuthRgst.do"; 
			var sInDatasets   	= "input1=ds_schVal";
			var sOutDatasets  	= "ds_exctAuthRgst=output1 ds_pbctCdtnRgst=output2";
			break; 
	   
	   case "selectPsblAreaRgst":	   // 입찰가능지역 / 입찰가능대상자 / 입찰참가조건 / 입찰참가제한자   조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectPsblAreaRgst.do"; 
			var sInDatasets   	= "input1=ds_schBid";
			var sOutDatasets  	= "ds_psblAreaRgst=output1 ds_psblRcpnRgst=output2 ds_bidPtctCdtnRgst=output3 ds_pbctCltrLmtRgst=output4";
			break;
		
	   case "selectOnbCltrRlstRgst": // 물건정보 조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/selectOnbCltrRlstRgst.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_onbCltrRlstRgst=output1 ds_pbctCltrRgst=output2";
			break; 
	   
	   case "savePlnmRgst":     // 공고정보 저장
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/savePlnmRgst.do"; 
			var sInDatasets   	= "input1=ds_plnmRgst:U input3=ds_onbAtchFile:U";
			var sOutDatasets  	= "";
			break; 
			
	   case "savePbctRgst":     // 공매정보 저장
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130301/savePbctRgst.do"; 
			var sInDatasets   	= "input1=ds_pbctRgst:U input2=ds_exctAuthRgst:U input3=ds_pbctCdtnRgst:U";
