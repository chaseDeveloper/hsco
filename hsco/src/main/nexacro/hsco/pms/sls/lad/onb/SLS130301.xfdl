        !this.fn_save_2_validation() ||
        !this.fn_save_3_validation() ) {
        return;
    }
    
    // 최종 validation 
    if (this.ds_plnmRgst.rowcount < 1) {
        this.alert("공고정보가 등록되지 않았습니다.");
        return;
    }
    // 입찰방식코드
    var BID_MTD_CD   = this.ds_plnmRgst.getColumn(this.ds_plnmRgst.rowposition, "BID_MTD_CD");
    // 경쟁방식코드
    var CPTN_MTD_CD  = this.ds_plnmRgst.getColumn(this.ds_plnmRgst.rowposition, "CPTN_MTD_CD");

    if (CPTN_MTD_CD == "0002" && this.ds_bidPtctCdtnRgst.rowcount + this.ds_psblAreaRgst.rowcount < 1) {
        this.alert("공고등록정보의 경쟁방식코드가 제한경쟁(0002)일 경우 입찰참가조건등록정보와 입찰가능지역등록정보중 둘 중 하나는 반드시 등록해야 합니다.");
        return;
    }
    if (CPTN_MTD_CD == "0003" && this.ds_bidPtctCdtnRgst.rowcount + this.ds_psblRcpnRgst.rowcount < 1) {
        this.alert("공고등록정보의 경쟁방식코드가 지명경쟁(0003)일 경우 입찰참가조건등록정보와 입찰가능대상자등록정보중 둘 중 하나는 반드시 등록해야 합니다.");
        return;
    }
    if (BID_MTD_CD == "0005" && this.ds_psblAreaRgst.rowcount < 1) {
        this.alert("공고등록정보의 입찰방식코드가 시담(0005)일 경우 입찰가능지역등록정보는 반드시 등록해야 합니다.");
        return;
    }
        
    if (this.ds_exctAuthRgst.rowcount < 1) {
        this.alert("집행권한 정보가 등록되지 않았습니다.");
        return;
    }    
    if (this.ds_pbctCltrRgst.rowcount < 1) {
        this.alert("물건정보가 등록되지 않았습니다.");
        return;
    }
    
	if( this.ds_plnmRgst.isUpdated()       ||    // 공고정보
	    this.ds_onbAtchFile.isUpdated()    ||    // 첨부파일
	    this.ds_pbctRgst.isUpdated()       ||    // 공매정보
	    this.ds_exctAuthRgst.isUpdated()   ||    // 집행권한
	    this.ds_pbctCdtnRgst.isUpdated()   ||    // 입찰조건
	    this.ds_psblAreaRgst.isUpdated()   ||    // 입찰가능지역
	    this.ds_psblRcpnRgst.isUpdated()   ||    // 입찰가능대상자
	    this.ds_bidPtctCdtnRgst.isUpdated()||    // 입찰참가조건
	    this.ds_pbctCltrLmtRgst.isUpdated()||    // 입찰참가제한자
	    this.ds_pbctCltrRgst.isUpdated()         // 물건정보
	  ) 
	{
	    this.alert("저장 후 진행바랍니다.");
	    return;
	}	
		
		
	var procSt = this.ds_plnmRgst.getColumn(this.ds_plnmRgst.rowposition, "PROC_ST");		
	this.pmsUtil.dumper(this.ds_plnmRgst);
	
	trace("########## PROC_ST : " + procSt);
	
	if ( procSt != "Z")
	{
	  this.alert("이미 전송되었습니다");
	  return;
	}	
	this.ds_plnmRgst.setColumn(this.ds_plnmRgst.rowposition, "IF_DS", "IFR_BD_PBCT_RGST_05");
	
    if(this.gfn_message("comm.행위여부","전송"))
	{     
	    this.fn_transaction("saveTxTrnsInfo");
	}
}

