	var addr = this.tab_master.tpg_issuForm.div_issuMstForm.edt_zip.value;

	this.gfn_popup('zipPopup', 500, 500, '', {arg_addr:addr}, "common::frmZipAddressPopup.xfdl", fn_cb);					
