 /*
 * 함수명  	: fn_grdOnSelectChangHandler
 * 설명 		: 그리드 onSelectChange 이벤트 처리(행 선택)
 * params  		: obj 	: 이벤트 발생 그리드 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <Grid onselectchanged="fn_grdOnSelectChangHandler">...</Grid>
 */
this.fn_grdOnSelectChangHandler = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{	
	var gridName = obj.name;
	var selRowPosition = e.row;
	switch(gridName){
	
		// 대상(계약) 목록
		case "grd_lcntrctrList" :
			this.fn_selectIssuDtlList(false, selRowPosition);	// 선택 대상 납부/이력목록 조회
			break;
			
		// 양식 목록
		case "grd_issueFormat" :
			this.fn_setInputIssuForm(obj, selRowPosition);	// 선택 양식에 따른 입력양식 설정
			break;
			
		default :
			break;
	}
}

/*
 * 함수명  	: fn_fieldOnKeyUpHandler
 * 설명 		: Edit onKeyUp 이벤트 처리(
 * params  		: obj 	: 이벤트 발생 콤보 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <Edit onitemchanged = "fn_fieldOnKeyUpHandler">...</Edit>
 */
this.fn_fieldOnKeyUpHandler = function(obj, e:nexacro.KeyEventInfo)
{
	var inputName = obj.name;
	var inputValue = obj.value;
	var dspNullText = obj.displaynulltext;
	var isNullValue = this.comUtils.isNullEmpty(inputValue);
	var isNullDspNullText = this.comUtils.isNullEmpty(dspNullText);
	var fontColor = isNullValue ? this.READONLY_COLOR : this.DEFAULT_COLOR;
	
	switch(inputName)
	{
		// 발급용도
		case "edt_issuPrpos" :
			// displaynulltext 값이 있을 경우 처리
			if(!isNullDspNullText){
				obj.style.set_color(fontColor);
			}
			break;
	
		// 기타내역
		case "tar_issuEtc" :
			// displaynulltext 값이 있을 경우 처리
			if(!isNullDspNullText){
				obj.style.set_color(fontColor);
			}
			break;
			
		// C8 : 예금이자 금액
		case "mse_dpstIntr" :
			
			var excclcAmount = this.ds_cond02.getColumn(0, "EXCCLC_AMOUNT");						// 정산금
			var excclcAmountIntr = Number(excclcAmount) + (isNullValue ? 0 : Number(inputValue));	// 정산금 + 입력 예금이자 계산
			this.ds_cond02.setColumn(0, "EXCCLC_AMOUNT_INTR", excclcAmountIntr);					// 계산값 적용
			if(isNullValue){
				this.ds_cond02.setColumn(0, "DPST_INTR", 0);
			}
			break;
			
		default :
			break;
	}
}

/*
 * 함수명  	: fn_fieldOnChangeHandler
 * 설명 		: Edit onChange 이벤트 처리(
 * params  		: obj 	: 이벤트 발생 콤보 Component
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <Edit onitemchanged = "fn_fieldOnChangeHandler">...</Edit>
 */
this.fn_fieldOnChangeHandler = function(obj, e:nexacro.ChangeEventInfo)
{
	var inputName = obj.name;
	var inputValue = e.postvalue;
	var isNullValue = this.comUtils.isNullEmpty(inputValue);

	switch(inputName)
	{
		// 발급용도
		case "edt_issuPrpos" :
			if(isNullValue){
				obj.set_value(null);
				this.ds_cond02.setColumn(0, "ISSU_PRPOS", null);
			}
			break;
	
		// 기타내역
		case "tar_issuEtc" :
			if(isNullValue){
				obj.set_value(null);
				this.ds_cond02.setColumn(0, "ISSU_ETC", null);
			}
			break;
	
		default :
			break;
	}
}

/*
 * 함수명  	: fn_dsOnColumnChangeHandler
 * 설명 		: Dataset onColumnChange 이벤트 처
 * params  		: obj 	: 이벤트 발생 Dataset
				  e		: 발생 이벤트
 * return Type 	: None
 * 사용법		: <Dataset oncolumnchanged = "fn_dsOnColumnChangeHandler">...</Dataset>
 */
this.fn_dsOnColumnChangeHandler = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var dsName = obj.name;
	var rowPos = e.row;
	var colId = e.columnid
	var newValue = e.newvalue;
	
	switch(dsName)
	{
		case "ds_payInfoList" :
		
			if(colId == "ARRRG_AMOUNT" || colId == "PREPAY_DSCNT_AMOUNT")
			{
				var inArrrgValue = colId == "ARRRG_AMOUNT" ? newValue : obj.getColumn(rowPos, "ARRRG_AMOUNT");
				var inPrepayDscntValue = "PREPAY_DSCNT_AMOUNT" ? newValue : obj.getColumn(rowPos, "PREPAY_DSCNT_AMOUNT");
				
				var numArrrg = this.comUtils.isNullEmpty(inArrrgValue) ? 0 : Number(inArrrgValue);
				var numPrepayDscnt = this.comUtils.isNullEmpty(inPrepayDscntValue) ? 0 : Number(inPrepayDscntValue);
				
				var newArrrgrepayDscnt = numArrrg + numPrepayDscnt;
				
				obj.setColumn(rowPos, "ARRRG_PREPAY_DSCNT_AMOUNT", newArrrgrepayDscnt);
			}
			break;
	
		default :
			break;
	}
}

