// 기본입력양식 설정
this.fn_setBaseInputForm = function(enabledArr, readonlyArr, descMapArr)
{
	var targetInfo = [
		  {FIELD_ID:"cal_issuDt", 			FIELD:"ISSU_DT" , 			COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.cal_issuDt}						// 발급일자
		, {FIELD_ID:"edt_issuNo", 			FIELD:"ISSU_NO" , 			COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.edt_issuNo}						// 발급번호
		, {FIELD_ID:"edt_issuPrtTo", 		FIELD:"ISSU_PRT_TO" , 		COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.edt_issuPrtTo}			// PRINT TO
		, {FIELD_ID:"edt_issuPrpos", 		FIELD:"ISSU_PRPOS" , 		COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.edt_issuPrpos}				// 발급용도
		, {FIELD_ID:"tar_issuEtc", 			FIELD:"ISSU_ETC" , 			COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.tar_issuEtc}					// 기타내역
		, {FIELD_ID:"cmb_issuOfc", 			FIELD:"ISSU_OFC" , 			COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.cmb_issuOfc}					// 발급주체
		, {FIELD_ID:"chk_issuSeal", 		FIELD:"ISSU_SEAL_YN" , 		COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.chk_issuSeal}				// 직인출력
		, {FIELD_ID:"edt_issuRgistRspner", 	FIELD:"ISSU_RGIST_RSPNER" , COMP:this.tab_master.tpg_issuForm.div_issuBaseForm.edt_issuRgistRspner}	// 등기의무자
	];
	
	var targetInfoLeng = targetInfo.length;
	
	// 대상 enable, readonly 체크
	var checkCompArr = function(targetId, type)
	{
		var result = false;
		
		var checkArrLeng = type == "ENABLE" ? enabledArr.length : (type == "READONLY" ? readonlyArr.length : 0);
		for(var j = 0; j < checkArrLeng; j++)
		{
			var checkId = type == "ENABLE" ? enabledArr[j] : (type == "READONLY" ? readonlyArr[j] : null);
			if(checkId == targetId){
				result = true;
				break;
			}
		}
	
		return result;
	}
	
	// 대상 안내글 체크
	var checkDescArr = function(targetId)
	{
		var descResult = "";
		
		var descArrLeng = descMapArr.length;
		for(var k = 0; k < descArrLeng; k++)
		{
			var descItem = descMapArr[k];
			var descId = descItem["FIELD_ID"];
			if(descId == targetId){
				descResult = descItem["FIELD_DESC"];
				break;
			}
		}
		
		return descResult;
	}
	
	// enable, readonly 설정
	for(var i = 0; i < targetInfoLeng; i++)
	{
		var targetItem = targetInfo[i];
		var targetId = targetItem["FIELD_ID"];
		var targetComp = targetItem["COMP"];
		var enabled = checkCompArr(targetId, "ENABLE");
		var readonly = checkCompArr(targetId, "READONLY");
		var fieldDesc = checkDescArr(targetId);
		var readonlyClass = readonly ? targetId.substring(0, targetId.indexOf("_")) + "_WF_Readonly" : "";
		
		// enable 설정
		targetComp.set_enable(enabled);
		
		// readonly 설정
		targetComp.set_readonly(readonly);
		targetComp.set_cssclass(readonlyClass);
		
		// displaynulltext 설정
		var hasNullTextProp = targetComp.hasOwnProperty("displaynulltext");
		var noFieldDesc = this.comUtils.isNullEmpty(fieldDesc);
		var fontColor = hasNullTextProp ? (noFieldDesc ? this.DEFAULT_COLOR : this.READONLY_COLOR) : this.DEFAULT_COLOR;
		if(hasNullTextProp)
		{
			targetComp.set_displaynulltext(fieldDesc);
		}
		
		// color 설정 - 오작동 방지를 위해 대상 지정
		if(targetId == "edt_issuPrpos" || targetId == "tar_issuEtc")
		{
			targetComp.style.set_color(fontColor);
		}
	}
}

