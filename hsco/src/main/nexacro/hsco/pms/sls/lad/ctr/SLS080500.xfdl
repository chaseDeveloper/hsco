/*
 * 함수명  	: fn_selectVaMastList
 * 설명 		: 선택 대상의 수납현황, 발급내역 목록 조회 이벤트 처리
 *                출력 대상 Dataset의 초기화 및 조회조건 값을 설정한다.
 * params  		: selRowPosition	: 선택행
 * return Type 	: None
 * 사용법		: this.fn_selectIssuDtlList(selRowPosition);
 */
this.fn_selectIssuDtlList = function(isReload, selRowPosition)
{
	var tranId = null;
	
	// 리로드 여부 확인 - 리로드 시 조회조건 값은 유지 - 민원서류 발급시 조회(발급이력)
	// 발금이력저장시 반환정보에 포함되므로 재 조회 필요없음.
	if(isReload){
		
		// Clear Datasets
		this.pmsUtil.clearDataSet([this.ds_issuFormatLogList]);	// 민원서류 발급내역 목록
	
		tranId = "selectIssuLogList";
	}else{
	
		// Clear Datasets
		// 납부내역/발급내역 조회 조건, 민원서류 발급내역 목록, 납부내역 목록, 민원서류양식목록, 기타(압류, 대출, 소유권)정보
		this.pmsUtil.clearDataSet([this.ds_cond01, this.ds_issuFormatLogList, this.ds_payInfoList, this.ds_issuFormatList, this.ds_etcInfo]);
		
		// 조회조건 설정
		// 대상목록 선택 값
		var selCntrctrItem = this.pmsUtil.getDatasetItem(this.ds_lcntrctrList, selRowPosition);
		var selLadCode = selCntrctrItem["LAD_CODE"];		// 토지코드
		var selCntrctrNo = selCntrctrItem["CNTRCTR_NO"];	// 계약자 번호
		var selCntrctTy = selCntrctrItem["CNTRCT_TY"];		// 계약타입(RN(임대),SL(분양))
		var selLttotRentSe = "S";	// 분양납부확인서를 위한 데이터 이므로 분양("S")로 고정. selCntrctTy == "SL" ? "S" : (selCntrctTy == "RN" ? "R" : null);
		
		if(selCntrctrNo == null){
			return;
		}

		var paramMap = {};
		// 민원서류, 발급이력 목록 조회용
		paramMap["JOB_SE"] = this.JOB_SE;					// 업무구분
		paramMap["JOB_KEY"] = selLadCode;					// 업무구분에 따른 코드 - 판매_토지 : 토지코드, 그 외 : 주택코드
		// 발급이력, 납부정보(분양납부확인서) 조회용
		paramMap["CNTRCTR_NO"] = selCntrctrNo;				// 토지계약자번호
		// 발급이력 조회용
		paramMap["FORMAT_SE"] = this.FORMAT_SE;				// 서식구분 - 보고서(01)/안내문(02)/문자(03)
		// 납부정보 조회(분양납부확인서)용
		paramMap["CNTRCT_TY"] = selCntrctTy;				// 계약타입(RN(임대),SL(분양))
		paramMap["LTTOT_RENT_SE"] = selLttotRentSe;			// 분양임화성분
		
		// 조회조건을 설정한다.
		this.pmsUtil.putMapToDataset(this.ds_cond01, paramMap, 0);
		
		tranId = "selectIssuDtlsAllList";
	}
	
	if(tranId == null){
		this.gfn_message(null, "요청 서비스 ID가 없습니다.");
		return;
	}
	
	// 선택 대상의 수납현황, 발급내역 목록을 조회한다.
	this.fn_transaction(tranId);
}

/*
 * 함수명  	: fn_selectReportContent
 * 설명 		: 선택 양식의 출력용 정보를 조회한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_selectReportContent();
 */
this.fn_selectReportContent = function()
{
	var issuFormatSize = this.ds_issuFormatList.getRowCount();

	if(issuFormatSize > 0)
	{
		if(this.ENABLED_REPORT_VIEW)
		{
			var formatCode = this.REPORT_TYPE;		// 레포트 구분(양식코드)
			
			var tranId = null;
			
			switch(formatCode)
			{
				case "A01" :	// A01 부동산거래계약신고서 조회
				case "C01" :	// C01 토지분양확인서 조회
				case "C02" :	// C02 사실확인서 조회
				case "C03" :	// C03 위임장(소유권이전)조회
				case "C04" :	// C04 환매특약말소등기신청 조회
				case "C05" :	// C05 위임장(환매권말소) 조회
				case "C06" :	// C06 해지증서(환매특약) 조회
				case "C07" :	// C07 추천서 조회
				case "C08" :	// C08 청구서 조회
				case "C09" :	// C09 토지사용승낙서 조회
				case "C10" :	// C10 공유자명부 조회
				case "C11" :	// C11 확인서 조회
				case "C12" :	// C12 계약체결결의서 조회
				
					tranId = "selectReport" + formatCode;
					break;
			
				default :
					break;
			}

			if(tranId == null){
				this.gfn_message(null, "요청 서비스 ID가 없습니다.");
				return;
			}else{
				this.pmsUtil.clearDataSet([this.ds_reportInfo, this.ds_reportSubList]);
			}

			// 선택 양식의 출력 내용을 조회한다.
			this.fn_transaction(tranId);
			
		}else{
			this.gfn_message("comm.항목.선택.요청", "민원서류 발급양식을");	// {0}을 선택하십시오.
		}
	}else{
		this.gfn_message(null, "조회된 민원서류 발급양식이 없습니다.");	// 선택된 데이터가 없습니다.
	}
}

/*
 * 함수명   	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	var sController = null;
	var oInDatasets = null;
	var sOutDatasets = null;
	var reqParam = null;
	switch (strSvcId) {
	
		// 토지계약자 발급이력 조회 - 계약자선택, 이력저장시 자동 처리되므로 조회할 필요없음.
		/*
		case "selectIssuLogList" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectIssuDtlsList.do";
			oInDatasets = "input1=ds_cond01";
			sOutDatasets = "ds_issuDtlsList=issuDtlsList";
			break;
		*/
		
		// 토지별 민원서류 목록 및  토지계약자 분양 납부정보, 발급이력 조회
		case "selectIssuDtlsAllList" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectIssuDtlsAllList.do";
			oInDatasets = "input1=ds_cond01";
			sOutDatasets = "ds_etcInfo=etcInfo ds_issuFormatList=issuFormatList ds_payInfoList=payInfoList ds_issuFormatLogList=issuFormatLogList";
			break;
			
		// [BEGIN] 보고서 내용 조회
		
		// A01 부동산거래계약신고서 조회
		case "selectReportA01" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportA01.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportA01Info ds_reportSubList=reportA01SubList";
			break;
			
		// C01 토지분양 확인서 조회
		case "selectReportC01" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC01.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC01Info";
			break;
			
		// C02 사실 확인서 조회
		case "selectReportC02" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC02.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC02Info";
			break;
			
		// C03 위임장(소유권이전)조회
		case "selectReportC03" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC03.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC03Info";
			break;
			
		// C04 환매특약말소등기신청 조회
		case "selectReportC04" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC04.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC04Info";
			break;
			
		// C05 위임장(환매권말소) 조회
		case "selectReportC05" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC05.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC05Info";
			break;
			
		// C06 해지증서(환매특약) 조회
		case "selectReportC06" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC06.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC06Info";
			break;
			
		// C07 추천서 조회
		case "selectReportC07" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC07.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC07Info ds_reportSubList=reportC07SubList";
			break;
			
		// C08 청구서 조회
		case "selectReportC08" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC08.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC08Info";
			break;
			
		// C09 토지사용승낙서 조회
		case "selectReportC09" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC09.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC09Info ds_reportSubList=reportC09SubList";
			break;
			
		// C10 공유자명부 조회
		case "selectReportC10" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC10.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC10List";
			break;
		
		// C11 확인서 조회
		case "selectReportC11" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC11.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC11Info";
			break;
			
		// C12 계약체결결의서 조회
		case "selectReportC12" :
			sController = "hsco/pms/sls/lad/ctr/SLS080500/selectReportC12.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_reportInfo=reportC12Info";
			break;
		
		// [ END ] 보고서 내용 조회
		
		default :
			break;
	}

	if(sController == null) return;
	
	// 서식 발급이력저장시 처리 - 이력목록을 받아 갱신할 수 있도록 한다.
	var isReportView = oInDatasets.indexOf("ds_cond02") > -1 ? true : false;	// ds_cond02 일때만
	if(isReportView)
	{
		var saveIssuLogYN = this.ds_cond02.getColumn(0, "ISSU_LOG_YN");
		var isSaveIssuLog = this.comUtils.isNullEmpty(saveIssuLogYN) ? false : (saveIssuLogYN == "Y" ? true : false);
		if(isSaveIssuLog){
			sOutDatasets += " ds_issuFormatLogList=issuFormatLogList";
		}
	}
	
	var sInDataType = typeof(oInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, oInDatasets, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, oInDatasets, sOutDatasets, null, null, null, bSync);
	}
}

/**********************************************************************
 * Transaction Callback 영역
 **********************************************************************/ 

/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess)
	{
		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId)
		{
			// 공통코드 목록 조회
			case "selectCommonCode" :

				// 조회영역
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				
				// 토지구분
				targetCombo = this.div_search.cmb_remndrLadSe;
				this.pmsUtil.setComboPrompt(targetCombo, comboOption, true);
				
				// 용도구분
				targetCombo = this.div_search.cmb_proposSeCode;
				this.pmsUtil.setComboPrompt(targetCombo, comboOption, true);
				
				// 계약구분
				targetCombo = this.div_search.cmb_ladSe;
				this.pmsUtil.setComboPrompt(targetCombo, comboOption, true);
				
				// 계약타입
				targetCombo = this.div_search.cmb_cntrctSttusCode;
				this.pmsUtil.setComboPrompt(targetCombo, comboOption, true);
				
				// 공급구분
				targetCombo = this.div_search.cmb_suplyCode;
				this.pmsUtil.setComboPrompt(targetCombo, comboOption, true);
				
				break;
				
			// 토지코드 목록 조회
			case "ladCode" :
			
				// 조회영역 - 토지코드
				targetCombo = this.div_search.cmb_ladCode;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_SELECT;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption, true);
				break;
				
			// 계약(해약) 목록 조회
			case "lcntrctrList" :
				//this.ds_lcntrctrList.set_rowposition(-1);	// 선택 해제
				break;
				
			// 토지계약자 발급이력 조회 - 이력저장시 자동 처리되므로 조회할 필요없음.
			//case "selectIssuLogList" :
			// 토지계약자 납부정보 및 발급이력 조회
			case "selectIssuDtlsAllList" :
				// 민원서류목록 선택 해제
				this.ds_issuFormatList.set_rowposition(-1);
				
				this.fn_setDetailInfoView(true);	// 상세정보영역 활성화 처리
				
				break;
				
			// [BEGIN] 보고서 내용 조회
			
			case "selectReportA01" :	// A01 부동산거래계약신고서 조회
			case "selectReportC01" :	// C01 토지분양 확인서 조회
			case "selectReportC02" :	// C02 사실 확인서 조회
			case "selectReportC03" :	// C03 위임장(소유권이전) 조회
			case "selectReportC04" :	// C04 환매특약말소등기신청 조회
			case "selectReportC05" :	// C05 위임장(환매권말소) 조회
			case "selectReportC06" :	// C06 해지증서(환매특약) 조회
			case "selectReportC07" :	// C07 추천서 조회
			case "selectReportC08" :	// C08 청구서 조회
			case "selectReportC09" :	// C09 토지사용승낙서 조회
			case "selectReportC10" :	// C10 공유자명부 조회
			case "selectReportC11" :	// C11 확인서 조회
			case "selectReportC12" :	// C11 계약체결결의서 조회
			
				this.fn_popupReportView();		// 레포트 팝업 호출
				
				break;
				
			// [ END ] 보고서 내용 조회
				
			default :
				break;
		}
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}

