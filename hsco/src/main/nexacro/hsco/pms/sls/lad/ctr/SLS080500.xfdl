// 레포트 별도 출력조건 확인
this.fn_checkEnableReportView = function(selFormatCode)
{
	var result = true;

	switch(selFormatCode)
	{
		case "C03" :	// 위임장(소유권이전)
			
			// 기타정보(압류, 대출, 소유권)중 압류, 대출정보 확인 > 압류 대출이 있는지 확인( 해지 일자가 없으면 해약이 안됨)
			var cditDecsnDe = this.ds_etcInfo.getColumn(0, "CDIT_DECSN_DE");	// 채권결정일
			var trnsfrDe = this.ds_etcInfo.getColumn(0, "TRNSFR_DE");			// 양도일자
			
			var isNoCditDecsnDe = this.comUtils.isNullEmpty(cditDecsnDe);	// 채권결정일 유무
			var isNoTrnsfrDe = this.comUtils.isNullEmpty(trnsfrDe);			// 양도일자 유무
			
			if(isNoCditDecsnDe && isNoTrnsfrDe)
			{
				result = true;
			}else{
				result = false;
			}
			break;
			
		case "C08" :	// 청구서(정산)
		
			// 기타정보중 정산정보 확인 > 정산정보가 없으면 내역 없음.
			var payamtSeCnt = this.ds_etcInfo.getColumn(0, "PAYAMT_SE_CNT");	// 정산횟수
			var hasPayamtSe = Number(payamtSeCnt) > 0 ? true : false;
			
			result = hasPayamtSe;
			break;
			
		default :
			break;
	}
	
	return result;
}

