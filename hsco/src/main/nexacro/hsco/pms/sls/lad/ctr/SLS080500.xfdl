// 출력대상 데이터 존재 유무 확인
this.fn_checkHasRptResult = function(targetDs)
{
	var result = true;
	
	var targetSize = targetDs.rowcount;
	if(targetSize < 1)
	{
		this.gfn_message("comm.출력.데이터.없음");	// 출력할 데이터가 없습니다.
		result = false;
	}
	
	return result;
}

// 출력 레포트 전달 Arguments 설정 
this.fn_getReportArguments = function(keyArr)
{
	var  result = {};
	  
	var keyLeng = keyArr ? keyArr.length : 0;
	
	if(keyLeng > 0)
	{
		// 공통적으로 직인 정보 설정
		keyArr.push("ISSU_SEAL_YN");
		keyLeng = keyArr.length;
		
		var inputFormMap = this.pmsUtil.getDatasetItem(this.ds_cond02, 0);
		
		for(var i = 0; i < keyLeng; i++)
		{
			var sKey = keyArr[i];

			var inValue = null;
			var setValue = "";
			switch(sKey)
			{
				case "ISSU_DT_Y" :		// 발급일자_년
					var inValue = inputFormMap["ISSU_DT"];	// 발급일자
					setValue = this.comUtils.isNullEmpty(inValue) ? "" : inValue.substring(0,4);	// 발급일자_년도
					break;
				
				case "ISSU_OFC_NM" :	// 발급주체 선택 명
					var selIssuOfcItem = this.pmsUtil.getComboSelectedItem(this.tab_master.tpg_issuForm.div_issuBaseForm.cmb_issuOfc);	// 발급주체 선택값
					setValue = selIssuOfcItem["CODE_NM"];		// 발급주체
					break;
					
				default :
					var hasProp = inputFormMap.hasOwnProperty(sKey);
					if(!hasProp){
						continue;
					}
					
					setValue = inputFormMap[sKey];
					break;
			}
			
			result[sKey] = this.comUtils.isNullEmpty(setValue) ? "" : setValue;
		}
	}
	
	return result;
}

