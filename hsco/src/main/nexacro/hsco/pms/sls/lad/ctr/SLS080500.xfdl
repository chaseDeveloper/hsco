this.fn_setIssuBaseDs = function(enabled, addParam)
{
	// 데이터셋 클리어
	this.pmsUtil.clearDataSet([this.ds_cond02]);
	this.ds_cond02.setColumn(0, "ISSU_LOG_YN" , 'Y');	
	
	// 상세정보 활성화시 설정
	if(enabled)
	{
		// 기본입력값 설정
		
		// 선택 토지계약자 정보
		var selLcntrctrInfo = this.pmsUtil.getGridSelectedItem(this.grd_lcntrctrList);
		var selLadCode = selLcntrctrInfo["LAD_CODE"];	// 선택 토지코드
		
		// 선택 양식정보
		var selIssuFormatPos = this.ds_issuFormatList.rowposition;
		
		var baseParam = {};
		
		if(selIssuFormatPos > -1)
		{
			var menuid = application.gv_curFormId.split("_")[1];
			var rowMenu = application.gds_menu.findRow("MENU_ID", menuid);
			var pageid = application.gds_menu.getColumn(rowMenu, "PAGE_ID");				
			
			// 선택 양식정보
			var selIssuFormatInfo = this.pmsUtil.getGridSelectedItem(this.tab_master.tpg_issuForm.grd_issueFormat);
		
			// 계약자정보 설정
			baseParam["CNTRCTR_NO"] 	= selLcntrctrInfo["CNTRCTR_NO"];
			baseParam["LAD_CODE"] 		= selLcntrctrInfo["LAD_CODE"];
			baseParam["CNTRCT_TY"] 		= selLcntrctrInfo["CNTRCT_TY"];
			baseParam["LTTOT_RENT_SE"] 	= "S";
			
			// 양식정보 설정
			baseParam["JOB_SE"] 		= selIssuFormatInfo["JOB_SE"];
			baseParam["FORMAT_SE"] 		= selIssuFormatInfo["FORMAT_SE"];
			baseParam["FORMAT_CODE"] 	= selIssuFormatInfo["FORMAT_CODE"];
			baseParam["FORMAT_ODR"] 	= selIssuFormatInfo["FORMAT_ODR"];
			baseParam["FORMAT_SN"] 		= selIssuFormatInfo["FORMAT_SN"];
			
			baseParam["ISSU_TY"] 	    = "01";					           // 발급방법
			baseParam["ISSUER"] 	    = selLcntrctrInfo["CSTMR_NM"];     // 신청자
			baseParam["ISSU_COURS"]     = pageid;                          // 페이지ID
			baseParam["ZIP_CODE"]       = selLcntrctrInfo["ZIP"];          // 우편번호
			baseParam["ADRES"]          = selLcntrctrInfo["ADRES"];        // 주소
			baseParam["ADRES_DETAIL"]   = selLcntrctrInfo["ADRES_DETAIL"]; // 상세주소			
			
		
			// 발급번호 PRT_TO
			var baseIssuPrtTo = null;
			if(selLadCode == "020" || selLadCode == "021" || selLadCode == "022")
			{
				baseIssuPrtTo = "동구청장";
			}else{
				baseIssuPrtTo = "화성광역시달성군수";
			}
			
			// 기본
			baseParam["ISSU_DT"] = this.DEFAULT_DATE;	// 발급일자
			baseParam["ISSU_PRT_TO"] = baseIssuPrtTo;	// 발급번호 PRT_TO
			
			// 추가(C08용)
			baseParam["EXCCLC_AMOUNT"] = this.ds_etcInfo.getColumn(0, "PAYAMT_SE");			// 정산금
			baseParam["EXCCLC_AMOUNT_INTR"] = this.ds_etcInfo.getColumn(0, "PAYAMT_SE");	// 정산금 + 예금이자 금액
			baseParam["DPST_INTR"] = 0;														// 예금이자 금액
			
			// 추가(C09용)
			baseParam["USE_PRARN_DE"] = this.DEFAULT_DATE;	// 토지사용예정일
			
			// 추가(각 양식선택 전달값)
			var noAddParam = this.comUtils.isNullEmpty(addParam);
			if(!noAddParam)
			{
				for(var sKey in addParam)
				{
					baseParam[sKey] = addParam[sKey];
				}
			}
			
			this.pmsUtil.putMapToDataset(this.ds_cond02, baseParam);	// 데이터 입력처리
			
			//this.dumper(this.ds_cond02);
			
		}
	}
}

