    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : SLS080401
 * 화면명   : 압류/대출정보
 * 화면설명 : 압류/대출정보를 관리한다.
 * 작성일   : 2015. 10. 03
 * 작성자   : 변승우
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    2015.00.00   홍길동   fn_xxx() 보완 수정
 ***********************************************************************
 */


/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "pms_lib::pmsUtilInclude.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
// 화면 전역에서 사용할 Form 변수를 선언한다.
this.SYS_SE = "PMS";						// 시스템 구분
this.JOB_SE = "SLS";						// 업무 구분
this.FILE_MANAGER = null;					// 파일 업/다운로더

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SLS080401_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	//this.fn_init();             // 공통 폼 초기화
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화	
}

this.SLS080401_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();		// Combo에 사용되는 Dataset Load
	
	// 파일 업/다운로더 설정
	var fileConfig = {
		onChange: this.fn_onFileSelected,
		onSuccess : this.fn_onFileSuccess,
		onError : this.fn_onFileError
	};
	this.FILE_MANAGER = new this.fileManager(this, this.SYS_SE, this.JOB_SE);
	this.FILE_MANAGER.init(fileConfig);
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*=========================================
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 *=========================================*/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}

/*==========================================================================================
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
*==========================================================================================*/
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);	// 조건에 사용되는 Dataset 초기화
	
	//조회, 종료시에 수정여부 체크할 데이터셋 지정
	this.checkDs.push(this.ds_lcntrctrSeizrList);
	this.checkDs.push(this.ds_lcntrctrLonList);
}

/*===================================================
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
*===================================================*/
this.fn_loadCombo = function()
{
	// 공통코드 목록 조회 - 토지구분, 용도구분, 급지구분, 목차구분
 	var comCodeParam = [
		  {OUT_PUT:"ds_cntrctSttusCodeSh", GRP_CODE:"LAD0A4", USE_AT:"1", PARAM_NAME:"CHRCTR_PREPAR_VALUE_0", PARAM_VALUE:"V"} // 계약타입
 		, {OUT_PUT:"ds_cntrctSeCodeSh",    GRP_CODE:"LAD0A3", USE_AT:"1"} // 계약구분(토지상태)
 		, {OUT_PUT:"ds_prposSeCodeSh",     GRP_CODE:"LAD003", USE_AT:"1"} // 용도구분
 		, {OUT_PUT:"ds_suplyCodeSh",       GRP_CODE:"LAD004", USE_AT:"1"} // 공급구분
 		, {OUT_PUT:"ds_ladSeCodeSh",       GRP_CODE:"LAD0A2", USE_AT:"1"} // 토지구분
 		, {OUT_PUT:"ds_seizrSeCode",       GRP_CODE:"LAD006", USE_AT:"1"} // 토지압류구분
 		, {OUT_PUT:"ds_lonSeCode",     	   GRP_CODE:"LAD0A5", USE_AT:"1"} // 대출구분
 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
	
	// 토지 코드 목록 조회
	var ladCodeParam = {};
	ladCodeParam["UASE_AT"] = "1";
	this.pmsUtil.getLadCodeList(ladCodeParam, "ds_ladCodeSh");
}


/*==========================================
 * 함수명  	    : fn_search
 * 설명 		: 계약목록 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
*=========================================*/
this.fn_search = function() 
{
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}
	
	// Clear Datasets
	this.pmsUtil.clearDataSet([this.ds_ladLcntrctrList]);
	
    // 계약목록 조회
    this.fn_transaction("selectLadLcntrctrList");
}

this.div_search_edt_nm_onkeyup = function(obj:Edit, e:nexacro.KeyEventInfo)
{

	if(e.keycode == 13) {
		this.fn_search();
	}
}

/*=======================================
 * 함수명  	    : fn_insert
 * 설명 		: 토지명의변경 신규등록
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 *======================================*/
this.fn_insert = function()
{
    
    var nRow = this.ds_ladLcntrctrList.rowposition;  //계약자 리스트 선택된 로우    
    if(nRow < 0){    	
    	this.alert("선택된 토지계약자 정보가 없습니다.");
        return false; 
    }

	var cntrctrNo = this.ds_ladLcntrctrList.getColumn(nRow, "CNTRCTR_NO");
	
	if (this.comUtils.isNotNull(cntrctrNo)) {
		
		if (this.div_detail02.Tab00.tabindex == 0) {
		
			// 토지계약자압류 추가
			var nAddRow = this.ds_lcntrctrSeizrList.addRow();
			this.ds_lcntrctrSeizrList.setColumn(nAddRow, "CNTRCTR_NO", cntrctrNo);
			this.div_detail02.Tab00.tabpage1.cmb_seizrSe.set_index(0);
		} 
		else if (this.div_detail02.Tab00.tabindex == 1) {
		
			// 토지계약자대출 추가
			var nAddRow = this.ds_lcntrctrLonList.addRow();
			this.ds_lcntrctrLonList.setColumn(nAddRow, "CNTRCTR_NO", cntrctrNo);
			this.ds_lcntrctrLonList.setColumn(nAddRow, "TRNSFR_DE", this.gfn_today());//접수일자
			this.div_detail02.Tab00.tabpage2.cmb_lonSe.set_index(0);			
		}
	}
    
}


//저장
this.fn_save = function()
{
	
	if (this.div_detail02.Tab00.tabindex == 0) {
		
		// 파일 첨부관련 체크 및 설정
		// 파일일련번호 채번 후 미첨부 항목 체크 및 수정
		this.fn_beforeCheckAttchFile();
		
		// 토지계약자압류
		if(!this.gfn_checkValidation(this.ds_lcntrctrSeizrList, this.ds_validation)) {
			return;
		}
		if(this.confirm("저장하시겠습니까?")){
		
			// 파일 첨부여부 확인
			var hasAttFile = this.FILE_MANAGER.hasFile();
			//alert("hasAttFile = " + hasAttFile);
			if(hasAttFile){
				var attFileCount = this.FILE_MANAGER.getfileCount();

				var sController   	= "hsco/pms/sls/lad/ctr/SLS080401/saveLcntrctrSeizrList.do";
				var sInDatasets   	= "input1=ds_lcntrctrSeizrList:U";
				var sOutDatasets  	= "";
							
				this.FILE_MANAGER.upload(sController, sInDatasets, sOutDatasets);
			}
			else{
				this.fn_transaction("saveLcntrctrSeizrList")  ;  //토지계약자압류 리스트 저장
			}			
		}
	} 
	else if (this.div_detail02.Tab00.tabindex == 1) {
	
		// 파일 첨부관련 체크 및 설정
		// 파일일련번호 채번 후 미첨부 항목 체크 및 수정
		this.fn_beforeCheckAttchFile();
		
		// 토지계약자대출
		if(!this.gfn_checkValidation(this.ds_lcntrctrLonList, this.ds_validation)) {
			return;
		}
		if(this.confirm("저장하시겠습니까?")){
		
			// 파일 첨부여부 확인
			var hasAttFile = this.FILE_MANAGER.hasFile();
			//alert("hasAttFile = " + hasAttFile);
			if(hasAttFile){
				var attFileCount = this.FILE_MANAGER.getfileCount();

				var sController   	= "hsco/pms/sls/lad/ctr/SLS080401/saveLcntrctrLonList.do";
				var sInDatasets   	= "input1=ds_lcntrctrLonList:U";
				var sOutDatasets  	= "";
							
				this.FILE_MANAGER.upload(sController, sInDatasets, sOutDatasets);
			}
			else{
				this.fn_transaction("saveLcntrctrLonList")  ;  //토지계약자대출 리스트 저장
			}			
		}
	}

}

/**
 * 저장전 파일첨부가 정상적으로 이루어 졌는지 확인
 * 파일일련번호를 채번하였으나 선택된 파일이 없는경우 파일 일련번호 초기화 처리
 */
this.fn_beforeCheckAttchFile = function(){
	
	if (this.div_detail02.Tab00.tabindex == 0) {
		var ORGINL_FILE_NM = this.ds_lcntrctrSeizrList.getColumn(this.ds_lcntrctrSeizrList.rowposition, "ORGINL_FILE_NM");
		
		if(this.comUtils.isNullEmpty(ORGINL_FILE_NM)){		
			this.ds_lcntrctrSeizrList.setColumn(this.ds_lcntrctrSeizrList.rowposition, "SEIZR_ATCHMNFL", "");
		}
	}
	else if (this.div_detail02.Tab00.tabindex == 1) {
		var ORGINL_FILE_NM = this.ds_lcntrctrLonList.getColumn(this.ds_lcntrctrLonList.rowposition, "ORGINL_FILE_NM");
		
		if(this.comUtils.isNullEmpty(ORGINL_FILE_NM)){		
			this.ds_lcntrctrLonList.setColumn(this.ds_lcntrctrLonList.rowposition, "SEIZR_ATCHMNFL", "");
		}
	}
}


//삭제 
