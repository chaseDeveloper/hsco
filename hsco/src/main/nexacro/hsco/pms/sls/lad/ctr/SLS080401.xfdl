/*
 * 함수명  	: fn_getNewFileSn
 * 설명 		: 파일 첨부용 파일일련번호(FILE_SN) 채번
 * params  		: null 	: 
 * return Type 	: None
 * 사용법		: this.fn_getNewFileSn()
 */
this.fn_getNewFileSn = function(){

	if (this.div_detail02.Tab00.tabindex == 0) {//압류
		var fileSn = this.ds_lcntrctrSeizrList.getColumn(this.ds_lcntrctrSeizrList.rowposition,"SEIZR_ATCHMNFL");
		if(this.comUtils.isNullEmpty(fileSn)){
			// 대상 DataSet 초기화
			this.pmsUtil.clearDataSet([this.ds_fileSn]);
			this.div_detail02.Tab00.tabpage1.btn_atchFile.set_enable(false);
				
			this.fn_transaction("getFileSn");
		}
	}
	else if (this.div_detail02.Tab00.tabindex == 1) {//대출
		var fileSn = this.ds_lcntrctrLonList.getColumn(this.ds_lcntrctrLonList.rowposition,"LON_ATCHMNFL");
		if(this.comUtils.isNullEmpty(fileSn)){
			// 대상 DataSet 초기화
			this.pmsUtil.clearDataSet([this.ds_fileSn]);
			this.div_detail02.Tab00.tabpage2.btn_atchFile.set_enable(false);
			
			this.fn_transaction("getFileSn");
		}
	}
	
}

// 파일일련번호 설정
this.fn_setFileSnInfo = function(addFile){

	// 파일 일련번호 존재 확인
	var fileSn = this.ds_fileSn.getColumn(this.ds_fileSn.rowposition, "FILE_SN");	
	if(this.comUtils.isNullEmpty(fileSn)){
		this.alert(null, "파일일련번호가 없습니다.");
		return;
	}
	
	// 상세 정보에 추가
	if (this.div_detail02.Tab00.tabindex == 0) {//압류
		this.ds_lcntrctrSeizrList.setColumn(this.ds_lcntrctrSeizrList.rowposition,"SEIZR_ATCHMNFL", fileSn);	
		if(addFile){
			this.div_detail02.Tab00.tabpage1.btn_atchFile.set_enable(true);		
		}
	}
	else if (this.div_detail02.Tab00.tabindex == 1) {//대출
		this.ds_lcntrctrLonList.setColumn(this.ds_lcntrctrLonList.rowposition,"LON_ATCHMNFL", fileSn);	
		if(addFile){
			this.div_detail02.Tab00.tabpage2.btn_atchFile.set_enable(true);		
		}		
	}
	
	this.fn_addAtchFile(false);//단건
}

/**
 * 함수명  		: fn_onFileSelected 이벤트 핸들러
 * 설명 		: 업로드 파일선택 첨부파일선택시 콜백 함수
 * params  		: fileFullName 선택 전체 파일명, type 실행구분(ExtFileUpload/ExtFileDownload)
 * return Type 	: None
 * 사용법		: this.fn_onFileSelected();
 */
this.fn_onFileSelected = function(type, fileFullName) {
//alert("type = " + type +"    fileFullName = " + fileFullName );
	if(type == this.FILE_MANAGER.TYPE_UPLOADER) {
		var isNmArray = this.comUtils.isArray(fileFullName);
		
		if(isNmArray) {
			// TODO - 본 화면은 단건 처리이므로 해당 없음.				
		} 
		else{
			if(typeof(fileFullName) == "boolean" && !fileFullName){
				// 선택파일이 조건에 맞지않아 제외되어 건수가 없을 경우
				// 선택행의 FILE_SN을 삭제한다.				
				if (this.div_detail02.Tab00.tabindex == 0) {//압류
					this.ds_lcntrctrSeizrList.setColumn(this.ds_lcntrctrSeizrList.rowposition, "SEIZR_ATCHMNFL", "");	// 첨부된 파일 데이터 셋 입력 취소 처리
				}
				else if (this.div_detail02.Tab00.tabindex == 1) {//대출
					this.ds_lcntrctrLonList.setColumn(this.ds_lcntrctrLonList.rowposition, "LON_ATCHMNFL", "");	// 첨부된 파일 데이터 셋 입력 취소 처리			
				}				
			}
			else{
							
				if (this.div_detail02.Tab00.tabindex == 0) {//압류
					this.ds_lcntrctrSeizrList.setColumn(this.ds_lcntrctrSeizrList.rowposition, "ORGINL_FILE_NM", fileFullName);	// 첨부된 파일 데이터 셋 입력 처리
				}
				else if (this.div_detail02.Tab00.tabindex == 1) {//대출
					this.ds_lcntrctrLonList.setColumn(this.ds_lcntrctrLonList.rowposition, "ORGINL_FILE_NM", fileFullName);	// 첨부된 파일 데이터 셋 입력 처리			
				}
				
				this.fn_setAtchFileView();		// 첨부파일 영역 처리
			}

		}
	}
}


/**
 * 함수명  		: fn_downloadAtchFile
 * 설명 		: 첨부파일 보기(다운로드)
 * params  		: null
 * return Type 	: None
 * 사용법		: this.fn_downloadAtchFile();
 */
this.fn_downloadAtchFile = function(){
	
	if (this.div_detail02.Tab00.tabindex == 0) {//압류
		var fileSn = this.ds_lcntrctrSeizrList.getColumn(this.ds_lcntrctrSeizrList.rowposition, "SEIZR_ATCHMNFL");			// 파일일련번호(파일경로)
		var fileOrder = this.ds_lcntrctrSeizrList.getColumn(this.ds_lcntrctrSeizrList.rowposition, "SEIZR_ATCHMNFL_COURS");
		var fileName = this.ds_lcntrctrSeizrList.getColumn(this.ds_lcntrctrSeizrList.rowposition, "ORGINL_FILE_NM");	// 실제 파일명
	}
	else if (this.div_detail02.Tab00.tabindex == 1) {//대출
		var fileSn = this.ds_lcntrctrLonList.getColumn(this.ds_lcntrctrLonList.rowposition, "LON_ATCHMNFL");			// 파일일련번호(파일경로)
		var fileOrder = this.ds_lcntrctrLonList.getColumn(this.ds_lcntrctrLonList.rowposition, "LON_ATCHMNFL_COURS");
		var fileName = this.ds_lcntrctrLonList.getColumn(this.ds_lcntrctrLonList.rowposition, "ORGINL_FILE_NM");	// 실제 파일명
	}
	
	this.FILE_MANAGER.download(fileSn, fileOrder, fileName);
}

/**
 * 파일업로드 선택 파일 취소
 * @param 
 * @return void
 */
this.fn_cancelAtchFile = function(){
	
	if (this.div_detail02.Tab00.tabindex == 0) {//압류
		var nCurRow = this.ds_lcntrctrSeizrList.rowposition;	
		var useAddFileSn = this.FILE_MANAGER.USE_ADD_FILE_SN;
		var cancelField = useAddFileSn ? "SEIZR_ATCHMNFL" : "ORGINL_FILE_NM";
		var selFileNm = this.ds_lcntrctrSeizrList.getColumn(nCurRow, cancelField);
		var result = this.FILE_MANAGER.cancelFiles(selFileNm);
	
		if(result){
			this.ds_lcntrctrSeizrList.setColumn(nCurRow, "ORGINL_FILE_NM", "");		// 표기용 파일명 정보 초기화
			
			if(useAddFileSn){
				this.ds_lcntrctrSeizrList.setColumn(nCurRow, "SEIZR_ATCHMNFL", "");	// 처리용 파일일련번호 정보 초기화
			}
		
			this.fn_setAtchFileView();		// 첨부파일 영역 처리
		}
	}
	else if (this.div_detail02.Tab00.tabindex == 1) {//대출
		var nCurRow = this.ds_lcntrctrLonList.rowposition;	
		var useAddFileSn = this.FILE_MANAGER.USE_ADD_FILE_SN;
		var cancelField = useAddFileSn ? "LON_ATCHMNFL" : "ORGINL_FILE_NM";
		var selFileNm = this.ds_lcntrctrLonList.getColumn(nCurRow, cancelField);
		var result = this.FILE_MANAGER.cancelFiles(selFileNm);
	
		if(result){
			this.ds_lcntrctrLonList.setColumn(nCurRow, "ORGINL_FILE_NM", "");		// 표기용 파일명 정보 초기화
			
			if(useAddFileSn){
				this.ds_lcntrctrLonList.setColumn(nCurRow, "LON_ATCHMNFL", "");	// 처리용 파일일련번호 정보 초기화
			}
		
			this.fn_setAtchFileView();		// 첨부파일 영역 처리
		}
	}
}

/**
 * 함수명  	: fn_deleteAtchFile
 * 설명 		: 첨부파일 삭제
 * params  		: null
 * return Type 	: None
 * 사용법		: this.fn_deleteAtchFile();
 */
this.fn_deleteAtchFile = function(){

	var confirmMsg = "첨부파일을 삭제하시겠습니까?";
	if(confirm(confirmMsg)){
	
		if (this.div_detail02.Tab00.tabindex == 0) {//압류
			var nCurRow = this.ds_lcntrctrSeizrList.rowposition;
			this.ds_lcntrctrSeizrList.setColumn(nCurRow, "SEIZR_ATCHMNFL_COURS", "");
			this.ds_lcntrctrSeizrList.setColumn(nCurRow, "SEIZR_ATCHMNFL", "");
		}
		else if (this.div_detail02.Tab00.tabindex == 1) {//대출
			var nCurRow = this.ds_lcntrctrLonList.rowposition;
			this.ds_lcntrctrLonList.setColumn(nCurRow, "LON_ATCHMNFL_COURS", "");
			this.ds_lcntrctrLonList.setColumn(nCurRow, "LON_ATCHMNFL", "");
		}	

		this.fn_transaction("deleteFileInfo");		// 토지 상세 첨부파일 삭제
	}
}

