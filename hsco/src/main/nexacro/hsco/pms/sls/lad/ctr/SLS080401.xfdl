// 체크클릭 : Grid의 Head의 체크 버튼을 클릭하였을 때, 리스트의 체크버튼 처리
this.fn_grdOnHeadClickHandler = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	if(e.col == 0) {
		this.gfn_setGridCheckAll(obj, e);
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
 /*=====================================================
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 *=====================================================*/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	
	    // 토지계약목록 조회
		case "selectLadLcntrctrList" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080401/selectLadLcntrctrList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_ladLcntrctrList=output1";	
			break;
			
		// 토지계약자압류 조회
		case "selectLcntrctrSeizrList": 
 		    var sController   	= "hsco/pms/sls/lad/ctr/SLS080401/selectLcntrctrSeizrList.do";
	        var sInDatasets   	= "input1=ds_cond01";
	        var sOutDatasets  	= "ds_lcntrctrSeizrList=output1";
		    break;   
			
	    // 토지계약자대출 조회
		case "selectLcntrctrLonList" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080401/selectLcntrctrLonList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_lcntrctrLonList=output1";
			break;
			
	    // 토지계약자압류 저장
		case "saveLcntrctrSeizrList" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080401/saveLcntrctrSeizrList.do";
			var sInDatasets   	= "input1=ds_lcntrctrSeizrList:U";
			var sOutDatasets  	= "";
			break;
			
		// 토지계약자대출 저장
		case "saveLcntrctrLonList" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS080401/saveLcntrctrLonList.do";
			var sInDatasets   	= "input1=ds_lcntrctrLonList:U";
			var sOutDatasets  	= "";
			break;
			
		// 파일 일련번호 조회
		case "getFileSn" :
			var sController   	= "hsco/cmm/file/getFileSn.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fileSn=output_fsn";
			break;
			
		// 파일정보 삭제
		case "deleteFileInfo" :			
			var sController 	= "hsco/pms/sls/lad/ctr/SLS080401/deleteFileInfo.do";			
			if (this.div_detail02.Tab00.tabindex == 0) {
				var sInDatasets 	= "input1=ds_lcntrctrSeizrList:U";
			}
			else if (this.div_detail02.Tab00.tabindex == 1) {
				var sInDatasets 	= "input1=ds_lcntrctrLonList:U";				
			}			
			var sOutDatasets 	= "";
			break;

		default :
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/




/*============================================================================================
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 *============================================================================================*/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var targetCombo = null;
		
		switch(strSvcId){
		
			// 공통코드 목록 조회
			case "selectCommonCode" :

				// 조회영역 - 토지구분
				targetCombo = this.div_search.cmb_ladSeCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";

				this.pmsUtil.setComboPrompt(targetCombo, comboOption);						

				// 조회영역 - 용도구분
				targetCombo = this.div_search.cmb_prposSeCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
								
				// 조회영역 - 계약구분
				targetCombo = this.div_search.cmb_cntrctSeCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";

				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				
				// 조회영역 - 계약타입
				targetCombo = this.div_search.cmb_cntrctSttusCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";

				this.pmsUtil.setComboPrompt(targetCombo, comboOption);

				// 조회영역 - 공급구분				
				targetCombo = this.div_search.cmb_suplyCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				
				// 상세영역 - 압류구분				
				targetCombo = this.div_detail02.Tab00.tabpage1.cmb_seizrSe;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_SELECT;
				comboOption["noNullText"] = "코드없음";			
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				this.div_detail02.Tab00.tabpage1.cmb_seizrSe.set_index(0);
				
				// 상세영역 - 대출구분
				targetCombo = this.div_detail02.Tab00.tabpage2.cmb_lonSe;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_SELECT;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);				
				this.div_detail02.Tab00.tabpage2.cmb_lonSe.set_index(0);
				
				break;				
				
			
			// 토지코드 목록 조회영역
			case "ladCodeSh" :
				targetCombo = this.div_search.cmb_ladCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_SELECT;
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);

				break;
				
			// 토지계약목록 조회
			case "selectLadLcntrctrList" :
				if(this.ds_ladLcntrctrList.getRowCount() > 0){
					this.div_detail01.btn_ladInfo.set_enable(true);
				}
				else{
					this.alert("조회된 데이타가 없습니다.");
				}
				break;		
					
				
			// 토지계약자압류 조회
			case "selectLcntrctrSeizrList": 
				
				break;   
				
			// 토지계약자대출 조회
			case "selectLcntrctrLonList" :
				
				break;
				
			// 토지계약자압류 저장
			case "saveLcntrctrSeizrList" :
				this.fn_transaction("selectLadLcntrctrList"); // 계약목록 조회
				break;
				
			// 토지계약자대출 저장
			case "saveLcntrctrLonList" :
				this.fn_transaction("selectLadLcntrctrList"); // 계약목록 조회
				break;
				
			// 파일일련번호 조회
			case "getFileSn" :
				//alert("this.ds_fileSn.getRowCount() = " + this.ds_fileSn.getRowCount());			
				// 채번 파일일련번호 설정
				this.fn_setFileSnInfo(true);
				break;

			// 파일정보 삭제
			case "deleteFileInfo" :
				this.alert("첨부파일이 삭제되었습니다.");
				this.ds_ladLcntrctrList_onrowposchanged();
				
				break;
				
			
		}
	}
	else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}

