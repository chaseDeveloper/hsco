// 토지계약목록 변경시 토지계약자압류/대출 조회
this.ds_ladLcntrctrList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(this.ds_ladLcntrctrList.rowcount > 0){
				
		//토지계약자압류/대출 조회
		this.div_detail02_Tab00_onchanged();
		
	}
}

//변경데이타 체크
this.div_detail02_Tab00_canchange = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{	
	
	if (this.div_detail02.Tab00.tabindex == 0) {
		// 토지계약자압류 조회
		if(Ex.util.isUpdated(this.ds_lcntrctrSeizrList) == true){
			if( this.confirm("변경된 데이타가 있습니다. 이동하시겠습니까?") ){
				return true;
			}
			else{
				return false;
			}
		}
	} 
	else if (this.div_detail02.Tab00.tabindex == 1) {	
		// 토지계약자대출 조회
		if(Ex.util.isUpdated(this.ds_lcntrctrLonList) == true){
			if( this.confirm("변경된 데이타가 있습니다. 이동하시겠습니까?") ){
				return true;
			}
			else{
				return false;
			}
		}		
	}
}

// 탭 조회
this.div_detail02_Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{

	var cntrctrNo = this.ds_ladLcntrctrList.getColumn(this.ds_ladLcntrctrList.rowposition, "CNTRCTR_NO");	
	
	if (this.comUtils.isNotNull(cntrctrNo)) {
		this.ds_cond01.setColumn(0, "CNTRCTR_NO", cntrctrNo);
		
		if (this.div_detail02.Tab00.tabindex == 0) {
		
			// 토지계약자압류 조회
			this.ds_lcntrctrSeizrList.clearData();				
			this.fn_transaction("selectLcntrctrSeizrList");
			
		} 
		else if (this.div_detail02.Tab00.tabindex == 1) {
		
			// 토지계약자대출 조회
			this.ds_lcntrctrLonList.clearData();		
			this.fn_transaction("selectLcntrctrLonList");
			
		}
	}

}


