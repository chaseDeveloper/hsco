			this.div_detail02.Tab00.tabpage1.btn_delAtchFile.set_visible(false);	 	// 파일삭제
			this.div_detail02.Tab00.tabpage1.btn_downAtchFile.set_visible(false);		// 파일보기(다운로드)
		}
