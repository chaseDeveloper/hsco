// 행추가
this.Div00_btn_add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	 var idx = this.Div00.Tab00.tabindex;
	
	 if(idx == 0)
	 {
		 var cnt = this.ds_onbCltrRlstRgst.getRowCount();
		 if(cnt < 1) return;
		 
		 var nrow = this.ds_onbCltrRlstDtRgst.addRow();		 
		 var trnsId = this.ds_trnsId.getColumn(0,"TRNS_ID");
		 
		 var cltrMnmtNo = this.ds_onbCltrRlstRgst.getColumn(this.ds_onbCltrRlstRgst.rowposition,"CLTR_MNMT_NO");
		 
		 // 물건정보
		 this.ds_onbCltrRlstDtRgst.setColumn(nrow,"TRNS_ID",trnsId);                //연계번호
		 this.ds_onbCltrRlstDtRgst.setColumn(nrow,"STND_LNK_ORG_NO",STND_LNK_ORG_NO);   //연계기관번호
		 this.ds_onbCltrRlstDtRgst.setColumn(nrow,"CLTR_MNMT_NO",cltrMnmtNo);       //물건관리번호
		 
		 this.fn_snInit(); 
	 }else{
	    var nrow = this.ds_onbAtchFile.addRow();
	 }
}

//행삭제
this.Div00_btn_del_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var idx = this.Div00.Tab00.tabindex;
	if(idx == 0)
	{
	  this.ds_onbCltrRlstDtRgst.deleteRow(this.ds_onbCltrRlstDtRgst.rowposition);
	  this.fn_snInit(); 
	}else{
	   this.ds_onbAtchFile.deleteRow(this.ds_onbAtchFile.rowposition);
	}
}

