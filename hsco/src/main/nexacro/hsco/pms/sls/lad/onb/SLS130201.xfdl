/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
    var trnsId = this.ds_trnsId.getColumn(0, "TRNS_ID");
    trace("########## fn_save() TRNS_ID : " + trnsId);
    
    if(trnsId == undefined)
	{
	   this.gfn_message("info.처리불가","전송ID 신규부여후 \n진행하시기바랍니다.");
	   return;
	}
	var procSt = this.ds_onbCltrRlstRgst.getColumn(this.ds_onbCltrRlstRgst.rowposition, "PROC_ST");
	
	//trace("########## PROC_ST : " + procSt);
	
	if (procSt != "Z")
	{
	  this.alert("전송된 건은 수정할 수 없습니다.");
	  return;
	}   
   
	// 토지정보
	if(this.ds_onbCltrRlstRgst.isUpdated() && !this.gfn_checkValidation(this.ds_onbCltrRlstRgst, this.ds_validation))
		return;
	
	// 정보세부
	if(this.ds_onbCltrRlstDtRgst.isUpdated() && !this.gfn_checkValidation(this.ds_onbCltrRlstDtRgst, this.ds_validation))  
		return;
		
	// 첨부파일
	if(this.ds_onbAtchFile.isUpdated() && !this.gfn_checkValidation(this.ds_onbAtchFile, this.ds_validation))
		return;
		 
	if(!this.ds_onbCltrRlstRgst.isUpdated() && 
	   !this.ds_onbCltrRlstDtRgst.isUpdated()  && 
	   !this.ds_onbAtchFile.isUpdated() && 
	   !LAD_IMPORTED) 
	{
	    this.gfn_message("comm.내역.변경.없음");	    
	    return;
	}

    LAD_IMPORTED = false;
    
    var CLTR_LDNM_ADRS = this.ds_onbCltrRlstRgst.getColumn(this.ds_onbCltrRlstRgst.rowposition, "CLTR_LDNM_ADRS");
    var CLTR_NMRD_ADRS = this.ds_onbCltrRlstRgst.getColumn(this.ds_onbCltrRlstRgst.rowposition, "CLTR_NMRD_ADRS");
    var PSCG_TPNO      = this.ds_onbCltrRlstRgst.getColumn(this.ds_onbCltrRlstRgst.rowposition, "PSCG_TPNO");
    
	if (this.comUtils.isNullEmpty(CLTR_LDNM_ADRS) && this.comUtils.isNullEmpty(CLTR_NMRD_ADRS)) 
	{
	    this.alert("물건지번주소 또는 물건도로명 주소 중 하나는 반드시 필수로 입력해야합니다.");
	    return;
	}
	if (PSCG_TPNO.indexOf("-") == -1  ) {
        this.alert("담당자전화번호는 (-)구분자로 표시되어야 합니다.");
        return false;
    }
    
    if(this.gfn_message("comm.행위여부","저장"))
	{     	      
	      this.ds_onbCltrRlstRgst.setColumn(this.ds_onbCltrRlstRgst.rowposition,"MAIN_ROW","Y");	      		  
	      this.ds_onbCltrRlstRgst.setColumn(this.ds_onbCltrRlstRgst.rowposition,"TRNS_ID", trnsId);
		  if (this.ds_onbAtchFile.isUpdated()) {
			  var sController   = "/hsco/pms/sls/lad/onb/SLS130201/saveOnbCltrRlstRgst.do"; 
			  var sInDatasets   = "input1=ds_onbCltrRlstRgst:U input2=ds_onbCltrRlstDtRgst:U input3=ds_onbAtchFile:U" ;
			  var sOutDatasets  = "";	      
			  this.FILE_MANAGER.upload(sController, sInDatasets, sOutDatasets);	      
	      } else {	      
			  this.fn_transaction("saveOnbCltrRlstRgst");
	      }
	}
}

// 물건정보삭제
this.fn_delete = function()
{
	if (this.ds_onbCltrRlstRgst.getColumn(this.ds_onbCltrRlstRgst.rowposition, "PROC_ST") != "Z")
	{
	  this.alert("전송된 건은 삭제할 수 없습니다.");
	  return;
	}
	
	if(this.gfn_message("comm.행위여부","삭제"))
	{ 
		this.ds_onbAtchFile.deleteAll();
		this.ds_onbCltrRlstDtRgst.deleteAll();
		this.ds_onbCltrRlstRgst.deleteRow(this.ds_onbCltrRlstRgst.rowposition);   
		this.ds_onbCltrRlstRgstList.deleteRow(this.ds_onbCltrRlstRgstList.rowposition);
		
		if (this.ds_onbAtchFile.isUpdated()) {
		  var sController   = "/hsco/pms/sls/lad/onb/SLS130201/saveOnbCltrRlstRgst.do"; 
		  var sInDatasets   = "input1=ds_onbCltrRlstRgst:U input2=ds_onbCltrRlstDtRgst:U input3=ds_onbAtchFile:U" ;
		  var sOutDatasets  = "";	      
		  this.FILE_MANAGER.upload(sController, sInDatasets, sOutDatasets);	      
		} else {	      
		  this.fn_transaction("saveOnbCltrRlstRgst");
		}
    }
}

