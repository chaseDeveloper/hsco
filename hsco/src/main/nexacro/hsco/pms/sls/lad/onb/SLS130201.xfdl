/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) 
	{		
	    case "selectCltrRlstRgstTransId":	// 최종 물건정보 trans_id 조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/selectCltrRlstRgstTransId.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_trnsId=output1";
			break; 
	   case "selectCltrRlstRgstList":	   // 물건정보 목록조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/selectCltrRlstRgstList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_onbCltrRlstRgstList=output1";
			break; 
	   case "selectCltrRlstRgst":	   // 물건정보 조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/selectCltrRlstRgst.do"; 
			var sInDatasets   	= "input1=ds_schVal";
			var sOutDatasets  	= "ds_onbCltrRlstRgst=output1 ds_onbAtchFile=output2 ds_popRtnLad=output3";
			break; 
	   case "selectCltrRlstDtRgstList":	   // 물건정보상세조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/selectCltrRlstDtRgstList.do"; 
			var sInDatasets   	= "input1=ds_schVal";
			var sOutDatasets  	= "ds_onbCltrRlstDtRgst=output1";
			break;
	   case "selectOnbAtchFile":	   	// 첨부파일조회
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/selectOnbAtchFile.do"; 
			var sInDatasets   	= "input1=ds_schVal";
			var sOutDatasets  	= "ds_onbAtchFile=output1";
			break;
	   
	   case "saveOnbCltrRlstRgst":		//물건정보 저장
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/saveOnbCltrRlstRgst.do"; 
			var sInDatasets   	= "input1=ds_onbCltrRlstRgst:U input2=ds_onbCltrRlstDtRgst:U input3=ds_onbAtchFile:U input4=ds_popRtnLad";
			var sOutDatasets  	= "";
			break; 
	   case "saveTxTrnsInfo": 			//전송
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/saveTxTrnsInfo.do"; 
			var sInDatasets   	= "input1=ds_onbCltrRlstRgst";
			var sOutDatasets  	= "";
			break; 			
	   case "selectGetTransId":			// 물건정보 trans_id 채번
			var sController   	= "/hsco/pms/sls/lad/onb/SLS130201/selectGetTransId.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_trnsId=output1";
			break; 	
	}
	if(!this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets))
		return;
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
		    case "selectCltrRlstRgstList":
				//if (this.ds_onbCltrRlstRgst.getColumn(this.ds_onbCltrRlstDtRgst.rowposition, "PROC_ST") != "Z")
				//{
				//     //this.Div00.div_info.set_enable(false);
				//}		          
		    break;
		    case "saveOnbCltrRlstRgst":
		          this.fn_search();
		          this.alert("저장되었습니다");
		    break;
		    case "saveTxTrnsInfo":
		          this.fn_search();
		          this.alert("전송되었습니다");
		    break;
		}
	}else{
	     this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess ? "Success : ":"Fail : ") + strSvcId);
}

