 ***********************************************************************
 */

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "pms_lib::pmsUtilInclude.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
// 화면 전역에서 사용할 Form 변수를 선언한다.
// TODO

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SLS090201_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();             // 공통 폼 초기화
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화
}

this.SLS090201_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.ds_cond00.setColumn(0, "PAY_TMLMT_FR", this.gfn_today());
	this.ds_cond00.setColumn(0, "PAY_TMLMT_TO", this.gfn_dateInfo("NOW_LAST_DATE"));
	this.ds_cond00.setColumn(0, "MDAT_DE", this.gfn_today());
	this.ds_cond00.setColumn(0, "DECDATE_SE", "0");
	this.ds_cond00.setColumn(0, "VOTE_SE", "0");	

	this.fn_loadCombo();	// 공통코드 조회
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	    : initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}


/*
 * 함수명  	    : fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);	// 조건에 사용되는 Dataset 초기화
}

/*
 * 함수명  	    : fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function()
{

	// 공통코드 목록 조회
 	var comCodeParam = [
 		  {OUT_PUT:"ds_dtaSeSh", GRP_CODE:"LAD0A1", USE_AT:"1"}  // 자료구분
 		 ,{OUT_PUT:"ds_payOdr",  GRP_CODE:"LAD0B4", USE_AT:"1"}  // 납부차수
 		 ,{OUT_PUT:"ds_paySe",   GRP_CODE:"LAD0A8", USE_AT:"1"}  // 납부구분
 		
 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);


	// 토지 코드 목록 조회
	var ladCodeParam = {};
	ladCodeParam["UASE_AT"] = "1";
	this.pmsUtil.getLadCodeList(ladCodeParam, "ds_ladCodeSh");
}


/*
 * 함수명  	    : fn_search
 * 설명 		: 토지수납조정 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function() 
{
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}
	
	// Clear Datasets
	this.pmsUtil.clearDataSet([this.ds_lttotRcivMdatList]);
			
	// 수납조정목록(분양)
	this.fn_transaction("selectLttotRcivMdatList");
}


/*
 * 함수명  	    : fn_save
 * 설명 		: 토지수납조정 - 조정일자 및 결의서 생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	var dsCount = this.ds_lttotRcivMdatList.rowcount;		
	var ds = this.ds_lttotRcivMdatList;
	
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}		

	// 조회여부 체크
	if (this.ds_lttotRcivMdatList.rowcount > 0) {
		
		// 저장여부 확인
		if(this.confirm("검색된 자료의 [월초조정] 처리를 하시겠습니까? [즉시처리]")) {
			this.ds_lad_actvote.clearData();
			// 체크된 로우에 조정일자 등록 및 프로시저 호출 
			for (var i = 0; i < dsCount; i++) {
				
				if(ds.getColumn( i , "CHK") == 1){
					var nRow = this.ds_lad_actvote.addRow();
					ds.setColumn(i, "MDAT_DE", this.cal_mdatDe.value);
					
					this.ds_lad_actvote.setColumn( nRow , "IN_FG" , 'UI');
					this.ds_lad_actvote.setColumn( nRow , "IN_CRATE_TYPE" , 'AL');
					this.ds_lad_actvote.setColumn( nRow , "IN_DEC_DATE" , this.cal_mdatDe.value);
					this.ds_lad_actvote.setColumn( nRow , "IN_PAY_NUM" , ds.getColumn( i , "F_PAY_NUM"));
					this.ds_lad_actvote.setColumn( nRow , "IN_CORT_CODE" , ds.getColumn( i , "F_CORT_CODE"));
					this.ds_lad_actvote.setColumn( nRow , "IN_CRT_ID" , ds.getColumn( i , "CRT_ID"));
					this.ds_lad_actvote.setColumn( nRow , "IN_VOTE_NUM" , '');
					this.ds_lad_actvote.setColumn( nRow , "IN_DEPT" , application.gds_userInfo.getColumn( 0 , "DEPT_CODE"));
					this.ds_lad_actvote.setColumn( nRow , "IN_STATE" , '');
					
					if(ds.getColumn( i , "VOTE_NUM") != null){
						alert("징수결의서가 생성된 자료가 있습니다.");
						return;
					}
				}	
			}		
		}
			
		this.fn_transaction("saveLttotRcivMdatList");			
	} 
	else{
		this.alert("검색된 자료가 없습니다.");
		return;
	}
}

/*
 * 함수명  	    : fn_delete
 * 설명 		: 토지수납조정 삭제
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{

	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}
		
	// 조정일자 삭제 체크
	if(this.confirm("검색된 자료의 [월초조정]을 취소(삭제)처리 하시겠습니까? [즉시처리]")) {
		
		var dsCount = this.ds_lttotRcivMdatList.rowcount;
		
		for (var i = 0; i < dsCount; i++) {
			this.ds_lttotRcivMdatList.setColumn(i, "MDAT_DE", "");
		}

		this.fn_transaction("saveLttotRcivMdatList");
	}
	
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
 /*
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	 
	    // 수납조정목록(분양) 조회
		case "selectLttotRcivMdatList" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS090201/selectLttotRcivMdatList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_lttotRcivMdatList=output1";			
			break;
						
	    // 수납조정목록(분양) 수정 - 조정일자 생성
		case "saveLttotRcivMdatList" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS090201/saveLttotRcivMdatList.do";
			var sInDatasets   	= "input1=ds_lttotRcivMdatList:U";
			var sOutDatasets  	= "";
			break;

	    // 수납조정목록(분양) 수정 - 결의서 생성 (프로시저 호출)
		case "saveLadaActVote" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS090201/saveLadaActVote.do";
			var sInDatasets   	= "input1=ds_lad_actvote";
			var sOutDatasets  	= "";
			break;			
			
	    // 수납조정목록(분양) 납부요청
		case "saveLttotRcivMdatVaBill" :
			var sController   	= "hsco/pms/sls/lad/ctr/SLS090201/saveLttotRcivMdatVaBill.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "";
			break;			
			
			
		// 납부서 일괄 출력			
		case "selectBillPrintListLadALL":	
				this.ds_billPrintList.clearData();
				var sController   	= "hsco/pms/sls/com/SLS000000/selectBillPrintListAll.do";
				var sInDatasets   	= "input1=ds_cond_report";
				var sOutDatasets  	= "ds_billPrintList=output1";
				break;				
			
		// 프로시저(임대료 , 지원금 납부요청 프로시저)
		case "selectSlaryList6": 
				//this.debug("start selectSlaryList6");
				var sController   	= "hsco/pms/sls/com/SLS000000/P7.do";
				var s  = "input1=ds_mt_rntchrg02:U"
				       + " input2=ds_mt_rntchrg01:U"
				       + " input3=ds_mt_rntchrg03:U";
				var sInDatasets   	=  s;
				var sOutDatasets  	= "";
				break;				

		case "selectSlaryList7": 
				//this.debug("start selectSlaryList6");
				var sController   	= "hsco/pms/sls/com/SLS000000/P7.do";
				var s  = "input1=ds_mt_rntchrg02:U"
				       + " input2=ds_mt_rntchrg01:U"
				       + " input3=ds_mt_rntchrg03:U";
				var sInDatasets   	=  s;
				var sOutDatasets  	= "";
				break;	

		default :
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


/*
 * 함수명  	    : fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId){
		
			// 수납조정목록(분양) 조회
			case "selectLttotRcivMdatList" :
				break;		
				
			// 수납조정목록(분양) 수정
			case "saveLttotRcivMdatList" :
			    this.fn_transaction("saveLadaActVote"); // 결의서 생성 호출
				this.fn_search();
				break;
				
			// 수납조정목록(분양) 납부요청
			case "saveLttotRcivMdatVaBill" :			 
				this.fn_saveLttotRcivMdatVaBill_callBack();
				this.alert("일괄납부요청이 정상적으로 처리되었습니다.");
				break;				
		
			// 공통코드 목록 조회
			case "selectCommonCode" :
			
				// Dataset Copy
				this.ds_payCode.copyData(this.ds_dtaSeSh);
			
				// 자료구분
				targetCombo = this.div_search.cmb_dtaSeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";	

				this.pmsUtil.setComboPrompt(targetCombo, comboOption);											
				break;

		
			// 토지코드 목록 조회
			case "ladCodeSh" :
			
			    // Dataset Copy
			    this.ds_ladCode.copyData(this.ds_ladCodeSh)  // 토지코드

				// 조회영역 - 토지코드
				targetCombo = this.div_search.cmb_ladCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL
				comboOption["noNullText"] = "코드없음";
				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
				break;
				
			case "selectSlaryList6" :
			
				this.div_search_btn_payRequest_onclick2();
				break;				
				
			case "selectSlaryList7" :
		
				this.alert("납부요청 완료");			
				break;								
				
			case "selectBillPrintListLadALL" :
				this.fn_billPrint();
				break;				
				
		}
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}


