//------- 
// 인쇄 
//------- 
this.fn_print = function()
{
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_work['div_inputType0' + sInputType], this.ds_validation)) {
		return;
	}
	
	sInputType = this.div_search.cmb_inputType.value;
	
	switch (sInputType) {
					
		case "1": // 수입조정일계표
			this.ds_ladIncmeMdatDailyList.clearData();
			this.fn_transaction("selectLadIncmeMdatDailyList");
			break;
					
		case "2": // 월별징수현황
			var se1 = this.div_work.div_inputType02.rdo_se1.value;
			if(se1 == "1"){ //목별
				this.ds_ladMonthlyLevList1.clearData();
				this.fn_transaction("selectLadMonthlyLevList1");
			}
			else{ //지구별
				this.ds_ladMonthlyLevList2.clearData();
				this.fn_transaction("selectLadMonthlyLevList2");
			}
			
			break;
			
		case "3": // 자금수입기록부
			this.ds_cptalIncmeList.clearData();
			this.fn_transaction("selectCptalIncmeList");		
			break;
						
		case "4": // 수입예산정리부
			var decsnDeFr = this.div_work.div_inputType04.cal_decsnDeFrSh.value + "";
			var decsnDeTo = this.div_work.div_inputType04.cal_decsnDeToSh.value + "";
			//alert("decsnDeFr = " + decsnDeFr + "   decsnDeTo = " + decsnDeTo);
			if( decsnDeFr.substring(0,6) != decsnDeTo.substring(0,6)){
				this.alert("같은달의 일자를 입력하세요.");
				return;
			}
			this.ds_incmeBdgArngList.clearData();
			this.fn_transaction("selectIncmeBdgArngList");		
			break;
									
		case "5": // 지구별예산정리부
			var se2 = this.div_work.div_inputType05.rdo_se2.value;
			if(se2 == "1"){ //보조장부
				this.ds_areaIncmeBdgArngList1.clearData();
				this.fn_transaction("selectAreaIncmeBdgArngList1");		
			}
			else{ //집계표
				this.ds_areaIncmeBdgArngList2.clearData();
				this.fn_transaction("selectAreaIncmeBdgArngList2");		
			}
			
			break;
	}
}

