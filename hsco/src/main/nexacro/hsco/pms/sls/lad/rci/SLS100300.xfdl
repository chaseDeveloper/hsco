}

this.fn_callbackAfter2 = function()
{
	this.ds_landCodeSh.insertRow(0);
	this.ds_landCodeSh.setColumn(0, "ALL_LAD_NM", "전체");
	this.div_work.div_inputType04.cmb_ladCodeSh.set_index(0);
	this.div_work.div_inputType05.cmb_ladCodeFrSh.set_index(0);
	this.div_work.div_inputType05.cmb_ladCodeToSh.set_index(0);	
}

	
