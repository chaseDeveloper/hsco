this.fn_print_report = function()
{
   var vOpts = new this.cf_Opts();
   switch (sInputType) {
					
		case "1": // 수입조정일계표
			var sPath = "/pms/sls/SLS100300_R01.jrf";
			var sFileName = "수입조정일계표";
			var sArg = "수입조정일계표";
			vOpts.setLocalDs(["ds_ladIncmeMdatDailyList", this.ds_ladIncmeMdatDailyList]);
			break;
					
		case "2": // 월별징수현황		
			var se1 = this.div_work.div_inputType02.rdo_se1.value;
			if(se1 == "1"){ //목별
				var sPath = "/pms/sls/SLS100300_R02.jrf";
				var sFileName = "월별수입징수현황 : 목별";
				var sArg = "월별징수현황 : 목별";
				vOpts.setLocalDs(["ds_ladMonthlyLevList1", this.ds_ladMonthlyLevList1]);
			}
			else{ //지구별
				var sPath = "/pms/sls/SLS100300_R03.jrf";
				var sFileName = "월별징수현황 : 지구별";
				var sArg = "월별징수현황 : 지구별";
				vOpts.setLocalDs(["ds_ladMonthlyLevList2", this.ds_ladMonthlyLevList2]);
			}
			
			break;
			
		case "3": // 자금수입기록부
			var sPath = "/pms/sls/SLS100300_R04.jrf";
			var sFileName = "자금수입기록부";
			var sArg = "자금수입기록부";
			vOpts.setLocalDs(["ds_cptalIncmeList", this.ds_cptalIncmeList]);
			break;
						
		case "4": // 수입예산정리부
			var sPath = "/pms/sls/SLS100300_R05.jrf";
			var sFileName = "수입예산정리부";
			var sArg = "수입예산정리부";
			vOpts.setLocalDs(["ds_incmeBdgArngList", this.ds_incmeBdgArngList]);
			break;
									
		case "5": // 지구별예산정리부
			var se2 = this.div_work.div_inputType05.rdo_se2.value;
			if(se2 == "1"){ //보조장부
				var sPath = "/pms/sls/SLS100300_R06.jrf";
				var sFileName = "지구별 수입예산정리부 : 보조장부";
				var sArg = "지구별 수입예산정리부 : 보조장부";
				vOpts.setLocalDs(["ds_areaIncmeBdgArngList1", this.ds_areaIncmeBdgArngList1]);
				break;
			}
			else{ //집계표
				var sPath = "/pms/sls/SLS100300_R07.jrf";
				var sFileName = "지구별 수입예산정리부 : 집계표";
				var sArg = "지구별 수입예산정리부 : 집계표";
				vOpts.setLocalDs(["ds_areaIncmeBdgArngList2", this.ds_areaIncmeBdgArngList2]);
				break;
			}
			
			break;
	}
	
	vOpts.setToolbar(false);
	vOpts.setScale(100);	
	vOpts.setReportPath(sPath);
	vOpts.setSaveFilename(sFileName);
	vOpts.setArgs("title", sArg);
	//vOpts.setArgs("DECSN_DE_FR", this.ds_cond.getColumn(this.ds_cond.rowposition, "DECSN_DE_FR"));
	//vOpts.setArgs("DECSN_DE_TO", this.ds_cond.getColumn(this.ds_cond.rowposition, "DECSN_DE_TO"));
	
	var parm = {
		arg_0 : this,
		opts : vOpts
	};
	
	this.gfn_popup("Popup_Report", 884, 735, sFileName, parm, "common::frmReportPopup.xfdl", "report_callback");
}

