	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);

	// 토지코드 목록 조회
	var paramMap = {};
	paramMap["USE_AT"] = "1";
	this.pmsUtil.getLadCodeList(paramMap, "ds_landCodeSh","fn_callbackAfter2");
}

this.fn_callbackAfter = function()
{		
	this.ds_acntSe.filter("CODE == '11' || CODE == '21' || CODE == '31'|| CODE == ''");
	
