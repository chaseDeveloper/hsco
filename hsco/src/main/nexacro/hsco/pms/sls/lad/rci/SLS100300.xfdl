			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_ladMonthlyLevList2=output1";
			break;
		
		//자금수입	기록부
		case "selectCptalIncmeList": 
			var sController   	= "hsco/pms/sls/lad/rci/SLS100300/selectCptalIncmeList.do";		
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cptalIncmeList=output1";
			break;
			
		//수입예산정리부
		case "selectIncmeBdgArngList": 
			var sController   	= "hsco/pms/sls/lad/rci/SLS100300/selectIncmeBdgArngList.do";		
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_incmeBdgArngList=output1";
			break;
						
		//지구별 수입예산정리부 : 보조장부
		case "selectAreaIncmeBdgArngList1": 
			var sController   	= "hsco/pms/sls/lad/rci/SLS100300/selectAreaIncmeBdgArngList1.do";		
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_areaIncmeBdgArngList1=output1";
			break;
			
		//지구별 수입예산정리부 : 집계표
		case "selectAreaIncmeBdgArngList2": 
			var sController   	= "hsco/pms/sls/lad/rci/SLS100300/selectAreaIncmeBdgArngList2.do";		
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_areaIncmeBdgArngList2=output1";
			break;
			
		//내부결재 번호 조회
		case "searchDecsn": 
			var sController   	= "hsco/pms/com/COM000000/selectDecsn.do";		
			var sInDatasets   	= "input1=ds_cond99";
			var sOutDatasets  	= "ds_sanction=output1";
			bSync = false; //동기방식으로 처리
			break;			
			
		//내부결재 상태조회
		case "sanknKnd": 
 		    var sController   	= "hsco/pms/sls/hou/ctr/SLS020201/SANCTNKND.do";
	        var sInDatasets   	= "input1=ds_cond05";
	        var sOutDatasets  	= "ds_sanctnknd=output1";
	        bSync = false; //동기방식으로 처리
		    break;      

		//내부결재 번호 등록
		case "insertDecsn": 
			var sController   	= "hsco/pms/com/COM000000/insertDecsn.do";		
			var sInDatasets   	= "input1=ds_cond99";
			var sOutDatasets  	= "";
			break;					
	}

	//Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}
