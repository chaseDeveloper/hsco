 * 작성자   : 이상준
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *    2015.10.01   이상준   신규생성
 *	  2016.02.12   이상준   레포트관련 수정
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "pms_lib::pmsUtilInclude.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
// 화면 전역에서 사용할 Form 변수를 선언한다.
this.SUPLY_DTLS_SELECT_ENABLED = false;		// 공급현황 선택 가능여부

this.ENABLED_REPORT_VIEW = false;			// 레포트 출력가능 여부
this.REPORT_TITLE = null;					// 레포트 타이틀
this.REPORT_TYPE = null;					// 레포트 구분(파일명)

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SLS110101_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	//this.fn_init_form();		// Form 초기화 - fn_init에 포함
	this.fn_init_dataset();		// DataSet 초기화	
}

this.SLS110101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_setDefaultInfo();	// 기본화면 정보 설정
	this.fn_loadCombo();		// Combo에 사용되는 Dataset Load
	//this.ds_cond00.addRow();
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.pmsUtil.clearDataSet([this.ds_cond00, this.ds_cond01, this.ds_cond02]);
}

/*
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function()
{

	// 공통코드 목록 조회 - 토지구분, 용도구분, 급지구분, 목차구분
	var comCodeParam = [
		  {OUT_PUT:"ds_schPrposSeCode", GRP_CODE:"LAD003", USE_AT:"1"}										// 토지용도코드
	];
	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
	
	// 토지 코드 목록 조회
	var ladCodeParam = {};
	ladCodeParam["UASE_AT"] = "1";
	this.pmsUtil.getLadCodeList(ladCodeParam, "ds_schLadCode");
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: callback 처리 후에 더 필요한 작업이 있을 경우, 작성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
 */
this.fn_callbackAfter = function()
{
	 // TODO
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/*--------------------------------------------------------------------
 * [BEGIN] 기본 버튼 처리
 --------------------------------------------------------------------*/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.div_print.set_visible(false);
	
	this.fn_selectLadSuplyDtlsList();
}

/*
 * 함수명  	: fn_print
 * 설명 		: 인쇄 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
 */
this.fn_print = function()
{
	
	this.div_print.chk_1.set_value(false);
	this.div_print.chk_2.set_value(false);
	
	if(!this.div_print.visible){
		this.div_print.set_right(136);
		this.div_print.set_visible(true);
	}
	else{
		this.div_print.set_visible(false);
	}
	//this.fn_selectPrintView();
}



this.div_print_chk_1_onclick = function(obj:CheckBox,  e:nexacro.ClickEventInfo)
{
	if(this.div_print.chk_1.isChecked()){
		this.div_print.chk_1.set_value(false);
		return;
	}

	var isBoolean = (this.ds_ladSuplyDtls.rowcount > 0) ? true : false;
	if(!isBoolean){
		this.alert("조회된 토지공급내역 내용이 없습니다");
		this.div_print.chk_1.set_value(false);
	}else{
		this.div_print.chk_1.set_value(true);
	}
}


this.div_print_chk_2_onclick = function(obj:CheckBox,  e:nexacro.ClickEventInfo)
{
	var isBoolean = (this.ds_pricRcivSttus.rowcount > 0) ? true : false;
	if(!isBoolean){
		this.alert("조회된 대금수납현황 내용이 없습니다");
		this.div_print.chk_2.set_value(false);
	}
}
	


this.printChkArr = [];	// 출력물 배열
this.div_print_btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{

	if(this.div_print.chk_1.isChecked()){
		if(this.ds_ladSuplyDtls.rowcount > 0){
			this.REPORT_TITLE = "토지공급관리대장";		// 레포트 타이틀
			this.REPORT_TYPE = "SLS110101_R01";		// 레포트 구분(파일명)
			
			//this.fn_selectReportContent(this.REPORT_TYPE);
			this.printChkArr.push(this.REPORT_TYPE+","+this.REPORT_TITLE);
		}
		else{
			this.alert("조회된 자료가 없습니다.");
		}
	}
	
	if(this.div_print.chk_2.isChecked()){
		if(this.ds_pricRcivSttus.rowcount > 0){
			this.REPORT_TITLE = "대금수납현황";		// 레포트 타이틀
			this.REPORT_TYPE = "SLS110101_R02";		// 레포트 구분(파일명)
			
			//this.fn_selectReportContent(this.REPORT_TYPE);
			this.printChkArr.push(this.REPORT_TYPE+","+this.REPORT_TITLE);
		}
		else{
			this.alert("조회된 자료가 없습니다.");
		}
	}
	
	this.printChkArr.reverse();
	this.fn_printExecut();
}

 /*
 * 함수명  	: fn_printExecut
 * 설명 		: 선택한 출력물 실행
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_printExecut();
 */
this.fn_printExecut = function()
{
	if(this.printChkArr.length == 0){
		this.div_print.set_visible(false);
		return;
	}

	// Clear Datasets
	this.pmsUtil.clearDataSet([this.ds_cond03, this.ds_rptSuplyDtlsInfo, this.ds_rptRcivePersonList, this.ds_rptPricRcivSttusList]);

	// 조회조건 설정
	var selLadInfo = this.pmsUtil.getGridSelectedItem(this.div_work.grd_ladSuplyDtls);	// 그리드 선택 행 데이터
	
	// 선택 토지정보 조회조건 반영
	this.pmsUtil.putMapToDataset(this.ds_cond03, selLadInfo, 0);
	
	var tranId = null;
	
	var temp = this.printChkArr.pop();
	var tempArr = temp.split(',');
	
	this.REPORT_TYPE = tempArr[0];
	this.REPORT_TITLE = tempArr[1];
	
	switch(this.REPORT_TYPE){
		case "SLS110101_R01" :	// 토지공급관리대장
			this.ds_cond03.setColumn(0, "REPORT_TY", "R1");
			tranId = "selectLadSuplyDtlsReportList";		// 레포트용 데이터 조회 - 토지공급관리대장
			break;
		case "SLS110101_R02" :	// 대금수납현황
		case "SLS110101_R03" :	// 대금수납현황
			this.ds_cond03.setColumn(0, "REPORT_TY", "R2");
			tranId = "selectLadPricRcivSttusReportList";	// 레포트용 데이터 조회 - 대금수납현황
			break;
		default :
			break;
	}
	
	if(tranId == null){
		this.alert("err.서비스요청.무효");
		return;
	}
	
	// 레포트용 데이터 조회
	this.fn_transaction(tranId);
}

