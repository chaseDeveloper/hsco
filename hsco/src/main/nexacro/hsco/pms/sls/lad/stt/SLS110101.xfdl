						  //, ["ds_rptPricRcivSttusListAdd", this.ds_rptPricRcivSttusListAdd]
						 ];
			isMulti = false;
			break;
		case "SLS110101_R02" :	// 대금수납현황
			reportPath = "/pms/sls/" + type + ".jrf";
			datasetArr = [["ds_rptSuplyDtlsInfo", this.ds_rptSuplyDtlsInfo]
						  , ["ds_rptPricRcivSttusList", this.ds_rptPricRcivSttusList]
						 ];
			isMulti = false;
