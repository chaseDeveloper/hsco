/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	var sController = null;
	var oInDatasets = null;
	var sOutDatasets = null;
	switch (strSvcId) {

		// 토지코드별 토지공급내역 조회
		case "selectLadSuplyDtlsList" :
		
			sController = "hsco/pms/sls/lad/stt/SLS110101/selectLadSuplyDtlsList.do";
			oInDatasets = "input1=ds_cond00";
			sOutDatasets = "ds_ladSuplyDtls=suplyDtlsList";
			break;
			
		// 현재 계약자내역 조회
		case "selectNowCntrctrDtlsList" :
			sController = "hsco/pms/sls/lad/stt/SLS110101/selectNowCntrctrDtlsList.do";
			oInDatasets = "input1=ds_cond01";
			sOutDatasets = "ds_nowCntrctrDtls=cntrctrDtlsList";
			break;
			
		// 현재 계약자의 공급받는자, 대금수납현황 조회
		case "selectNowCntrctrDetailList" :
			sController = "hsco/pms/sls/lad/stt/SLS110101/selectNowCntrctrDetailList.do";
			oInDatasets = "input1=ds_cond02";
			sOutDatasets = "ds_suplyRcivePerson=rcivePersonList ds_pricRcivSttus=pricRcivSttusList";
			break;
				
		// 레포트용 데이터 조회 - 토지공급관리대장
		case "selectLadSuplyDtlsReportList" :
			sController = "hsco/pms/sls/lad/stt/SLS110101/selectLadSuplyDtlsReportList.do";
			oInDatasets = "input1=ds_cond03";
			sOutDatasets = "ds_rptSuplyDtlsInfo=rptSuplyDtlsInfo ds_rptRcivePersonList=rptRcivePersonList ds_rptPricRcivSttusList=rptPricRcivSttusList";
			break;
			
		// 레포트용 데이터 조회 - 대금수납현황
		case "selectLadPricRcivSttusReportList" :
			sController = "hsco/pms/sls/lad/stt/SLS110101/selectLadSuplyDtlsReportList.do";
			oInDatasets = "input1=ds_cond03";
			sOutDatasets = "ds_rptSuplyDtlsInfo=rptSuplyDtlsInfo ds_rptPricRcivSttusList=rptPricRcivSttusList";
			break;
		
		default :
			break;
	}

	if(sController == null) return;
	
	var sInDataType = typeof(oInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, reqParam, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, oInDatasets, sOutDatasets, null, null, null, bSync);
	}

}

