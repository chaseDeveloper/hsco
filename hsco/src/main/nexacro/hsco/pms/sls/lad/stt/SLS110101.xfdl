{	
	var gridName = obj.name;
	var oldRowPosition = e.oldrow;
	var selRowPosition = e.row;
	var selGridItem = null;
	this.debug("gridName ==> " + gridName);
	
	this.div_print.set_visible(false);
	this.div_print.chk_1.set_value(false);
	this.div_print.chk_2.set_value(false);
	
