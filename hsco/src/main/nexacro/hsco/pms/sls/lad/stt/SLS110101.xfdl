// 				if(resultLeng == 1){
// 					schItem = this.pmsUtil.getDatasetItem(this.ds_ladSuplyDtls, 0);
// 					this.fn_searchNowCntrctrDtls(schItem);	// 현재 계약자내역 조회
// 				}else{

				// 자동 조회 방지
				//if(resultLeng > 1){
					//this.ds_ladSuplyDtls.set_rowposition(-1);
				//}
				
				this.fn_setViewInfo(false);
				break;
				
			// 토지별 현재 계약자 내역 조회
			case "selectNowCntrctrDtlsList" :
				// 조회결과 목록수가 1개이면 바로 조회 아니면 포커스 삭제
				/*
				resultLeng = this.ds_nowCntrctrDtls.getRowCount();
				if(resultLeng == 1){
					schItem = this.pmsUtil.getDatasetItem(this.ds_nowCntrctrDtls, 0);
					//this.fn_searchSuplyRcivePerson(schItem);	// 공급받는자 조회
					//this.fn_searchPricRcivSttus(schItem)		// 대금수납현황 조회
					this.fn_searchNowCntrctrDetail(schItem);	// 현재 계약자의 공급받는자, 대금수납현황 조회
				}else{
					this.ds_nowCntrctrDtls.set_rowposition(-1);
				}
				*/
				break;
				
			// 현재 계약자의 공급받는자, 대금수납현황 조회
			case "selectNowCntrctrDetailList" :
				this.fn_setViewInfo(true);
				break;
				
			// 레포트용 데이터 조회 - 토지공급관리대장
			case "selectLadSuplyDtlsReportList" :
			// 레포트용 데이터 조회 - 대금수납현황
			case "selectLadPricRcivSttusReportList" :
			
