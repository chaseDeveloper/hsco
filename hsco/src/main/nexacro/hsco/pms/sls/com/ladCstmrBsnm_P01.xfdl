/***********************************************************************
* 사용자 정의 함수
************************************************************************/
/*
 * 함수명  	    : fn_confirm
 * 설명 		: 확인시 부모폼에 데이터 전달
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_confirm = function(ds){
		
	var opener = this.opener; //Runtime 작동시 부모 폼
	if(opener[this.callback] == null){
		opener = this.opener.div_workFrame.div_main; //브라우저 작동시 부모 폼
	}
	
	//부모 폼의 콜백 함수 직접 호출
	bResult = opener[this.callback]();
	
	return bResult;
}

