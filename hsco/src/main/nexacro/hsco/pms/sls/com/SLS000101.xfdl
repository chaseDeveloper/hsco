				 ,year_S     : null            // 예산년도(srchDs보다 우선순위를 가짐)
				 ,bsnsSe_S   : null            // 사업구분(srchDs보다 우선순위를 가짐)
				 ,balcSe_S   : null            // 수지구분(srchDs보다 우선순위를 가짐)
				 ,deptCode_S : null            // 부서코드(srchDs보다 우선순위를 가짐)
				 ,acntPopup  : false            // (true : 예산계정코드선택팝업, false : 지출단위선택팝업)
			};
			this.gfn_popup("mnthngCptalList", 900, 635, "예산선택팝업", parm, "com::bdgAcntPop.xfdl", "fn_PopupCallback");		
	}
	else{
		alert("계정구분이 선택되지 않았습니다");
	}

}

this.fn_PopupCallback = function(sPopupId, sReturn)
{
    var dsRtn = new Dataset();
    nRowCnt = dsRtn.loadXML(sReturn);
    
	if(nRowCnt > 0){
				
		if(sPopupId == "mnthngCptalList") {
			var bsnsSe        = dsRtn.getColumn(0, "BSNS_SE");
			var detailBsnsSe  = dsRtn.getColumn(0, "DETAIL_BSNS_CODE");
			var deptCode  	  = dsRtn.getColumn(0, "DEPT_CODE");
			var deptNm  	  = dsRtn.getColumn(0, "DEPT_NM");
			var acntCode      = dsRtn.getColumn(0, "BUDGET_ACNT_CODE");
			var acntNm        = dsRtn.getColumn(0, "BUDGET_ACNT_NM");
			var expndtrUnitCd = dsRtn.getColumn(0, "EXPNDTR_UNIT_CODE");
			var expndtrUnitNm = dsRtn.getColumn(0, "EXPNDTR_UNIT_NM");		
			var year = dsRtn.getColumn(0, "YEAR");	
			var nRow = this.ds_etcIncmePayList.rowposition;
			
			//alert("bsnsSe = " + bsnsSe + " detailBsnsSe =" + detailBsnsSe+ " deptCode =" + deptCode + " deptNm =" + deptNm);
			this.ds_etcIncmePayList.setColumn(nRow, "BUDGET_YEAR", year );
			this.ds_etcIncmePayList.setColumn(nRow, "BSNS_SE", bsnsSe  );
			this.ds_etcIncmePayList.setColumn(nRow, "DETAIL_BSNS_CODE", detailBsnsSe  );
			//this.ds_etcIncmePayList.setColumn(nRow, "DEPT_CODE", deptCode  );
			//this.ds_etcIncmePayList.setColumn(nRow, "DEPT_NM", deptNm  );
			
			this.ds_etcIncmePayList.setColumn(nRow, "BUDGET_ACNTCTGR", acntCode  );
			this.ds_etcIncmePayList.setColumn(nRow, "BUKIP_CODE", expndtrUnitCd  );
			
			this.ds_etcIncmePayList.setColumn(nRow, "BUDGET_ACNT_CODE", acntCode  );
			this.ds_etcIncmePayList.setColumn(nRow, "BUDGET_ACNT_NM",   acntNm    );	
			this.ds_etcIncmePayList.setColumn(nRow, "EXPNDTR_UNIT_CODE",expndtrUnitCd  );
			this.ds_etcIncmePayList.setColumn(nRow, "EXPNDTR_UNIT_NM",  expndtrUnitNm  );
		}
	}
}

