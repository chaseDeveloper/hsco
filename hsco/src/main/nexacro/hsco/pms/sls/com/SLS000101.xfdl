/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
 /*=====================================================
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 *=====================================================*/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
	    			
	    // 기타수입관리 조회
		case "selectEtcIncmePayList" :
			var sController   	= "/hsco/pms/sls/com/SLS000101/selectEtcIncmePayList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_etcIncmePayList=output1";
			break;
			
	    // 기타수입관리 저장
		case "saveEtcIncmePayList" :
			var sController   	= "/hsco/pms/sls/com/SLS000101/saveEtcIncmePayList.do";
			var sInDatasets   	= "input1=ds_etcIncmePayList:U";
			var sOutDatasets  	= "";
			break;
			
		
		//부서콤보 조회
		case "selectDeptcodeList":
			//this.ds_cond02.clearData();			
			var sController   	= "/hsco/pms/sls/com/SLS000101/selectDeptCodeList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_deptSh=output1 ds_dept=output2";
			break;
			
		default :
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/




/*============================================================================================
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 *============================================================================================*/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var targetCombo = null;
		
		switch(strSvcId){

						
			// 기타수입관리 조회
			case "selectEtcIncmePayList" :
				if(this.ds_etcIncmePayList.getRowCount() > 0){
					
				}
				else{					
					this.alert("조회된 데이터가 없습니다.");
					this.gfn_message("info.조회.데이터.없음");
				}
				break;
				
			// 기타수입관리 저장			
			case "saveEtcIncmePayList" :
				this.alert("저장되었습니다.");
				this.gfn_message("success.정상등록");
				this.fn_transaction("selectEtcIncmePayList"); // 기타수입관리 조회
				break;		

		}
	}
	else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}

