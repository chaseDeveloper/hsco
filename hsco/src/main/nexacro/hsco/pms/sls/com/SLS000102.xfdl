// ----------------------------------------------------------------------------
// 사업자등록 정보
// ----------------------------------------------------------------------------
this.ds_etcIncmeList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(this.ds_etcIncmeList.rowcount > 0){
		//사업자순번
		var BSNM_SN = this.ds_etcIncmeList.getColumn(e.newrow, "BSNM_SN");
		if(BSNM_SN == "" || BSNM_SN == null ||  BSNM_SN == "undefined"){
			
			this.div_detail00.edt_cntrctrMtlty.set_readonly(true);
			this.div_detail00.edt_mtltyNm.set_readonly(true);
			this.div_detail00.edt_bizcnd.set_readonly(true);
			this.div_detail00.edt_induty.set_readonly(true);
			this.div_detail00.cal_bsnsConfmDe.set_readonly(true);
			this.div_detail00.edt_bsnsAdres.set_readonly(true);
			
			this.div_detail00.edt_cntrctrMtlty.set_cssclass("edt_WF_Readonly");
			this.div_detail00.edt_mtltyNm.set_cssclass("edt_WF_Readonly");
			this.div_detail00.edt_bizcnd.set_cssclass("edt_WF_Readonly");
			this.div_detail00.edt_induty.set_cssclass("edt_WF_Readonly");
			this.div_detail00.cal_bsnsConfmDe.set_cssclass("cal_WF_Readonly");
			this.div_detail00.edt_bsnsAdres.set_cssclass("edt_WF_Readonly");
		}
		else{
			
			this.div_detail00.edt_cntrctrMtlty.set_readonly(false);
			this.div_detail00.edt_mtltyNm.set_readonly(false);
			this.div_detail00.edt_bizcnd.set_readonly(false);
			this.div_detail00.edt_induty.set_readonly(false);
			this.div_detail00.cal_bsnsConfmDe.set_readonly(false);
			this.div_detail00.edt_bsnsAdres.set_readonly(false);
			
			this.div_detail00.edt_cntrctrMtlty.set_cssclass("");
			this.div_detail00.edt_mtltyNm.set_cssclass("");
			this.div_detail00.edt_bizcnd.set_cssclass("");
			this.div_detail00.edt_induty.set_cssclass("");
			this.div_detail00.cal_bsnsConfmDe.set_cssclass("");
			this.div_detail00.edt_bsnsAdres.set_cssclass("");
		
		}
			
		//사업자순번
		var DECSN_SE1 = this.ds_etcIncmeList.getColumn(e.newrow, "DECSN_SE1");
		var DECSN_SE2 = this.ds_etcIncmeList.getColumn(e.newrow, "DECSN_SE2");
		//alert("DECSN_SE1 = " + DECSN_SE1 + "   DECSN_SE2 = " + DECSN_SE2);
		if(DECSN_SE1 == "1" || DECSN_SE2 == "1"){			
			this.div_detail.cmb_rcivDept.set_cssclass("cmb_WF_Essential");
		}
		else{
			this.div_detail.cmb_rcivDept.set_cssclass("");
		}
		
		
	}
}

