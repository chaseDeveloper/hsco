this.div_work_Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	// 조회조건
	var cntrctrNo = this.ds_etcIncmeList.getColumn(this.ds_etcIncmeList.rowposition, "CNTRCTR_NO");
	var cntrctrCstmrNo = this.ds_etcIncmeList.getColumn(this.ds_etcIncmeList.rowposition, "HOUSE_CSTMR_NO");			                         
	
	this.ds_cond02.setColumn(0, "CNTRCTR_NO", cntrctrNo);
	this.ds_cond02.setColumn(0, "CNTRCTR_CSTMR_NO", cntrctrCstmrNo);

	if (this.div_work.Tab00.tabindex == 0) {			
				// 약정사항 조회
		this.fn_transaction("selectEtcIncmeList");				
	}
	else if (this.div_work.Tab00.tabindex == 1) {														
		// 가상계좌 조회
		this.fn_transaction("selectVirtlAcnutList");
	}
	
}

