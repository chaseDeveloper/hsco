}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
 /*=====================================================
 * 함수명  	    : fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 *=====================================================*/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {	
	    			
	    // 기타수입조정관리(tabpage1) 조회
		case "selectEtcIncmeList" :
			var sController   	= "/hsco/pms/sls/com/SLS000102/selectEtcIncmeList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_etcIncmeList=output1";
			break;
			
		// 가상계좌(tabpage2) 조회
		case "selectVirtlAcnutList":
			var sController   	= "hsco/pms/sls/hou/ctr/SLS020101/selectVirtlAcnutList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_virtlAcnutList=output1";
			break;	
			
	    // 기타수입조정관리 저장
		case "saveEtcIncmeList" :
			var sController   	= "/hsco/pms/sls/com/SLS000102/saveEtcIncmeList.do";
			var sInDatasets   	= "input1=ds_etcIncmeList:U";
			var sOutDatasets  	= "";
			break;
			
		
		//부서콤보 조회
		case "selectDeptcodeList":
			//this.ds_cond02.clearData();			
			var sController   	= "/hsco/pms/sls/com/SLS000102/selectDeptCodeList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_deptSh=output1 ds_dept=output2";
			
			break;


		default :
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

/***********************************************************************
* Transaction 영역 끝
************************************************************************/




/*============================================================================================
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 *============================================================================================*/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var targetCombo = null;
		
		switch(strSvcId){
			/*
			// 공통코드 목록 조회
			case "selectCommonCode" :

				// 조회영역 - 납부코드
				targetCombo = this.div_search.cmb_payCodeSh;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_ALL;
				comboOption["noNullText"] = "코드없음";
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);				
				
				// 상세영역 - 납부코드				
				targetCombo = this.div_detail.cmb_payCode;
				comboOption = {};
				comboOption["type"] = this.pmsUtil.PROMPT_SELECT;
				comboOption["noNullText"] = "코드없음";				
				this.pmsUtil.setComboPrompt(targetCombo, comboOption);
								
				break;
			*/
			//부서코드 조회 콜백
			case "selectDeptcodeList" :
				this.ds_deptSh.insertRow(0);
				this.ds_deptSh.setColumn(0, "DEPT_NM", "전체");
				this.div_search.cmb_deptSh.set_index(0);
				this.div_search.cmb_rcivDeptSh.set_index(0);
				
				this.ds_dept.insertRow(0);
				this.ds_dept.setColumn(0, "DEPT_NM", "선택");
				this.div_detail.cmb_rcivDept.set_index(0);
				
			break;		
			
			
			// 기타수입조정관리(tabpage1) 조회
			case "selectEtcIncmeList" :
				if(this.ds_etcIncmeList.getRowCount() > 0){
					
				}
				else{
					this.gfn_message("info.조회.데이터.없음");
				}
			
			// 가상계좌(tabpage2) 조회break;
			case "selectVirtlAcnutList":
				if(this.ds_virtlAcnutList.getRowCount() > 0){
					
				}
				else{
					this.gfn_message("info.조회.데이터.없음");
				}
				break;
				
			// 기타수입조정관리 저장			
			case "saveEtcIncmeList" :
				this.gfn_message("success.정상등록");
				this.fn_transaction("selectEtcIncmeList"); // 기타수입조정관리 조회
				break;
				
			
		}
	}
	else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}
}

