			this.div_detail00.edt_bsnsAdres.set_cssclass("edt_WF_Readonly");
		}
		else{
			
			this.div_detail00.edt_cntrctrMtlty.set_readonly(false);
			this.div_detail00.edt_mtltyNm.set_readonly(false);
			this.div_detail00.edt_bizcnd.set_readonly(false);
			this.div_detail00.edt_induty.set_readonly(false);
			this.div_detail00.cal_bsnsConfmDe.set_readonly(false);
			this.div_detail00.edt_bsnsAdres.set_readonly(false);
			
			this.div_detail00.edt_cntrctrMtlty.set_cssclass("");
			this.div_detail00.edt_mtltyNm.set_cssclass("");
			this.div_detail00.edt_bizcnd.set_cssclass("");
			this.div_detail00.edt_induty.set_cssclass("");
			this.div_detail00.cal_bsnsConfmDe.set_cssclass("");
			this.div_detail00.edt_bsnsAdres.set_cssclass("");
		
