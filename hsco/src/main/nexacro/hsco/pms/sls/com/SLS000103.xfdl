 ***********************************************************************
 *    2015.00.00   홍길동   fn_xxx() 보완 수정
 ***********************************************************************
 */


/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
//include "lib::comInclude.xjs";
include "pms_lib::pmsUtilInclude.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
// 화면 전역에서 사용할 Form 변수를 선언한다.
// TODO

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.SLS000103_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{

	this.fn_init();             // 공통 폼 초기화
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화
}

this.SLS000103_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();		// Combo에 사용되는 Dataset Load
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*=========================================
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 *=========================================*/
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}

/*==========================================================================================
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
*==========================================================================================*/
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);	// 조건에 사용되는 Dataset 초기화
}

/*===================================================
 * 함수명  	: fn_loadCombo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
*===================================================*/
this.fn_loadCombo = function()
{
	/*
	// 공통코드 목록 조회 - 토지구분, 용도구분, 급지구분, 목차구분
 	var comCodeParam = [
		
 		  {OUT_PUT:"ds_payCodeSh",   GRP_CODE:"LAD0B3", USE_AT:"1"} // 납부코드
 		, {OUT_PUT:"ds_payCode",     GRP_CODE:"LAD0B3", USE_AT:"1"} // 납부코드
 		//, {OUT_PUT:"ds_jobSeCode",   GRP_CODE:"PCOM01", USE_AT:"1"} // 업무구분

 	];
 	this.pmsUtil.getCommonCode("selectCommonCode", comCodeParam);
 	*/
 	var comboParams = [
				  ["ds_payCodeSh","HOU0A1" ,"1" ,"전체"]   //납부코드
                , ["ds_payCode"  ,"HOU0A1" ,"1" ,"선택"]   //납부코드
              ];

	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
	
	
	//부서
	this.deptcode();
	
	// 사용자 콤보 생성
	var paramMap = {};	
	paramMap["USE_AT"] = "1";
	paramMap["HOUSE_SE"] = "S1,S2,S3,S4,S5,S9,R1,R2,R3,R9";	
	this.pmsUtil.getHouseCode(paramMap, "ds_houseCode", "fn_callbackAfter2");
}

this.fn_callbackAfter = function()
{
	this.ds_payCodeSh.filter("VAL2 == '9' || CODE == ''");
	this.ds_payCode.filter("VAL2 == '9' || CODE == ''");
}

this.fn_callbackAfter2 = function()
{
	this.ds_houseCode.insertRow(0);
	this.ds_houseCode.setColumn(0, "ALL_HOUSE_NM", "선택");
	this.div_detail.cmb_houseCode.set_index(0);
}
/***********************************************************************
* 부서콤보박스 조회
************************************************************************/

this.deptcode = function()
{
	// clear Datasets
	this.ds_dept.clearData();
	
	// transaction
	this.fn_transaction("selectDeptcodeList");
}

/*==========================================
 * 함수명  	    : fn_search
 * 설명 		: 계약목록 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
*=========================================*/
this.fn_search = function() 
{
	// 검색영역 Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}
	
    // 기타수입목록 조회
    this.fn_transaction("selectEtcIncmeList");
}

this.div_search_edt_sumrySh_onkeyup = function(obj:Edit, e:nexacro.KeyEventInfo)
{

	if(e.keycode == 13) {
		this.fn_search();
	}
}

/*=======================================
 * 함수명  	    : fn_insert
 * 설명 		: 기타수입관리 신규등록
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 *======================================*/
this.fn_insert = function()
{         
	var nAddRow = this.ds_etcIncmeList.addRow()   ; //기타수입관리 리스트
	
	//기타수입수납
	this.div_detail.cmb_payCode.set_index(0);
	this.ds_etcIncmeList.setColumn(nAddRow, "JOB_SE", "HOU"); //업무구분
	this.ds_etcIncmeList.setColumn(nAddRow, "RQEST_YM", this.gfn_today().substring(0,6)); //청구년월
	this.ds_etcIncmeList.setColumn(nAddRow, "BUDGET_YEAR", this.gfn_today().substring(0,4)); //예산년도
	this.ds_etcIncmeList.setColumn(nAddRow, "VAT_AT", 0); //부가세여부
	this.ds_etcIncmeList.setColumn(nAddRow, "OUTPT_YN", 0); //고지서발행여부
	
	this.ds_etcIncmeList.setColumn(nAddRow, "DECSN_SE1", 0); //조정결의
	this.ds_etcIncmeList.setColumn(nAddRow, "DECSN_SE2", 0); //수입결의
	
	//기타수입결의
	//this.ds_etcIncmeList.setColumn(nAddRow, "BUDGET_YEAR", this.gfn_today().substring(0,4)); //예산년도	
	this.ds_etcIncmeList.setColumn(nAddRow, "DECSN_DE", this.gfn_today()); //결의일자
	this.ds_etcIncmeList.setColumn(nAddRow, "RQESTDE", this.gfn_today()); //요청일자
	this.ds_etcIncmeList.setColumn(nAddRow, "DECSN_YM", this.gfn_today().substring(0,6)); //결의년월
	
}


//저장
this.fn_save = function()
{
	
	if(!this.gfn_checkValidation(this.ds_etcIncmeList, this.ds_validation)) {
    	return;
	}
	
	//수납부서 체크
	for(i=0; i < this.ds_etcIncmeList.getRowCount(); i++){
		
		var DECSN_SE1 = this.ds_etcIncmeList.getColumn(i, "DECSN_SE1");
		var DECSN_SE2 = this.ds_etcIncmeList.getColumn(i, "DECSN_SE2");
		var RCIV_DEPT = this.ds_etcIncmeList.getColumn(i, "RCIV_DEPT");
		
		if(DECSN_SE1 == "1" || DECSN_SE2 == "1"){
			if(RCIV_DEPT == "" || RCIV_DEPT == null ||  RCIV_DEPT == "undefined"){				
				this.gfn_message("comm.항목.필수", "","조정결의,수입결의 체크한 경우\n["+ (i+1) +"]행 수납부서는 필수선택 입니다.");
				return false;
			}			
		}		
	}
	
	//if(this.confirm("저장하시겠습니까?")){
	if(this.gfn_message("confirm.저장.여부","저장")){
		this.fn_transaction("saveEtcIncmeList")  ;  //기타수입관리 리스트 저장
	}
}


