 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
/*
params  		: 1. param_1    : 반환받을 DATASET 명 			(예: ds_comCode00)
                  2. param_2	: 그룹코드						(예 : COM007)
                  3. param_3	: 사용여부		    			(예 : 1(사용), ""(사용안함))
                  4. param_4	: 콤보박스INDEX(0) 표시		(예 : "전체", "선택", " "(공백), ""(첫번째데이터부터시작) )
*/
	var comboParams;
	if(this.ds_cond00.getColumn(0, "HOUSE_CODE") != null){
		comboParams = [
			["ds_code", "HOU0A1", "1", ""],
			["ds_gb", "HOU0A8", "1", ""],
			["ds_sn", "HOU0A9", "1", ""],
			["ds_bank", "COM021", "1", "전체"],
			["ds_virtual", "COM021", "1", ""]
		];
	}else{
		comboParams = [
			["ds_code", "LAD0A1", "1", ""],
			["ds_gb", "LAD0A8", "1", ""],
			["ds_sn", "LAD0A9", "1", ""],
			["ds_bank", "COM021", "1", "전체"],
			["ds_virtual", "COM021", "1", ""]
		];	
	}
	
	// 문자발송 내용
	this.fn_fill_code(this.ds_smsCn,
					   "/hsco/cmm/ums/mssageManageList.do", 
					   "",
					   {CODE : "SLS003"},		// 문자코드 [SLS003:토지납부청구안내]
					   "");
					   
	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
	
	// 토지코드 목록 조회
	var paramMap = {};
	paramMap["USE_AT"] = "";
	this.pmsUtil.getLadCodeList(paramMap, "ds_landCode", "fn_loadCombo_callback");	
}
