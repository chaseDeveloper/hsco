/*
 * 함수명  	: msg_onclick
 * 설명 		: 문자 발송
 * params  		: 
 * return Type 	: 
 * 사용법		: this.msg_onclick()
 */
this.msg_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rRow = this.ds_bill.rowposition;

	if(this.ds_bill.rowcount == 0){
		this.alert("납부진행내역이 존재하지 않습니다.");
		return;
	}
	if(rRow == -1){
		this.alert("선택된 납부진행내역이 없습니다.");
		return;
	}

    var nm      = this.ds_cond00.getColumn(0,"NM") || '';				// 고객명
	var telno   = this.ds_cond00.getColumn(0,"TELNO") || '';			// 연락처1
	var mbtlno  = this.ds_cond00.getColumn(0,"MBTLNO") || '';			// 연락처2	
	var bankNm  = this.ds_virtual.getColumn(this.ds_virtual.rowposition, "BANK_NM") || '';	// 은행명
	var vcctNo  = this.ds_bill.getColumn(rRow, "VACCT_NO") || '';	// 계좌번호
	var billAmt = this.ds_bill.getColumn(rRow, "BILL_AMT") || '';	// 청구금액
	var accntSe = '';												// 청구종류
	var content = this.ds_smsCn.getColumn(0, "MSSAGE_CN") || '';	// 메시지 내용

	vcctNo  = "계좌번호(" + bankNm + ":" + vcctNo + ")";
	billAmt = this.comUtils.formatComma(billAmt);	// 청구금액 단위표시
	
	if(mbtlno.indexOf("01") == 0 && (mbtlno.length == 10 || mbtlno.length == 11))
		mbtlno = mbtlno;
	else if(telno.indexOf("01") == 0 && (telno.length == 10 || telno.length == 11))
		mbtlno = telno;
	else
		mbtlno = '';
		
		
	this.debug("MBTLNO = " + mbtlno);
	this.debug("NM = " + nm);
	this.debug("CHANGE_WORD0 = " + nm);
	this.debug("CHANGE_WORD1 = " + vcctNo);
	this.debug("CHANGE_WORD2 = " + billAmt);
	this.debug("CHANGE_WORD3 = " + accntSe);
	this.debug("CHRCTR_PREPAR_VALUE_1 = " + this.ds_cond00.getColumn(0 , "CNTRCTR_NO"));
	this.debug("CHRCTR_PREPAR_VALUE_2 = " + this.ds_cond00.getColumn(0 , "LAD_CODE"));
	



	this.ds_sms.clearData();
	var nRow = this.ds_sms.addRow();
	this.ds_sms.setColumn(0, "MBTLNO", mbtlno);
	this.ds_sms.setColumn(0, "NM", nm);
	this.ds_sms.setColumn(0, "CHANGE_WORD0", nm);
	this.ds_sms.setColumn(0, "CHANGE_WORD1", vcctNo);
	this.ds_sms.setColumn(0, "CHANGE_WORD2", billAmt);
	this.ds_sms.setColumn(0, "CHANGE_WORD3", accntSe);
	this.ds_sms.setColumn(0, "CHANGE_WORD4", ' ');
	this.ds_sms.setColumn(0, "CHRCTR_PREPAR_VALUE_0" , "2");  // 대기자 : 1 , 계약자 : 2
	this.ds_sms.setColumn(0, "CHRCTR_PREPAR_VALUE_1" , this.ds_cond00.getColumn(0 , "CNTRCTR_NO"));  // 고객 번호
	this.ds_sms.setColumn(0, "CHRCTR_PREPAR_VALUE_2" , this.ds_cond00.getColumn(0 , "LAD_CODE"));  // 대기자 코드

	this.gfn_popup("COM010700_P01.xfdl"
			   , 1000
			   , 600
			   , "SMS발송" 
			   , {
					CONTENT : content,
					MXTR_WRD0 : "고객명",
					MXTR_WRD1 : "계좌번호(은행:계좌)",
					MXTR_WRD2 : "청구금액",
					MXTR_WRD3 : "청구종류",
					MXTR_WRD4 : "",
					JOB_SE : "SLS001",
					ds : this.ds_sms
				 }
			   , "com::COM010700_P01.xfdl"
			   , "sms_callback", true);
}


/*
 * 종료버튼
*/
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}
