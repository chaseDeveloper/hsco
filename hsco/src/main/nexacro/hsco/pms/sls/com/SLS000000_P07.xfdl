/*
 * 함수명  	: fn_search
 * 설명 		: 신청자 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		// 주택코드
		case "houseCodeList" :
				var sController = "hsco/pms/sls/hou/rci/SLS040500/houseCodeList.do";
				var sInDatasets = "";
				var sOutDatasets = "ds_houseCodeList=output1";
				break;		
		
		case "selectSlaryList": // 기타수입 조회 		
				var sController   	= "hsco/pms/sls/com/SLS000000/P005.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_mt_rntchrg=output1";
				break;
				
		case "selectSlaryList2": // 사용자별 가상계좌 종류
				var sController   	= "hsco/pms/sls/com/SLS000000/P02.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_virtual=output1";
				break;	
				
		case "selectSlaryList3": // 가상계좌
				var sController   	= "hsco/pms/rnt/rnt030306/P4.do";
				var sInDatasets   	= "input4=ds_cond00";
				var sOutDatasets  	= "ds_receipt=output4";
				break;				
				
		case "selectSlaryList4": // 가상계좌 (va_vill)
				this.ds_bill.clearData();
				this.ds_bill_detail.clearData();
			
				var sController   	= "hsco/pms/sls/com/SLS000000/P5.do";
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_bill=output1";
				break;			
														
		case "selectSlaryList5": // 가상계좌 (va_vill_detail)
				this.ds_cond01.clearData();
				var nRow = this.ds_cond01.addRow();
				this.ds_cond01.setColumn(nRow , "CRT_ID" , this.ds_bill.getColumn(this.ds_bill.rowposition , "CRT_ID"));
				this.ds_cond01.setColumn(nRow , "TR_NO" , this.ds_bill.getColumn(this.ds_bill.rowposition , "TR_NO"));
				this.ds_cond01.setColumn(nRow , "BANK_CD" , this.ds_bill.getColumn(this.ds_bill.rowposition , "BANK_CD"));
				
				var sController   	= "hsco/pms/sls/com/SLS000000/P6.do";
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_bill_detail=output1";
				break;			
				
		case "selectSlaryList6": // 프로시저(임대료 , 지원금 납부요청 프로시저)
				//this.debug("start selectSlaryList6");
				var sController   	= "hsco/pms/sls/com/SLS000000/P7.do";
				var s  = "input1=ds_mt_rntchrg02:U"
				       + " input2=ds_mt_rntchrg01:U"
				       + " input3=ds_mt_rntchrg03:U";
				var sInDatasets   	=  s;
				var sOutDatasets  	= "";
				break;		
				
		case "selectSlaryList7": // 은행콤보 
				//this.debug("start selectSlaryList7");
				var sController   	= "hsco/pms/sls/com/SLS000000/P8.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_va=output1";
				break;																											

		case "selectBillPrintList":		// 납부서출력
			var sController   	= "/hsco/pms/sls/com/SLS000000/selectBillPrintList.do";
			var sInDatasets   	= "input1=ds_cond_report";
			var sOutDatasets  	= "ds_billPrintList=output1";
			break;	
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 	

}
 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{		
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		
		var comboOption = null;
		var targetCombo = null;
		switch(strSvcId){
		
			// 미납내역
			case "selectSlaryList" :
			
				//납부진행내역 전체 옵션 Default값 - 체크
				if(strSvcId == "selectSlaryList"){
					for(var i=0; i < this.ds_mt_rntchrg.rowcount; i++){
						this.ds_mt_rntchrg.setColumn(i ,"CHK" ,'1');
					}
				}
	
				this.fn_transaction("selectSlaryList3");	
				break;
				
			// 사용자별 가상계좌 종류
			case "selectSlaryList2" :
				
				this.fn_transaction("selectSlaryList7");	
// 				for(var i = 0 ; i < this.ds_virtual.rowcount ; i++)
// 				{
// 					var nRow = this.ds_va.addRow();
// 					
// 					this.ds_va.setColumn(nRow , "BANK_NM" , this.ds_virtual.getColumn( i , "BANK_NM"));
// 					this.ds_va.setColumn(nRow , "VACCT_NO" , this.ds_virtual.getColumn( i , "VACCT_NO"));
// 					this.ds_va.setColumn(nRow , "BANK_CD" , this.ds_virtual.getColumn( i , "BANK_CD"));
// 					this.ds_va.setColumn(nRow , "ALL_NM" , '[' + this.ds_va.getColumn( i , "BANK_NM") + '] ' + this.ds_virtual.getColumn( i , "VACCT_NO"));
// 				}  
				break;
				
			// 가상계좌
			case "selectSlaryList3" :
				this.fn_transaction("selectSlaryList2");	
				break;
				
			// 가상계좌
			case "selectSlaryList4" :
				if(this.ds_va.rowcount > 0) 
				{
					this.fn_transaction("selectSlaryList5");	
				}
				break;

			// 가상계좌
			case "selectSlaryList6" :
			//this.debug("cb selectSlaryList6");
				this.alert("납부요청 완료");
				//this.Button05_onclick();	
				this.fn_transaction("selectSlaryList7");
				break;

			// 은행 계좌번호	
			case "selectSlaryList7" :
				this.Combo00.set_index(0);
				this.Combo00_onitemchanged(this.Combo00);
				break;

			case "selectBillPrintList":
				this.fn_billPrint();
				break;
				
			default :
				break;
		}
		
	}else{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	}	
}

