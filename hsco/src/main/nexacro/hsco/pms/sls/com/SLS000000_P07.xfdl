 * return Type 	: None
 * 사용법		: this.Button01_onclick();
 */
this.Button01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_bill.rowcount == 0){
		this.alert("출력할 내역이 없습니다.");
		return;
	}

	this.ds_cond_report.clearData();
	this.ds_cond_report.addRow();
	this.ds_cond_report.setColumn(0, "BANK_CD",  	this.ds_bill.getSelectColumn("BANK_CD"));		// 은행코드
	this.ds_cond_report.setColumn(0, "VACCT_NO", 	this.ds_bill.getSelectColumn("VACCT_NO"));		// 가상계좌번호
	this.ds_cond_report.setColumn(0, "CNTRCTR_NO", 	this.ds_cond00.getColumn(this.ds_cond00.rowposition , "CNTRCTR_NO"));	// 계약자번호

	this.fn_transaction("selectBillPrintList");
}

