	var param = {
	               arg_0 : this,
	               arg_1 : this.ds_param
	             }
	
	this.gfn_popup("popup_acntCode"
				   , 351
				   , 176
				   , "계정코드팝업"
				   , param
				   , "pms_frm::FRM02010100_P01.xfdl"
				   , "fn_acntCodeCallback" );
   }
}
