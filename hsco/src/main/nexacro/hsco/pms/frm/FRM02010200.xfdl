{
	
	this.ds_frmDelngTy.setColumn(this.ds_frmDelngTy.rowposition,"DEL_CHK","Y");
	this.ds_frmDelngTy.deleteRow(this.ds_frmDelngTy.rowposition);
	this.fn_snInit();
}


   
