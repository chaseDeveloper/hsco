		this.gfn_message("info.처리불가","결산마감된 자료입니다.");
		return;
	}
      */
	if(this.gfn_message("comm.행위여부", "["+this.ds_schVal.getColumn(0,"accnutYear")+"]년 이월처리를 실행") ){
		this.fn_transaction("cyfdProc");
	}
}

   
