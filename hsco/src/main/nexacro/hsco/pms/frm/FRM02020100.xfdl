 * 사용법		: 
 */
this.chkfrmEndwChit = function(){
	var trow = this.ds_frmEndwChit.getRowCount();
    if(trow < 1)
    {
       var accnutYear        = this.ds_frmEndwInDecsn.getColumn(0,"ACCNUT_YEAR");            // 회계년도   
       var endwIncmeDescnNo  = this.ds_frmEndwInDecsn.getColumn(0,"ENDW_INCME_DECSN_NO"); // 수입결의번호
       var today             = this.dateUtils.today();
       
       var nRow = this.ds_frmEndwChit.addRow();
     
       this.ds_frmEndwChit.setColumn(nRow,"ACCNUT_YEAR",accnutYear);
       this.ds_frmEndwChit.setColumn(nRow,"ENDW_INCME_DECSN_NO",endwIncmeDescnNo);
       this.ds_frmEndwChit.setColumn(nRow,"CHIT_DE" ,today);
    }
	
