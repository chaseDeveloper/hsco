/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";
include "lib::comInclude.xjs";
include "lib::comInnerSanct.xjs";

var year = this.gfn_today("NOW_YEAR");;
var today= this.dateUtils.today();

this.FRM02020100_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}

this.FRM02020100_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{

  this.div_search.cal_incmeFrDt.set_value(today);
  this.div_search.cal_incmeToDt.set_value(today);

  this.fn_load_combo();
  
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function() 
{
	this.gfn_initCondDs(this, this.div_search);
}


/*
 * 함수명  	: fn_insert
 * 설명 		: 추가
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_insert = function(){
	 var cnt = 0;
     var totAmount = 0;
     
     if( this.ds_frmEndwInDecsn.isUpdated() == false && this.ds_frmEndwChit.isUpdated() == false
	     &&  this.ds_frmEndwChitDetail.isUpdated() == false )
	  {
	  }else{
	     	
			 if(this.gfn_message("comm.행위여부","변경중인 내역이 존재합니다. \n진행"))
				 {   	  
					}else{
						return false;
				 } 
	  }
     
	 for(i = 0 ; i < this.ds_rntLfstsRciv.getRowCount() ; i++)
	 {
		  var chk = this.ds_rntLfstsRciv.getColumn(i,"CHK");
		  if(chk == "1") 
		  { cnt++;
		    totAmount = totAmount + Number(this.ds_rntLfstsRciv.getColumn(i,"RCIV_AMOUNT"));
		  }
	 }
 
   if(cnt < 1)
   {
      this.gfn_message("comm.항목.선택.요청","수입결의 대상");
      return;
   }else{
      this.fn_inDcsnProc(cnt,totAmount); 
   }
 }


/*
 * 함수명  	: fn_autoJrnlz
 * 설명 		: 자동분개
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_autoJrnlz = function(){
	var accnutYear  = this.ds_frmEndwInDecsn.getColumn(0,"ACCNUT_YEAR");    // 회계년도
	var delngTycode = this.ds_frmEndwInDecsn.getColumn(0,"DELNG_TY_CODE");  // 거래유형코드
	
	if(delngTycode == "" || delngTycode == null)
	{
	    this.gfn_message("info.항목.선택.알림","거래유형")
	    return;
	}
	 
	
	
	// 분개정보 조회
   
    this.ds_schVal.setColumn(0,"accnutYear",accnutYear);
    this.ds_schVal.setColumn(0,"delngTyCode",delngTycode);
    this.ds_frmDelngTy.clearData();
    this.fn_transaction("selectfrmDelngTyInfo");
	
}

/*
 * 함수명  	: fn_inDcsnProc
 * 설명 		: 수입결의생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_inDcsnProc = function(rowCount,totAmount){
	
   // 초기화
   this.ds_frmEndwInDecsn.clearData();
   this.ds_frmEndwInDeTarList.clearData();
   this.ds_frmEndwChit.clearData();
   this.ds_frmEndwChitDetail.clearData();
  
   
   var userId = this.gfn_getUserId();
   var userNm = this.gfn_getUserName();
   
 
   

   var nRow = this.ds_frmEndwInDecsn.addRow();
   

   
   this.ds_frmEndwInDecsn.setColumn(nRow,"SN",1); 
   this.ds_frmEndwInDecsn.setColumn(nRow,"ACCNUT_YEAR",year); 
   this.ds_frmEndwInDecsn.setColumn(nRow,"INCME_SE","1");    // 현재 임대료만 처리
   this.ds_frmEndwInDecsn.setColumn(nRow,"IN_CNT",rowCount);  // 수입건수
   this.ds_frmEndwInDecsn.setColumn(nRow,"INCME_DECSN_AMOUNT",totAmount);  // 수입금액합계
   this.ds_frmEndwInDecsn.setColumn(nRow,"DECSN_DE",today);  // 결의일자
   this.ds_frmEndwInDecsn.setColumn(nRow,"ITNC_DE",today);  // 발의일자
   this.ds_frmEndwInDecsn.setColumn(nRow,"ITNC_MAN_ID",userId);  // 발의자
   this.ds_frmEndwInDecsn.setColumn(nRow,"ITNC_MAN_NM",userNm);  // 발의자명
   
  
   
   for(i = 0 ; i < this.ds_rntLfstsRciv.getRowCount() ; i++)
   {
       var chk = this.ds_rntLfstsRciv.getColumn(i,"CHK");
       if(chk == "1")
       {
          this.fn_addList(i);
       }
   }
   
}
/*
 * 함수명  	: fn_inDcsnProc
 * 설명 		: 수입결의대상 수납리스트생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_addList = function(targRow)
{   
     
   var rcivDe    = this.ds_rntLfstsRciv.getColumn(targRow,"RCIV_DE");             // 수납일자
   var cntrctrNo = this.ds_rntLfstsRciv.getColumn(targRow,"CNTRCTR_NO");          // 계약자번호
   var rcivNo    = this.ds_rntLfstsRciv.getColumn(targRow,"RCIV_NO");             // 수납번호
   var incmeSe    = this.ds_rntLfstsRciv.getColumn(targRow,"INCME_SE");           // 수입구문
   var rcivAmount    = this.ds_rntLfstsRciv.getColumn(targRow,"RCIV_AMOUNT");     // 수입금액
   var areaSe    = this.ds_rntLfstsRciv.getColumn(targRow,"AREA_SE");             // 지역구분
   var dong    = this.ds_rntLfstsRciv.getColumn(targRow,"DONG");                  // 동
   var ho    = this.ds_rntLfstsRciv.getColumn(targRow,"HO");                      // 호
   var dedtDe    = this.ds_rntLfstsRciv.getColumn(targRow,"DEDT_DE");             // 납기일자
   
   var nRow = this.ds_frmEndwInDeTarList.addRow();
   
   this.ds_frmEndwInDeTarList.setColumn(nRow,"SN",nRow + 1);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"RCIV_DE",rcivDe);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"CNTRCTR_NO",cntrctrNo);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"RCIV_NO",rcivNo);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"INCME_SE",incmeSe);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"RCIV_AMOUNT",rcivAmount);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"AREA_SE",areaSe);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"DONG",dong);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"HO",ho);
   this.ds_frmEndwInDeTarList.setColumn(nRow,"DEDT_DE",dedtDe);

   
}
 


/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	
	
	this.ds_rntLfstsRciv.clearData();
	

	this.Div00.grd_rntLfstsRciv.setCellProperty("head",14,"text","0");
	
	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.fn_transaction("selectRntLfstsRciv");
}

