/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {		
		
	   case "selectRntLfstsRciv":		//임대료 수입결의대상조회
			var sController   	= "/hsco/pms/frm/FRM02020100/selectRntLfstsRciv.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rntLfstsRciv=output1";
			break;
	   case "selectfrmDelngTyInfo":		//거래유형코드별 정보조회(분개정보)
			var sController   	= "/hsco/pms/frm/FRM02010200/selectfrmDelngTyInfo.do"; 
			var sInDatasets   	= "input1=ds_schVal";
			var sOutDatasets  	= "ds_frmDelngTy=output1";
			break; 
	   case "saveFrmEndwInDecsn":		//기타수입결의 저장
			var sController   	= "/hsco/pms/frm/FRM02020100/saveFrmEndwInDecsn.do"; 
			var sInDatasets   	= "input1=ds_frmEndwInDecsn:U input2=ds_frmEndwChit:U input3=ds_frmEndwChitDetail:A input4=ds_frmEndwInDeTarList:U";
			var sOutDatasets  	= "ds_retunEdNo=output1";
			break; 
	  case "selectFrmEndwInDecsn":		// 수입결의 조회
			var sController   	= "/hsco/pms/frm/FRM02020100/selectFrmEndwInDecsn.do"; 
			var sInDatasets   	= "input1=ds_schDcsn";
			var sOutDatasets  	= "ds_frmEndwInDecsn=output1";
			break; 
	  case "selectFrmEndwChit":		// 수입결의 전표 조회
			var sController   	= "/hsco/pms/frm/FRM02020100/selectFrmEndwChit.do"; 
			var sInDatasets   	= "input1=ds_schDcsn";
			var sOutDatasets  	= "ds_frmEndwChit=output1";
			break; 
	  case "selectFrmEndwChitDetail":	//기타수입결의 전표상세 조회
			var sController   	= "/hsco/pms/frm/FRM02020100/selectFrmEndwChitDetail.do"; 
			var sInDatasets   	= "input1=ds_schChit";
			var sOutDatasets  	= "ds_frmEndwChitDetail=output1";
			break; 	
	  case "GetInnerSanctnId": // 내부결재id 조회   
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
			break;
	  case "saveInnerSanctionId":  // 내부결재 번호 저장   
			var sController   	= "/hsco/pms/frm/FRM02020100/saveInnerSanctionId.do";
			var sInDatasets   	= "input1=ds_frmEndwInDecsn:U";
			var sOutDatasets  	= "";
		    break;
	   case "innerSanctnUbiDataset":  // 임대료 수입결의서 조회
			var sController   	= "/hsco/pms/frm/FRM02020100/innerSanctnUbiDataset.do";
			var sInDatasets   	= "input1=ds_cond02"; 
			var sOutDatasets  	= "ds_ubiDataset=output1 ds_ubiDatasetDetail=output2"
		    break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
			case "saveFrmEndwInDecsn":
				    
				    this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
                   
				    if(this.ds_retunEdNo.getRowCount() > 0)
				    {
				      
				      var decsnNo = this.ds_retunEdNo.getColumn(0,"ENDW_INCME_DECSN_NO");
				      var acntYear = this.ds_retunEdNo.getColumn(0,"ACCNUT_YEAR");
				      
	                  this.ds_schDcsn.setColumn(0,"acntYear",acntYear);
	                  this.ds_schDcsn.setColumn(0,"endwInDecsnNo",decsnNo);
	                 }
	                 
			
				      this.ds_frmEndwInDecsn.clearData();
				      this.fn_transaction("selectFrmEndwInDecsn");
				    
				    
				    this.fn_search();
				    
				  break;
			case "":
				 this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
		    case "selectfrmDelngTyInfo":  
			      this.fn_jrnlzInfoSet(); // 분개정보 적용
			  break;
			 case "selectFrmEndwChit":  
			      this.fn_chitDetail(); // 전표상세조회
			  break;
			 case "selectFrmEndwInDecsn":   
			       this.ds_frmEndwChit.clearData();
	               this.fn_transaction("selectFrmEndwChit");
	           break;
			 case "GetInnerSanctnId":  // 내부결재
				this.fn_popup_InnerSanctionDrft();
				break;
			 case "saveInnerSanctionId":  // 내부결재번호 저장
			    this.ds_frmEndwInDecsn.clearData();
			    this.fn_transaction("selectFrmEndwInDecsn");
				this.fn_search();
				break;
			 case "innerSanctnUbiDataset":  // 임대료 수입결의서 조회
				this.fn_printProc();
			 break;
		}
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


// 일괄등록
this.Div00_grd_rntLfstsRciv_onheadclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var type = obj.getCellProperty( "Head",e.cell,"edittype" );
	var text = obj.getCellProperty( "Head",e.cell,"text");
	
	
	if(type == "checkbox")
	{	  
		   
		  obj.binddataset = "";
		  
		  for(var i = 0 ; i < this.ds_rntLfstsRciv.getRowCount() ; i++)
		  {
				this.ds_rntLfstsRciv.setColumn(i,"CHK",Math.abs(Number(text)-1));
			   
		  }
		  obj.binddataset = "ds_rntLfstsRciv";
		  
		  obj.setCellProperty("Head",e.cell,"text",Math.abs(Number(text)-1));
		    
		
	}
	
}

