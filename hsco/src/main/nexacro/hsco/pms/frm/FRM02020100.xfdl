	var deAmount = [0,0,0];
	var crAmount = [0,0,0];

	deAmount[0] = this.ds_frmEndwInDecsn.getColumn(0,"INCME_DECSN_AMOUNT");
	crAmount[1] = this.ds_frmEndwInDecsn.getColumn(0,"INCME_DECSN_AMOUNT");
	
	
    for(var i = 0 ; i < totCnt ; i++)
