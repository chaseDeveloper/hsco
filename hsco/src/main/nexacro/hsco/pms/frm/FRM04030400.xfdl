 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{

	this.fn_get_frmCmmCode(this.ds_execYearSe, "", {grpCode:"FRM125"});  //대출실행연도구분코드
	this.fn_get_frmCmmCode(this.ds_ldmySe, "", {grpCode:"FRM126"});  //대출금구분코드


				
}

