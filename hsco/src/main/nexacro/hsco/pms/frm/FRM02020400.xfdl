/*
 * 함수명  	: fn_jrnlzInfoSet
 * 설명 		: 분개정보적용
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_jrnlzInfoSet();
 */
this.fn_jrnlzInfoSet = function(){
	
	var totCnt = this.ds_frmDelngTy.getRowCount();
	
	if(totCnt < 1 ){
	 this.gfn_message("info.처리불가","해당 거래유형에 분개정보가 존재하지않습니다.");
	 return;
	}

	this.chkfrmEndwChit(); // 전표기본 확인
	this.ds_frmEndwChitDetail.clearData();
	
	var deAmount = [0,0];
	var crAmount = [0,0];

	deAmount[0] = this.ds_overrpayDecsn.getColumn(0,"OVERRPAY_AMOUNT");
	crAmount[1] = this.ds_overrpayDecsn.getColumn(0,"OVERRPAY_AMOUNT");
	
	
    for(var i = 0 ; i < totCnt ; i++)
    {
       var nRow        = this.ds_frmEndwChitDetail.addRow();
       var accnutYear  = this.ds_frmDelngTy.getColumn(i,"ACCNUT_YEAR");            // 회계년도
       var chitNo      = this.ds_frmEndwChit.getColumn(0,"CHIT_NO");               // 전표번호
       var dbrtSe      = this.ds_frmDelngTy.getColumn(i,"DBRT_SE");                // 차화성분
       var acntCode    = this.ds_frmDelngTy.getColumn(i,"ACNT_CODE");              // 계정코드
       var acntctgrNm  = this.ds_frmDelngTy.getColumn(i,"ACNTCTGR_NM");            // 계정명
      
       this.ds_frmEndwChitDetail.setColumn(nRow,"SN", i + 1);
       this.ds_frmEndwChitDetail.setColumn(nRow,"ACCNUT_YEAR",accnutYear);
       this.ds_frmEndwChitDetail.setColumn(nRow,"CHIT_NO",chitNo);
       this.ds_frmEndwChitDetail.setColumn(nRow,"DBRT_SE",dbrtSe);
       this.ds_frmEndwChitDetail.setColumn(nRow,"ACNT_CODE",acntCode);
       this.ds_frmEndwChitDetail.setColumn(nRow,"ACNTCTGR_NM",acntctgrNm);
       this.ds_frmEndwChitDetail.setColumn(nRow,"DEBTOR_AMOUNT",deAmount[i]);
       this.ds_frmEndwChitDetail.setColumn(nRow,"CRDIT_AMOUNT" ,crAmount[i]);
       this.ds_frmEndwChitDetail.setColumn(nRow,"SUMRY","");
    }
     
    
}


/*
 * 함수명  	: fn_snInit
 * 설명 		: 전표기본 확인
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.chkfrmEndwChit = function(){
	var trow = this.ds_frmEndwChit.getRowCount();
    if(trow < 1)
    {
       var accnutYear        = this.ds_overrpayDecsn.getColumn(0,"ACCNUT_YEAR");            // 회계년도   
       var endwIncmeDescnNo  = this.ds_overrpayDecsn.getColumn(0,"ENDW_INCME_DECSN_NO"); // 수입결의번호
       var overrPayDecsnNo   = this.ds_overrpayDecsn.getColumn(0,"OVERRPAY_DECSN_NO"); // 과오납결의번호
       var today             = this.dateUtils.today();
       
       var nRow = this.ds_frmEndwChit.addRow();
     
       this.ds_frmEndwChit.setColumn(nRow,"ACCNUT_YEAR",accnutYear);
