/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {		
		
	   case "selectFrmoverrpayDecsnTarList":		//과오납대상 조회
			var sController   	= "/hsco/pms/frm/FRM02020400/selectFrmoverrpayDecsnTarList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_frmOverrpayDecsnTarList=output1";
			break; 
	  case "selectfrmDelngTyInfo":		//거래유형코드별 정보조회(분개정보)
			var sController   	= "/hsco/pms/frm/FRM02010200/selectfrmDelngTyInfo.do"; 
			var sInDatasets   	= "input1=ds_schVal";
			var sOutDatasets  	= "ds_frmDelngTy=output1";
			break; 
	  case "saveOverrpayDecsn":		//과오납결의 저장
			var sController   	= "/hsco/pms/frm/FRM02020400/saveOverrpayDecsn.do"; 
			var sInDatasets   	= "input1=ds_overrpayDecsn:U input2=ds_frmEndwChit:U input3=ds_frmEndwChitDetail:A";
			var sOutDatasets  	= "ds_retunEdNo=output1";
			break; 
	  case "selectOverrPayDecsnNo":		//과오납결의서 조회
			var sController   	= "/hsco/pms/frm/FRM02020400/selectOverrPayDecsnNo.do"; 
			var sInDatasets   	= "input1=ds_schDcsn";
			var sOutDatasets  	= "ds_overrpayDecsn=output1";
			break; 
	  case "selectFrmEndwChit":		// 과오납결의 전표 조회
			var sController   	= "/hsco/pms/frm/FRM02020400/selectFrmEndwChit.do"; 
			var sInDatasets   	= "input1=ds_schDcsn";
			var sOutDatasets  	= "ds_frmEndwChit=output1";
			break;
	  case "selectFrmEndwChitDetail":	// 과오납결의 전표상세 조회
			var sController   	= "/hsco/pms/frm/FRM02020400/selectFrmEndwChitDetail.do"; 
			var sInDatasets   	= "input1=ds_schChit";
			var sOutDatasets  	= "ds_frmEndwChitDetail=output1";
			break; 	
	  case "GetInnerSanctnId": // 내부결재id 조회   
		    var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
		    var sInDatasets   	= "input1=ds_recomInfo";
		    var sOutDatasets  	= "ds_recomInfo=output1";
			break;
	  case "saveInnerSanctionId":  // 내부결재 번호 저장   
			var sController   	= "/hsco/pms/frm/FRM02020400/saveInnerSanctionId.do";
			var sInDatasets   	= "input1=ds_overrpayDecsn:U";
			var sOutDatasets  	= "";
		    break;
	  case "selectReturnAnact":  // 반환결의서 
			var sController   	= "/hsco/pms/frm/FRM02020400/selectReturnAnact.do";
			var sInDatasets   	= "input1=ds_schPrint";
			var sOutDatasets  	= "ds_returnAnact=output1";
		    break;    
	
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
			case "saveOverrpayDecsn":
				    
				    this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
                   
				    if(this.ds_retunEdNo.getRowCount() > 0)
				    {
				      
				      var decsnNo = this.ds_retunEdNo.getColumn(0,"ENDW_INCME_DECSN_NO");
				      var overrPayDecsnNo = this.ds_retunEdNo.getColumn(0,"OVERRPAY_DECSN_NO");
				      var acntYear = this.ds_retunEdNo.getColumn(0,"ACCNUT_YEAR");
				      
	                  this.ds_schDcsn.setColumn(0,"acntYear",acntYear);
	                  this.ds_schDcsn.setColumn(0,"endwInDecsnNo",decsnNo);
	                  this.ds_schDcsn.setColumn(0,"overrPayDecsnNo",overrPayDecsnNo);   
	                  
	                 }
	                 
	                   
				      this.ds_overrpayDecsn.clearData();
				      this.fn_transaction("selectOverrPayDecsnNo");
				    
				    
				  break;
		    case "selectfrmDelngTyInfo":  
			      this.fn_jrnlzInfoSet(); // 분개정보 적용
			  break;
			 case "selectFrmEndwChit":  
			      this.fn_chitDetail(); // 전표상세조회
			  break;
			case "selectOverrPayDecsnNo":   
			       this.ds_frmEndwChit.clearData();
	               this.fn_transaction("selectFrmEndwChit");
	        break;
	        case "GetInnerSanctnId":  // 내부결재
				this.fn_popup_InnerSanctionDrft();
		    break;
		    case "saveInnerSanctionId":  // 내부결재번호 저장
				this.ds_overrpayDecsn.clearData();
				this.fn_transaction("selectOverrPayDecsnNo");
				this.fn_search();
			break;
			case "selectReturnAnact":  // 반환결의서 조회(출력)
				this.fn_printProc();
			break;
		    
		}
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


