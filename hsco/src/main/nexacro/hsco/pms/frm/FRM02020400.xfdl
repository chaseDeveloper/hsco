 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";
include "lib::comInclude.xjs";
include "lib::comInnerSanct.xjs";

this.FRM02020400_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}

this.FRM02020400_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var year = this.gfn_today("NOW_YEAR");
	this.div_search.spn_acntYear.set_value(year);
	this.fn_load_combo();
	
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{

	this.fn_get_frmCmmCode(this.ds_sancSe, "전체", {grpCode:"COM022"});//결재구분
    this.fn_get_frmCmmCode(this.ds_dbrtSe,"", {grpCode:"FRM002"}); //기금차화성분
    this.fn_get_frmCmmCode(this.ds_areaSe,"", {grpCode:"RNT004" , value:"1"});    //지역구분
	this.fn_get_frmCmmCode(this.ds_incmeSe,"", {grpCode:"FRM007"}); //기금수입구분
	this.fn_get_frmCmmCode(this.ds_bank,"", {grpCode:"COM021"});    //은행코드
		
}

/*
 * 함수명  	: fn_print
 * 설명 		: 결의서 출력
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_print();
  */
this.fn_print = function()
{
    var accnutYear =  this.ds_overrpayDecsn.getColumn(0,"ACCNUT_YEAR");
    var overrpayDecsnNo =  this.ds_overrpayDecsn.getColumn(0,"OVERRPAY_DECSN_NO");
    var prtGb = this.Div00.chk_prtGb.value;
   
    if(overrpayDecsnNo == undefined || overrpayDecsnNo == null)
	{
	  this.gfn_message("info.처리불가","과오납결의 진행후 \n출력하시기 바랍니다.");
	  return;
	}
	
	
	 
	this.ds_schPrint.setColumn(0,"ACCNUT_YEAR",accnutYear);
    this.ds_schPrint.setColumn(0,"OVERRPAY_DECSN_NO",overrpayDecsnNo);
    
    this.ds_returnAnact.clearData();
    this.fn_transaction("selectReturnAnact");
	
}

this.fn_printProc = function()
{
    var prtGb = this.Div00.chk_prtGb.value;
    
    switch(prtGb){		
		
	   case "1":		// 반환청구서
            	var vOpts = new this.cf_Opts();
				vOpts.setToolbar(false);
				vOpts.setScale(100);
				vOpts.setReportPath("/pms/frm/FRM02020400_R1.jrf");
				vOpts.setSaveFilename("과오납반환청구서");
				vOpts.setLocalDs(["ds_returnAnact", this.ds_returnAnact]);
				var parm = {
					arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
					opts : vOpts
				};

				this.gfn_popup("Popup_Report3", 884, 735, "과오납금 반환 결의서", parm, "common::frmReportPopup.xfdl", "report_callback");	
			break; 
