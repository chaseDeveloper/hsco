/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function(){
   
	// 과오납결의
	if(this.ds_overrpayDecsn.isUpdated() == true){
	
	   if(this.gfn_checkValidation(this.ds_overrpayDecsn, this.ds_validation) == false){  
		  return;
	   }
	}

	
	// 기금전표
	if(this.ds_frmEndwChit.isUpdated() == true){
	
	   if(this.gfn_checkValidation(this.ds_frmEndwChit, this.ds_validation) == false){  
		  return;
	   }
	}
	
	// 기금전표 상세
	if(this.ds_frmEndwChitDetail.isUpdated() == true){
	
	   if(this.gfn_checkValidation(this.ds_frmEndwChitDetail, this.ds_validation) == false){  
		  return;
	   }
	}
	
	 
	if( this.ds_overrpayDecsn.isUpdated() == false && this.ds_frmEndwChit.isUpdated() == false
	   &&  this.ds_frmEndwChitDetail.isUpdated() == false )
	{
	    this.gfn_message("comm.내역.변경.없음");
	    
	    return;
	}
	
	 if(this.gfn_message("comm.행위여부","저장"))
	 { 
	      this.ds_retunEdNo.clearData();
	      this.fn_transaction("saveOverrpayDecsn");
	   
	    
	 }else{
	    return;
	 }           
}


