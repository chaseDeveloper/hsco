this.fn_sanction = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var curRow = this.ds_frmEndwInDecsn.rowposition;
	var decsnNo = this.ds_frmEndwInDecsn.getColumn(curRow,"ENDW_INCME_DECSN_NO");
	
	if(decsnNo == undefined || decsnNo == "" || decsnNo == null)
	{
