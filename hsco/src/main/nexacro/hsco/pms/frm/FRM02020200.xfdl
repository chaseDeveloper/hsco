	   return;
	}
		
	var sanctnNo = this.ds_frmEndwInDecsn.getColumn(curRow,"INNER_SANCTN_NO");
	var sanctnStt = this.ds_frmEndwInDecsn.getColumn(curRow,"SANCTN_STTUS");
		
	switch(sanctnStt){
	case "T12":
	case "T05":
	case "T07":
	case "T11":
	case "T14":
	case "T15":
		/*
		if(this.div_search.sta_sanctnForm.visible==false){
			if(this.gfn_message("comm.행위여부","내부결재 재상신을 진행") ){
				this.ds_expDsnMastr.setColumn(0, "SANCTN_NO", "");
				this.ds_expDsnMastr.setColumn(0, "SANCTN_SE", "");
				
				this.div_search.sta_sanctnForm.set_visible(true);
				this.div_search.cmb_sanctnForm.set_visible(true);
				this.div_search.sta_sanctn.set_visible(false);
				this.div_search.cmb_sanctn.set_visible(false);
				this.gfn_message("comm.항목.선택.요청", "결재양식");
			}
			break;
		}*/
	default:
		var innerSanctnMdlId = "FRM-100046" // 내부결재모형ID
		var sysSeCode = "SYS";
		var jobSe = "PMS";
		var svcId = "GetInnerSanctnId";
		
		var key = this.ds_frmEndwInDecsn.getColumn(curRow, "ACCNUT_YEAR") + "^" + this.ds_frmEndwInDecsn.getColumn(curRow, "ENDW_INCME_DECSN_NO");
