this.ds_frmEndwInDecsn_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	    if(e.columnid == "SUPLY_AMOUNT" || e.columnid == "VAT" || e.columnid == "ADVPAY_CRRX")
	    {
	         var calAmount = 0;
	         var suplyAmount = obj.getColumn(e.row,"SUPLY_AMOUNT");
	         var vat         = obj.getColumn(e.row,"VAT");
	         var advpayCrrx  = obj.getColumn(e.row,"ADVPAY_CRRX");
	       
	         calAmount = Number(suplyAmount) + Number(vat) - Number(advpayCrrx);  
	         
	         obj.setColumn(e.row,"INCME_DECSN_AMOUNT",calAmount);     //결의금액
	         obj.setColumn(e.row,"INCME_PREARNGE_AMOUNT",calAmount);  //수입금액
	         
	    }
}

