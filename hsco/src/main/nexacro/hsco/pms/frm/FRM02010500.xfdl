 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	this.ds_frmEmdwFnlt.clearData();
	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.fn_transaction("selectFrmEmdwFnlt");
}



/*
