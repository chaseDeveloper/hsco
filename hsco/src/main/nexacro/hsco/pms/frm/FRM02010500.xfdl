	    return;
	}
	
	var nRow = this.ds_frmEmdwFnltSpot.addRow();
    var endwFnltCd = this.ds_frmEmdwFnlt.getColumn(this.ds_frmEmdwFnlt.rowposition,"ENDW_FNLT_CODE");
   
    this.ds_frmEmdwFnltSpot.setColumn(nRow,"ENDW_FNLT_CODE",endwFnltCd);

}

