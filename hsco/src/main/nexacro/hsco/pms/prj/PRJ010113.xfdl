/**
* 설명 : 행추가
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Add_onclick()
*/

this.btn_Add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	//필수 데이터 확인	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	  switch (obj.name) {
	  case "btn_Add01":
		
		
		var nRow = this.ds_mtrlSpl.addRow();
		this.ds_mtrlAdtg.clearData();
		this.ds_fileList.clearData();
		
		if(!Ex.util.isUpdated(this.ds_cond)){
			this.ds_mtrlSpl.setColumn(nRow, "CNTRCT_NO", this.ds_cond.getColumn(0, "CNTRCT_NO"));
			this.ds_mtrlSpl.setColumn(nRow, "CNTRCT_NM", this.ds_cond.getColumn(0, "CNTRCT_NM"));
			this.ds_mtrlSpl.setColumn(nRow, "RQS_DT",		this.comUtils.getClientDate("YYYYMMDD"));
			this.ds_mtrlSpl.setColumn(nRow, "REGISTER_NM", this.fn_getEmpnm());
		}
		break;
		
	  case "btn_Add02":
		var bzRow = this.ds_mtrlSpl.rowposition;
		if(bzRow < 0){
			this.gfn_message("comm.항목.선택.요청", "사급자재공급원");
			return false;
		}
		
		if(this.ds_mtrlSpl.getRowType(this.ds_mtrlSpl.rowposition) ==  2){
			this.gfn_message("comm.정보.저장.요청", "사급자재공급원");
			return false;
		}
		
		var nRow = this.ds_mtrlAdtg.addRow();
		this.ds_mtrlAdtg.setColumn(nRow, "CNTRCT_NO", 	 this.ds_mtrlSpl.getColumn(bzRow, "CNTRCT_NO"));
		this.ds_mtrlAdtg.setColumn(nRow, "CNTRCT_NM", 	 this.ds_mtrlSpl.getColumn(bzRow, "CNTRCT_NM"));
		this.ds_mtrlAdtg.setColumn(nRow, "MTRL_SN",		 this.ds_mtrlSpl.getColumn(bzRow, "MTRL_SN"));
		break;	
	  }
			
}

/**
* 설명 : 행삭제
* 파라미터	: obj:Button,  e:ClickEventInfo
