/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/


/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(계약목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_mtrlSpl.clearData();
	this.ds_mtrlAdtg.clearData();
	this.ds_fileList.clearData();
	
	this.fn_transaction("selectDsMain");
}


/*
 * 함수명  	: fn_searchSub
 * 설명 		: 조회(검사입회내역, 사급자재공급원내역)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_searchSub();
 */
this.fn_searchSub = function()
{
	this.ds_mtrlAdtg.clearData();
	this.ds_fileList.clearData();
	
	this.fn_transaction("selectDsSub");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (   !this.comUtils.isDatasetUpdated(this.ds_mtrlAdtg)	// 참여기술자 변경
		&& !this.comUtils.isDatasetUpdated(this.ds_mtrlSpl)	// 사급자재 공급원
	   ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	//validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_mtrlSpl, this.ds_validation)) {	// 사급자재 공급원
		return;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_mtrlAdtg, this.ds_validation)) {		// 검사자
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		saveRow1 = this.ds_mtrlSpl.rowposition;
		saveRow2 = this.ds_mtrlAdtg.rowposition;
		this.fn_transaction("saveDsMain");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectDsMain":
			var sController   	= "hsco/pms/prj/PRJ010113/selectDsMain.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_mtrlSpl=output1";
			break;
					
		case "selectDsSub":
			var sController   	= "hsco/pms/prj/PRJ010113/selectDsSub.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_mtrlAdtg=output1";
			break;
					
		case "saveDsMain":
			var sController   	= "hsco/pms/prj/PRJ010113/saveDsMain.do";
			var sInDatasets   	= "input1=ds_mtrlSpl:U input2=ds_mtrlAdtg:U";
			var sOutDatasets  	= "";
			break;
			
		// ======== 첨부파일 관련 transaction ========
		case "selectCtgryFile":
			var sController   	= "/hsco/pms/prj/PRJ010113/selectCtgryFile.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "saveCtgryFile":
			var sController   	= "/hsco/pms/prj/PRJ010113/saveCtgryFile.do";
			var sInDatasets   	= "input1=ds_ctgry_file:A";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "selectAtchFile":
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_cond_file";
			var sOutDatasets = "ds_fileList=output1";
			break;
		case "deleteFile":
			var sController  = "hsco/cmm/file/deleteFile.do";
			var sInDatasets  = "input1=ds_fileList:U";
			var sOutDatasets = "ds_fileList=output1";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(nErrorCode != 0) {
	  this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	if (nErrorCode != 0){
	} else {
		switch (strSvcId) {
			case "selectDsMain":
				if(saveRow1>-1){
					this.ds_mtrlSpl.set_rowposition(saveRow1);
					saveRow1 = -1;
				}
				break;
						
			case "selectDsSub":
				if(saveRow2>-1){
					this.ds_mtrlAdtg.set_useclientlayout(false);
					this.ds_mtrlAdtg.set_rowposition(saveRow2);
					this.ds_mtrlAdtg.set_useclientlayout(true);
					saveRow2 = -1;
				}
				this.ds_fileList.clearData();
				if(this.ds_mtrlSpl.getRowCount()>0){
					this.fn_transaction("selectMtrlSplFile");
				}
				this.ds_fileList.filter("USE_AT == '1'");
				break;
						
			case "saveDsMain":
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
				
			// ======== 첨부파일 관련 transaction ========
			case "selectCtgryFile":
				this.fn_transaction("selectDsSub");
				break;
			case "selectAtchFile":
				if(this.ds_fileList.getRowCount() > 0) {
					var fRow     = this.ds_fileList.rowposition;
					var fileSn   = this.ds_fileList.getColumn(fRow, "FILE_SN");
					var fileNm   = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
					var fileOrdr = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
					
					var cRow = this.ds_ctgry_file.rowposition;
					this.ds_ctgry_file.setColumn(cRow, "FILE_SN", fileSn);
					this.ds_ctgry_file.setColumn(cRow, "ORGINL_FILE_NM", fileNm);
					this.ds_ctgry_file.setColumn(cRow, "FILE_ORDR", fileOrdr);
					
					var docSn  = this.ds_ctgry_file.getColumn(cRow, "DOC_SN");
					var fileId = this.ds_ctgry_file.getColumn(cRow, "CTGRY_FILE_ID");
					if(this.comUtils.isNull(docSn) || this.comUtils.isNull(fileId)) {
						this.fn_transaction("saveCtgryFile");
					}
				}
				break;
			case "deleteFile":
				this.fn_transaction("selectCtgryFile");
				break;
			case "saveCtgryFile":
				var cfRow = this.ds_ctgry_file.rowposition;
				var docSn = this.ds_ctgry_file.getColumn(cfRow, "DOC_SN");
				
				this.ds_mtrlSpl.set_enableevent(false);
				this.ds_mtrlSpl.set_updatecontrol(false);
				var fRow = this.ds_mtrlSpl.rowposition;
				this.ds_mtrlSpl.setColumn(fRow, "DOC_SN", docSn);
				this.ds_mtrlSpl.set_updatecontrol(true);
				this.ds_mtrlSpl.set_enableevent(true);
				break;
		}
	}
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.fn_popupCallBack = function(strID, variant)
{
	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(variant);
	
	// 값이 존재하지 않을 경우에는 아무런 값을 덮어쓰지 않음
	if(!loadCnt) {
		return false;
	}
	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(variant);
	if(loadCnt) {
		switch(strID) {
		// 계약번호 선택(검색조건)
			case "CTR030100_P08":
				var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
				var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
				var cntrcyYr = cntrctNo.substr(0,4);
				
				this.ds_cond.setColumn(0, "CNTRCT_YEAR", cntrcyYr);	// 계약년도
				this.ds_cond.setColumn(0, "CNTRCT_NO",   cntrctNo);	// 계약번호
				this.ds_cond.setColumn(0, "CNTRCT_NM",   cntrctNm);	// 계약명
				this.fn_search();
				break;
			case "grd_mtrlSpl":
				var rPos     = this.ds_mtrlSpl.rowposition;
				var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
				var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
				var cntrcyYr = cntrctNo.substr(0,4);
				
				this.ds_mtrlSpl.setColumn(rPos, "CNTRCT_NO",   cntrctNo);	// 계약번호
				this.ds_mtrlSpl.setColumn(rPos, "CNTRCT_NM",   cntrctNm);	// 계약명
				break;
			case "grd_mtrlAdtg":
				var rPos = this.ds_mtrlAdtg.rowposition;
				this.ds_mtrlAdtg.setColumn(rPos, "ADTG_EMPNO",  dsTemp.getColumn(0, "EMPNO"));
				this.ds_mtrlAdtg.setColumn(rPos, "EMPNM", 		dsTemp.getColumn(0, "EMPNM"));
				this.ds_mtrlAdtg.setColumn(rPos, "DEPT_NM",		dsTemp.getColumn(0, "DEPT_NM"));
				this.ds_mtrlAdtg.setColumn(rPos, "OFCPS_SE", 	dsTemp.getColumn(0, "OFCPS_SE"));
				this.ds_mtrlAdtg.setColumn(rPos, "HR_CLSF_SE", 	dsTemp.getColumn(0, "HR_CLSF_SE"));
				break;
		}
	}
}

