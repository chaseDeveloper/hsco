/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.PRJ010113_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
}
this.PRJ010113_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
