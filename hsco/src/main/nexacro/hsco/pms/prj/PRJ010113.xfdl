	}
	
	  switch (obj.name) {
	  case "btn_Del01":
			if(this.ds_mtrlSpl.rowposition>-1){
				this.ds_mtrlSpl.deleteRow(this.ds_mtrlSpl.rowposition);
				
				this.ds_condSub.clearData();
				this.ds_mtrlAdtg.clearData();
				
				var nRow = this.ds_condSub.addRow();
				this.ds_condSub.setColumn(nRow, "CNTRCT_NO",   obj.getColumn(e.newrow, "CNTRCT_NO"));
				this.ds_condSub.setColumn(nRow, "MTRL_SN",	   obj.getColumn(e.newrow, "MTRL_SN"));
				this.ds_condSub.setColumn(nRow, "DOC_SN",      obj.getColumn(e.newrow, "DOC_SN"));
				
				this.fn_searchSub();
			}
			break;	
	  case "btn_Del02":
			if(this.ds_mtrlAdtg.rowposition>-1){
				this.ds_mtrlAdtg.deleteRow(this.ds_mtrlAdtg.rowposition);
			}
			break;
	  }
	
}


/**
* 설명 : 행취소
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Delete_onclick()
*/
this.btn_Cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	switch (obj.name) {
	  case "btn_Cancel01":
		if( Ex.util.isUpdated(this.ds_mtrlSpl) ) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
			this.ds_mtrlSpl.applyChange();
			this.ds_mtrlSpl.reset();
		}
		break;	
	  case "btn_Cancel02":
		if( Ex.util.isUpdated(this.ds_mtrlAdtg) ) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
			this.ds_mtrlAdtg.applyChange();
			this.ds_mtrlAdtg.reset();
		}
		break;
	  }
	
}
/**********************************************************************
* dataset 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: ds_mtrlSpl_onrowposchanged
 * 설명 		: Dataset 행 변경 후 이벤트 처리(계약목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_mtrlSpl_onrowposchanged(obj,e);
 */
this.ds_mtrlSpl_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1 && obj.getRowType(e.newrow) != Dataset.ROWTYPE_INSERT) {
		this.ds_condSub1.clearData();
		this.ds_mtrlAdtg.clearData();
		
		var nRow = this.ds_condSub1.addRow();
		this.ds_condSub1.setColumn(nRow, "CNTRCT_NO",   obj.getColumn(e.newrow, "CNTRCT_NO"));
		this.ds_condSub1.setColumn(nRow, "MTRL_SN",	   obj.getColumn(e.newrow, "MTRL_SN"));
		this.ds_condSub1.setColumn(nRow, "DOC_SN",      obj.getColumn(e.newrow, "DOC_SN"));
		
		if(this.ds_mtrlSpl.getColumn(this.ds_mtrlSpl.rowposition, "APV_AT") == "1"){
			this.btn_Add02.set_enable(false);
			this.btn_Del02.set_enable(false);
			this.btn_Cancel02.set_enable(false);
			this.btn_Del01.set_enable(false);
		}else{
			this.btn_Add02.set_enable(true);
			this.btn_Del02.set_enable(true);
			this.btn_Cancel02.set_enable(true);
			this.btn_Del01.set_enable(true);
		}
		
		this.fn_search_ctgryFile();
	}else{
		this.ds_mtrlAdtg.clearData();
		this.ds_fileList.clearData();
	}
}

this.ds_mtrlSpl_cancolumnchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.columnid=="APV_AT"){
		if(this.gfn_authGrpId("PRJ_ADMIN") == -1 && this.gfn_authGrpId("CTR_ADMIN") == -1 ) {			// 사업, 계약담당자 아닐때
			return false;
		}
	}
}


/*
 * 함수명  	: ds_mtrlSpl_oncolumnchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(계약목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_mtrlSpl_onrowposchanged(obj,e);
 */
this.ds_mtrlSpl_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid=="APV_AT"){
		switch(e.newvalue){
			case "1":
				obj.setColumn(e.row, "APV_DT", this.comUtils.getClientDate("YYYYMMDD"));
				obj.setColumn(e.row, "UPDUSR_NM", this.fn_getEmpnm());
				this.btn_Del01.set_enable(false);
				this.btn_Add02.set_enable(false);
				this.btn_Del02.set_enable(false);
				this.btn_Cancel02.set_enable(false);
			break;
			default:
				obj.setColumn(e.row, "APV_DT", "");
				obj.setColumn(e.row, "UPDUSR_NM", "");
				this.btn_Add02.set_enable(true);
				this.btn_Del02.set_enable(true);
				this.btn_Cancel02.set_enable(true);
				this.btn_Del01.set_enable(true);
			break;
		}
	}
}


/***********************************************************************
 * grid 관련 이벤트 처리
 ***********************************************************************/

/*
 * 함수명  	: grd_ctgry_file_oncellclick
 * 설명 		: Grid Cell 클릭 후 이벤트 처리(첨부파일)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_ctgry_file_oncellclick(obj,e);
 */
this.grd_ctgry_file_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	// 업로드
	if(e.cell == '5') {
		if(this.ds_mtrlSpl.getColumn(this.ds_mtrlSpl.rowposition, "APV_AT") == "1"){
			this.gfn_message("errors.값확인.요청", "미승인건이|승인여부를");
			return false;
		}
		if(this.ds_fileList.getRowCount() > 0){
			this.gfn_message("info.파일.업로드.허용건수초과", "1");
			return false;
		}
		this.fn_autoFileSn(
			 "PMS"							// 대분류   (SYS_SE)
			,"PRJ"							// 업무구분 (JOB_SE)
			,this.File_UpDownManager		// fileManager 오브젝트
			,this.ds_fileList				// fileObject
			,this.ds_ctgry_file				// fileSn을 불러오거나 반영해야 하는 컬럼
			,"TBBH_CTGRY_FILE"				// fileSn을 저장할 테이블
			,"FILE_SN"						// fileSn 컬럼명이 다를 경우를 대비
			,["CTGRY_FILE_ID"]				// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
		);
	}
	
	// 삭제
	if(e.cell == '6') {
		if(this.ds_mtrlSpl.getColumn(this.ds_mtrlSpl.rowposition, "APV_AT") == "1"){
			this.gfn_message("errors.값확인.요청", "미승인건이|승인여부를");
			return false;
		}
		if(this.ds_fileList.getRowCount == 0){
			this.gfn_message("info.파일정보.없음");
			return false;
		}
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		this.ds_fileList.deleteAll();
		this.ds_cond_file.clearData();
		this.fn_transaction("deleteFile");
		
		var cRow = this.ds_ctgry_file.rowposition;
		this.ds_ctgry_file.set_enableevent(false);
		this.ds_ctgry_file.deleteRow(cRow);
		this.ds_ctgry_file.set_enableevent(true);
		this.fn_transaction("saveCtgryFile");
	}
	
	// 다운로드
	if(e.cell == '7') {
		if(this.ds_fileList.getRowCount() == 0) {
			this.gfn_message("err.파일.다운로드.정보없음");
			return false;
		}
		for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
			var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
			var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
			var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
			
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}

this.grd_onexpanddown = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	switch(obj.name){
		case "grd_mtrlSpl":
			var parm = {
				 arg_0      : this
				,cntrctYear : this.ds_cond.getColumn(0, "CNTRCT_YEAR")
				,infrmlCntrctAt : 0
			};
			this.gfn_popup(obj.name, 800, 530, "계약번호 선택", parm, "mis_ctr::CTR030100_P08.xfdl", "fn_popupCallBack", true);
		break;
		case "grd_mtrlAdtg":
			var parm = {
				 arg_0    : this
			};
			
			this.gfn_popup(obj.name, 400, 400, "", parm, "com::empno_P01.xfdl", "fn_popupCallBack", true);
		break;
	}
}


