 /*
 * 함수명  	: ds_sftMntList_onrowposchanged
 * 설명 		: 안전점검 및 이력관리 등록 데이터셋 행 변경시 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_sftMntList_onrowposchanged();
 */
this.ds_sftMntList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1) {
		
		var cRow = this.ds_sftMntList.rowposition;
		
		// 첨부파일
		this.ds_fileCond.clearData();
		this.ds_fileCond.addRow();
		this.ds_fileCond.setColumn(0, "FILE_SN", this.ds_sftMntList.getColumn(cRow, "ATCH_FL_NO"));
		
		this.ds_fileList.clearData();
		this.fn_transaction("selectAtchFile");
	}else {
		this.ds_ctgry_file.clearData();
		this.ds_ctgry_file2.clearData();
	}
}


 /*
 * 함수명  	: ds_cond_oncolumnchanged
 * 설명 		: 검색조건 데이터 변경 후 발생
 * params  		: obj:Dataset, e:nexacro.DSColChangeEventInfo
 * return Type 	: None
 * 사용법		: this.ds_cond_oncolumnchanged(obj, e);
 */
this.ds_cond_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "CNTRCT_NO") {
		if(this.comUtils.isNull(e.newvalue)) {
			obj.setColumn(0, "CNTRCT_NM", "");
		}
	}
}


