this.ds_sftMntList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	// 지적사항 첨부파일
	this.ds_condSub.clearData();
	var nRow = this.ds_condSub.addRow();
	
	var docSn      = obj.getColumn(e.newrow, "IDC_DOC_SN");
	var cntrctNo = obj.getColumn(e.newrow, "CNTRCT_NO");
	var mntSn    = obj.getColumn(e.newrow, "MNT_SN");
	
	this.ds_condSub.setColumn(nRow, "DOC_SN"     , docSn);
	this.ds_condSub.setColumn(nRow, "CNTRCT_NO"  , cntrctNo);
	this.ds_condSub.setColumn(nRow, "MNT_SN"     , mntSn);

	this.fn_search_ctgryFile();
	
	// 조치사항 첨부파일
	this.ds_condSub2.clearData();
	var nRow = this.ds_condSub2.addRow();
	
	var docSn      = obj.getColumn(e.newrow, "ACTN_DOC_SN");
	var cntrctNo = obj.getColumn(e.newrow, "CNTRCT_NO");
	var mntSn    = obj.getColumn(e.newrow, "MNT_SN");
	
	this.ds_condSub2.setColumn(nRow, "DOC_SN"     , docSn);
	this.ds_condSub2.setColumn(nRow, "CNTRCT_NO"  , cntrctNo);
	this.ds_condSub2.setColumn(nRow, "MNT_SN"     , mntSn);

	this.fn_search_ctgryFile2();
}

this.ds_ctgry_file_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	// clear Datasets
	this.ds_fileList.clearData();
	
	if(e.newrow > -1) {
		var fileSn = obj.getColumn(e.newrow, "FILE_SN");
		
		this.ds_cond_file.clearData();
		var nRow = this.ds_cond_file.addRow();
		this.ds_cond_file.setColumn(nRow, "FILE_SN", fileSn);
		
		this.fn_transaction("selectAtchFile");
	}
}


this.ds_ctgry_file2_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_fileList2.clearData();
	
