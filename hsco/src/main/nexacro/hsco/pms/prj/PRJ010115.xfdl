/*
 * 함수명      : fn_transaction
 * 설명         : Transaction 요청 처리 함수
 * params          : strSvcId : 서비스요청 ID
 * return Type     : None
 * 사용법        : this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
    switch (strSvcId) {
        // 안전점검 및 이력관리 목록 조회
        case "selectSftMntList":
            var sController       = "hsco/pms/prj/PRJ010115/selectSftMntList.do";
            var sInDatasets       = "input1=ds_cond";
            var sOutDatasets      = "ds_sftMntList=output1";
            break;
        // 안전점검 및 이력관리 목록 저장
        case "saveSftMntList":
            var sController       = "hsco/pms/prj/PRJ010115/saveSftMntList.do";
            var sInDatasets       = "input1=ds_sftMntList:U";
            var sOutDatasets      = "";
            break;
      // ======== 첨부파일 관련 transaction ========
	  // ==== 지적사항 ====	
		case "selectCtgryFile":
			var sController   = "/hsco/pms/prj/PRJ010115/selectCtgryFile.do";
			var sInDatasets	  = "input1=ds_condSub";
			var sOutDatasets  = "ds_ctgry_file=output1";
			break;
		case "saveCtgryFile":
			var sController   = "/hsco/pms/prj/PRJ010115/saveCtgryFile.do";
			var sInDatasets   = "input1=ds_ctgry_file:A";
			var sOutDatasets  = "ds_ctgry_file=output1";
			break;
		case "selectAtchFile":
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_cond_file";
			var sOutDatasets = "ds_fileList=output1";
			break;
		case "deleteFile":
			var sController  = "hsco/cmm/file/deleteFile.do";
			var sInDatasets  = "input1=ds_fileList:U";
			var sOutDatasets = "ds_fileList=output1";
			break;
		// ==== 조치사항 ====
		case "selectCtgryFile2":
			var sController   = "/hsco/pms/prj/PRJ010115/selectCtgryFile2.do";
			var sInDatasets	  = "input1=ds_condSub2";
			var sOutDatasets  = "ds_ctgry_file2=output1";
			break;
		case "saveCtgryFile2":
			var sController   = "/hsco/pms/prj/PRJ010115/saveCtgryFile2.do";
			var sInDatasets   = "input1=ds_ctgry_file2:A";
			var sOutDatasets  = "ds_ctgry_file2=output1";
			break;
		case "selectAtchFile2":
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_cond_file2";
			var sOutDatasets = "ds_fileList2=output1";
			break;
		case "deleteFile2":
			var sController  = "hsco/cmm/file/deleteFile.do";
			var sInDatasets  = "input1=ds_fileList2:U";
			var sOutDatasets = "ds_fileList2=output1";
			break;
    }

    Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명      : fn_callBack
 * 설명         : Transaction Callback 처리
 * params          : strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type     : None
 * 사용법        :
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
    if(nErrorCode != '0') {
        this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
    }

    if (nErrorCode == '0'){
		switch (strSvcId) {
			// 안전점검 및 이력관리 목록 조회
			case "selectSftMntList":
				if(this.saveRow > -1) {
					this.ds_sftMntList.rowposition(saveRow);
					saveRow = -1;
				}
				break;
			// 안전점검 및 이력관리 목록 저장
			case "saveSftMntList":
				this.fn_search();
				break;
			// ======== 첨부파일 관련 transaction ========
			// ==== 지적사항 ====
			case "selectAtchFile":
				if(this.ds_fileList.getRowCount() > 0) {
					var fRow     = this.ds_fileList.rowposition;
					var fileSn   = this.ds_fileList.getColumn(fRow, "FILE_SN");
					var fileNm   = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
					var fileOrdr = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
					
					var cRow = this.ds_ctgry_file.rowposition;
					this.ds_ctgry_file.setColumn(cRow, "FILE_SN", fileSn);
					this.ds_ctgry_file.setColumn(cRow, "ORGINL_FILE_NM", fileNm);
					this.ds_ctgry_file.setColumn(cRow, "FILE_ORDR", fileOrdr);
					
					var docSn  = this.ds_ctgry_file.getColumn(cRow, "DOC_SN");
					var fileId = this.ds_ctgry_file.getColumn(cRow, "CTGRY_FILE_ID");
					if(this.comUtils.isNull(docSn) || this.comUtils.isNull(fileId)) {
						this.fn_transaction("saveCtgryFile");
					}
				}
				break;
			case "deleteFile":
				this.fn_transaction("selectCtgryFile");
				break;
			case "saveCtgryFile":
				var cfRow = this.ds_ctgry_file.rowposition;
				var docSn = this.ds_ctgry_file.getColumn(cfRow, "DOC_SN");
				
				this.ds_sftMntListList.set_enableevent(false);
				this.ds_sftMntListList.set_updatecontrol(false);
				var fRow = this.ds_sftMntListList.rowposition;
				this.ds_sftMntListList.setColumn(fRow, "IDC_DOC_SN", docSn);
				this.ds_sftMntListList.set_updatecontrol(true);
				this.ds_sftMntListList.set_enableevent(true);
				break;
			// ==== 조치사항 ====
			case "selectAtchFile2":
				if(this.ds_fileList2.getRowCount() > 0) {
					var fRow     = this.ds_fileList2.rowposition;
					var fileSn   = this.ds_fileList2.getColumn(fRow, "FILE_SN");
					var fileNm   = this.ds_fileList2.getColumn(fRow, "ORGINL_FILE_NM");
					var fileOrdr = this.ds_fileList2.getColumn(fRow, "FILE_ORDR");
					
					var cRow = this.ds_ctgry_file2.rowposition;
					this.ds_ctgry_file2.setColumn(cRow, "FILE_SN", fileSn);
					this.ds_ctgry_file2.setColumn(cRow, "ORGINL_FILE_NM", fileNm);
					this.ds_ctgry_file2.setColumn(cRow, "FILE_ORDR", fileOrdr);
					
					var docSn  = this.ds_ctgry_file2.getColumn(cRow, "DOC_SN");
					var fileId = this.ds_ctgry_file2.getColumn(cRow, "CTGRY_FILE_ID");
					if(this.comUtils.isNull(docSn) || this.comUtils.isNull(fileId)) {
						this.fn_transaction("saveCtgryFile2");
					}
				}
				break;
			case "deleteFile2":
				this.fn_transaction("selectCtgryFile2");
				break;
			case "saveCtgryFile2":
				var cfRow = this.ds_ctgry_file2.rowposition;
				var docSn = this.ds_ctgry_file2.getColumn(cfRow, "DOC_SN");
				this.ds_sftMntListList.set_enableevent(false);
				this.ds_sftMntListList.set_updatecontrol(false);
				var fRow = this.ds_sftMntListList.rowposition;
				this.ds_sftMntListList.setColumn(fRow, "ACTN_DOC_SN", docSn);
				this.ds_sftMntListList.set_updatecontrol(true);
				this.ds_sftMntListList.set_enableevent(true);
				break;
		}
    }
}
