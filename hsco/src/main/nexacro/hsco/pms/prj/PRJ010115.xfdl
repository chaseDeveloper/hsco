/*
 * 함수명  	: div_search_searchCtrt_onclick
 * 설명 		: '계약번호선택' 버튼 클릭 후 이벤트 처리(검색조건 Dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_search_searchCtrt_onclick(obj,e);
 */
this.div_search_btn_cntrctPopup_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_popup("CTR030100_P08");
}



 /*
 * 함수명  	: fn_popup
 * 설명 		: Popup 호출 이벤트 처리
 * params  		: sPopupId    : 팝업 ID
 * return Type 	: None
 * 사용법		: this.fn_popup(sPopupId)
 */
this.fn_popup = function(sPopupId)
{
	// 계약번호 팝업(검색조건)
	if(sPopupId == "CTR030100_P08") {
		var parm = {
			 arg_0      : this
		};
		this.gfn_popup(sPopupId, 800, 530, "계약번호 선택", parm, "mis_ctr::CTR030100_P08.xfdl", "fn_popupCallBack", true);
	
	// 사원검색 팝업
	}else if(sPopupId == "popEmpSearch") {	
		
		var parm = {
			arg_0      : this
		};
		this.gfn_popup(sPopupId, 500, 465, "사원선택", parm, "com::empno_P01.xfdl", "fn_popupCallBack", true);	
	}
}

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 계약번호 팝업 콜백(검색조건)
	if(sPopupId == "CTR030100_P08") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
			var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
			
			this.ds_cond.setColumn(0, "CNTRCT_NO",   cntrctNo);	// 계약번호
			this.ds_cond.setColumn(0, "CNTRCT_NM",   cntrctNm);	// 계약명
			this.fn_search();
		}
	// 사원선택 팝업 콜백(검색조건)
	}else if(sPopupId == "popEmpSearch") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var cRow = this.ds_sftMntList.rowposition;
		
			var empno  = dsTemp.getColumn(0, "EMPNO");
			var empnm  = dsTemp.getColumn(0, "EMPNM");
			var deptNm = dsTemp.getColumn(0, "DEPT_NM");
						
 			this.ds_sftMntList.setColumn(cRow, "MNT_EMPNO", empno);		// 사원번호
 			this.ds_sftMntList.setColumn(cRow, "EMPNM"    , empnm);		// 사원명
 			this.ds_sftMntList.setColumn(cRow, "DEPT_NM"  , deptNm);	// 사원명
		}
	}
}


