/*
 * 함수명  	: btn_masterAdd_onclick
 * 설명 		: 마스터 행추가 버튼 클릭
 * params  		: obj:Button,  e:nexacro.ClickEventInfo
 * return Type 	: None
 * 사용법		: this.btn_masterAdd_onclick(obj, e);
 */
this.btn_masterAdd_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var cntrctNo = this.ds_cond.getColumn(0, "CNTRCT_NO");
	var cntrctNm = this.ds_cond.getColumn(0, "CNTRCT_NM");
	
	if(this.comUtils.isNull(cntrctNo)) {
		alert("검색조건의 계약번호를 선택해주세요.");
		return false;
	}
	
	var cRow = this.ds_sftMntList.addRow();
	this.ds_sftMntList.setColumn(cRow, "CNTRCT_NO"  , cntrctNo);
	this.ds_sftMntList.setColumn(cRow, "CNTRCT_NM"  , cntrctNm);
	this.ds_sftMntList.setColumn(cRow, "MNT_DT"     , this.dateUtils.format(this.gfn_today(),"yyyymmdd"));
}


/*
 * 함수명  	: btn_masterDelete_onclick
 * 설명 		: 마스터 행삭제 버튼 클릭
 * params  		: obj:Button,  e:nexacro.ClickEventInfo
 * return Type 	: None
 * 사용법		: this.btn_masterDelete_onclick(obj, e);
 */
this.btn_masterDelete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{ 
	if(this.ds_sftMntList.getRowCount() > 0) {
		if (this.gfn_message("confirm.삭제여부")) {
            var cRow = this.ds_sftMntList.rowposition;
            this.ds_sftMntList.deleteRow(cRow);
        }
    }
}


/*
 * 함수명  	: btn_masterCancel_onclick
 * 설명 		: 마스터 행취소 버튼 클릭
