/*
 * 함수명  	: grd_ctgry_file_oncellclick
 * 설명 		: Grid Cell 클릭 후 이벤트 처리(첨부파일)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_ctgry_file_oncellclick(obj,e);
 */
this.grd_ctgry_file_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
// 업로드
	if(e.cell == '5') {
		if(this.ds_fileList.getRowCount() > 0){
			this.gfn_message("info.파일.업로드.허용건수초과", "1");
			return false;
		}
		this.fn_autoFileSn(
			 "PMS"							// 대분류   (SYS_SE)
			,"PRJ"							// 업무구분 (JOB_SE)
			,this.File_UpDownManager		// fileManager 오브젝트
			,this.ds_fileList				// fileObject
			,this.ds_ctgry_file				// fileSn을 불러오거나 반영해야 하는 컬럼
			,"TBBH_CTGRY_FILE"				// fileSn을 저장할 테이블
			,"FILE_SN"						// fileSn 컬럼명이 다를 경우를 대비
			,["CTGRY_FILE_ID"]				// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
		);
	}
	
	// 삭제
	if(e.cell == '6') {
		if(this.ds_fileList.getRowCount == 0){
			this.gfn_message("info.파일정보.없음");
			return false;
		}
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		this.ds_fileList.deleteAll();
		this.ds_cond_file.clearData();
		this.fn_transaction("deleteFile");
		
		var cRow = this.ds_ctgry_file.rowposition;
		this.ds_ctgry_file.set_enableevent(false);
		this.ds_ctgry_file.deleteRow(cRow);
		this.ds_ctgry_file.set_enableevent(true);
		this.fn_transaction("saveCtgryFile");
	}
	
	// 다운로드
	if(e.cell == '7') {
		if(this.ds_fileList.getRowCount() == 0) {
			this.gfn_message("err.파일.다운로드.정보없음");
			return false;
		}
		for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
			var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
			var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
			var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
			
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}



/*
 * 함수명  	: grd_ctgry_file2_oncellclick
 * 설명 		: Grid Cell 클릭 후 이벤트 처리(첨부파일)
 * params  		: None
 * return Type 	: None1
 * 사용법		: this.grd_ctgry_file2_oncellclick(obj,e);
 */
this.grd_ctgry_file2_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
// 업로드
	if(e.cell == '5') {
	
		if(this.ds_fileList2.getRowCount() > 0){
			this.gfn_message("info.파일.업로드.허용건수초과", "1");
			return false;
		}
		this.fn_autoFileSn(
			 "PMS"							// 대분류   (SYS_SE)
			,"PRJ"							// 업무구분 (JOB_SE)
			,this.File_UpDownManager		// fileManager 오브젝트
			,this.ds_fileList2				// fileObject
			,this.ds_ctgry_file2			// fileSn을 불러오거나 반영해야 하는 컬럼
			,"TBBH_CTGRY_FILE"				// fileSn을 저장할 테이블
			,"FILE_SN"						// fileSn 컬럼명이 다를 경우를 대비
			,["CTGRY_FILE_ID"]				// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
		);
	}
	
	// 삭제
	if(e.cell == '6') {
		if(this.ds_fileList2.getRowCount == 0){
			this.gfn_message("info.파일정보.없음");
			return false;
		}
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		this.ds_fileList2.deleteAll();
		this.ds_cond_file2.clearData();
		this.fn_transaction("deleteFile2");
		
		var cRow = this.ds_ctgry_file2.rowposition;
		this.ds_ctgry_file2.set_enableevent(false);
		this.ds_ctgry_file2.deleteRow(cRow);
		this.ds_ctgry_file2.set_enableevent(true);
		this.fn_transaction("saveCtgryFile2");
	}
	
	// 다운로드
	if(e.cell == '7') {
		if(this.ds_fileList2.getRowCount() == 0) {
			this.gfn_message("err.파일.다운로드.정보없음");
			return false;
		}
		for(var fRow = 0; fRow < this.ds_fileList2.getRowCount(); fRow++) {
			var fileSn    = this.ds_fileList2.getColumn(fRow, "FILE_SN");
			var fileOrder = this.ds_fileList2.getColumn(fRow, "FILE_ORDR");
			var fileName  = this.ds_fileList2.getColumn(fRow, "ORGINL_FILE_NM");
			
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}
