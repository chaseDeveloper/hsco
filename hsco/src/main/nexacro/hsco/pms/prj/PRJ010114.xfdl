/**
* 설명 : 행추가
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Add_onclick()
*/

this.btn_Add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	//필수 데이터 확인	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	  switch (obj.name) {
	  case "btn_Add01":
		
		var nRow = this.ds_exeEvlReq.addRow();
		this.ds_exeEvlArsl.clearData();
		this.ds_fileList.clearData();
		
		this.ds_exeEvlReq.setColumn(nRow, "RQS_DT",			this.comUtils.getClientDate("YYYYMMDD"));
		this.ds_exeEvlReq.setColumn(nRow, "REGISTER_NM",	this.fn_getEmpnm());
		this.ds_exeEvlReq.setColumn(nRow, "SUB_COUNT",		0);
		
		if(!Ex.util.isUpdated(this.ds_cond)){
			this.ds_exeEvlReq.setColumn(nRow, "CNTRCT_NO", this.ds_cond.getColumn(0, "CNTRCT_NO"));
			this.ds_exeEvlReq.setColumn(nRow, "CNTRCT_NM", this.ds_cond.getColumn(0, "CNTRCT_NM"));
		}
		break;
		
	  case "btn_Add02":
		var bzRow = this.ds_exeEvlReq.rowposition;
		if(bzRow < 0){
			this.gfn_message("comm.항목.선택.요청", "시공평가요청");
			return false;
		}
		if(this.ds_exeEvlReq.getRowType(this.ds_exeEvlReq.rowposition) ==  2){
			this.gfn_message("comm.정보.저장.요청", "시공평가요청");
			return false;
		}
		
		var nRow = this.ds_exeEvlArsl.addRow();
		this.ds_exeEvlArsl.setColumn(nRow, "CNTRCT_NO", 	 this.ds_exeEvlReq.getColumn(bzRow, "CNTRCT_NO"));
		this.ds_exeEvlArsl.setColumn(nRow, "EXE_REQ_SN",	 this.ds_exeEvlReq.getColumn(bzRow, "EXE_REQ_SN"));
		break;	
	  }
			
}

/**
* 설명 : 행삭제
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Delete_onclick()
*/
this.btn_Delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//필수 데이터 확인	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	  switch (obj.name) {
	  case "btn_Del01":
			if(this.ds_exeEvlReq.rowposition > -1) {
			
				this.ds_exeEvlReq.deleteRow(this.ds_exeEvlReq.rowposition);
				// 첨부파일
				this.ds_condSub1.clearData();
				this.ds_exeEvlArsl.clearData();
				
				var nRow = this.ds_condSub1.addRow();
				this.ds_condSub1.setColumn(nRow, "CNTRCT_NO",   this.ds_exeEvlReq.getColumn(this.ds_exeEvlReq.rowposition, "CNTRCT_NO"));
				this.ds_condSub1.setColumn(nRow, "EXE_REQ_SN",  this.ds_exeEvlReq.getColumn(this.ds_exeEvlReq.rowposition, "EXE_REQ_SN"));
				this.ds_condSub1.setColumn(nRow, "DOC_SN",      this.ds_exeEvlReq.getColumn(this.ds_exeEvlReq.rowposition, "DOC_SN"));
				
				this.fn_search_ctgryFile();
			}else{
				this.ds_exeEvlArsl.clearData();
				this.ds_fileList.clearData();
			}
			break;	
	  case "btn_Del02":
			if(this.ds_exeEvlArsl.rowposition>-1){
				this.ds_exeEvlArsl.deleteRow(this.ds_exeEvlArsl.rowposition);
			}
			break;
	  }
	
}


/**
* 설명 : 행취소
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Delete_onclick()
*/
this.btn_Cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	switch (obj.name) {
	  case "btn_Cancel01":
		if( Ex.util.isUpdated(this.ds_exeEvlReq) ) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
			this.ds_exeEvlReq.reset();
			this.ds_exeEvlReq.applyChange();
		}
		break;	
	  case "btn_Cancel02":
		if( Ex.util.isUpdated(this.ds_exeEvlArsl) ) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
			this.ds_exeEvlArsl.reset();
			this.ds_exeEvlArsl.applyChange();
		}
		break;
	  }
	
}
/**********************************************************************
* dataset 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: ds_exeEvlReq_onrowposchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(계약목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_exeEvlReq_onrowposchanged(obj,e);
 */
this.ds_exeEvlReq_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1 && obj.getRowType(e.newrow) != Dataset.ROWTYPE_INSERT) {
		// 첨부파일
		this.ds_condSub1.clearData();
		this.ds_exeEvlArsl.clearData();
		
		var nRow = this.ds_condSub1.addRow();
		this.ds_condSub1.setColumn(nRow, "CNTRCT_NO",   obj.getColumn(e.newrow, "CNTRCT_NO"));
		this.ds_condSub1.setColumn(nRow, "EXE_REQ_SN",  obj.getColumn(e.newrow, "EXE_REQ_SN"));
		this.ds_condSub1.setColumn(nRow, "DOC_SN",      obj.getColumn(e.newrow, "DOC_SN"));
		
		if(obj.getColumn(e.newrow, "SUB_COUNT") == "0"){
			this.btn_Del01.set_enable(true);
		}else{
			this.btn_Del01.set_enable(false);
		}
		
		this.fn_search_ctgryFile();
	}else{
		this.ds_exeEvlArsl.clearData();
		this.ds_fileList.clearData();
	}
}


this.ds_exeEvlReq_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid=="APV_AT"){
		if(this.gfn_authGrpId("PRJ_ADMIN") == -1 && this.gfn_authGrpId("CTR_ADMIN") == -1 ) {			// 사업, 계약담당자 아닐때
			return false;
		}
	}
}


this.ds_exeEvlReq_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid=="APV_AT"){
		switch(e.newvalue){
			case "1":
				obj.setColumn(e.row, "APV_DT", this.comUtils.getClientDate("YYYYMMDD"));
				obj.setColumn(e.row, "UPDUSR_NM", this.fn_getEmpnm());
				this.btn_Del01.set_enable(false);
			break;
			default:
				obj.setColumn(e.row, "APV_DT", "");
				obj.setColumn(e.row, "UPDUSR_NM", "");
				this.btn_Del01.set_enable(true);
			break;
		}
	}
}


