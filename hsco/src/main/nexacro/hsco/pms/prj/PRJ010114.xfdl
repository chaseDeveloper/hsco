/***********************************************************************
 * Form 오브젝트 초기화
 ***********************************************************************/ 
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 초기 검색조건
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	
	/*******************************************************************************
	 * 함수명  	: gfn_comboLoad
	 * 설명 		: 공통코드를 조회하여 콤보박스를 만든다.
	 * params  		: 1. param_1    : 반환받을 DATASET 명 			(예: DS_AREA_NO_CODE)
					  2. param_2	: 그룹코드						(예 : COM007)
					  3. param_3	: 사용여부		    			(예 : A(전체), Y(사용), N(사용안함))
					  4. param_4	: 콤보박스INDEX(0) 표시		(예 : A(전체), S(선택), E(공백), D(첫번째데이터부터시작) )
					  5. param_5	: 코드+코드명					(예 : G(코드명), B(코드-코드명) )
	 ******************************************************************************/
	var comboParams = [
	    ["ds_prj037", "PRJ037", "Y", "D", "B"]	// ds_prj037 : 시공평가구분
	];
	var callback = function (svcId, errorCode, errorMsg) {
		if(this.gfn_authGrpId("PRJ_ADMIN") == -1 && this.gfn_authGrpId("CTR_ADMIN") == -1 ) {			// 사업, 계약담당자 아닐때
			this.ds_cond.setColumn(nRow, "DEPT_CODE", this.gfn_getDeptId());	// 부서코드
			this.ds_cond.setColumn(nRow, "DEPT_NM", this.gfn_getDeptName());	// 부서아이디
			this.grd_exeEvlArsl.set_readonly(true);
			this.btn_Add02.set_enable(false);
			this.btn_Del02.set_enable(false);
			this.btn_Cancel02.set_enable(false);
			this.grd_exeEvlArsl.setCellProperty("body", 0, "editdisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 1, "editdisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 2, "editdisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 3, "calendardisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 4, "calendardisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 5, "calendardisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 6, "editdisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 7, "editdisplay", "edit");
			this.grd_exeEvlArsl.setCellProperty("body", 8, "editdisplay", "edit");
		}else{
			this.btn_Add02.set_enable(true);
			this.btn_Del02.set_enable(true);
			this.btn_Cancel02.set_enable(true);
			this.div_search.edt_deptCode.set_visible(false);
			this.div_search.edt_deptNm.set_visible(false);
			this.div_search.sta_dept.set_visible(false);
		}
		this.fn_search();
	};
	this.gfn_comboLoad(comboParams, callback);
}

/***********************************************************************
 * Dataset 오브젝트 초기화
