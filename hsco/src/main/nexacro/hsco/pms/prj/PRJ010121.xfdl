* 
***********************************************************************/


/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(계약목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_flwMntCtrt.clearData();
	this.ds_flwMntEpa.clearData();
	this.ds_ctgry_file.clearData();
	
	this.fn_transaction("selectFlwMnt");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (   !this.comUtils.isDatasetUpdated(this.ds_flwMnt)		// 하자점검 내역
		&& !this.comUtils.isDatasetUpdated(this.ds_flwMntEpa)	// 관련계약
		&& !this.comUtils.isDatasetUpdated(this.ds_flwMntCtrt)	// 하자점검 검사자 명단
	   ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	//validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_flwMnt, this.ds_validation)) {
		return;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_flwMntCtrt, this.ds_validation)) {
		return;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_flwMntEpa, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		saveRow1 = this.ds_flwMnt.rowposition;
		saveRow2 = this.ds_flwMntCtrt.rowposition;
		saveRow3 = this.ds_flwMntEpa.rowposition;
		this.fn_transaction("saveDs");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectFlwMnt":
			var sController   	= "hsco/pms/prj/PRJ010121/selectFlwMnt.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_flwMnt=output1";
			break;
					
		case "selectFlwMntCtrt":
			var sController   	= "hsco/pms/prj/PRJ010121/selectFlwMntCtrt.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_flwMntCtrt=output1";
			break;
			
		case "selectFlwMntEpa":
			var sController   	= "hsco/pms/prj/PRJ010121/selectFlwMntEpa.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_flwMntEpa=output1";
			break;
					
		case "saveDs":
			var sController   	= "hsco/pms/prj/PRJ010121/save.do";
			var sInDatasets   	= "input1=ds_flwMnt:U input2=ds_flwMntCtrt:U input3=ds_flwMntEpa:U";
			var sOutDatasets  	= "";
			break;
			
		// ======== 첨부파일 관련 transaction ========
		case "selectCtgryFile":
			var sController   	= "/hsco/pms/prj/PRJ010121/selectCtgryFile.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "saveCtgryFile":
			var sController   	= "/hsco/pms/prj/PRJ010121/saveCtgryFile.do";
			var sInDatasets   	= "input1=ds_ctgry_file:A";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "selectAtchFile":
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_cond_file";
			var sOutDatasets = "ds_fileList=output1";
			break;
		case "deleteFile":
			var sController  = "hsco/cmm/file/deleteFile.do";
			var sInDatasets  = "input1=ds_fileList:U";
			var sOutDatasets = "ds_fileList=output1";
			break;

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(nErrorCode != 0) {
	  this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	if (nErrorCode != 0){
	} else {
		switch (strSvcId) {
			case "selectFlwMnt":
				if(saveRow1>-1){
					this.ds_flwMnt.set_rowposition(saveRow1);
					saveRow1 = -1;
				}
				break;
						
			case "selectFlwMntCtrt":
				if(saveRow2>-1){
					this.ds_flwMntCtrt.set_rowposition(saveRow2);
					saveRow2 = -1;
				}
				break;
						
			case "selectFlwMntEpa":
				if(saveRow3>-1){
					this.ds_flwMntEpa.set_rowposition(saveRow2);
					saveRow3 = -1;
				}
				break;
						
			case "saveDs":
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
				
			// ======== 첨부파일 관련 transaction ========
			case "selectCtgryFile":
				this.fn_transaction("selectFlwMntCtrt");
				break;
			case "selectAtchFile":
				if(this.ds_fileList.getRowCount() > 0) {
					var fRow     = this.ds_fileList.rowposition;
					var fileSn   = this.ds_fileList.getColumn(fRow, "FILE_SN");
					var fileNm   = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
					var fileOrdr = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
					
					var cRow = this.ds_ctgry_file.rowposition;
					this.ds_ctgry_file.setColumn(cRow, "FILE_SN", fileSn);
					this.ds_ctgry_file.setColumn(cRow, "ORGINL_FILE_NM", fileNm);
					this.ds_ctgry_file.setColumn(cRow, "FILE_ORDR", fileOrdr);
					
					var docSn  = this.ds_ctgry_file.getColumn(cRow, "DOC_SN");
					var fileId = this.ds_ctgry_file.getColumn(cRow, "CTGRY_FILE_ID");
					if(this.comUtils.isNull(docSn) || this.comUtils.isNull(fileId)) {
						this.fn_transaction("saveCtgryFile");
					}
				}
				break;
			case "deleteFile":
				this.fn_transaction("selectCtgryFile");
				break;
			case "saveCtgryFile":
				var cfRow = this.ds_ctgry_file.rowposition;
				var docSn = this.ds_ctgry_file.getColumn(cfRow, "DOC_SN");
				
				this.ds_flwMntCtrt.set_enableevent(false);
				this.ds_flwMntCtrt.set_updatecontrol(false);
				var fRow = this.ds_flwMntCtrt.rowposition;
				this.ds_flwMntCtrt.setColumn(fRow, "DOC_SN", docSn);
				this.ds_flwMntCtrt.set_updatecontrol(true);
				this.ds_flwMntCtrt.set_enableevent(true);
				break;
		}
	}
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.fn_popupCallBack = function(strID, variant)
{
	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(variant);
	
	// 값이 존재하지 않을 경우에는 아무런 값을 덮어쓰지 않음
	if(!loadCnt) {
		return false;
	}
	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(variant);
	if(loadCnt) {
		switch(strID) {
			case "btn_bcncPopup":
				this.ds_cond.setColumn(0, "BSNS_CODE", dsTemp.getColumn(0, "BSNS_CODE"));
				this.ds_cond.setColumn(0, "BSNS_NM",   dsTemp.getColumn(0, "BSNS_NM"));
				this.fn_search();
				break;
			case "grd_flwMntCtrt":
				var rPos     = this.ds_flwMntCtrt.rowposition;
				var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
				var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
				var cntrcyYr = cntrctNo.substr(0,4);
				
				this.ds_flwMntCtrt.setColumn(rPos, "CNTRCT_NO",   cntrctNo);	// 계약번호
				this.ds_flwMntCtrt.setColumn(rPos, "CNTRCT_NM",   cntrctNm);	// 계약명
				break;
			case "grd_flwMntEpa":
				var rPos = this.ds_flwMntEpa.rowposition;
				this.ds_flwMntEpa.setColumn(rPos, "ADTG_EMPNO",  dsTemp.getColumn(0, "EMPNO"));
				this.ds_flwMntEpa.setColumn(rPos, "EMPNM", 		dsTemp.getColumn(0, "EMPNM"));
				this.ds_flwMntEpa.setColumn(rPos, "DEPT_NM",		dsTemp.getColumn(0, "DEPT_NM"));
				this.ds_flwMntEpa.setColumn(rPos, "OFCPS_SE", 	dsTemp.getColumn(0, "OFCPS_SE"));
				this.ds_flwMntEpa.setColumn(rPos, "HR_CLSF_SE", 	dsTemp.getColumn(0, "HR_CLSF_SE"));
				break;
		}
	}
}

