}
/**********************************************************************
* dataset 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: ds_flwMnt_onrowposchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(계약목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_flwMntCtrt_onrowposchanged(obj,e);
 */
this.ds_flwMnt_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1 && obj.getRowType(e.newrow) != Dataset.ROWTYPE_INSERT) {
		this.ds_condSub1.clearData();
		this.ds_flwMntEpa.clearData();
		
		var nRow = this.ds_condSub1.addRow();
		this.ds_condSub1.setColumn(nRow, "BSNS_CODE",   obj.getColumn(e.newrow, "BSNS_CODE"));
		this.ds_condSub1.setColumn(nRow, "FLW_SN",	    obj.getColumn(e.newrow, "FLW_SN"));
		
		this.fn_search_ctgryFile();
	}else{
		this.ds_flwMntCtrt.clearData();
		this.ds_flwMntEpa.clearData();
	}
}


/*
 * 함수명  	: ds_flwMntCtrt_onrowposchanged
 * 설명 		: Dataset 데이터 변경 후 이벤트 처리(계약목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_flwMntCtrt_onrowposchanged(obj,e);
 */
this.ds_flwMntCtrt_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1 && obj.getRowType(e.newrow) != Dataset.ROWTYPE_INSERT) {
		this.ds_condSub1.clearData();
		this.ds_flwMntEpa.clearData();
		
		var nRow = this.ds_condSub2.addRow();
		this.ds_condSub2.setColumn(nRow, "BSNS_CODE",   obj.getColumn(e.newrow, "BSNS_CODE"));
		this.ds_condSub2.setColumn(nRow, "FLW_SN",	    obj.getColumn(e.newrow, "FLW_SN"));
		this.ds_condSub2.setColumn(nRow, "FLW_CTRT_SN", obj.getColumn(e.newrow, "FLW_CTRT_SN"));
		
		this.fn_transaction("selectFlwMntEpa");
	}else{
		this.ds_flwMntEpa.clearData();
	}
}


/*
 * 함수명  	: ds_flwMntCtrt_canrowposchange
 * 설명 		: Dataset 데이터 변경 전 이벤트 처리(계약목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_flwMntCtrt_canrowposchange(obj,e);
 */
this.ds_flwMntCtrt_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(this.comUtils.isDatasetUpdated(this.ds_flwMntEpa)){
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
}


/*
 * 함수명  	: ds_flwMnt_canrowposchange
 * 설명 		: Dataset 데이터 변경 전 이벤트 처리(계약목록 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_flwMnt_canrowposchange(obj,e);
 */
this.ds_flwMnt_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(this.comUtils.isDatasetUpdated(this.ds_flwMntCtrt) || this.comUtils.isDatasetUpdated(this.ds_flwMntEpa)){
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
}

/***********************************************************************
 * grid 관련 이벤트 처리
 ***********************************************************************/

/*
 * 함수명  	: grd_ctgry_file_oncellclick
 * 설명 		: Grid Cell 클릭 후 이벤트 처리(첨부파일)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.grd_ctgry_file_oncellclick(obj,e);
 */
this.grd_ctgry_file_oncellclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	// 업로드
	if(e.cell == '5') {
		if(this.ds_fileList.getRowCount() > 0){
			this.gfn_message("info.파일.업로드.허용건수초과", "1");
			return false;
		}
		this.fn_autoFileSn(
			 "PMS"							// 대분류   (SYS_SE)
			,"PRJ"							// 업무구분 (JOB_SE)
			,this.File_UpDownManager		// fileManager 오브젝트
			,this.ds_fileList				// fileObject
			,this.ds_ctgry_file				// fileSn을 불러오거나 반영해야 하는 컬럼
			,"TBBH_CTGRY_FILE"				// fileSn을 저장할 테이블
			,"FILE_SN"						// fileSn 컬럼명이 다를 경우를 대비
			,["CTGRY_FILE_ID"]				// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
		);
	}
	
	// 삭제
	if(e.cell == '6') {
		if(this.ds_fileList.getRowCount == 0){
			this.gfn_message("info.파일정보.없음");
			return false;
		}
		if(!this.gfn_message("confirm.선택파일.삭제여부", "들")){
			return false;
		}
		this.ds_fileList.deleteAll();
		this.ds_cond_file.clearData();
		this.fn_transaction("deleteFile");
		
		var cRow = this.ds_ctgry_file.rowposition;
		this.ds_ctgry_file.set_enableevent(false);
		this.ds_ctgry_file.deleteRow(cRow);
		this.ds_ctgry_file.set_enableevent(true);
		this.fn_transaction("saveCtgryFile");
	}
	
	// 다운로드
	if(e.cell == '7') {
		if(this.ds_fileList.getRowCount() == 0) {
			this.gfn_message("err.파일.다운로드.정보없음");
			return false;
		}
		for(var fRow = 0; fRow < this.ds_fileList.getRowCount(); fRow++) {
			var fileSn    = this.ds_fileList.getColumn(fRow, "FILE_SN");
			var fileOrder = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
			var fileName  = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
			
			this.File_UpDownManager.download(fileSn, fileOrder, fileName);
		}
	}
}



/**********************************************************************
* 첨부파일 관련
***********************************************************************/

/*
 * 함수명  	: fn_search_ctgryFile
 * 설명 		: 표준문서 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_ctgryFile();
 */
this.fn_search_ctgryFile = function()
{
	
	
	// clear Datasets
	this.ds_ctgry_file.set_enableevent(false);
	this.ds_ctgry_file.clearData();
	this.ds_ctgry_file.set_enableevent(true)
	
	// transaction
	if(this.ds_flwMnt.getRowCount() > 0) {	// 부모데이터셋이 존재할때만 조회
		this.fn_transaction("selectCtgryFile");
	}
}

this.ds_ctgry_file_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	// clear Datasets
	this.ds_fileList.clearData();
	
	if(e.newrow > -1) {
		var fileSn = obj.getColumn(e.newrow, "FILE_SN");
		
		this.ds_cond_file.clearData();
		var nRow = this.ds_cond_file.addRow();
		this.ds_cond_file.setColumn(nRow, "FILE_SN", fileSn);
		
		this.fn_transaction("selectAtchFile");
	}
}]]></Script>
