/**
* 설명 : 행추가
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Add_onclick()
*/

this.btn_Add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	//필수 데이터 확인	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	  switch (obj.name) {
	  case "btn_Add01":
		var nRow = this.ds_flwMnt.addRow();
		this.ds_flwMntEpa.clearData();
		this.ds_ctgry_file.clearData();
		
		if(!Ex.util.isUpdated(this.ds_cond)){
			this.ds_flwMnt.setColumn(nRow, "BSNS_CODE", this.ds_cond.getColumn(0, "BSNS_CODE"));
			this.ds_flwMnt.setColumn(nRow, "MNT_YR", this.comUtils.getClientDate("YYYY"));	// 점검년도
		}
		break;
		
	  case "btn_Add02":
		var mRow = this.ds_flwMnt.rowposition;
		if(mRow < 0){
			this.gfn_message("comm.항목.선택.요청", "하자점검 내역");
			return false;
		}
		if(this.ds_flwMnt.getRowType(this.ds_flwMnt.rowposition) ==  2){
			this.gfn_message("comm.정보.저장.요청", "하자점검 내역");
			return false;
		}
		
		var nRow = this.ds_flwMntCtrt.addRow();
		this.ds_flwMntCtrt.setColumn(nRow, "BSNS_CODE",  this.ds_flwMnt.getColumn(mRow, "BSNS_CODE"));
		this.ds_flwMntCtrt.setColumn(nRow, "FLW_SN", 	 this.ds_flwMnt.getColumn(mRow, "FLW_SN"));
		break;
		
	  case "btn_Add03":
		var mRow = this.ds_flwMntCtrt.rowposition;
		if(mRow < 0){
			this.gfn_message("comm.항목.선택.요청", "관련계약");
			return false;
		}
		if(this.ds_flwMntCtrt.getRowType(this.ds_flwMntCtrt.rowposition) ==  2){
			this.gfn_message("comm.정보.저장.요청", "관련계약");
			return false;
		}
		
		var nRow = this.ds_flwMntEpa.addRow();
		this.ds_flwMntEpa.setColumn(nRow, "BSNS_CODE", 	 this.ds_flwMntCtrt.getColumn(mRow, "BSNS_CODE"));
		this.ds_flwMntEpa.setColumn(nRow, "FLW_SN",  	 this.ds_flwMntCtrt.getColumn(mRow, "FLW_SN"));
		this.ds_flwMntEpa.setColumn(nRow, "FLW_CTRT_SN", this.ds_flwMntCtrt.getColumn(mRow, "FLW_CTRT_SN"));
		break;	
	  }
			
}

/**
* 설명 : 행삭제
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Delete_onclick()
*/
this.btn_Delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//필수 데이터 확인	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	  switch (obj.name) {
	  case "btn_Del01":
			if(this.ds_flwMnt.rowposition>-1){
				this.ds_flwMnt.deleteRow(this.ds_flwMnt.rowposition);
				
				this.ds_condSub1.clearData();
				this.ds_flwMntCtrt.clearData();
				this.ds_flwMntEpa.clearData();
				
				var nRow = this.ds_condSub1.addRow();
				this.ds_condSub1.setColumn(nRow, "BSNS_CODE",   obj.getColumn(e.newrow, "BSNS_CODE"));
				this.ds_condSub1.setColumn(nRow, "FLW_SN",	   obj.getColumn(e.newrow, "FLW_SN"));
				this.ds_condSub1.setColumn(nRow, "DOC_SN",      obj.getColumn(e.newrow, "DOC_SN"));
				
				this.fn_search_ctgryFile();
			}
			break;
	  case "btn_Del02":
			if(this.ds_flwMntCtrt.rowposition>-1){
				this.ds_flwMntCtrt.deleteRow(this.ds_flwMntCtrt.rowposition);
				
				this.ds_condSub.clearData();
				this.ds_flwMntEpa.clearData();
				
				var nRow = this.ds_condSub.addRow();
				this.ds_condSub2.setColumn(nRow, "BSNS_CODE",   obj.getColumn(e.newrow, "BSNS_CODE"));
				this.ds_condSub2.setColumn(nRow, "FLW_SN",	   obj.getColumn(e.newrow, "FLW_SN"));
				this.ds_condSub2.setColumn(nRow, "DOC_SN",      obj.getColumn(e.newrow, "DOC_SN"));
				
				this.fn_search_ctgryFile();
			}
	  case "btn_Del03":
			if(this.ds_flwMntEpa.rowposition>-1){
				this.ds_flwMntEpa.deleteRow(this.ds_flwMntEpa.rowposition);
			}
			break;
	  }
	
}


