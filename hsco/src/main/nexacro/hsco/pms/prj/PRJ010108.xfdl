/**
* 설명 : 행추가
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Add_onclick()
*/

this.btn_Add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	//필수 데이터 확인	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	  switch (obj.name) {
	  case "btn_Add01":
		
		var nRow = this.ds_bzPcp.addRow();
		this.ds_fileList.clearData();
		this.ds_bzPcpHist.clearData();
		this.ds_fileListHist.clearData();
		
		this.ds_bzPcp.setColumn(nRow, "CNTRCT_YEAR", this.ds_cond.getColumn(0, "CNTRCT_YEAR"));
		this.ds_bzPcp.setColumn(nRow, "CNTRCT_NO",   this.ds_cond.getColumn(0, "CNTRCT_NO"));
		this.ds_bzPcp.setColumn(nRow, "REGISTER_NM", this.fn_getEmpnm());
		this.ds_bzPcp.setColumn(nRow, "PCP_CHG_RTV", 1);
		this.ds_bzPcp.setColumn(nRow, "APV_YN", 1);
		this.ds_bzPcp.setColumn(nRow, "LST_PCP_YN", 'Y');
		break;
		
	  case "btn_Add02":
		var bzRow = this.ds_bzPcp.rowposition;
		if(bzRow < 0){
			this.gfn_message("comm.항목.선택.요청", "최종 참여기술자 명단");
			return false;
		}
		if(this.ds_bzPcp.getRowType(this.ds_bzPcp.rowposition) ==  2){
			this.gfn_message("comm.정보.저장.요청", "최종 참여기술자 명단");
			return false;
		}
		
		var nRow = this.ds_bzPcpHist.addRow();
		this.ds_bzPcpHist.setColumn(nRow, "CNTRCT_YEAR",   this.ds_cond.getColumn(0, "CNTRCT_YEAR"));
		this.ds_bzPcpHist.setColumn(nRow, "CNTRCT_NO", 	   this.ds_cond.getColumn(0, "CNTRCT_NO"));
		this.ds_bzPcpHist.setColumn(nRow, "FST_BZ_PCP_SN", this.ds_bzPcp.getColumn(bzRow, "FST_BZ_PCP_SN"));
		this.ds_bzPcpHist.setColumn(nRow, "UPP_BZ_PCP_NM", this.ds_bzPcp.getColumn(bzRow, "BZ_PCP_NM"));
		this.ds_bzPcpHist.setColumn(nRow, "UPP_BZ_PCP_SN", this.ds_bzPcp.getColumn(bzRow, "BZ_PCP_SN"));
		this.ds_bzPcpHist.setColumn(nRow, "UPP_PCP_ED_DT", this.ds_bzPcp.getColumn(bzRow, "PCP_ED_DT"));
		this.ds_bzPcpHist.setColumn(nRow, "REGISTER_NM",   this.fn_getEmpnm());
		this.ds_bzPcpHist.setColumn(nRow, "PCP_CHG_RTV",   nexacro.toNumber(this.ds_bzPcp.getColumn(bzRow, "PCP_CHG_RTV"),0,0,0)+1);
		this.ds_bzPcpHist.setColumn(nRow, "APV_YN", 1);
		this.ds_bzPcpHist.setColumn(nRow, "LST_PCP_YN", 'Y');
		this.ds_bzPcp.setColumn(nRow, "LST_PCP_YN", 'N');
		break;	
	  }
	
}

/**
* 설명 : 행삭제
* 파라미터	: obj:Button,  e:ClickEventInfo
* 사용예  	: btn_Delete_onclick()
*/
this.btn_Delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//필수 데이터 확인	
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	  switch (obj.name) {
	  case "btn_Del01":
			if (this.ds_bzPcp.getColumn(this.ds_bzPcp.rowposition,"LST_PCP_YN")=='N') {
				alert("다른 사람으로 변경되었습니다.");
				return false;
			}
			this.ds_bzPcp.deleteRow(this.ds_bzPcp.rowposition);

			this.ds_condSub1.clearData();
			this.ds_bzPcpHist.clearData();
			
			var nRow = this.ds_condSub1.addRow();
			this.ds_condSub1.setColumn(nRow, "CNTRCT_NO",   obj.getColumn(e.newrow, "CNTRCT_NO"));
			this.ds_condSub1.setColumn(nRow, "BZ_PCP_SN",   obj.getColumn(e.newrow, "BZ_PCP_SN"));
			this.ds_condSub1.setColumn(nRow, "DOC_SN",      obj.getColumn(e.newrow, "DOC_SN"));
			
			this.fn_search_ctgryFile();
			break;	
	  case "btn_Del02":
			if(	this.ds_bzPcp.getColumn(this.ds_bzPcp.rowposition,"BZ_PCP_SN")
				!= this.ds_bzPcpHist.getColumn(this.ds_bzPcpHist.rowposition,"BZ_PCP_SN")
			) {
				alert("현계약자가 아닙니다.")
				return false;
			}
			else {
				this.ds_bzPcpHist.deleteRow(this.ds_bzPcpHist.rowposition);
				
				if (this.ds_bzPcpHist.rowcount==0) {
					this.ds_bzPcp.setColumn(this.ds_bzPcp.rowposition,"LST_PCP_YN", 'Y');
				}
				else {
					this.ds_bzPcpHist.setColumn(this.ds_bzPcpHist.getRowCount()-1, "LST_PCP_YN", 'Y');
				}
			}
			break;
	  }
	
}

