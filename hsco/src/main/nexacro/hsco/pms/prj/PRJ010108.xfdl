    <Script type="xscript5.0"><![CDATA[/**********************************************************************
* 화면ID	: PRJ010108.xfdl
* 화면명	: 검사검수관리
* 화면설명	: 검사검수를 관리한다
* 작성일	: 2017.11.07
* 작성자	: 이수지
* 수정이력 : 
***********************************************************************
*     수정일     수정자   내용
***********************************************************************
*
***********************************************************************
*/

/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";


/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.File_UpDownManager = null;
this.chargerAt = false;
var saveRow1 = -1;
var saveRow2 = -1;

/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.PRJ010108_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
}
this.PRJ010108_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// form 초기화
	this.fn_init_form();

	// dataset 초기화
	this.fn_init_dataset();
}

/***********************************************************************
 * Form 오브젝트 초기화
 ***********************************************************************/ 
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	// 초기 검색조건
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	this.ds_cond.setColumn(nRow, "CNTRCT_YEAR", this.comUtils.getClientDate("YYYY"));	// 당해년도
}

/***********************************************************************
 * Dataset 오브젝트 초기화
 ***********************************************************************/ 
this.fn_init_dataset = function()
{
	var fileConfig = {
		useMultiUpload : true,
		onChange: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess : this.fn_onFileSuccess	// 전송 성공 이벤트 핸들러
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "PMS", "PRJ");
	this.File_UpDownManager.init(fileConfig);
}


/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/


/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(계약목록)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	this.ds_bzPcp.clearData();
	this.ds_bzPcpHist.clearData();
	this.ds_fileList.clearData();
	//this.ds_fileListHist.clearData();
	
	this.fn_transaction("selectDsMain");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if (   !this.comUtils.isDatasetUpdated(this.ds_bzPcpHist)	// 참여기술자 변경
		&& !this.comUtils.isDatasetUpdated(this.ds_bzPcp)	// 참여기술자
	   ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	//validation 체크
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_bzPcpHist, this.ds_validation)) {	// 검사입회내역
		return;
	}
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_bzPcpHist, this.ds_validation)) {		// 입회검사자
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		saveRow1 = this.ds_bzPcp.rowposition;
		saveRow2 = this.ds_bzPcpHist.rowposition;
		this.fn_transaction("saveDsMain");
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectDsMain":
			var sController   	= "hsco/pms/prj/PRJ010108/selectDsMain.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_bzPcp=output1";
			break;
					
		case "selectDsSub":
			var sController   	= "hsco/pms/prj/PRJ010108/selectDsSub.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_bzPcpHist=output1";
			break;
					
		case "saveDsMain":
			var sController   	= "hsco/pms/prj/PRJ010108/saveDsMain.do";
			var sInDatasets   	= "input1=ds_bzPcp:U input2=ds_bzPcpHist:U";
			var sOutDatasets  	= "";
			break;
		// ======== 첨부파일 관련 transaction ========
		case "selectCtgryFile":
			var sController   	= "/hsco/pms/prj/PRJ010108/selectCtgryFile.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "saveCtgryFile":
			var sController   	= "/hsco/pms/prj/PRJ010108/saveCtgryFile.do";
			var sInDatasets   	= "input1=ds_ctgry_file:A";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "selectAtchFile":
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_cond_file";
			var sOutDatasets = "ds_fileList=output1";
			break;
		case "deleteFile":
			var sController  = "hsco/cmm/file/deleteFile.do";
			var sInDatasets  = "input1=ds_fileList:U";
			var sOutDatasets = "ds_fileList=output1";
			break;
			
		// ======== Hist 첨부파일 관련 transaction ========
		case "selectCtgryFileHist":
			var sController   	= "/hsco/pms/prj/PRJ010108/selectCtgryFile.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_ctgry_fileHist=output1";
			break;
		case "saveCtgryFileHist":
			var sController   	= "/hsco/pms/prj/PRJ010108/saveCtgryFile.do";
			var sInDatasets   	= "input1=ds_ctgry_fileHist:A";
			var sOutDatasets  	= "ds_ctgry_fileHist=output1";
			break;
		case "selectAtchFileHist":
			var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets  = "input1=ds_cond_FileHistHist";
			var sOutDatasets = "ds_fileListHist=output1";
			break;
		case "deleteFileHist":
			var sController  = "hsco/cmm/file/deleteFile.do";
			var sInDatasets  = "input1=ds_fileListHist:U";
			var sOutDatasets = "ds_fileListHist=output1";
			break;
			

	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(nErrorCode != 0) {
	  this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	if (nErrorCode != 0){
	} else {
		switch (strSvcId) {
			case "selectDsMain":
				if(saveRow1>-1){
					this.ds_bzPcp.set_useclientlayout(false);
					this.ds_bzPcp.set_rowposition(saveRow1);
					this.ds_bzPcp.set_useclientlayout(true);
					saveRow1 = -1;
				}
				break;
						
			case "selectDsSub":
				if(saveRow2>-1){
					this.ds_bzPcpHist.set_useclientlayout(false);
					this.ds_bzPcpHist.set_rowposition(saveRow2);
					this.ds_bzPcpHist.set_useclientlayout(true);
					saveRow2 = -1;
				}
				this.ds_fileListHist.clearData();
				if(this.ds_bzPcpHist.getRowCount()>0){
					this.fn_search_ctgryFileHist();
				}
				break;
						
			case "saveDsMain":
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
				
			// ======== 첨부파일 관련 transaction ========
			case "selectCtgryFile":
				this.fn_transaction("selectDsSub");
				break;
			case "selectAtchFile":
				if(this.ds_fileList.getRowCount() > 0) {
					var fRow     = this.ds_fileList.rowposition;
					var fileSn   = this.ds_fileList.getColumn(fRow, "FILE_SN");
					var fileNm   = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
					var fileOrdr = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
					
					var cRow = this.ds_ctgry_file.rowposition;
					this.ds_ctgry_file.setColumn(cRow, "FILE_SN", fileSn);
					this.ds_ctgry_file.setColumn(cRow, "ORGINL_FILE_NM", fileNm);
					this.ds_ctgry_file.setColumn(cRow, "FILE_ORDR", fileOrdr);
					
					var docSn  = this.ds_ctgry_file.getColumn(cRow, "DOC_SN");
					var fileId = this.ds_ctgry_file.getColumn(cRow, "CTGRY_FILE_ID");
					if(this.comUtils.isNull(docSn) || this.comUtils.isNull(fileId)) {
						this.fn_transaction("saveCtgryFile");
					}
				}
				break;
			case "deleteFile":
				this.fn_transaction("selectCtgryFile");
				break;
			case "saveCtgryFile":
				var cfRow = this.ds_ctgry_file.rowposition;
				var docSn = this.ds_ctgry_file.getColumn(cfRow, "DOC_SN");
				
				this.ds_bzPcp.set_enableevent(false);
				this.ds_bzPcp.set_updatecontrol(false);
				var fRow = this.ds_bzPcp.rowposition;
				this.ds_bzPcp.setColumn(fRow, "DOC_SN", docSn);
				this.ds_bzPcp.set_updatecontrol(true);
				this.ds_bzPcp.set_enableevent(true);
				break;
				
				
			// ======== Hist 첨부파일 관련 transaction ========
			case "selectAtchFileHist":
				if(this.ds_fileList.getRowCount() > 0) {
					var fRow     = this.ds_fileListHist.rowposition;
					var fileSn   = this.ds_fileListHist.getColumn(fRow, "FILE_SN");
					var fileNm   = this.ds_fileListHist.getColumn(fRow, "ORGINL_FILE_NM");
					var fileOrdr = this.ds_fileListHist.getColumn(fRow, "FILE_ORDR");
					
					var cRow = this.ds_ctgry_fileHist.rowposition;
					this.ds_ctgry_fileHist.setColumn(cRow, "FILE_SN", fileSn);
					this.ds_ctgry_fileHist.setColumn(cRow, "ORGINL_FILE_NM", fileNm);
					this.ds_ctgry_fileHist.setColumn(cRow, "FILE_ORDR", fileOrdr);
					
					var docSn  = this.ds_ctgry_fileHist.getColumn(cRow, "DOC_SN");
					var fileId = this.ds_ctgry_fileHist.getColumn(cRow, "CTGRY_FILE_ID");
					if(this.comUtils.isNull(docSn) || this.comUtils.isNull(fileId)) {
						this.fn_transaction("saveCtgryFileHist");
					}
				}
				break;
			case "deleteFileHist":
				this.fn_transaction("selectCtgryFileHist");
				break;
			case "saveCtgryFileHist":
				var cfRow = this.ds_ctgry_fileHist.rowposition;
				var docSn = this.ds_ctgry_fileHist.getColumn(cfRow, "DOC_SN");
				
				this.ds_bzPcpHist.set_enableevent(false);
				this.ds_bzPcpHist.set_updatecontrol(false);
				var fRow = this.ds_bzPcpHist.rowposition;
				this.ds_bzPcpHist.setColumn(fRow, "DOC_SN", docSn);
				this.ds_bzPcpHist.set_updatecontrol(true);
				this.ds_bzPcpHist.set_enableevent(true);
				break;
		}
	}
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: 팝업 콜백
 * params  		: None
 * return Type 	: None
 * 사용법		: n/a
 */
this.fn_popupCallBack = function(strID, variant)
{
	var dsTemp = new Dataset();
	var loadCnt = dsTemp.loadXML(variant);
	
	// 값이 존재하지 않을 경우에는 아무런 값을 덮어쓰지 않음
	if(!loadCnt) {
		return false;
	}
	
	switch(strID) {
	// 계약번호 선택(검색조건)
		case "CTR030100_P08":
			var dsTemp = new Dataset();
			var loadCnt = dsTemp.loadXML(variant);
			if(loadCnt) {
				var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
				var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
				var cntrcyYr = cntrctNo.substr(0,4);
				
				this.ds_cond.setColumn(0, "CNTRCT_YEAR", cntrcyYr);	// 계약년도
				this.ds_cond.setColumn(0, "CNTRCT_NO",   cntrctNo);	// 계약번호
				this.ds_cond.setColumn(0, "CNTRCT_NM",   cntrctNm);	// 계약명
				this.fn_search();
			}
			break;
	}
}

