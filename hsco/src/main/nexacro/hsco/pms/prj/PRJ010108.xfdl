/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if(   Ex.util.isUpdated(this.ds_bzPcp)			// 계약목록
	   || Ex.util.isUpdated(this.ds_bzPcpHist)	// 검사입회내역
	  ) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
		this.ds_bzPcp.reset();
		this.ds_bzPcpHist.reset();
		
		this.ds_bzPcp.applyChange();
		this.ds_bzPcpHist.applyChange();
	}
}



