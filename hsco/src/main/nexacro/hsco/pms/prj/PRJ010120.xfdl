			this.ds_flaw_assrnc.setColumn(fRow, "BCNC_NM",   bcncNm  );
		}
	}
	
	// 부서 선택
	if(sPopupId == "deptCode_P01") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
