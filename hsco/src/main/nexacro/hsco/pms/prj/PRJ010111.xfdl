this.grd_acnut_onexpandup = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	var aRow = this.ds_acnut.rowposition;
	var bcncCode = this.ds_acnut.getColumn(aRow, "BCNC_CODE");
