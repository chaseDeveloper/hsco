		,bcncCode   : bcncCode
	};
	this.gfn_popup("CTR010101_P03", 620, 200, "업체별계좌 선택", parm, "mis_ctr::CTR010101_P03.xfdl", "fn_popupCallBack", true);
}



/**********************************************************************
* Popup Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 계약번호 선택(검색조건)
	if(sPopupId == "CTR030100_P08") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
			var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
			var cntrcyYr = cntrctNo.substr(0,4);
			
			this.ds_cond.setColumn(0, "CNTRCT_YEAR", cntrcyYr);	// 계약년도
			this.ds_cond.setColumn(0, "CNTRCT_NO",   cntrctNo);	// 계약번호
			this.ds_cond.setColumn(0, "CNTRCT_NM",   cntrctNm);	// 계약명
			this.fn_search();
		}
	}
	
	// 계약업체 선택
	if(sPopupId == "CTR030100_P10") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var bcncCode     = dsTemp.getColumn(0, "BCNC_CODE");
			var bcncNm       = dsTemp.getColumn(0, "BCNC_NM");
			
			var eascRow    = this.ds_subcntr.rowposition;
			var entrpsCode = this.ds_subcntr.getColumn(eascRow, "SUBCNTR_ENTRPS_CODE");
			if(!this.comUtils.isNull(entrpsCode)) {
				var entrpsNm   = this.ds_subcntr.getColumn(eascRow, "SUBCNTR_ENTRPS");
				var exprStr    = "BCNC_CODE == '"+bcncCode+"' && SUBCNTR_ENTRPS_CODE == '"+entrpsCode+"'";
				var findRow    = this.ds_subcntr.findRowExpr(exprStr);
				if(findRow > -1) {
					var errMsg = "[하도급업체]" + "\n\n";
					errMsg    += "선택하신 하도급업체 '" + entrpsNm + "(" + entrpsCode + ")'는 이미 존재합니다." + "\n";
					errMsg    += "(" + (findRow+1)  + "행)" + "\n\n";
					errMsg    += "다른 거래처를 선택하여 주시기 바랍니다.";
					alert(errMsg);
					return false;
				}
			}
			
			this.ds_subcntr.setColumn(eascRow, "BCNC_CODE",      bcncCode );	// 원도급업체코드
			this.ds_subcntr.setColumn(eascRow, "ENTRPS_BCNC_NM", bcncNm   );	// 원도급업체명
		}
	}
	
	// 하도급 거래처 선택
	if(sPopupId == "CTR010101_P02") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var entrpsCode = dsTemp.getColumn(0, "BCNC_CODE");
			var entrpsNm   = dsTemp.getColumn(0, "BCNC_NM");
			
			var eascRow  = this.ds_subcntr.rowposition;
			var bcncCode = this.ds_subcntr.getColumn(eascRow, "BCNC_CODE");
			var exprStr  = "BCNC_CODE == '"+bcncCode+"' && SUBCNTR_ENTRPS_CODE == '"+entrpsCode+"'";
			var findRow  = this.ds_subcntr.findRowExpr(exprStr);
			if(findRow > -1) {
				var errMsg = "[하도급업체]" + "\n\n";
				errMsg    += "선택하신 하도급업체 '" + entrpsNm + "(" + entrpsCode + ")'는 이미 존재합니다." + "\n";
				errMsg    += "(" + (findRow+1)  + "행)" + "\n\n";
				errMsg    += "다른 거래처를 선택하여 주시기 바랍니다.";
				alert(errMsg);
				return false;
			}
			
			this.ds_subcntr.setColumn(eascRow, "SUBCNTR_ENTRPS_CODE", entrpsCode);	// 거래처번호
			this.ds_subcntr.setColumn(eascRow, "SUBCNTR_ENTRPS_NM",   entrpsNm  );	// 거래처명
		}
	}
	
	// 입금계좌
	if(sPopupId == "CTR010101_P03") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var bankCode    = dsTemp.getColumn(0, "BANK_CODE");			// 은행코드
			var dpstrNm     = dsTemp.getColumn(0, "DPSTR_NM");			// 예금주명
			var acnutno     = dsTemp.getColumn(0, "ACNUTNO");			// 계좌번호
			var mnbsAcnutAt = dsTemp.getColumn(0, "MNBS_ACNUT_AT");		// 주거래계좌여부
			
			var aRow = this.ds_acnut.rowposition;
			this.ds_acnut.setColumn(aRow, "BANK_CODE",     bankCode);
			this.ds_acnut.setColumn(aRow, "DPSTR_NM",      dpstrNm);
			this.ds_acnut.setColumn(aRow, "ACNUTNO",       acnutno);
			this.ds_acnut.setColumn(aRow, "MNBS_ACNUT_AT", mnbsAcnutAt);
		}
	}
}

