***********************************************************************/


/*
 * 함수명  	: fn_init
 * 설명 		: 초기화 처리(은행코드)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_init();
 */
this.fn_init = function()
{	
	// clear Datasets
	this.ds_fnncInsttCode.clearData();
	
	// transaction
	this.fn_transaction("init");
}


/*
 * 함수명  	: fn_search
 * 설명 		: 계약 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{	
	// clear Datasets
	this.ds_acnut.clearData();
	
	this.ds_subcntr.set_enableevent(false);
	this.ds_subcntr.clearData();
	this.ds_subcntr.set_enableevent(true)
	
	this.ds_cntrct.set_enableevent(false);
	this.ds_cntrct.clearData();
	this.ds_cntrct.set_enableevent(true);
	
	// masked object
	var cntrctNo = this.ds_cond.getColumn(0, "CNTRCT_NO");
	if(cntrctNo != null) {
		this.ds_cond.setColumn(0, "CNTRCT_NO", nexacro.trim(cntrctNo));
	}
	
	// transaction
	this.fn_transaction("selectCntrct");
}


/*
 * 함수명  	: fn_search_subcntr
 * 설명 		: 하도급 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_searchSub();
 */
this.fn_search_subcntr = function()
{
	// clear Datasets
	this.ds_acnut.clearData();
	
	this.ds_subcntr.set_enableevent(false);
	this.ds_subcntr.clearData();
	this.ds_subcntr.set_enableevent(true)
	
	// transaction
	if(this.ds_cntrct.getRowCount() > 0) {
		this.fn_transaction("selectSubcntr");
	}
}


/*
 * 함수명  	: fn_search_acnut
 * 설명 		: 하도급 계좌 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_searchSub();
 */
this.fn_search_acnut = function()
{
	// clear Datasets
	this.ds_acnut.clearData();
	
	// transaction
	if(this.ds_subcntr.getRowCount() > 0) {
		this.fn_transaction("selectAcnut");
	}
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리(계약결의정보)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// 데이터셋 수정 여부
	if (!this.comUtils.isDatasetUpdated(this.ds_subcntr) && !this.comUtils.isDatasetUpdated(this.ds_acnut)) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation 체크(하도급)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_subcntr, this.ds_validation)) {
		return;
	}
	
	// validation 체크(계좌)
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_acnut, this.ds_validation)) {
		return;
	}
	
	if(this.gfn_message("confirm.저장.여부")) {
		this.saveRow = this.ds_cntrct.rowposition;
		this.fn_transaction("save");
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "init":
			var sController   	= "/hsco/mis/ctr/CTR010101/selectFnncInsttCodeList.do";
			var sInDatasets   	= "ds_cond=ds_cond";
			var sOutDatasets  	= "ds_fnncInsttCode=ds_fnncInsttCode";
			break;
		case "selectCntrct":
			var sController   	= "/hsco/pms/prj/PRJ010111/selectCntrct.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cntrct=output1";
			break;
		case "selectSubcntr":
			var sController   	= "/hsco/pms/prj/PRJ010111/selectSubcntr.do";
			var sInDatasets   	= "input1=ds_condSub1";
			var sOutDatasets  	= "ds_subcntr=output1";
			break;
		case "selectAcnut":
			var sController   	= "/hsco/pms/prj/PRJ010111/selectAcnut.do";
			var sInDatasets   	= "input1=ds_condSub2";
			var sOutDatasets  	= "ds_acnut=output1";
			break;
		case "save":
			var sController   	= "/hsco/pms/prj/PRJ010111/save.do";
			var sInDatasets   	= "input1=ds_subcntr:U input2=ds_acnut:U";
			var sOutDatasets  	= "";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0'){
		switch (strSvcId) {
			case "init":
				this.fn_search();
				break;
			case "selectCntrct":
				if(this.saveRow > -1){
					this.ds_cntrct.set_rowposition(this.saveRow);
					this.saveRow = -1;
				}
				break;
			case "save":
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}



