 /*
 * 함수명  	: ds_chng_oncolumnchanged
 * 설명 		: 분양가 변경 입력또는 변경시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_chng_oncolumnchanged();
 */
this.ds_chng_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(obj.getRowType(e.row) == 2 ||			// 신규
	   obj.getRowType(obj.rowcount - 1) == 4)	// 수정
	{
		var Tot = parseInt(e.newvalue)||0;
		switch(e.columnid)
		{
			default:
				return;
			case 'PLOT_CT':				// 대지비
				Tot += parseInt(obj.getColumn(e.row , 'BILDNG_CT'))||0;
				break;
			case 'BILDNG_CT':			// 건축비
				Tot += parseInt(obj.getColumn(e.row , 'PLOT_CT'))||0;
				break;
			case 'AR_PPRTY_AMOUNT':		// 면적당금액
				break;
		}
		obj.setColumn(e.row , 'TOTAL_CT' , Tot);

		var ds = this.ds_calc;
		ds.setColumn(ds.rowposition, e.columnid, e.newvalue);
	}
}

 /*
 * 함수명  	: ds_tot_oncolumnchanged
 * 설명 		: 분양가상한금액 대지비 . 건축비 입력또는 변경시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_tot_oncolumnchanged();
 */
this.ds_tot_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var Tot = parseInt(e.newvalue)||0;
	switch(e.columnid)
	{
		default:
			return;
		case 'PLOT_CT':				// 대지비
			Tot += parseInt(obj.getColumn(e.row, 'BILDNG_CT'))||0;
			break;
		case 'BILDNG_CT':			// 건축비
			Tot += parseInt(obj.getColumn(e.row, 'PLOT_CT'))||0;
			break;
	}

	obj.setColumn(e.row, 'TOTAL_CT', Tot);
}

 /*
 * 함수명  	: ds_calc_oncolumnchanged
 * 설명 		: 분양가산정 대지비 . 건축비 입력또는 변경시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_calc_oncolumnchanged();
 */
this.ds_calc_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var Tot = parseInt(e.newvalue)||0;
	switch(e.columnid)
	{
		default:
			return;
		case 'PLOT_CT':				// 대지비
			Tot += parseInt(obj.getColumn(e.row , 'BILDNG_CT'))||0;
			break;
		case 'BILDNG_CT':			// 건축비
			Tot += parseInt(obj.getColumn(e.row , 'PLOT_CT'))||0;
			break;
	}

	obj.setColumn(e.row , 'TOTAL_CT' , Tot);
}
