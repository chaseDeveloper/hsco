// 사업코드 팝업
this.fn_PopupBsns = function()
{
	var fn_cb = function(sPopupId, sReturn) {
		if(!sReturn) return;
		this.debug('OK\n'  + sReturn);

		var dsRet = new Dataset();
		nRowCnt = dsRet.loadXML(sReturn);
		if(nRowCnt < 1) return;

		this.debug('BSNS:' + dsRet.getColumn(0, 'BSNS_CODE'));

		var ds = this.ds_cond1;
		var row = ds.rowposition;
		ds.copyRow(row , dsRet, 0, 
			 'BSNS_CODE=BSNS_CODE,'
			+'BSNS_NM=BSNS_NM,'
			+'BSNS_TY=BSNS_TY,'
			+'STRWRK_DE=STRWRK_DE,'
			+'COMPET_DE=COMPET_DE,'
		);

		var val = ds.getColumn(row, 'STRWRK_DE');
		if(val)
			ds.setColumn(row, 'YEAR', val.substr(0,4));

		this.fn_Profile(ds, 1);

		this.fn_search();
	}

	var param = {
		BSNS_GB:1
	};
	this.gfn_popup('popBsns', 500, 500, '', param, "pms_prj::popBsns.xfdl", fn_cb, 1);
}

/**********************************************************************
 * 동적 그리드 처리
 ***********************************************************************/
this.fn_makeGrid = function(ds_s:Dataset)
{
	var grd = this.div_work.div_1.grd_1;
	grd.set_autofittype('');

	var yr;
	var col; 
	var nMin = 3;
	var nMax = nMin + 10;
	var nCnt = ds_s.rowcount;
	var nSize = 80;//83;
	var n = 0;
	var i = nMin;
	for (; i < nMin + nCnt; i++) 
	{
		yr = ds_s.getColumn(n, 'BSNS_YEAR');
		this.debug('n:'+n+'/'+i+'=' + yr + ' AMT'+n);

		grd.setCellProperty("head", i, "text", yr);
		grd.setFormatColProperty(i, "size", nSize);
		n++;
	}

	for (; i < nMax; i++) 
		grd.setFormatColProperty(i, "size", 0);

	this.fn_initSumDS(grd, this.ds_1);
	this.fn_copyData(grd, this.ds_1, ds_s);
	this.ds_1.applyChange();
//	grd.set_autofittype('col');
//	this.fn_makeGrid2(ds_s);
}

this.fn_initSumDS = function(grd:Grid, ds:Dataset)
{
	var g_GBs = {
		 1:'수입부문\n(천원)'	// 구분1
		,2:'지출부문\n(천원)'

		,3:'수지차액(계획)'		// 구분(집계)
		,4:'수지차액(실적)'
		,5:'계획대비실적'

		,6:'계획금액'			// 구분2
		,7:'실적금액'
		,8:'차액'
	};

	var g_GB1 = [1,1,1,2,2,2];//,0,0,0];
	var g_GB2 = [6,7,8,6,7,8,3,4,5];

	var row;
	ds.clearData();
	for (var i = 0; i < g_GB1.length; i++)
	{
		row = ds.addRow();
		ds.setColumn(row, 'GB', ''+g_GB1[i]+g_GB2[i]);	// Col 0,1
//		ds.setColumn(row, 'GB1', g_GB1[i]);	// Col 0
//		ds.setColumn(row, 'GB2', g_GB2[i]);	// Col 1

 		ds.setColumn(row, 'GB1', g_GBs[g_GB1[i]]);	// Col 0
 		ds.setColumn(row, 'GB2', g_GBs[g_GB2[i]]);	// Col 1
	}
}

this.fn_copyData = function(grd:Grid, ds:Dataset, ds_s:Dataset)
{
	if(!ds_s) return;
	if(ds_s.rowcount < 1) return;

	var nCnt = ds_s.rowcount;

	var g_SumCols = [1,2,'_D1',3,4,'_D2','_D3','_D4','_D5'];

	// 합계
	for (var i = 0; i < g_SumCols.length; i++)
	{
		ds.setColumn(i, 'AMT_S', ds_s.getSum('parseInt(AMT' + g_SumCols[i]+')||0'));	// Col 2
//		this.debug('AMT_S i:'+i+'='+ds_s.getSum('parseInt(AMT' + g_SumCols[i]+')||0'));
	}

	for (var i = 0; i < nCnt; i++) 
	{
//	this.debug('i:'+i+'='+ds_s.getColumn(i,'AMT1'));
		ds.setColumn(0, 'AMT'+i, ds_s.getColumn(i,'AMT1'));		// 수입-계획
		ds.setColumn(1, 'AMT'+i, ds_s.getColumn(i,'AMT2'));		// 수입-실적
		ds.setColumn(2, 'AMT'+i, ds_s.getColumn(i,'AMT_D1'));	// 수입-차액
		ds.setColumn(3, 'AMT'+i, ds_s.getColumn(i,'AMT3'));		// 지출-계획
		ds.setColumn(4, 'AMT'+i, ds_s.getColumn(i,'AMT4'));		// 지출-실적
		ds.setColumn(5, 'AMT'+i, ds_s.getColumn(i,'AMT_D2'));	// 지출-차액
		ds.setColumn(6, 'AMT'+i, ds_s.getColumn(i,'AMT_D3'));	// 수지차액(수입)
		ds.setColumn(7, 'AMT'+i, ds_s.getColumn(i,'AMT_D4'));	// 수지차액(지출)
		ds.setColumn(8, 'AMT'+i, ds_s.getColumn(i,'AMT_D5'));	// 계획대비실적
	}
	ds.set_rowposition(0);
}
