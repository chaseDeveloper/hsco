/*
 * 함수명  	: btn_add_onclick
 * 설명 		: '행추가' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_add_onclick(obj,e);
 */
this.btn_add_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow = this.ds_competInspct.rowposition;
	var ccwAdtgSn = this.ds_competInspct.getColumn(pRow, "CCW_ADTG_SN");
	
	if(pRow < 0) {
		this.gfn_message("comm.항목.선택.요청","준공검사 진행 현황");
		return false;
	}
	
	var nRow = this.ds_insptrList.addRow();
	
	if(!this.comUtils.isNull(ccwAdtgSn)) {
		this.ds_insptrList.setColumn(nRow, "CCW_ADTG_SN", ccwAdtgSn);
	}
}

/*
 * 함수명  	: btn_delete_onclick
 * 설명 		: '행삭제' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_delete_onclick(obj,e);
 */
this.btn_delete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_insptrList.rowposition;
	
	if(nRow < 0) {
		this.gfn_message("comm.항목.선택.요청","검사자 명단");
		return false;
	}
	if(this.gfn_message("confirm.삭제여부")){
		this.ds_insptrList.deleteRow(nRow);
	}
}

/*
 * 함수명  	: btn_cancel_onclick
 * 설명 		: '취소' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_cancel_onclick(obj,e);
 */
this.btn_cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_insptrList)){
		if(this.gfn_message("confirm.취소여부")){
			this.ds_insptrList.reset();
			this.ds_insptrList.applyChange();
			return;
		}	
	}else{
		this.gfn_message("comm.내역.변경.없음");
	}
}

/*
 * 함수명  	: btn_cnfirm_onclick
 * 설명 		: '확인' 버튼 클릭 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.btn_cnfirm_onclick(obj,e);
 */
this.btn_cnfirm_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var pRow = this.ds_competInspct.rowposition;
	var cnfirmAt  = this.ds_competInspct.getColumn(pRow, "CNFIRM_AT");
	var ccwAdtgSn = this.ds_competInspct.getColumn(pRow, "CCW_ADTG_SN");
	var iCnt = this.ds_insptrList.rowcount;
	
	if(pRow < 0) {
		this.gfn_message("comm.항목.선택.요청","준공검사 진행 현황");
		return false;
	}
	if(this.comUtils.isNull(ccwAdtgSn)) {
		this.gfn_message("info.처리불가", "준공검사 진행 현황을 저장 후 다시 시도하여 주십시오.");
		return false;
	}
	if(iCnt < 1) {
		this.gfn_message("info.처리불가", "검사자 명단 정보가 없습니다.\n확인 후 다시 시도하여 주십시오.");
		return false;
	}
	
	if(cnfirmAt != '1') {
		if(!this.gfn_message("comm.행위여부", "선택된 준공검사 진행 현황을 확인합니다.\n계속 진행")) {
			return false;
		}
	}
	else {
		if(!this.gfn_message("comm.행위여부", "선택된 준공검사 진행 현황을 취소합니다.\n계속 진행")) {
			return false;
		}
	}
	
	this.ds_updateCnfirmAt.clearData();
	var nRow = this.ds_updateCnfirmAt.addRow();
	this.ds_updateCnfirmAt.setColumn(nRow, "CCW_ADTG_SN", ccwAdtgSn);
	
	this.fn_transaction("updateCnfirmAt");
}


/**********************************************************************
* 공통함수 처리
***********************************************************************/

 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function()
{
	var nRow = this.ds_cntrct.rowposition;
	if(nRow < 0) {
		this.gfn_message("info.처리불가", "계약정보가 존재하지 않습니다. 조회 후 다시 시도하여 주십시오.");
		return false;
	}
	
	if(Ex.util.isUpdated(this.ds_insptrList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	
	var parm = {
	     arg_0		: this
	    ,cntrctYr	: this.ds_cond.getColumn(0, "CNTRCT_YEAR")
	    ,cntrctNo	: this.ds_cond.getColumn(0, "CNTRCT_NO")
	    ,cntrctNm	: this.ds_cond.getColumn(0, "CNTRCT_NM")
	};
	this.gfn_popup("pymntPop", 850, 400, "", parm, "pms_prj::PRJ010119_P01.xfdl", "fn_popupCallBack", true);
}


 /*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function()
{
	var nRow = this.ds_competInspct.rowposition;
	var enableAt = this.ds_competInspct.getColumn(nRow, "ENABLE_AT");
	if(nRow < 0) {
		this.gfn_message("comm.항목.선택.요청","준공검사 진행 현황");
		return false;
	}
	if(enableAt == '0') {
		this.gfn_message("info.처리불가", "이미 확인된 준공검사는 수정할 수 없습니다.");
		return false;
	}
	
	if(this.gfn_message("confirm.삭제여부")) {
		this.ds_competInspct.deleteRow(nRow);
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 이벤트 처리(Dataset 변경 내역 Reset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel= function()
{
	if(Ex.util.isUpdated(this.ds_competInspct) || Ex.util.isUpdated(this.ds_insptrList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
		this.ds_competInspct.reset();
		this.ds_competInspct.applyChange();
		this.ds_insptrList.reset();
		this.ds_insptrList.applyChange();
		
		this.ds_condSub.clearData();
		var nRow = this.ds_condSub.addRow();
		var pRow = this.ds_competInspct.rowposition;
		var ccwAdtgSn = this.ds_competInspct.getColumn(pRow, "CCW_ADTG_SN");
		var docSn     = this.ds_competInspct.getColumn(pRow, "DOC_SN");
		
		this.ds_condSub.setColumn(nRow, "CCW_ADTG_SN", ccwAdtgSn);
		this.ds_condSub.setColumn(nRow, "DOC_SN"     , docSn);
		
		this.fn_search_sub();
	}
}



/**********************************************************************
* Dataset 관련 이벤트 처리
***********************************************************************/

/*
 * 함수명  	: ds_competInspct_onrowposchanged
 * 설명 		: Dataset 행 변경 후 이벤트 처리(준공검사 dataset)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_competInspct_onrowposchanged(obj,e);
 */
this.ds_competInspct_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	if(e.newrow > -1) {
		// 변경여부 체크
		if(Ex.util.isUpdated(this.ds_insptrList)) {
			if(!this.gfn_message("confirm.변경.취소.여부")) {
				return false;	// 아니오
			}
		}
		
		this.ds_condSub.clearData();
		var nRow = this.ds_condSub.addRow();
		var ccwAdtgSn = obj.getColumn(e.newrow, "CCW_ADTG_SN");
		var docSn     = obj.getColumn(e.newrow, "DOC_SN");
		
		this.ds_condSub.setColumn(nRow, "CCW_ADTG_SN", ccwAdtgSn);
		this.ds_condSub.setColumn(nRow, "DOC_SN"     , docSn);
		
		this.fn_search_sub();
	}
	else {
		this.ds_insptrList.clearData();
		this.ds_ctgry_file.clearData();
	}
}


/*
 * 함수명  	: ds_ctgry_file_onrowposchanged
 * 설명 		: Dataset 행 변경 후 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_ctgry_file_onrowposchanged(obj,e);
 */
this.ds_ctgry_file_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	// clear Datasets
	this.ds_fileList.clearData();
	
	if(e.newrow > -1) {
		var fileSn = obj.getColumn(e.newrow, "FILE_SN");
		
		this.ds_cond_file.clearData();
		var nRow = this.ds_cond_file.addRow();
		this.ds_cond_file.setColumn(nRow, "FILE_SN", fileSn);
		
		this.fn_transaction("selectAtchFile");
	}
}


