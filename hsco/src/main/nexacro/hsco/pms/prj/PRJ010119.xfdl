// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "mis_lib::misUtil.xjs";
include "lib::comUpDownUtils.xjs";


/**********************************************************************
* 화면 변수 선언부
***********************************************************************/
this.File_UpDownManager = null;
this.chargerAt = false;
var saveRow = -1;


/**********************************************************************
* onInit / onLoad 영역
***********************************************************************/

this.PRJ010119_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// ..
}

this.PRJ010119_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// form 초기화
	this.fn_init_form();

	// dataset 초기화
	this.fn_init_dataset();
}



/**********************************************************************
* Form/Dataset 오브젝트 초기화
***********************************************************************/

/*
 * 함수명  	: initForm
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);
	this.fn_misFormInit(this);
	
	this.ds_cond.clearData();
	var nRow = this.ds_cond.addRow();
	this.ds_cond.setColumn(nRow, "CNTRCT_YEAR"   , this.comUtils.getClientDate("YYYY"));	// 당해년도
	this.ds_cond.setColumn(nRow, "APRV_PGM_ID"   , "준공검사등록");
	this.ds_cond.setColumn(nRow, "CCW_ADTG_DS_CD", "2");
 	
 	// 공통코드
	var comboParams = [
	    ["ds_hrm002", "HRM002", "Y", "D", "B"]	// ds_hrm002 : 직급
	   ,["ds_hrm003", "HRM003", "Y", "D", "B"]	// ds_hrm003 : 직위
	];
	var callback = function (svcId, errorCode, errorMsg) {
		// 접속자 권한 체크
		if((this.gfn_authGrpId("CTR_ADMIN") != -1)) {	// 계약담당자
			this.chargerAt = true;
		}
		if((this.gfn_authGrpId("PRJ_ADMIN") != -1)) {	// 사업담당자
			this.chargerAt = true;
		}
		
		if(this.chargerAt) {
			this.btn_cnfirm.set_visible(true);
		}
	};
	this.gfn_comboLoad (comboParams, callback);
}


/*
 * 함수명  	: initDataset
 * 설명 		: Dataset을 초기화한다
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	var fileConfig = {
		useMultiUpload	: true,
		onChange		: this.fn_onFileSelected,	// 파일 선택 이벤트 핸들러
		onSuccess		: this.fn_onFileSuccess		// 전송 성공 이벤트 핸들러
	};
	this.File_UpDownManager = new this.fileUpDownManager(this, "PMS", "PRJ");
	this.File_UpDownManager.init(fileConfig);
}



/**********************************************************************
* Transaction 영역 시작
***********************************************************************
*
* 1. Transaction 관련 이벤트 핸들러
* 2. call Transaction 영역
* 3. callback 영역(Transaction, Popup)
* 
***********************************************************************/



/**********************************************************************
* Transaction 관련 이벤트 핸들러 
***********************************************************************/

/*
 * 함수명  	: fn_search
 * 설명 		: 조회 이벤트 처리(준공검사)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
		return;
	}
	
	if(Ex.util.isUpdated(this.ds_competInspct) || Ex.util.isUpdated(this.ds_insptrList)) {
		if(!this.gfn_message("confirm.변경.취소.여부")) {
			return false;	// 아니오
		}
	}
	
	// clear Datasets
	this.ds_competInspct.set_enableevent(false);
	this.ds_competInspct.clearData();
	this.ds_competInspct.set_enableevent(true);
	this.ds_insptrList.clearData();
	this.ds_ctgry_file.set_enableevent(false);
	this.ds_ctgry_file.clearData();
	this.ds_ctgry_file.set_enableevent(true);
	
	// transaction
	this.fn_transaction("selectCompetInspct");
}


/*
 * 함수명  	: fn_search_sub
 * 설명 		: 조회 이벤트 처리(검사자명단, 첨부파일)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_sub();
 */
this.fn_search_sub = function()
{
	this.ds_insptrList.clearData();
	this.ds_ctgry_file.set_enableevent(false);
	this.ds_ctgry_file.clearData();
	this.ds_ctgry_file.set_enableevent(true);
	
	// transaction
	this.fn_transaction("selectInsptrList");
}


/*
 * 함수명  	: fn_search_ctgryFile
 * 설명 		: 표준문서 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search_ctgryFile();
 */
this.fn_search_ctgryFile = function()
{
	// clear Datasets
	this.ds_ctgry_file.set_enableevent(false);
	this.ds_ctgry_file.clearData();
	this.ds_ctgry_file.set_enableevent(true)
	
	// transaction
	if(this.ds_competInspct.getRowCount() > 0) {	// 부모데이터셋이 존재할때만 조회
		this.fn_transaction("selectCtgryFile");
	}
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	// update check
	if (   !this.comUtils.isDatasetUpdated(this.ds_competInspct) && !this.comUtils.isDatasetUpdated(this.ds_insptrList)
	   ) {
		this.gfn_message("comm.데이터.변경.없음");
		return false;
	}
	
	// validation check
	if(!this.gfn_checkValidation_NoUpdChk(this.ds_competInspct, this.ds_validation)) {			// 준공검사
		return false;
	} else if(!this.gfn_checkValidation_NoUpdChk(this.ds_insptrList, this.ds_validation)) {		// 검사자명단
		return false;
	}
	
	// save
	if(this.gfn_message("confirm.저장.여부"))	{
		saveRow = this.ds_competInspct.rowposition;
		this.fn_transaction("save");
	}
}



/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	switch (strSvcId) {
		case "selectCompetInspct":	// 조회(계약정보, 준공검사)
			var sController   	= "hsco/pms/prj/PRJ010119/selectCompetInspct.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_cntrct=output1 ds_competInspct=output2";
			break;
		case "selectInsptrList":		// 조회(검사자명단, 첨부파일)
			var sController   	= "hsco/pms/prj/PRJ010119/selectInsptrList.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_insptrList=output1 ds_ctgry_file=output2";
			break;
		case "save":					// 저장
			var sController   	= "hsco/pms/prj/PRJ010119/save.do";
			var sInDatasets   	= "input1=ds_competInspct:U input2=ds_insptrList:U";
			var sOutDatasets  	= "";
			break;
		case "selectCtgryFile":			// 표준문서조회
			var sController   	= "hsco/pms/prj/PRJ010119/selectCtgryFile.do";
			var sInDatasets   	= "input1=ds_condSub";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "saveCtgryFile":			// 표준문서갱신
			var sController   	= "hsco/pms/prj/PRJ010119/saveCtgryFile.do";
			var sInDatasets   	= "input1=ds_ctgry_file:A";
			var sOutDatasets  	= "ds_ctgry_file=output1";
			break;
		case "selectAtchFile":			// 파일조회
			var sController 	= "hsco/cmm/file/selectAtchmnFileList.do";
			var sInDatasets		= "input1=ds_cond_file";
			var sOutDatasets	= "ds_fileList=output1";
			break;
		case "deleteFile":				// 파일삭제
			var sController		= "hsco/cmm/file/deleteFile.do";
			var sInDatasets		= "input1=ds_fileList:U";
			var sOutDatasets	= "ds_fileList=output1";
			break;
		case "updateCnfirmAt":			// 확인여부 업데이트
			var sController   	= "hsco/pms/prj/PRJ010119/updateCnfirmAt.do";
			var sInDatasets		= "input1=ds_updateCnfirmAt";
			var sOutDatasets	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}



/**********************************************************************
* Transaction Callback 영역
***********************************************************************/

 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != '0') {
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	if (nErrorCode == '0'){
		switch (strSvcId) {
			case "selectCompetInspct":	// 조회(준공검사)
				if(saveRow > -1){
					this.ds_competInspct.set_rowposition(saveRow);
					saveRow = -1;
				}
				break;
			case "selectInsptrList":		// 조회(검사자명단, 첨부파일)
				break;
			case "save":					// 저장
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
			case "saveCtgryFile":			// 표준문서갱신
				var cfRow = this.ds_ctgry_file.rowposition;
				var docSn = this.ds_ctgry_file.getColumn(cfRow, "DOC_SN");
				
				this.ds_competInspct.set_enableevent(false);
				this.ds_competInspct.set_updatecontrol(false);
				var fRow = this.ds_competInspct.rowposition;
				this.ds_competInspct.setColumn(fRow, "DOC_SN", docSn);
				this.ds_competInspct.set_updatecontrol(true);
				this.ds_competInspct.set_enableevent(true);
				break;
			case "deleteFile":				// 파일삭제
				this.fn_transaction("selectCtgryFile");
				break;
			case "selectAtchFile":			// 파일조회
				if(this.ds_fileList.getRowCount() > 0) {
					var fRow     = this.ds_fileList.rowposition;
					var fileSn   = this.ds_fileList.getColumn(fRow, "FILE_SN");
					var fileNm   = this.ds_fileList.getColumn(fRow, "ORGINL_FILE_NM");
					var fileOrdr = this.ds_fileList.getColumn(fRow, "FILE_ORDR");
					
					var cRow = this.ds_ctgry_file.rowposition;
					this.ds_ctgry_file.setColumn(cRow, "FILE_SN", fileSn);
					this.ds_ctgry_file.setColumn(cRow, "ORGINL_FILE_NM", fileNm);
					this.ds_ctgry_file.setColumn(cRow, "FILE_ORDR", fileOrdr);
					
					var docSn = this.ds_ctgry_file.getColumn(cRow, "DOC_SN");
					var fileId = this.ds_ctgry_file.getColumn(cRow, "CTGRY_FILE_ID");
					if(this.comUtils.isNull(docSn) || this.comUtils.isNull(fileId)) {
						this.fn_transaction("saveCtgryFile");
					}
				}
				break;
			case "updateCnfirmAt":			// 확인여부 업데이트
				this.gfn_message("success.처리.성공");
				this.fn_search();
				break;
		}
	}
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	// 계약번호
	if(sPopupId == "cntrctPop") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var cntrctNo = dsTemp.getColumn(0, "CNTRCT_NO");
			var cntrctNm = dsTemp.getColumn(0, "CNTRCT_NM");
			var cntrctYr = cntrctNo.substr(0,4);
			
			this.ds_cond.setColumn(0, "CNTRCT_YEAR", cntrctYr);	// 계약년도
			this.ds_cond.setColumn(0, "CNTRCT_NO",   cntrctNo);	// 계약번호
			this.ds_cond.setColumn(0, "CNTRCT_NM",   cntrctNm);	// 계약명
			this.fn_search();
		}
	}
	
	// 검사자
	if(sPopupId == "insptrPop") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			var empNo    = dsTemp.getColumn(0, "EMPNO");
			var empNm    = dsTemp.getColumn(0, "EMPNM");
			var deptCode = dsTemp.getColumn(0, "DEPT_CODE");
			var deptNm   = dsTemp.getColumn(0, "DEPT_NM");
			var ofcpsSe  = dsTemp.getColumn(0, "OFCPS_SE");
			var clsfSe   = dsTemp.getColumn(0, "HR_CLSF_SE");
			
			var nRow = this.ds_insptrList.rowposition;
			this.ds_insptrList.setColumn(nRow, "EMPNO"     , empNo);		// 사번
			this.ds_insptrList.setColumn(nRow, "EMPNM"     , empNm);		// 성명
			this.ds_insptrList.setColumn(nRow, "DEPT_CODE" , deptCode);		// 부서코드
			this.ds_insptrList.setColumn(nRow, "DEPT_NM"   , deptNm);		// 부서명
			this.ds_insptrList.setColumn(nRow, "OFCPS_SE"  , ofcpsSe);		// 직위
			this.ds_insptrList.setColumn(nRow, "HR_CLSF_SE", clsfSe);		// 직급
		}
	}
	
	// 준공금지급내역
	if(sPopupId == "pymntPop") {
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
		if(loadCnt) {
			this.ds_insptrList.clearData();
			this.ds_ctgry_file.clearData();
			
			var cntrctNo         = dsTemp.getColumn(0, "CNTRCT_NO");				// 계약번호
			var cntrctNm         = dsTemp.getColumn(0, "CNTRCT_NM");				// 계약명
			var frstCntrctAmount = dsTemp.getColumn(0, "FRST_CNTRCT_AMOUNT");		// 최초계약금액
			var cntrctAmount     = dsTemp.getColumn(0, "CNTRCT_AMOUNT");			// 계약금액
			var cntrctDe         = dsTemp.getColumn(0, "CNTRCT_DE");				// 계약일자
			var strwrkDe         = dsTemp.getColumn(0, "STRWRK_DE");				// 착공일자
			var competDe         = dsTemp.getColumn(0, "COMPET_DE");				// 준공일자
			var ccwAmt           = dsTemp.getColumn(0, "PRSEC_AMOUNT");				// 변경금액(준공금액)
			var adtgPsncSn       = dsTemp.getColumn(0, "WTNESS_SN");				// 입회순번
			var prsecDe          = dsTemp.getColumn(0, "PRSEC_DE");					// 검사일자
			var prsecTm          = dsTemp.getColumn(0, "PRSEC_TM");					// 검사시각
			var prsecPlace       = dsTemp.getColumn(0, "PRSEC_PLACE");				// 검사장소
			var sbmDt            = dsTemp.getColumn(0, "PRSEC_DOC_PRESENTN_DE");	// 검사원제출일
			
			var nRow = this.ds_competInspct.addRow();
			this.ds_competInspct.setColumn(nRow, "CNTRCT_NO"         , cntrctNo);			// 계약번호
			this.ds_competInspct.setColumn(nRow, "CNTRCT_NM"         , cntrctNm);			// 계약명
			this.ds_competInspct.setColumn(nRow, "FRST_CNTRCT_AMOUNT", frstCntrctAmount);	// 최초계약금액
			this.ds_competInspct.setColumn(nRow, "CNTRCT_AMOUNT"     , cntrctAmount);		// 계약금액
			this.ds_competInspct.setColumn(nRow, "CNTRCT_DE"         , cntrctDe);			// 계약일자
			this.ds_competInspct.setColumn(nRow, "STRWRK_DE"         , strwrkDe);			// 착공일자
			this.ds_competInspct.setColumn(nRow, "COMPET_DE"         , competDe);			// 준공일자
			this.ds_competInspct.setColumn(nRow, "CCW_AMT"           , ccwAmt);				// 준공금액
			this.ds_competInspct.setColumn(nRow, "ADTG_PSNC_SN"      , adtgPsncSn);			// 입회순번
			this.ds_competInspct.setColumn(nRow, "PRSEC_DE"          , prsecDe);			// 검사일자
			this.ds_competInspct.setColumn(nRow, "PRSEC_TM"          , prsecTm);			// 검사시각
			this.ds_competInspct.setColumn(nRow, "PRSEC_PLACE"       , prsecPlace);			// 검사장소
			this.ds_competInspct.setColumn(nRow, "SBM_DT"            , sbmDt);				// 검사원제출일
			this.ds_competInspct.setColumn(nRow, "CCW_ADTG_DS_CD"    , "2");				// 준공검사구분코드
			this.ds_competInspct.setColumn(nRow, "CNFIRM_AT"         , "0");
			this.ds_competInspct.setColumn(nRow, "ENABLE_AT"         , "1");
		}
	}
}


