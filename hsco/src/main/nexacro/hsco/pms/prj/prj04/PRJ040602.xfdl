/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명		: fn_transaction
 * 설명			: Transaction 요청 처리 함수
 * params		: strSvcId : 서비스요청 ID
 * return Type	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	if(this.ds_cond1.rowcount < 1) return;

	var ds_s;
	var sCols;
	var sController;
	var sInDatasets = "input1=ds_cond1";
	var sOutDatasets = "";
	switch (strSvcId) {
		case "ctrList":			// 계약목록 조회
			ds = this.ds_cond1;
			ds_s = this.div_PRJ.ds_PRJ;
			sCols = "BSNS_CODE=BSNS_CODE,";
			sController = "hsco/pms/prj/ctrList.do";
			sOutDatasets = "ds_ctr=output1";
			this.ds_cond1.setColumn(0, "CTR_GB", "2");		// 계약구분 [2 : 용역]
			break;
		case "mList":			// 기성내역 조회
			ds = this.ds_cond1;
			ds_s = this.ds_ctr;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,";
			sOutDatasets = "ds_1=output1";
			this.ds_cond1.setColumn(0, "PXCOND_SE", "2");	// 기성구분 [1 : 기성]
			break;
		case "fList":			// 상세 조회
			ds_s = this.ds_1;
			var cd = ds_s.getColumn(ds_s.rowposition, 'FILE_SN');
			var div = this.div_work.div_32;
			return div.fn_fileList(cd);
		case "rList":			// 상세 조회
			ds = this.ds_2;
			ds_s = this.ds_1;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"PXCOND_SE=PXCOND_SE,"
				   +"ODR=ODR,"
				   +"REQST_SE=REQST_SE,";
			sOutDatasets  =  "ds_2=output1";	// 감독원
			sOutDatasets += " ds_3=output2";	// 기성양식구분
			break;
		case "save":			// 기성내역 저장
			this.g_row1 = this.ds_1.rowposition;

			sController = "hsco/pms/prj/PRJ050401/save.do";
			sInDatasets  =  "input1=ds_1:U";	// 기성내역
			sInDatasets += " input2=ds_2:U";	// 감독원
			sInDatasets += " input3=ds_3:U";	// 기성양식구분
			/*
			// 2016.08.24
			sInDatasets += " inputF=ds_F:U";
			var div = this.div_work.div_32;
			if(div.fn_upload(sController, sInDatasets, '')) return;
			*/
			break;
		case "approve":			// 전자결재
			sInDatasets  =  "input1=ds_aud:U";
			sOutDatasets = " ds_approve=output1";
			break;
	}

	// clearData
	switch (strSvcId) {
		case "ctrList":		// 계약목록 조회
			this.ds_ctr.clearData();
		case "mList":		// 기성내역 조회
			this.ds_1.clearData();
		case "rList":		// 상세 조회
			this.ds_2.clearData();
			this.ds_3.clearData();
			//this.ds_f.clearData();
	}

	if(sCols) {
		this.ds_cond1.copyRow(0, ds_s, ds_s.rowposition, sCols);
	}

	if(!sController) {
		sController = "hsco/pms/prj/PRJ050401/" + strSvcId + ".do";
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);// , "", sCallbackFunc, sShowProgress, bAsync, bBinary); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명		: fn_callBack
 * 설명			: Transaction Callback 처리
 * params		: strSvcId    : 서비스요청 ID
 *					nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *					strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0) 
	{
		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		return this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	switch(strSvcId)
	{
		case "mList":		// 기성내역 조회
			break;
		case "rList":
			if(this.g_row1) {
				this.ds_1.set_rowposition(this.g_row1);
				this.g_row1 = null;
			}

			if(!this.ds_3_tmp) return;

			for(var i=0;i < this.ds_3_tmp.rowcount;i++) {
				this.ds_3.addRow();
				this.ds_3.copyRow(i, this.ds_3_tmp, i);
				this.ds_3.copyRow(i, this.ds_1, this.ds_1.rowposition);
			}
			this.ds_3_tmp = null;

			break;
		case "save":		// 기성내역 저장
			this.fn_transaction("mList");
			break;
		case "approve":		// 전자결재
			this.fn_popup_sanction();
			break;
	}
}

/**********************************************************************
 * Transaction Callback 영역 끝
 ***********************************************************************/
 
/**********************************************************************
 * Non-Transaction 영역
 ***********************************************************************/
this.fn_print = function()
{
	var vOpts = new this.cf_Opts();

	var ds1 = this.ds_1;
