this.fn_insertDS = function(ds:Dataset)
{
	var cd;
	var ds_s;
	var sCols;

	var row = ds.addRow();
	switch(ds)
	{
		case this.ds_1:
			ds_s = this.ds_ctr;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"CNTRCT_SE=CNTRCT_SE,"
			;
			var dt = this.gfn_today();
			cd = (row < 1) ? '1' : '' + (1 + parseInt(ds.getColumn(row-1, 'ODR')));
			ds.setColumn(row, 'ODR', cd);			// 차수
			ds.setColumn(row, 'REQUST_DE', dt);		// 기성요청일자
			ds.setColumn(row, 'PXCOND_SE', '2');	// 기성구분 [2 : 완성]
			break;
		case this.ds_2:
			ds_s = this.ds_1;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"PXCOND_SE=PXCOND_SE,"
				   +"ODR=ODR,"
			;
			break;
		case this.ds_3:
			ds_s = this.ds_1;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"PXCOND_SE=PXCOND_SE,"
				   +"ODR=ODR,"
			;
			cd = this.div_work.div_4.div_1.cmb_SANCTN_FORM_SE.value;
			ds.setColumn(row, 'SANCTN_FORM_SE', cd);
			break;
	}

	if(sCols)
		ds.copyRow(row, ds_s, ds_s.rowposition, sCols);
}

this.fn_deleteDS = function(ds:Dataset)
{
	ds.deleteRow(ds.rowposition);
}


this.fn_checkUpdated = function(bConfirm)
{
	if(!this.checkDs)
		return false;

	for(var i = 0; i < this.checkDs.length; i++) {
		if(this.comUtils.isDatasetUpdated(this.checkDs[i])) {
			if(!bConfirm) return true;

			var bCancel = this.gfn_message("confirm.변경.취소.여부");
			if(!bCancel) {
				this.checkDs[i].reset();	// 수정사항 되도록리기
			}

			return bCancel;
		}
	}
}

