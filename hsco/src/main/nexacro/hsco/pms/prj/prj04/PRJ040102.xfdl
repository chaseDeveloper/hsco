		this.ds_1.copyRow(this.ds_1.rowposition, dsRet, 0, sCols);
	}

	var param = {
//		BSNS_GB:2,		// 사업유형 : 1-진행사업, 2-완료사업
	};
	this.gfn_popup('popBsns', 500, 500, '', param, "com::empno_P01.xfdl", fn_cb, 1);
