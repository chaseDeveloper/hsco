this.fn_xls_dn_post = function(ds:Dataset)
{
	if(this.ds_1.rowcount < 1) {
		var msg = '공종코드|\n 1건';
		this.gfn_message("errors.항목.입력값.이상",msg);	// {0}항목은 {1} 이상 입력하셔야 합니다.
		return;
	}

	if(ds.rowcount < 1) {
		for(var i=0;i < 8;i++)		// 자료 없을 경우 8줄 삽입 처리
			ds.addRow();
	}

	this.g_xls_BsnsCode = this.div_PRJ.getCode();
	this.g_xls_BsnsName = this.div_PRJ.getLabel();
	var ds = this.ds_cond1;
	this.g_xls_ctr_no = ds.getColumn(0, 'CNTRCT_NO');
	this.g_xls_ctr_nm = ds.getColumn(0, 'CNTRCT_NM');

	this.grd_xls.set_visible(0);this.grd_xls.set_visible(1);

//	Ex.core.exportExcel(this,this.grd_xls, "exportExl");
	this.gfn_exportExcel(this.grd_xls, "exportExl");
}

this.g_xls_Cols_list = 'CNST_CODE CL_SE CODE_NM SN PRDNM STNDRD UNIT_NM QY MATRL_CT_UNTPC MATRL_CT_AMOUNT LBRC_CT_UNTPC LBRC_CT_AMOUNT EXPENS_UNTPC EXPENS_AMOUNT'.split(' ');
this.g_xls_Cols;

// 엑셀 업로드
this.btn_xls_up_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	if (!this.gfn_message("confirm.삭제.진행", "기존 자료가 삭제 됩니다")) return;

	var ds = this.ds_xls;
	var ds_s = this.ds_tmp;
	ds.clearData();

	var fn_xls_cb = function()
	{
		if(!this.g_xls_Cols) {
			var cList = this.g_xls_Cols_list;
			for(var i=0;i < cList.length;i++)
				cList[i] = cList[i] + '=Column' + i + ',';
			this.g_xls_Cols = cList.join('\n');
		}

		var bsnc_cd = this.div_PRJ.getCode();
		var bsnc_nm = this.div_PRJ.getLabel();
		var ctr_no = this.div_search.edt_1.value;
		var rpt_de = this.div_work.cal_3.value;
		//var rpt_de = this.div_search.cal_3.value;

		var r, v;
		for(var i=0;i < ds_s.rowcount;i++)
		{
			v = ds_s.getColumn(i, 'Column0');
			if(0 == (parseInt(v)||0)) continue;

			this.debug(i + ':' + (parseInt(v)||0) + '=' + v);
			r = ds.addRow();
			ds.copyRow(r, ds_s, i, this.g_xls_Cols);
			ds.setColumn(r, 'BSNS_CODE', bsnc_cd);
			ds.setColumn(r, 'CNTRCT_NO', ctr_no);
			ds.setColumn(r, 'REPORT_DE', rpt_de);
		}

		ds.set_keystring('S:+CNST_CODE+CL_SE+SN');

		var cd;

		var x0 = this.ds_x0;
		var x1 = this.ds_x1;

		x0.clearData();

		r = x0.addRow();
		x0.setColumn(r, 'BSNS_CODE', bsnc_cd);
		x0.setColumn(r, 'CNTRCT_NO', ctr_no);
		x0.setColumn(r, 'REPORT_DE', rpt_de);

		x1.clearData();
		for(var i=0;i < ds.rowcount;i++)
		{
			cd = ds.getColumn(i, 'SN');

			r = x1.findRowExpr('SN=='+cd);
			if(cd) {
				x1.copyRow(x1.addRow(), ds, i);
			}
		}

		this.fn_transaction("xSave");
	};

	var arg = {
		targetDs : ds_s,
		headSet : false,
		callBack : fn_xls_cb
	};

	this.gfn_importExcel(arg);
}







