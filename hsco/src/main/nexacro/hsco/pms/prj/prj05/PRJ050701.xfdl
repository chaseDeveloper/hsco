	var grd;
//	grd = this.grd_xls;grd.set_top(300);grd.set_bottom(50);grd.set_visible(1);
//	grd = this.grd_cd;grd.set_top(300);grd.set_bottom(50);grd.set_visible(1);
}

this.PRJ050701_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// 공통코드처리
	this.fn_loadCombo();
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명		: initForm
 * 설명			: Form을 초기화한다.
 * params		: None
 * return Type	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{}

/*
 * 함수명		: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *					주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_init_dataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);

	this.checkDs.push(this.ds_1);
}

/***********************************************************************
 * 공통 코드 조회
 ***********************************************************************/
/*
 * 함수명		: fn_loadCombo
 * 설명			: 화면의 Combo 에 데이터를 불러온다.
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	var comboParams = [
		 ['ds_PRJ010','PRJ010',1,'']		// 분류구분
		,['ds_PRJ016','PRJ016',1,'']		// 
	];

	var callback = null;
	this.gfn_comboLoad (comboParams, callback);
	
	this.fn_fill_combo(this.div_search.cmb_bsns_code, "hsco/pms/prj/bsnsCdList.do", "전체", null);		// 사업코드목록
}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/

/*
 * 함수명		: fn_search
 * 설명			: 조회 이벤트 처리
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	var bsnsCode = this.ds_BSNS.getColumn(this.ds_BSNS.rowposition, "BSNS_CODE") || '';	// 사업코드
	
	if(bsnsCode == '')
		return;
	
	this.ds_cond1.setColumn(0, "BSNS_CODE", bsnsCode);
	this.ds_cond1.setColumn(0, "CTR_GB",    "1");		// 계약구분[1:공사, 2;용역]

	this.ds_ctr.clearData();
	this.ds_ctrDtl.clearData();
	this.ds_1.clearData();
	
	this.fn_transaction("ctrList");		// 계약목록 조회
}

/*
 * 함수명		: fn_save
 * 설명 		: 저장 이벤트 처리
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if(this.gfn_message("confirm.저장.여부")) {
		this.fn_transaction("save");
	}
}

		
		
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명		: fn_transaction
 * 설명			: Transaction 요청 처리 함수
 * params		: strSvcId : 서비스요청 ID
 * return Type	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	if(this.ds_cond1.rowcount < 1) return;

	var ds = this.ds_cond1;
	var ds_s;
	var sCols;
	var sController = "hsco/pms/prj/PRJ050701/" + strSvcId + ".do";
	var sInDatasets = "input1=ds_cond1";
	var sOutDatasets = "";
	switch (strSvcId) {
		case "ctrList":		// 계약목록 조회
			sController  = "hsco/pms/prj/ctrList.do";
			sInDatasets  = "input1=ds_cond1";
			sOutDatasets = "ds_ctr=output1";
			break;
		case "ctrDtlList":	// 업체별계약내역 조회
			sController  = "hsco/pms/prj/ctrDtlList.do";
			sInDatasets  = "input1=ds_cond1";
			sOutDatasets = "ds_ctrDtl=output1";
			break;
			
		case "mList":			// 일위대가 조회
			ds_s = this.ds_cnst;
			sCols = "REPORT_DE=REPORT_DE,"
				   +"CNST_CODE=CNST_CODE," ;
			//ds.setColumn(0, 'REPORT_DE', this.div_search.cal_3.value);
			ds.setColumn(0, 'REPORT_DE', this.div_work.cal_3.value);
			sInDatasets = "input1=ds_cond1";
			sOutDatasets = "ds_1=output1";
			break;
		case "save":		// 저장
			sInDatasets  =  "input1=ds_1:U";
			break;
		case "xlsList":		// 엑셀 다운로드
			sOutDatasets = "ds_xls=output1";
			break;
		case "xSave":		// 저장
			sInDatasets  =  "input0=ds_x0";
			sInDatasets += " input1=ds_x1:U";
			break;
	}

	this.fn_clearData(strSvcId);

	if(sCols) {
		ds.copyRow(0, ds_s, ds_s.rowposition, sCols);
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);// , "", sCallbackFunc, sShowProgress, bAsync, bBinary); 
}

this.fn_clearData = function (strSvcId)
{
	switch (strSvcId) {
		case "ctrList":			// 계약목록 조회
			this.ds_ctr.clearData();
		case "mList":			// 일위대가 조회
			this.ds_1.clearData();

		case "xlsList":
			this.ds_xls.clearData();
			break;
	}
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명		: fn_callBack
 * 설명			: Transaction Callback 처리
 * params		: strSvcId    : 서비스요청 ID
 *					nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *					strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		return this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	switch(strSvcId)
	{
		case "ctrList":			// 계약목록 조회
			if(this.ds_ctr.rowcount == 0)
				this.div_work.cmb_4.set_enable(false);
			else {
				this.div_work.cmb_4.set_enable(true);
				this.div_work.cmb_4.set_index(0);
				this.fn_transaction("cnstList");
			}
			break;
		case "mList":
			break;
		case "save":
			//this.fn_search();
			this.fn_transaction("mList");		// 일위대가 조회
			break;
		case "xlsList":		// 엑셀 다운로드
			this.fn_xls_dn_post(this.ds_xls);
			break;
		case "xSave":
			this.fn_search();
			this.gfn_message("success.작업완료.성공", "엑셀 업로드");
			break;
	}
}

