this.fn_make = function()
{
	var ds_ctr = this.ds_ctr;
	if(ds_ctr.rowcount < 1) return;

	var ds_s = this.ds_1;
	if(ds_s.rowcount < 1) return;

	var ds = this.ds_3;
	var r = ds.rowposition;
	var cd = parseInt(ds.getColumn(r, 'SANCTN_FORM_SE'))||0;	// 양식구분

	var r_ctr = ds_ctr.rowposition;
	var r_s = ds_s.rowposition;

	var val2, etc1, etc2, etc3;

	var cd1 = cd % 10;
	switch(cd1)
	{
		case 1:	// 검사조서
		case 2:	// 감독조서
			val2 = {
				차수		:ds_s.getColumn(r_s, 'ODR'),			// 차수
			};
			etc1 = {
				계약명		:ds_ctr.getColumn(r_ctr, 'CNTRCT_NM'),	// 계약명
				계약금액	:this.comUtils.formatComma(ds_ctr.getColumn(r_ctr, 'TOT_CNTRCT_AMOUNT')),			// 계약금액
				착공일자	:this.dateUtils.format(ds_ctr.getColumn(r_ctr, 'STRWRK_DE'),"yyyy년 mm월 dd일"),	// 착공일자
				준공일자	:this.dateUtils.format(ds_ctr.getColumn(r_ctr, 'COMPET_DE'),"yyyy년 mm월 dd일"),	// 준공일자
				시공사		:ds_ctr.getColumn(r_ctr, 'COMP'),		// 시공사
				총기성금액	:this.comUtils.formatComma(ds_s.getColumn(r_s, 'S_AMT')),			// 누계기성금액
				총기성율	:this.comUtils.formatComma(ds_s.getColumn(r_s, 'S_RT')),			// 누계기성율
				기성금액	:this.comUtils.formatComma(ds_s.getColumn(r_s, 'PXCOND_AMOUNT')),	// 기성금액
				기성율		:this.comUtils.formatComma(ds_s.getColumn(r_s, 'PXCOND_RT')),		// 기성율
			};
			etc2 = {
				착공일자	:this.dateUtils.format(ds_ctr.getColumn(r_ctr, 'STRWRK_DE'),"yyyy년 mm월 dd일"),	// 착공일자
				시공사		:ds_ctr.getColumn(r_ctr, 'COMP'),		// 시공사
			};
			etc3 = {
				착공일자	:this.dateUtils.format(ds_ctr.getColumn(r_ctr, 'STRWRK_DE'),"yyyy년 mm월 dd일"),	// 착공일자
				준공일자	:this.dateUtils.format(ds_ctr.getColumn(r_ctr, 'COMPET_DE'),"yyyy년 mm월 dd일"),	// 준공일자
				차수		:ds_s.getColumn(r_s, 'ODR'),			// 차수
				기성율		:this.comUtils.formatComma(ds_s.getColumn(r_s, 'S_RT')),			// 누계기성율
			};
			break;
		case 3:	// 검사관서약서
		case 4:	// 감독관서약서
			val2 = {};	// 제목
			etc1 = {
				부서명		:this.fn_genNames('DEPT_NM'),		// 부서명
				성명		:this.fn_genNames('TCHNLGY_QUALF,TCHNLGY_GRAD,EMPNM'),		// 성명
			};
			etc2 = {};	// 기타
			etc3 = {};	// 본문
			break;
		case 5:	// 입회인서약서
			val2 = {};	// 제목
			etc1 = {
				계약명		:ds_ctr.getColumn(r_ctr, 'CNTRCT_NM'),	// 계약명
				검사자		:this.fn_genNames('TCHNLGY_QUALF,TCHNLGY_GRAD,EMPNM'),		// 검사자
			};
			etc2 = {};	// 기타
			etc3 = {};	// 본문
			break;
		case 0:
			etc1 = {
				S_RT		:ds_s.getColumn(r_s, 'S_RT'),			// 누계기성율
			};
			break;
		default:
			break;
	}

	if(val2) this.fn_genTemplate(ds,cd,'VAL2','SANCTN_SJ',val2);	// 제목
	if(etc1) this.fn_genTemplate(ds,cd,'ETC1','SANCTN_SUMM',etc1);	// 요약
	if(etc2) this.fn_genTemplate(ds,cd,'ETC2','SANCTN_ETC',etc2);	// 기타
	if(etc3) this.fn_genTemplate(ds,cd,'ETC3','SANCTN_CN',etc3);	// 본문
}


this.fn_genTemplate = function(ds:Dataset,cd,colTmpl,colMake,vals)
{
	var ds_cd = this.ds_PRJ025;
	var r = ds_cd.findRow('CODE', cd);
	if(r < 0) return '';

	var sTemplate = ds_cd.getColumn(r, colTmpl);		// 요약
	if(!sTemplate) return '';

	for (var p in vals) {
		sTemplate = sTemplate.replace('{' + p + '}', vals[p]);
	}
	r = ds.rowposition;
	ds.setColumn(r, colMake, sTemplate);
}


this.fn_genNames = function(cols)
{
	var ds2 = this.ds_2;
	var ds = new Dataset();
	ds.copyData(ds2, true);
	ds.filter('CHK==1');

	cols = cols.split(',');
	var sRet = '';
	for(var i=0;i < ds.rowcount;i++) {
		if(i > 0) sRet += ', ';
		for(var j=0;j < cols.length;j++) {
			if(j > 0) sRet += ' ';
			sRet += ds.getColumn(i, cols[j]);
		}
	}

	return sRet;
}

