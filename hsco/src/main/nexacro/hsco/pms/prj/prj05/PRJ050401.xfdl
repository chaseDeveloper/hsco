		,['ds_PRJ011','PRJ011',1,'']		// 판매구분
		,['ds_PRJ014','PRJ014',1,'']		// 사업단계
		,['ds_PRJ017','PRJ017',1,'']		// 계약구분:1-공사,2-용역
		,['ds_PRJ023','PRJ023',1,'']		// 기성구분:1-기성,2-준공
