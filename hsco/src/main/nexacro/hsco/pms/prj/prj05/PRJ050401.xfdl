}

/***********************************************************************
* Transaction 영역 시작
************************************************************************/

/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/

/*
 * 함수명		: fn_search
 * 설명			: 조회 이벤트 처리
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	var bsnsCode = this.ds_BSNS.getColumn(this.ds_BSNS.rowposition, "BSNS_CODE") || '';	// 사업코드
	
	if(bsnsCode == ''){
		this.alert("사업코드를 선택하세요.");
		return;
	}
	
	this.ds_cond1.setColumn(0, "BSNS_CODE", bsnsCode);
	this.ds_cond1.setColumn(0, "CTR_GB", "1");	 // 계약구분[1:공사, 2:용역]

	this.ds_ctr.clearData();
	this.ds_ctrDtl.clearData();
	this.ds_1.clearData();
	
	this.fn_transaction("ctrList");		// 계약목록 조회
}





/*
 * 함수명		: fn_save
 * 설명 		: 저장 이벤트 처리
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
    if (this.ds_ctr.getColumn(this.ds_ctr.rowposition, "TOT_CNTRCT_AMOUNT") < (this.ds_1.getSum('parseInt(ADVAN_EXCCLC_AMOUNT)||0') + this.ds_1.getSum('parseInt(PXCOND_AMOUNT)||0'))) {
		return this.gfn_message("info.처리불가","기성금액 및 선금정산액의 합이 계약금액보다 큽니다."); 
    } else {
		if(this.gfn_message("confirm.저장.여부")) {
			this.fn_transaction("save");
		}
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명		: fn_transaction
 * 설명			: Transaction 요청 처리 함수
 * params		: strSvcId : 서비스요청 ID
 * return Type	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	if(this.ds_cond1.rowcount < 1) return;

	var ds_s;
	var sCols;
	var sController;
	var sInDatasets = "input1=ds_cond1";
	var sOutDatasets = "";
	switch (strSvcId) {
		case "ctrList":		// 계약목록 조회
			sController  = "hsco/pms/prj/ctrList.do";
			sInDatasets  = "input1=ds_cond1";
			sOutDatasets = "ds_ctr=output1";
			break;
		case "ctrDtlList":	// 업체별계약내역 조회
			sController  = "hsco/pms/prj/ctrDtlList.do";
			sInDatasets  = "input1=ds_cond1";
			sOutDatasets = "ds_ctrDtl=output1";
			break;
			
		case "mList":			// 기성내역 조회
			ds = this.ds_cond1;
			ds_s = this.ds_ctr;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,";
			sOutDatasets = "ds_1=output1";
			this.ds_cond1.setColumn(0, "PXCOND_SE", "2");	// 기성구분 [2 : 기성금]
			break;
		case "rList":			// 감독원 조회
			ds = this.ds_cond1;
			ds_s = this.ds_1;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"PXCOND_SE=PXCOND_SE,"
				   +"ODR=ODR,";
			sOutDatasets  =  "ds_4=output1";	// 감독원
			break;
		case "save":			// 기성내역 저장
			g_row1 = this.ds_1.rowposition;

			sController = "hsco/pms/prj/PRJ050401/save.do";
			sInDatasets  =  "input1=ds_1:U";	// 기성내역
			sInDatasets += " input2=ds_4:U";	// 감독원		
			break;
		case "approve":			// 전자결재
			sInDatasets  =  "input1=ds_aud:U";
			sOutDatasets = " ds_approve=output1";
			break;
	}

	// clearData
	switch (strSvcId) {
		case "ctrList":		// 계약목록 조회
			this.ds_ctr.clearData();
		case "mList":		// 기성내역 조회
			this.ds_1.clearData();
		case "rList":		// 상세 조회
			this.ds_4.clearData();
	}

	if(sCols) {
		this.ds_cond1.copyRow(0, ds_s, ds_s.rowposition, sCols);
	}

	if(!sController) {
		sController = "hsco/pms/prj/PRJ050401/" + strSvcId + ".do";
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);// , "", sCallbackFunc, sShowProgress, bAsync, bBinary); 
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명		: fn_callBack
 * 설명			: Transaction Callback 처리
 * params		: strSvcId    : 서비스요청 ID
 *					nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *					strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0) 
	{
		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		return this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	switch(strSvcId)
	{
		case "ctrList":
			this.fn_transaction("ctrDtlList");	// 업체별계약내역 조회
			break;	
		case "mList":		// 기성내역 조회
			if (g_row1 > -1) {
				this.ds_1.set_rowposition(g_row1);
				g_row1 = -1;
			}
			break;
		case "rList":		// 감독원 조회
			break;
		case "save":		// 기성내역 저장
			this.fn_transaction("mList");
			break;
		case "approve":		// 전자결재
			this.fn_popup_sanction();
			break;
	}
}

/**********************************************************************
 * Transaction Callback 영역 끝
 ***********************************************************************/
 
/**********************************************************************
