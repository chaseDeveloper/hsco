this.fn_print = function()
{
	var vOpts = new this.cf_Opts();

	var ds1 = this.ds_1;
	var ds3 = this.ds_3;
	var row1 = ds1.rowposition;
	var row3 = ds3.rowposition;
	var cd = parseInt(ds3.getColumn(row3, 'SANCTN_FORM_SE'))||0;	// 양식구분
	var title = ' ';

	var row = this.ds_PRJ025.findRow('CODE', cd);
	if(row >= 0)
		title = this.ds_PRJ025.getColumn(row, 'CODE_NM');//cb.text;

	var dsR = this.ds_ubiReport00;
	dsR.clearData();
	var rowR = dsR.addRow();

	dsR.copyRow(rowR, ds3, row3);//, sCols1);
	dsR.setColumn(rowR, 'PRSEC_DE', ds1.getColumn(row1, 'PRSEC_DE'));	// 검사일자

	// val1에 따른 구분처리 : 1-감독조서,2-서약서,3-검사조서
	// cd(PRJ025)에 따른 구분처리 : 111-공사기성부분감독조서, 112-검사자서약서, 113-공사기성부분검사조서, 121-준공검사조서, 211-설계용역 기성부분 검사조서 ...
	var f_name = '/pms/prj/PRJ050401_1.jrf';;
	var sNM, sEMPNM = '';
	var cd1 = cd % 10;
	switch(cd1)
	{
		case 1:	// 검사조서
		case 2:	// 감독조서
			break;
		case 3:	// 검사관서약서
		case 4:	// 감독관서약서
			break;
		case 3:	// 입회인서약서
			break;
		default:
			break;
	}

	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setReportPath(f_name);
	vOpts.setSaveFilename(title);
	vOpts.setLocalDs(["ds_ubiReport00", dsR]);	// 기성양식구분
	
	var parm = {
