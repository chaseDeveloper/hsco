// 감독원 상태 처리
this.fn_procChk_Mgr = function(ds:Dataset)
{
	if(ds.rowcount < 1) return;

	ds.set_updatecontrol(false);

	var rType;
	for(var r=0; r < ds.rowcount;r++) {
		rType = ds.getRowType(r);
		if(rType <= Dataset.ROWTYPE_NORMAL) continue;

		// 감독원 체크에 따라 추가/삭제처리
		rType = ('1' == ds.getColumn(r, 'CHK')) ? Dataset.ROWTYPE_INSERT : Dataset.ROWTYPE_DELETE;
		ds.setRowType(r, rType);
	}
	ds.set_updatecontrol(true);
}




