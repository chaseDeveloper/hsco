this.g_xls_Cols_list = 'CNST_CODE CL_SE CNST_NM_LRGE CNST_NM_MIDDL STRWRK_DE_P COMPET_DE_P PLAN_FAIR_RT SN CNTRWK_ZONE_NM FAIR_CODE FAIR_NM_P FAIR_NO FAIR_NM STRWRK_DE COMPET_DE'.split(' ');
this.g_xls_Cols;


// 엑셀 업로드
this.btn_xls_up_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	if (!this.gfn_message("confirm.삭제.진행", "기존 자료가 삭제 됩니다")) return;

	var ds = this.ds_xls;
	var ds_s = this.ds_tmp;
	ds.clearData();

	var fn_xls_cb = function()
	{
		if(!this.g_xls_Cols) {
			var cList = this.g_xls_Cols_list;
			for(var i=0;i < cList.length;i++)
				cList[i] = cList[i] + '=Column' + i + ',';
			this.g_xls_Cols = cList.join('\n');
		}

		var bsnc_cd = this.div_PRJ.getCode();
		var bsnc_nm = this.div_PRJ.getLabel();
		var ctr_no = this.div_search.edt_1.value;

		var r, v;
		for(var i=0;i < ds_s.rowcount;i++)
		{
			v = ds_s.getColumn(i, 'Column0');
			if(0 == (parseInt(v)||0)) continue;

			r = ds.addRow();
			ds.copyRow(r, ds_s, i, this.g_xls_Cols);
			ds.setColumn(r, 'BSNS_CODE', bsnc_cd);
			ds.setColumn(r, 'CNTRCT_NO', ctr_no);
		}

		ds.set_keystring('S:+CNST_CODE+SN+FAIR_CODE+FAIR_NO');

		var sCols = 'BSNS_CODE=BSNS_CODE,'
					+'CNTRCT_NO=CNTRCT_NO,'
					+'CNST_CODE=CNST_CODE,'
					+'SN=SN,'
					+'FAIR_CODE=FAIR_CODE,'
					+'FAIR_NO=FAIR_NO,';

		var sCols1 = 'BSNS_CODE=BSNS_CODE,'
					+'CNTRCT_NO=CNTRCT_NO,'
					+'CNST_CODE=CNST_CODE,'
					+'CNST_NM_LRGE=CNST_NM_LRGE,'
					+'CNST_NM_MIDDL=CNST_NM_MIDDL,'
					+'STRWRK_DE=STRWRK_DE_P,'
					+'COMPET_DECOMPET_DE_P,'
					+'PLAN_FAIR_RT=PLAN_FAIR_RT,';

		var sCols3 = 'BSNS_CODE=BSNS_CODE,'
					+'CNTRCT_NO=CNTRCT_NO,'
					+'CNST_CODE=CNST_CODE,'
					+'FAIR_CODE=FAIR_CODE,'
					+'FAIR_NM=FAIR_NM_P,';

		var cd1, cd2, cd3, cd4;

		var x0 = this.ds_x0;
		var x1 = this.ds_x1;
		var x2 = this.ds_x2;
		var x3 = this.ds_x3;
		var x4 = this.ds_x4;

		x0.clearData();

		r = x0.addRow();
		x0.setColumn(r, 'BSNS_CODE', bsnc_cd);
		x0.setColumn(r, 'CNTRCT_NO', ctr_no);

		x1.clearData();
		x2.clearData();
		x3.clearData();
		x4.clearData();
		for(var i=0;i < ds.rowcount;i++)
		{
			cd1 = ds.getColumn(i, 'CNST_CODE');
			cd2 = ds.getColumn(i, 'SN');
			cd3 = ds.getColumn(i, 'FAIR_CODE');
			cd4 = ds.getColumn(i, 'FAIR_NO');

			r = x1.findRowExpr('CNST_CODE=='+cd1);
			if(r < 0) {
				x1.copyRow(x1.addRow(), ds, i);
			}

			if(cd2) {
				r = x2.findRowExpr('CNST_CODE+SN=="'+cd1+cd2+'"');
				if(r < 0) {
					x2.copyRow(x2.addRow(), ds, i);
				}
			}

			if(cd3) {
				r = x3.findRowExpr('CNST_CODE+SN+FAIR_CODE=="'+cd1+cd2+cd3+'"');
				if(r < 0) {
					x3.copyRow(x3.addRow(), ds, i);
				}
			}

			if(cd4) {
				x4.copyRow(x4.addRow(), ds, i);
			}
		}

		this.fn_transaction("xSave");
	};

	var arg = {
		targetDs : ds_s,
		headSet : false,
		callBack : fn_xls_cb
	};

	this.gfn_importExcel(arg);
}



