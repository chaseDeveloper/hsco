this.ds_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var val = e.newvalue;
	var row = e.row;
	switch(e.columnid) {
		case 'CL_SE':		// ds_1
			if(val == 1)
				obj.setColumn(row, 'PLAN_FAIR_RT', 100);
			break;
	}
}


// dataset cancolumnchange 공통 이벤트
// 중복체크
this.ds_cancolumnchange = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var col;
	var val = e.newvalue;
	switch(e.columnid)
	{
		case 'CNST_CODE':	// ds_1
			col = e.columnid;
			break;
		case 'SN':			// ds_2
			col = e.columnid;
			break;
		case 'FAIR_CODE':	// ds_3
			col = e.columnid;
			break;
		case 'FAIR_NO':		// ds_4
			col = e.columnid;
			break;
	}

	var row = obj.findRowExpr(col + " == '" + val + "'");
	if(-1 < row) {
			alert('중복 오류 : ' + val);
		return false;
	}
}

