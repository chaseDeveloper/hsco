// dataset 행추가 공통
this.fn_insertDS = function(ds:Dataset)
{
	var ds_s;
	var sCols;
	var sCd_Col;
	var sCd_len = 3;

	var row = ds.addRow();
	switch(ds)
	{
		case this.ds_1:
			ds_s = this.ds_cond1;
			sCd_Col = 'CNST_CODE';
			sCd_len = 4;
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,";
			ds.setColumn(row, 'CL_SE', 1);
			
			var dt = this.gfn_today();
			ds.setColumn(row, 'STRWRK_DE', dt);
			ds.setColumn(row, 'COMPET_DE', dt);
			break;
		case this.ds_2:
			ds_s = this.ds_1;
			sCd_Col = 'SN';
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"CNST_CODE=CNST_CODE,";
			break;
		case this.ds_3:
			ds_s = this.ds_1;
			sCd_Col = 'FAIR_CODE';
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"CNST_CODE=CNST_CODE,";
			break;
		case this.ds_4:
			ds_s = this.ds_3;
			sCd_Col = 'FAIR_NO';
			sCols = "BSNS_CODE=BSNS_CODE,"
				   +"CNTRCT_NO=CNTRCT_NO,"
				   +"CNST_CODE=CNST_CODE,"
				   +"FAIR_CODE=FAIR_CODE,";
			var dt = this.gfn_today();
			ds.setColumn(row, 'STRWRK_DE', dt);
			ds.setColumn(row, 'COMPET_DE', dt);
			break;
	}

	if(sCd_Col) {
		var cd;
		cd = (row < 1) ? '1' : (parseInt(ds.getColumn(row-1, sCd_Col)) + 1) + "";
		cd = cd.padLeft(3, "0");
		cd = cd.substr(cd.length - sCd_len, sCd_len)
		ds.setColumn(row, sCd_Col, cd);
	}

	if(sCols)
		ds.copyRow(row, ds_s, ds_s.rowposition, sCols);
}


