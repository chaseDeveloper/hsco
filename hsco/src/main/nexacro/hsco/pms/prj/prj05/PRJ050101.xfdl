	this.fn_init_form();		//폼 초기화
	this.fn_init_dataset();		//Dataset 초기화

	this.form_id = application.gv_curFormId + '_';
}

this.PRJ050101_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_loadCombo();	// 공통코드처리
}

/***********************************************************************
 * Form/Dataset 오브젝트 초기화
 ***********************************************************************/
/*
 * 함수명		: initForm
 * 설명			: Form을 초기화한다.
 * params		: None
