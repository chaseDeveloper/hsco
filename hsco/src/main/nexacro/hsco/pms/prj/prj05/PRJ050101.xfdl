 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
}


/*
 * 함수명		: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *					주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_init_dataset();
 */
this.fn_init_dataset = function()
{
	// 조건에 사용되는 Dataset 초기화
	this.gfn_initCondDs(this, this.div_search);

	this.checkDs.push(this.ds_1);
	this.checkDs.push(this.ds_2);
	this.checkDs.push(this.ds_3);
	this.checkDs.push(this.ds_4);
}



/***********************************************************************
 * 공통 코드 조회
 ***********************************************************************/
/*
 * 함수명		: fn_loadCombo
 * 설명			: 화면의 Combo 에 데이터를 불러온다.
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_loadCombo = function ()
{
	var comboParams = [
		 ['ds_PRJ010', 'PRJ010', 1, '']
		,['ds_PRJ021', 'PRJ021', 1, '']
	];

	/*
	// 2016.08.24
	var callback = function (svcId, errorCode, errorMsg) {
		if(g_gb) {
			this.div_PRJ.set_index(g_gb);
			g_bsns_cd = application.getPrivateProfile(this.form_id + 'BSNS_CODE');
			g_ctr_no = application.getPrivateProfile(this.form_id + 'CNTRCT_NO');
			g_ctr_nm = application.getPrivateProfile(this.form_id + 'CNTRCT_NM');
			g_ctr_se_nm = application.getPrivateProfile(this.form_id + 'CNTRCT_SE_NM');
		}

		this.div_PRJ.fn_search(g_bsns_cd);
	};
	*/
	
	var callback = null;
	this.gfn_comboLoad (comboParams, callback);
	
	this.fn_fill_combo(this.div_search.cmb_bsns_code, "hsco/pms/prj/bsnsCdList.do", "전체", null);		// 사업코드목록
}



/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명		: fn_search
 * 설명			: 조회 이벤트 처리
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	/*
	// 2016.08.24
	// Validation 체크
	if(!this.gfn_checkValidation(this.div_search, this.ds_validation)) {
		return;
	}
	
	this.fn_transaction("mList");	// 조회
	*/
	
	var bsnsCode = this.div_search.cmb_bsns_code.value;
	this.div_PRJ.set_BSNS_CODE(bsnsCode);
	this.div_PRJ.fn_search();
}


/*
 * 함수명		: fn_save
 * 설명 		: 저장 이벤트 처리
 * params		: None
 * return Type	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if(this.gfn_message("confirm.저장.여부")) {
		this.fn_transaction("save");
	}
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명		: fn_transaction
 * 설명			: Transaction 요청 처리 함수
 * params		: strSvcId : 서비스요청 ID
 * return Type	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	if(this.ds_cond1.rowcount < 1) return;

	var ds_s;
	var sCols;
	var sController = "hsco/pms/prj/PRJ050101/" + strSvcId + ".do";
	var sInDatasets = "input1=ds_cond1";
	var sOutDatasets = "";
	switch (strSvcId) {
		case "ctrList":			// 계약목록 조회
			ds_s = this.div_PRJ.ds_PRJ;
			sCols = "BSNS_CODE=BSNS_CODE,";
			sController = "hsco/pms/prj/ctrList.do";		
			sOutDatasets = "ds_ctr=output1";
			break;	
		case "mList":		// 공종코드 조회
			ds_s = this.ds_ctr;
			sCols = 'BSNS_CODE=BSNS_CODE,'
				  + 'CNTRCT_NO=CNTRCT_NO,'
				  + 'CNTRCT_NM=CNTRCT_NM,';
			sController = "hsco/pms/prj/cnstList.do";
			sOutDatasets = "ds_1=output1";
			break;
		case "rList":		// 상세 조회
			ds_s = this.ds_1;
			sCols = "CNST_CODE=CNST_CODE,";
			sOutDatasets  =  "ds_2=output1";
			sOutDatasets += " ds_3=output2";
			break;
		case "r3List":		// 상세 조회
			ds_s = this.ds_3;
			sCols = "FAIR_CODE=FAIR_CODE,";
			sOutDatasets = "ds_4=output1";
			break;
		case "save":		// 저장
			sInDatasets  =  "input1=ds_1:U";
			sInDatasets += " input2=ds_2:U";
			sInDatasets += " input3=ds_3:U";
			sInDatasets += " input4=ds_4:U";
			break;
		case "xlsList":		// 엑셀 다운로드
			sOutDatasets = "ds_xls=output1";
			break;
		case "xSave":		// 저장
			sInDatasets  =  "input0=ds_x0";
			sInDatasets += " input1=ds_x1:U";
			sInDatasets += " input2=ds_x2:U";
			sInDatasets += " input3=ds_x3:U";
			sInDatasets += " input4=ds_x4:U";
			break;
	}

	this.fn_clearData(strSvcId);

	if(ds_s) {
		this.ds_cond1.copyRow(0, ds_s, ds_s.rowposition, sCols);
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);// , "", sCallbackFunc, sShowProgress, bAsync, bBinary); 
}


this.fn_clearData = function (strSvcId)
{
	switch (strSvcId) {
		case "ctrList":	
			//this.div_search.edt_1.set_value('');
			//this.div_search.edt_2.set_value('');
			this.ds_ctr.clearData();
		case "mList":		// 조회
			this.ds_1.clearData();
		case "rList":		// 상세 조회
			this.ds_2.clearData();
			this.ds_3.clearData();
		case "r3List":		// 상세 조회
			this.ds_4.clearData();
			break;

		case "xlsList":
			this.ds_xls.clearData();
			break;
	}
}



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명		: fn_callBack
 * 설명			: Transaction Callback 처리
 * params		: strSvcId    : 서비스요청 ID
 *					nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *					strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode != 0){
		return this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	switch(strSvcId) {
		case "ctrList":			// 계약목록 조회
			if(this.ds_ctr.rowcount == 0)
				this.div_work.div_1.cmb_4.set_enable(false);
			else {
				this.div_work.div_1.cmb_4.set_enable(true);
				this.div_work.div_1.cmb_4.set_index(0);
				this.fn_transaction("mList");
			}
			break;
		case "save":
			this.fn_search();
			break;
		case "xlsList":		// 엑셀 다운로드
			this.fn_xls_dn_post(this.ds_xls);
			break;
		case "xSave":
			this.fn_search();
			this.gfn_message("success.작업완료.성공", "엑셀 업로드");
			break;
	}
}


 
/**********************************************************************
 * Non-Transaction 영역
 ***********************************************************************/



