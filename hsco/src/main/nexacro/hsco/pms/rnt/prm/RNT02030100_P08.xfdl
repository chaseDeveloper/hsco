 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
this.cntrctrNo = null;			//임대계약자 번호
 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.gtnManage_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	//this.ds_cond01.addRow();
	this.fn_init(false);//공통 폼 초기화
	this.fn_init_form();//폼 초기화
	this.fn_init_dataset();//Dataset 초기화
	this.fn_load_combo();
}

this.gtnManage_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var today = this.gfn_today();
	//this.div_WORK.DECSN_RCEPT_DE.set_value(today);
	
	this.fn_transaction("rentGtnList");
}

/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function(){
	
	if(this.ds_parent.rowcount == 0) this.ds_parent.addRow();
	this.ds_parent.copyRow(0,this.parent.arg_1,0);
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	//this.ds_cond01.setColumn(0, "cntrctrNo", this.cntrctrNo);
}

this.fn_load_combo = function ()
{
	this.fn_get_cmmCode(this.ds_qualfSeList, "", {grpCode:"RNT001"});//자격구분
	this.fn_get_cmmCode(this.ds_gtnSe, "", {grpCode:"RNT009", option0:'S'}); //보증금구분 
}
/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "rentGtnList":			//보증금 목록
			this.ds_cond01.clearData();
			this.ds_cond01.addRow();
			this.ds_cond01.setColumn(0, "cntrctrNo", this.ds_parent.getColumn(0, "CNTRCTR_NO"));
			
			var sController   	= "/hsco/pms/rnt/prm/RNT02040101/selectPuchasGtnList.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentGtnList=output1";
			break;
			
		case "rentGtnCUD": // 저장
			trace(this.ds_rentGtnList.saveXML());
		
			var sController   	= "/hsco/pms/rnt/prm/RNT02040101/savePuchasGtnCUD.do";
			var sInDatasets   	= "input1=ds_rentGtnList:U";
			var sOutDatasets  	= "";
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
	
	if(bSuccess){
		switch(strSvcId){
			case "rentGtnCUD": // 저장
				this.fn_transaction("rentGtnList");
				
				this.gfn_message("success.처리.성공");
				break;
			
		}		
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
}

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: btn_close_onclick
 * 설명 		: 종료
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}

// 보증금 등록
this.btn_reCntrctProcess_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(Ex.util.isUpdated(this.ds_rentGtnList) == true){
		if(this.gfn_message("confirm.저장.여부")){
			
			this.ds_rentGtnList.setSelectColumn("CNTRCTR_NO", this.ds_parent.getColumn(0, "CNTRCTR_NO"));
			//this.ds_rentGtnList.setSelectColumn("QUALF_SE", this.ds_parent.getColumn(0, "QUALF_SE"));
			
			this.fn_transaction("rentGtnCUD");
		}
	}else {
		this.gfn_message("comm.데이터.변경.없음");
	}
}

