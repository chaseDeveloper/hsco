	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// DataSet 초기화	
}


this.RNT02040205_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	//임대주택 코드 조회
	this.fn_get_houseCode(this.div_search.cmb_houseCode, "전체", "");
