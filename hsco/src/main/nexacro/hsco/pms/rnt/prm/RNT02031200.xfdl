	this.fn_get_cmmCode(this.ds_explnSeList,   "", {grpCode:"RNT013"});		// 소명구분
	this.fn_get_cmmCode(this.ds_qualfSeList,   "", {grpCode:"RNT001"});		// 자격구분
	this.fn_get_cmmCode(this.ds_roomStructure, "", {grpCode:"RNT027"});		// 방구조
