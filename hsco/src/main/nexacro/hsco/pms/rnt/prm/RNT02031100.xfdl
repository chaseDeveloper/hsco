 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::pmsUtilInclude.xjs";
include "pms_lib::rntUtilInclude.xjs";
include "pms_lib::puchasUtilInclude.xjs";


this.RNT02031100_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
	
	this.fn_init_tab_search(this.Tab00.tabpage1.div_search);
	this.fn_init_tab_search(this.Tab00.tabpage2.div_search);
	this.fn_init_tab_search(this.Tab00.tabpage3.div_search);
}

this.RNT02031100_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_load_combo();
	
	var month = this.gfn_today("NOW_YEAR_MONTH");
	this.Tab00.tabpage2.div_search.div_enddeBfchg.setValue(month);
	
	var today = this.gfn_today();
	this.Tab00.tabpage3.div_search.cal_reqstDate.set_value('19000101');
	this.Tab00.tabpage3.div_search.cal_reqedDate.set_value(today);
	
	this.Tab00.tabpage1.div_WORK.cal_rqestDe.set_value(today);
	this.Tab00.tabpage2.div_WORK.cal_rqestDe.set_value(today);
	this.Tab00.tabpage3.div_WORK.cal_rqestDe.set_value(today);
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
}


/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	this.fn_get_houseCode(this.Tab00.tabpage1.div_search.cmb_rentHouseCode, "전체", {USE_AT:"1"});		// 주택목록
	this.fn_get_houseCode(this.Tab00.tabpage2.div_search.cmb_rentHouseCode, "전체", {USE_AT:"1"});		// 주택목록
	this.fn_get_cmmCode(this.ds_familyRelate, "", {grpCode:"HRM013"});			// 가족관계
	this.fn_get_cmmCode(this.ds_areaCode, "전체/", {grpCode:"RNT004"});			// 지역코드
}


/*
 * 함수명  	: fn_search
 * 설명 		: 
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	this.ds_cond00.clearData();
	this.ds_cond00.addRow();
	
	switch(this.Tab00.tabindex) {
		case 0:		//사회보장정보신청(계약자) 목록
			this.fn_divToDS(this.ds_cond00, this.Tab00.tabpage1.div_search);
			this.fn_transaction("reqstList1");
			break;
		case 1:		//사회보장정보신청(재계약자) 목록
			this.fn_divToDS(this.ds_cond00, this.Tab00.tabpage2.div_search);
			this.fn_transaction("reqstList2");
			break;
		case 2:		//사회보장정보신청(대기자) 목록
			this.fn_divToDS(this.ds_cond00, this.Tab00.tabpage3.div_search);
			this.fn_transaction("reqstList3");
			break;
	}
}


/*
 * 함수명  	: btn_inqireDeUpdate_onclick
 * 설명 		: 신청일자 일괄수정
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.btn_inqireDeUpdate_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var tabindex = this.Tab00.tabindex;
	var rqestDe  = obj.parent.cal_rqestDe.value.toString();
	var dataset  = this['ds_reqstList' + (tabindex + 1)];
	var rowcount = dataset.rowcount;
	
	for(var i = 0; i < rowcount; i++){
		dataset.setColumn(i, "RQEST_DE", rqestDe);
	}
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "reqstList1":		//사회보장정보신청(계약자) 목록
			var sController   	= "/hsco/pms/rnt/prm/RNT02031100/reqstList1.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_reqstList1=output1";
			break;
		case "reqstList2":		//사회보장정보신청(재계약자) 목록
			var sController   	= "/hsco/pms/rnt/prm/RNT02031100/reqstList2.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_reqstList2=output1";
			break;
		case "reqstList3":		//사회보장정보신청(대기자) 목록
			var sController   	= "/hsco/pms/rnt/prm/RNT02031100/reqstList3.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_reqstList3=output1";
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	var bSuccess = nErrorCode == 0 ? true : false;
}



/*
 * 함수명  	: fn_init_tab_search
 * 설명 		: div 영역의 components에 addEventHandler
 * params  		: div   : div 영역 obj
 * return Type 	: None
 * 사용법		: this.fn_init_tab_search(div);
 */
this.fn_init_tab_search = function(div){	
	if(div == null){
		return;
	}
	
	var len = div.components.length;
	for(var i=0; i<len; i++){
		var item = div.components.get_item(i);
		switch(item.toString()){
			case "[object Combo]":
				item.set_type("filter");
				break;
				
			case "[object Edit]":
			case "[object MaskEdit]":
			case "[object TextArea]":
				item.addEventHandler("onkeydown", this._fn_searchEditOnkeydownHandler, this); 
				break;
			//case "[object Spin]":
			//case "[object Calendar]": 
				//컨셉 확정될때까지 일단 막음
				//엔터키 입력시 조회되도록...
				//item.addEventHandler("onkeydown", this._fn_searchEditOnkeydownHandler, this); 
				//break;
		}
	}
}
