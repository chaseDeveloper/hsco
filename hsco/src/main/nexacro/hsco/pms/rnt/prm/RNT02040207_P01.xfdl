	this.fn_transaction("infoDocListU");
}



this.fn_print = function(){

	if(this.rdo_gubun.value == 0){
		var reportPath = "/pms/rnt/prm/RNT02040207_P01R01.jrf"
	}else{
		var reportPath = "/pms/rnt/prm/RNT02040207_P01R02.jrf"
	}

	var rowcount = this.parent.ds_notice01.rowcount;
	
	for(var i=0; i<rowcount; i++){
		this.ds_notice01.setColumn(i, "DOC_NO", this.div_WORK.edt_docNo.value);
		this.ds_notice01.setColumn(i, "ST_DE", this.div_WORK.cal_stDe.value);
		this.ds_notice01.setColumn(i, "SUBJECT", this.div_WORK.edt_subject.value);
		this.ds_notice01.setColumn(i, "MANAGE_NO", this.div_WORK.edt_manageNo.value);
		this.ds_notice01.setColumn(i, "STD_DE", this.div_WORK.cal_stdrDe.value);
	}
	
	this.ds_notice02.clearData();
	this.ds_notice02.addRow();
	
	this.ds_notice02.setColumn(0, "ZIP", this.div_WORK.edt_zip.value);
	this.ds_notice02.setColumn(0, "ADRES", this.div_WORK.edt_adres.value);
	this.ds_notice02.setColumn(0, "TEL_NO", this.div_WORK.edt_telno.value);		
	this.ds_notice02.setColumn(0, "FAX", this.div_WORK.edt_fax.value);
	this.ds_notice02.setColumn(0, "JANG", this.div_WORK.edt_jang.value);
	this.ds_notice02.setColumn(0, "DAMDANG", this.div_WORK.edt_damdang.value);
	this.ds_notice02.setColumn(0, "SUBJECT", this.div_WORK.edt_subject.value);		
	this.ds_notice02.setColumn(0, "DOC_NO", this.div_WORK.edt_docNo.value);
	this.ds_notice02.setColumn(0, "ST_DE", this.div_WORK.cal_stDe.value);
	this.ds_notice02.setColumn(0, "PAY_TMLMT", this.div_WORK.cal_payTmlmt.value);
	this.ds_notice02.setColumn(0, "MANAGE_NO", this.div_WORK.edt_manageNo.value);
	this.ds_notice02.setColumn(0, "STD_DE", this.div_WORK.cal_stdrDe.value);		
		
	var vOpts = new this.cf_Opts();
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setReportPath(reportPath);
	vOpts.setSaveFilename("강제집행 안내문");
	
	vOpts.setLocalDs(["ds_notice01", this.ds_notice01]);
	vOpts.setLocalDs(["ds_notice02", this.ds_notice02]);
	
	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		opts : vOpts
	};

	this.gfn_popup("Popup_Report", 884, 735, "강제집행 안내문", parm, "common::frmReportPopup.xfdl", "report_callback");

}


this.rdo_gubun_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	if(this.rdo_gubun.value == 0){
		this.ds_cond00.setColumn(0,"GDCC_KND",1);
		this.fn_transaction("selectInfoDocList");
	}else{
		this.ds_cond00.setColumn(0,"GDCC_KND",2)
		this.fn_transaction("selectInfoDocList");
	}
}
