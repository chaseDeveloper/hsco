 ***********************************************************************/
this.RNT02040103_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	// 공통 폼 초기화
	this.fn_init();
	     
	// Form 초기화        
	this.fn_init_form();
	
	// DataSet 초기화
