    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : RNT02020300
 * 화면명   : 소유물건등록
 * 화면설명 : 소유물건등록
 * 작성일   : 2015.10.21
 * 작성자   : 지효정
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */

/***********************************************************************
 * Script Include
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";

/*
 * 함수명  	: RNT02020300_oninit
 * 설명 		: form oninit
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.RNT02020300_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}


/*
 * 함수명  	: RNT02020300_onload
 * 설명 		: form onload
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.RNT02020300_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var yyyy = this.gfn_today("NOW_YEAR");
	this.div_search.spn_stRceptYr.set_max(yyyy);
	this.div_search.spn_edRceptYr.set_max(yyyy);
	this.div_search.spn_stRceptYr.set_min(1900);
	this.div_search.spn_edRceptYr.set_min(1900);
	this.div_search.spn_stRceptYr.set_value(1900);
	this.div_search.spn_edRceptYr.set_value(yyyy);

	this.fn_load_combo();

	var fileConfig = {
		onChange: 	this.fn_onFileSelected,
		onSuccess: 	this.fn_onFileSuccess,
		useMultiSelect : true             // 파일 다중선택 옵션
	};

	this.FILE_MANAGER = new this.fileManager(this, "PMS", "PRM");
	this.FILE_MANAGER.init(fileConfig);
}


/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}


/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	this.fn_get_cmmCode(this.ds_qualfSeList,  "", {grpCode:"RNT001"});		// 자격구분
	this.fn_get_cmmCode(this.ds_familyRelate, "", {grpCode:"HRM013"});		// 가족관계
	this.fn_get_cmmCode(this.ds_prposSeList,  "", {grpCode:"RNT012"});		// 용도
	this.fn_get_cmmCode(this.ds_explnSeList,  "", {grpCode:"RNT013"});		// 소명구분
	this.fn_get_cmmCode(this.ds_roomStructure,"", {grpCode:"RNT027"});		// 방구조
	
	// 지역코드 조회
	var comboParams = [["ds_areaCode","RNT004", "1", "전체" ]];
	this.gfn_comboLoad(comboParams, "");
}


/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_puchasWaitrList.clearData();
	this.ds_puchasPosesnThingList.clearData();
	this.ds_waitrFamilyList.clearData();

	this.fn_divToDS(this.ds_cond00, this.div_search);	
	this.ds_cond00.setColumn(0, "anact1", this.div_WORK.chk_anact1.value);		// 물건소유자 경우 사용
	
	this.fn_transaction("puchasWaitrList");		// 매입임대 대기자 목록조회
}


/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 : 소유물건 추가
 * params  		: None
 * return Type 	: None
 * 사용법		:
 */
this.fn_insert = function()
{
	if(this.ds_puchasWaitrList.rowposition == -1){
		this.alert("선택된 대기자가 없습니다.");
	}
	else{
		var rRow = this.ds_puchasWaitrList.rowposition;
		var nRow = this.ds_puchasPosesnThingList.addRow();
          
		this.ds_puchasPosesnThingList.setColumn(nRow, 'WAITR_CNTRCTR_SE', '1');		// 대기자,계약자구분 : [1:대기자, 2:계약자]
		this.ds_puchasPosesnThingList.setColumn(nRow, "AREA_CODE", 		this.ds_puchasWaitrList.getSelectColumn("AREA_CODE"));			// 지역코드
		this.ds_puchasPosesnThingList.setColumn(nRow, "RCEPT_DE", 		this.ds_puchasWaitrList.getSelectColumn("RCEPT_DE"));			// 접수일자
		this.ds_puchasPosesnThingList.setColumn(nRow, "WAIT_RCEPT_SN", 	this.ds_puchasWaitrList.getSelectColumn("WAIT_RCEPT_SN"));		// 접수일련번호
	}
