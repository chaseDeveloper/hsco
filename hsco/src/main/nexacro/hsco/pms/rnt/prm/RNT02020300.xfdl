/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	switch (strSvcId) {
		case "puchasWaitrList":			// 매입임대 대기자 목록
			var sController   	= "/hsco/pms/rnt/prm/RNT02020300/puchasWaitrList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_puchasWaitrList=output1";
			break;

		case "waitrFamilyList":			// 매입임대 고객 가족목록 조회
			var sController   	= "/hsco/pms/rnt/prm/puchasRentCstmr/selectPuchasCstmrFamilyList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_waitrFamilyList=output1";
			break;

		case "puchasCstmrFamilyCUD":	// 매입임대 고객 가족사항 CUD
			var sController   	= "/hsco/pms/rnt/prm/puchasRentCstmr/puchasCstmrFamilyCUD.do";
			var sInDatasets   	= "input1=ds_waitrFamilyList:U";
			var sOutDatasets  	= "";
			break;

		case "puchasPosesnThingList":	// 소유물건정보 목록
			var sController   	= "/hsco/pms/rnt/prm/RNT02020300/puchasPosesnThingList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_puchasPosesnThingList=output1";
			break;

		case "posesnThingCUD":			// 소유물건정보 생성
			var sController   	= "/hsco/pms/rnt/prm/RNT02020300/puchasPosesnThingCUD.do";
			var sInDatasets   	= "input1=ds_puchasPosesnThingList:U";
			var sOutDatasets  	= "";
			break;
	}

	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync);
}



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;

	if(bSuccess == true){
		switch(strSvcId){
			case "posesnThingCUD":
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				break;
		}
	}
	else{
		this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
}



