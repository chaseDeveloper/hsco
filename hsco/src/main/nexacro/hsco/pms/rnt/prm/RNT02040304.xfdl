/*
 * 함수명  	: div_work_grd_managectInfoList_onselectchanged
 * 설명 		: 주택정보 클릭시, 관리비 디테일 정보 읽어오기
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_work_grd_managectInfoList_onselectchanged();
 */
 this.div_work_grd_managectInfoList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	var stOccrrncYm = this.div_work.div_sToccrrncYm.value;
	var edOccrrncYm = this.div_work.div_eDoccrrncYm.value;
	
	var unTotal = this.ds_managectInfo.getColumn(this.ds_managectInfo.rowposition , "OCCRRNC_YM")
	var cntrctr = this.ds_managectInfo.getColumn(this.ds_managectInfo.rowposition , "CNTRCTR_NO")
	
	this.ds_managectInfoDetail.clearData();	
	this.ds_cond02.clearData();
	this.ds_cond02.addRow();
	
	this.ds_cond02.setColumn(0, "CNTRCTR_NO" , cntrctr);
	
	//미납 체크여부
	if(this.div_work.chk_minab.value == true && this.div_work.chk_total.value == true){
		this.ds_cond02.setColumn(0, "MI_CHECK", "0")
		this.ds_cond02.setColumn(0,"ST_OCCRRNC_YM", stOccrrncYm);
		this.ds_cond02.setColumn(0,"ED_OCCRRNC_YM", edOccrrncYm);
	}else if(this.div_work.chk_minab.value == true && this.div_work.chk_total.value == false){
		this.ds_cond02.setColumn(0, "MI_CHECK", "0")
		this.ds_cond02.setColumn(0,"ST_OCCRRNC_YM", unTotal);
		this.ds_cond02.setColumn(0,"ED_OCCRRNC_YM", unTotal);	
	}else if (this.div_work.chk_minab.value == false && this.div_work.chk_total.value == true){
		this.ds_cond02.setColumn(0, "MI_CHECK", "%")
		this.ds_cond02.setColumn(0,"ST_OCCRRNC_YM", stOccrrncYm);
		this.ds_cond02.setColumn(0,"ED_OCCRRNC_YM", edOccrrncYm);
	}else{
		this.ds_cond02.setColumn(0, "MI_CHECK", "%");
		this.ds_cond02.setColumn(0,"ST_OCCRRNC_YM", unTotal);
		this.ds_cond02.setColumn(0,"ED_OCCRRNC_YM", unTotal);			
	}
		
	this.fn_transaction("selectManagectInfoDetailList")
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
 
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {	
	
			//매입임대 주택정보
			case "selectItemList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02000000/selectItemList.do"; 
				var sInDatasets   	= "";
				var sOutDatasets  	= "ds_managectCode=output1";
			break;	
			
			//매입임대 주택정보
			case "selectPuchasHouseInfoList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040304/selectPuchasHouseInfoList.do"; 
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_puchasHouseInfo=output1";
			break;				

			//매입임대 주택상세정보
			case "selectPuchasHouseInfoDetailList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040304/selectPuchasHouseInfoDetailList.do"; 
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_puchasHouseInfoDetail=output1";
			break;	
			
			//매입임대 관리비정보
			case "selectManagectInfoList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040304/selectManagectInfoList.do"; 
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_managectInfo=output1";
			break;	
						
						
			//매입임대 관리비정보
			case "selectManagectInfoDetailList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040304/selectManagectInfoDetailList.do"; 
				var sInDatasets   	= "input1=ds_cond02";
				var sOutDatasets  	= "ds_managectInfoDetail=output1";
			break;					
			
			//매입임대 관리비정보
			case "save":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040304/saveCUD.do"; 
				var sInDatasets   	= "input1=ds_managectInfoDetail:U";
				var sOutDatasets  	= "";
			break;		
									
		}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}	



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
			
			case "selectPuchasHouseInfoDetailList":
				this.fn_transaction("selectManagectInfoList");
			break;					
			
		}
	}
	
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	//this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}		


