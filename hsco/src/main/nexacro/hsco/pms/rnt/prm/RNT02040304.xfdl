 */
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var today = this.dateUtils.format(this.dateUtils.today(),'yyyymmdd');		
	var nRow = this.ds_managectInfoDetail.addRow();
	this.ds_managectInfoDetail.setColumn( nRow , "PAY_TMLMT" , this.gfn_lastDateNum(today));
	this.ds_managectInfoDetail.setColumn( nRow , "CNTRCTR_NO" , this.ds_puchasHouseInfoDetail.getColumn( this.ds_puchasHouseInfoDetail.rowposition , "CNTRCTR_NO"));
}

