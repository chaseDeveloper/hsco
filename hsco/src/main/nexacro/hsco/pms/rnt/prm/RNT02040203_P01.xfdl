 * callback : function(obj)
 ***********************************************************************/
this.houseCode = null;			// 주택코드
this.nm = null;					// 계약자명
this.dong = null;				//동
this.ho = null;					//호


this.RNT02040203_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	//공통 폼 초기화
	this.fn_init(false);
	
	//폼 초기화
	this.fn_init_form();
	
	//Dataset 초기화
	this.fn_init_dataset();
}


this.RNT02040203_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	//공통 코드 조회
	this.fn_load_combo();
	
	//임대주택 코드 조회
	this.fn_transaction("selectHouseCode");
	
	this.div_monCal.setValue(this.gfn_today());
	this.cal_payTmlmt.set_value(this.gfn_today());
	
	this.edt_categoryAmount.set_value(0);
	this.edt_npymAmount.set_value(0);
	this.edt_ovdintr.set_value(0);
	this.edt_rntchrg.set_value(0);
	this.edt_gtn.set_value(0);
	
	this.edt_categoryAmount.autoselect = true; 
	this.edt_categoryAmount.setFocus();
	
	this.edt_npymAmount.autoselect = true; 
	this.edt_npymAmount.setFocus();
	
	this.edt_ovdintr.autoselect = true; 
	this.edt_ovdintr.setFocus();
	
	this.edt_rntchrg.autoselect = true; 
	this.edt_rntchrg.setFocus();
	
	this.edt_gtn.autoselect = true; 
	this.edt_gtn.setFocus();
}


/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.houseCode = this.parent.houseCode;
	this.nm = this.parent.nm;
	this.dong = this.parent.dong;
	this.ho = this.parent.ho;
	
	
}
	
	
/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.ds_userNht.addRow();
	this.ds_userNht.setColumn(0, "RENT_HOUSE_CODE", this.houseCode);
	this.ds_userNht.setColumn(0, "NM", this.nm);
	this.ds_userNht.setColumn(0, "DONG", this.dong);
	this.ds_userNht.setColumn(0, "HO", this.ho);
}	


/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{		
	this.fn_get_cmmCode(this.ds_paySe, "", {grpCode:"RNT009"}); //납부구분
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
	
			//매입임대 주택코드 조회
			case "selectHouseCode":			
				var sController   	= "hsco/pms/rnt/prm/RNT02000000/puchasHouseCodeList.do";
				var sInDatasets   	= "";
				var sOutDatasets  	= "ds_houseCode=output1";	
			break;		

	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
		
		}
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


