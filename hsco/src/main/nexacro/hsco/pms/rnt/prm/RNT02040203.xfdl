				if(this.ds_puchasRntchrg.getColumn( i , "CHK") == 1){
					trace(" i = " + i);
					this.ds_cond02.setColumn(0, "cntrctrNo", this.ds_puchasRentCntrct.getSelectColumn("CNTRCTR_NO"));
					this.ds_cond02.setColumn(0, "payTmlmt", this.div_WORK.cal_payTmlmt.value);

				}	
			}
			this.fn_transaction("savePuchasRentMtRntchrgU");	
		}
}



