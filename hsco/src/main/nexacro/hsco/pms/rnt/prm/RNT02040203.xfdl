/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
		
			//매입임대 계약 고객정보
			case "selectPuchasRentCntrctList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02030100/selectPuchasRentCntrctList.do"; 
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_puchasRentCntrct=output1";
			break	
			
			//매입임대 계약 고객정보 상세사항
			case "selectPuchasRentCntrctDetailList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040203/selectPuchasRentCntrctDetailList.do"; 
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_puchasRentCntrctDetail=output1";
			break;	

			//매입임대 보증금 상세정보
			case "selectPuchasRntchrgList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040203/selectPuchasRntchrgList.do"; 
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_puchasRntchrg=output1";
			break;
										
			//매입임대 연체이자생성
			case "savePuchasRentMtRntchrgU":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040203/savePuchasRentMtRntchrgU.do"; 
				var sInDatasets   	= "input1=ds_cond02";
				var sOutDatasets  	= "";
			break;	
				
			//매입임대 월임대료(저장,수정,삭제)
			case "savePuchasRntchrgCUD":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040203/savePuchasRntchrgCUD.do"; 
				var sInDatasets   	= "input1=ds_puchasRntchrg:U";
				var sOutDatasets  	= "";
			break;	
			
			//매입임대 월임대료(파일생성)				
			case "downloadFile":
				var sController   	= "/hsco/pms/rnt/prm/RNT02040203/downloadFile.do";
				var sInDatasets   	= "input1=ds_file";
				var sOutDatasets  	= "";
				break;							
												
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	if(strSvcId == "downloadFile"){
		this.FILE_MANAGER.dataFileDownload(sController, sInDatasets, FILE_NM);
	}else{
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
	}
	//Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


/**
 * 파일 업/다운로드 성공 이벤트 핸들러
 * @param fileFullName 선택 전체 파일명
 * @param type 실행구분(ExtFileUpload/ExtFileDownload)
 * @param e 발생이벤트
 * @return void
 */
this.fn_onFileSuccess = function(type, e){
	if(type == this.FILE_MANAGER.TYPE_DOWNLOADER){
		this.alert("다운로드완료");
	}
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
		
				case "savePuchasRentMtRntchrgU":
					this.alert("연체이자 생성완료");
					this.fn_transaction("selectPuchasRntchrgList");
				break;

				case "savePuchasRntchrgCUD":
					this.gfn_message("success.처리.성공")
					this.fn_transaction("selectPuchasRntchrgList");
				break;
				
				
		}
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
 this.fn_insert = function(){
	if(this.ds_puchasRentCntrct.rowcount == 0){
		this.alert("선택된 계약자가 없습니다.")
	}else{
		this.ds_puchasRntchrg.addRow();
