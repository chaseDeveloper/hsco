this.div_WORK_btn_fileSave_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var cnt = this.ds_puchasRntchrg.rowcount;
	if(cnt < 1){
		this.alert("조회된 임대료 목록이 없습니다.");
		return;
	}	
	
	//제목
	var yyyymm = this.ds_puchasRntchrg.getColumn( cnt-1, "PAY_TMLMT");
	var TITLE = "제  목 : " + yyyymm.substr(0,4) + "년 " + yyyymm.substr(4,2) + "월 임대료 납부";//제목	
	
	//납부자
	var RENT_HOUSE_NM = this.ds_puchasRntchrg.getColumn( this.ds_puchasRntchrg.rowposition, "RENT_HOUSE_NM");//주택
	var DONG = this.ds_puchasRntchrg.getColumn( this.ds_puchasRntchrg.rowposition, "DONG");//호
	var HO = this.ds_puchasRntchrg.getColumn( this.ds_puchasRntchrg.rowposition, "HO");//호
	var NM = this.ds_puchasRntchrg.getColumn( this.ds_puchasRntchrg.rowposition, "NM");//납부자
	var CSTMR_NM = "납부자 : " + RENT_HOUSE_NM + " " + DONG + "동 " + HO + "호  " + NM
	
	//납부기한
	var payTmlmt = this.div_WORK.cal_payTmlmt.value + "";//납부기한
	var PAY_TMLMT = "납부기한 : " + payTmlmt.substr(0,4) + "년 " + payTmlmt.substr(4,2) + "월 " + payTmlmt.substr(6,2) + "일" //납부기한	
	 alert(TITLE + " " + CSTMR_NM+ " " + PAY_TMLMT);	
	//금액
	var TOTAL_AMOUNT_SUM = "납부금액 : " + this.ds_puchasRntchrg.getSum("PAY_TOTAL"); //납부금액
	var RNTCHRG = "당월금액 : " + this.ds_puchasRntchrg.getColumn( cnt-1, "RNTCHRG"); //당월금액
	var PAY_TOTAL = "연체금액 : " + this.ds_puchasRntchrg.getColumn( cnt-1, "PAY_TOTAL"); //연체금액
	var OVDINTR_SUM = "연체이자 : " + this.ds_puchasRntchrg.getSum("OVDINTR"); //연체이자 합	
	var ETC1 = "※ 납부기한 경과 시 공사 또는 관리사무소에서 고지서를 재발급 받아 납부하셔야합니다.";
	var ETC2 = "기타 문의사항이 있으시면 350-0301(0303)으로 연락해주세요.";
	var ETC3 = "화성도시공사 주거복지처";
	FILE_NM = RENT_HOUSE_NM +DONG + "동" + HO + "호" + NM + yyyymm.substr(0,6) + ".txt";
	
	this.ds_file.clearData();
	this.ds_file.addRow();	
	this.ds_file.setColumn(this.ds_file.rowposition, "TITLE", TITLE);
	this.ds_file.setColumn(this.ds_file.rowposition, "CSTMR_NM", CSTMR_NM);
	this.ds_file.setColumn(this.ds_file.rowposition, "PAY_TMLMT", PAY_TMLMT);
	this.ds_file.setColumn(this.ds_file.rowposition, "TOTAL_AMOUNT_SUM", TOTAL_AMOUNT_SUM);
	this.ds_file.setColumn(this.ds_file.rowposition, "RNTCHRG", RNTCHRG);
	this.ds_file.setColumn(this.ds_file.rowposition, "PAY_TOTAL", PAY_TOTAL);
	this.ds_file.setColumn(this.ds_file.rowposition, "OVDINTR_SUM", OVDINTR_SUM);
	this.ds_file.setColumn(this.ds_file.rowposition, "ETC1", ETC1);
	this.ds_file.setColumn(this.ds_file.rowposition, "ETC2", ETC2);
	this.ds_file.setColumn(this.ds_file.rowposition, "ETC3", ETC3);
	this.ds_file.setColumn(this.ds_file.rowposition, "FILE_NM", FILE_NM);

	this.fn_transaction("downloadFile");
	
}
