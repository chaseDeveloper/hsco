 * 설명 		: 납부서 출력
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.fn_print1 = function()
{
	if(this.ds_reCntrctList.rowcount == 0){
		this.alert("출력할 자료가 없습니다.");
		return;
	}
	
	if(this.div_search.div_stEndde.getValue() == null || this.div_search.div_edEndde.getValue() == null){
		this.alert("임대종료월을 입력하세요.");
		return;
	}
	/*
	if(this.div_search.div_stEndde.getValue() != this.div_search.div_edEndde.getValue()){
		this.alert("임대종료월을 다르게 지정하셨습니다.");
		return;
	}
	*/
	
	this.ds_billPrintList.clearData();
	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.ds_cond00.setColumn(0, "stdrMt", this.div_search.div_edEndde.getValue());
	this.fn_transaction("billPrintList");		// 납부서 조회
}


