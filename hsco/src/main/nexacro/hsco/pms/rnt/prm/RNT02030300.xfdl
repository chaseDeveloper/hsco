 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_btn_noticePop_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rowcount = this.ds_reCntrctList.rowcount;
	if(rowcount == 0){
		this.alert("출력할 자료가 없습니다.");
		return;
	}
	
	this.ds_reCntrctPrintList.clearData();
	for(var i = 0; i < rowcount; i++){		
		// 미계약건만 출력
		if(this.comUtils.isNotNull(this.ds_reCntrctList.getColumn(i, "CNTRCT_DE") || ''))
			continue;
			
		var nRow = this.ds_reCntrctPrintList.addRow();
		this.ds_reCntrctPrintList.copyRow(nRow, this.ds_reCntrctList, i);
	}

	if(this.ds_reCntrctPrintList.rowcount == 0){
		this.alert("미계약 자료가 없습니다.");
		return;
	}

	this.gfn_popup("RNT02030300_P02"
			   , 600
			   , 399
			   , ""
			   , {ds_reCntrctList : this.ds_reCntrctPrintList}
			   , "pms_rnt_prm::RNT02030300_P02.xfdl"		// 재계약 안내문 출력
			   , "", true);
}


