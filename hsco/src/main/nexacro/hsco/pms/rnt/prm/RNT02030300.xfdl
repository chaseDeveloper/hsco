/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "selectPuchasReCntrctList":	// 재계약 목록 조회
			var sController   	= "/hsco/pms/rnt/prm/RNT02030300/selectPuchasReCntrctList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_reCntrctList=output1";
			break;
			
		case "puchasReCntrctUD":			// 재계약 정보 UD
			var sController   	= "/hsco/pms/rnt/prm/RNT02030300/puchasReCntrctUD.do";
			var sInDatasets   	= "input1=ds_reCntrctList:U";
			var sOutDatasets  	= "";
			break;

		case "billPrintList":				// 납부서 조회
			var sController   	= "/hsco/pms/rnt/prm/RNT02030300/billPrintList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_billPrintList=output1";
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	try {
		// fn_callback_message 공통함수 버그로 try~catch 처리함.
		this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	catch(exception) {}
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if(bSuccess == true){
		switch(strSvcId){
			case "selectPuchasReCntrctList":	// 재계약 목록
				if(this.ds_reCntrctList.rowcount == 0)
					this.alert("검색된 결과가 없습니다.")
				break;
				
			case "puchasReCntrctUD":	// 재계약 정보 UD
				this.fn_search();
				break;

			case "billPrintList":		// 납부서 조회
				this.fn_payDocPrint();
				break;
		}
	}
}



