/*
 * 함수명  	: Div00_Button00_onclick
 * 설명 		: 납부서 출력물 조회
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.Div00_Button00_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var acctGubun = "";
	var opt = "";
	
	if(this.ds_billList.rowcount == 0){
		this.alert("출력할 청구내역이 없습니다.");
		return;
	}
	
	if(this.div_WORK.rdo_accntGubun.value == "R0"){
		acctGubun = "보 증 금";
		opt = "2";
	}else if(this.div_WORK.rdo_accntGubun.value == "RN"){
		if(this.objCntrct['CORT_CODE'] == "T2"){
			acctGubun = "소 송 비";
		}else {
			acctGubun = "임 대 료";
		}
		opt = "1";
	}else{
		acctGubun = "소 송 비";
	}
	
	this.ds_cond04.clearData();
	this.ds_cond04.addRow();
	this.ds_cond04.setColumn(0, "bankCd",  		this.ds_billList.getSelectColumn("BANK_CD"));		// 은행코드
	this.ds_cond04.setColumn(0, "vacctNo", 		this.ds_billList.getSelectColumn("VACCT_NO"));		// 가상계좌번호
	this.ds_cond04.setColumn(0, "cntrctrNo", 	this.objCntrct['CNTRCTR_NO']);						// 계약자번호
	this.ds_cond04.setColumn(0, "crtDiv", 		this.objCntrct['CRT_DIV']);
	this.ds_cond04.setColumn(0, "mdatYm", 		this.gfn_today("NOW_YEAR_MONTH"));
	this.ds_cond04.setColumn(0, "acctGubun", acctGubun);
	this.ds_cond04.setColumn(0, "opt", opt);

	var get_CRT_DIV = this.objCntrct['CRT_DIV'];
	if (get_CRT_DIV == "MA") {
		// 관리비
		this.fn_transaction("selectManagectBillPrint");
	} else {
		// 임대료, 보증금, 소송비
		this.fn_transaction("selectBillPrintList");		
	}
}


