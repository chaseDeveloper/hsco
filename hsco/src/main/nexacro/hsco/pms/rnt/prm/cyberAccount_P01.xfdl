		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_PAY_AMT",  0);										// 납부대상금액
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_SALE_AMT", 0);										// 차감금액
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_INT_AMT",  0);										// 연체금액
