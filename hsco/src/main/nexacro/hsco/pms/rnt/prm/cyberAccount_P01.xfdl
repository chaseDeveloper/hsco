/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
	
		case "nonPaymentList":			// 미납 목록
			var sController   	= "/hsco/pms/rnt/prm/puchasCyberAccount/nonPaymentList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_nonPaymentList=output1";
			break;
			
		case "receiptList":				// 입금내역 목록
			var sController   	= "/hsco/pms/rnt/prm/puchasCyberAccount/receiptList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_receiptList=output1";
			break;
			
		case "billList":				// 납부내역 목록
			var sController   	= "/hsco/pms/rnt/prm/puchasCyberAccount/billList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_billList=output1";
			break;
			
		case "billDetailList":			// 납부내역상세 목록
			var sController   	= "/hsco/pms/rnt/prm/puchasCyberAccount/billDetailList.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_billDetailList=output1";
			break;

		case "spVaBillUid":				// 납부요청
			var sController   	= "/hsco/pms/rnt/prm/puchasCyberAccount/spVaBillUid.do";
			var sInDatasets   	= "input1=ds_mt_rntchrgDelete input2=ds_mt_rntchrgDetail input3=ds_mt_rntchrgMaster";
			var sOutDatasets  	= "";
			break;		

		case "selectBillPrintList":		// 납부서출력 (임대료, 보증금, 소송비)
			var sController   	= "/hsco/pms/rnt/prm/puchasCyberAccount/selectBillPrintList.do";
			var sInDatasets   	= "input1=ds_cond04";
			var sOutDatasets  	= "ds_billPrintList=output1";
			break;

		case "selectManagectBillPrint":	// 납부서출력 (관리비)
			var sController   	= "/hsco/pms/rnt/prm/puchasCyberAccount/selectManagectBillPrint.do";
			var sInDatasets   	= "input1=ds_cond04";
			var sOutDatasets  	= "ds_billPrintListMng=output1";
			break;
	}

	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{ 
	var bSuccess = nErrorCode == 0 ? true : false;

	if (bSuccess == true){
		switch(strSvcId){
			case "spVaBillUid":
				this.alert("처리되었습니다.")
				this.fn_paySearch();
				break;
				
			case "selectBillPrintList":
				this.fn_billPrint();
				break;

			case "selectManagectBillPrint":
				this.fn_billPrintMng();
				break;
		}
	}
	else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}	
}



