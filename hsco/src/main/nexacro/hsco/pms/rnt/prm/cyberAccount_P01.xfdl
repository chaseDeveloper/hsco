	this.ds_cond01.setColumn(0, "vacctNo", this.ds_vacctNoList.getColumn(index, "VACCT_NO"));	// 가상계좌코드
	this.ds_cond01.setColumn(0, "cntrctrNo", this.objCntrct['CNTRCTR_NO']);						// 계약자번호
	this.ds_cond01.setColumn(0, "crtDiv",  this.objCntrct['CRT_DIV']);							// 계약구분 [주택, 임대, 전세 ....]
	this.fn_transaction("billList");		// 납부진행내역 조회
