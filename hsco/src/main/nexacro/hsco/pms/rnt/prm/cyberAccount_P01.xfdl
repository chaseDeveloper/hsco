/*
 * 함수명  	: fn_get_checkValue
 * 설명 		: 체크된 체크박스의 id 목록을 문자열로 반환
 * params  		: area : 영역id
 * return Type 	: String 
 * 사용법		: this.fn_get_checkValue(area)
 */
this.fn_get_checkValue = function(area){
	var valueArr = [];
	var len = area.components.length;
	for(var i = 0; i < len; i++){
		var item = area.components.get_item(i);
		if(item instanceof CheckBox){
			if(item.value == true){
				var argName = item.id.substr(item.id.indexOf("_") + 1, item.id.length);
				valueArr.push(argName);
			}
		}
	}
	return valueArr.toString();
}


