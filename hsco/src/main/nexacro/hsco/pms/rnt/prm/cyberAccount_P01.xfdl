			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_REC_SEQ",   dsObj["REC_SEQ"]);					// 납부순번
			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_PAYEND_DATE", dsObj["PAY_TMLMT"]);				// 납입기한
			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_BILL_AMT",  dsObj["TOT_AMOUNT"]);				// 결제금액
			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_PAY_AMT",   dsObj["CORT_AMOUNT"]);				// 납부대상금액
			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_SALE_AMT", dsObj['RDCAMT_AMOUNT']);			//차감금액
			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_INT_AMT", dsObj['OVDINTR']);					//연체금액
			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_BILL_DIV", "B");						//납부구분(A-일괄,B-개별)
			this.ds_mt_rntchrgDetail.setColumn(nRow, "IN_BJ_YN", "R");							//보증금전용계좌 등록
		}
		
		// 청구내역마스터 등록
		nRow = this.ds_mt_rntchrgMaster.addRow();
		this.ds_mt_rntchrgMaster.copyRow(nRow, this.ds_mt_rntchrgDelete, 0);		// ds_mt_rntchrgDelete dataset copy후 값이 다른 column값만 setColumn
