    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID    	: cyberAccount_P01.xfdl
 * 화면명    	: 가상계좌 결제요청 팝업
 * 화면설명  	: 가상계좌 결제요청 팝업
 * 작성일    	: 2015.10.26
 * 작성자    	: 변승우
 * 수정이력  	: 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
  
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";

/***********************************************************************
CRT_DIV 'RN'
 - CORT_CODE 
	'RN':'임대료'
	'R0':'보증금'
	'T2':'소송비'
	'BS':'지원금'

CRT_DIV 'MA'
 - CORT_CODE 
	'MA':'관리비'

CRT_DIV 'BS':'지원금'
 ***********************************************************************/

this.objCntrct = null;		// 파라미터
/*
 * 함수명  	: cyberAccount_P01_oninit
 * 설명 		: form oninit
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.cyberAccount_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	//공통 폼 초기화
	this.fn_init(false);
	this.fn_init_form();		// Form 초기화
	this.fn_init_dataset();		// dataSet 초기화
}


/*
 * 함수명  	: cyberAccount_P01_onload
 * 설명 		: form onload
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.cyberAccount_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var cortCode = this.objCntrct['CORT_CODE'];
	
	// 상단 checkbox set
	this.div_search['chk_' + cortCode].set_value(true);
	if(cortCode == 'MA'){
		this.div_search.chk_R0.set_enable(false);	// 보증금
		this.div_search.chk_RN.set_enable(false);	// 임대료
		this.div_search.chk_T2.set_enable(false);	// 소송비		
		this.div_WORK.rdo_accntGubun.set_enable(false);
		this.div_WORK.rdo_accntGubun.set_value('');
		this.div_WORK.rdo_accntGubun1.set_value('MA');
	}
	else{
		this.div_search.chk_MA.set_enable(false);	// 관리비
		this.div_WORK.rdo_accntGubun1.set_enable(false);
		this.div_WORK.rdo_accntGubun1.set_value('');
		
		if(cortCode == 'T2'){		// 소송비
			this.ds_accntGubun.setColumn(0, "CODE_NM", '보증금외');
			this.div_WORK.rdo_accntGubun.set_value('RN');
		}
		else{
			this.div_WORK.rdo_accntGubun.set_value(cortCode);
		}
	}

	this.fn_load_combo();
	this.fn_transaction("nonPaymentList"); 					// 미납내역 목록
	this.fn_transaction("receiptList");						// 입금내역 목록
}


/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.fn_init_form = function()
{
	this.objCntrct = this.parent.objPuchasRentCntrctDetail;
	
	this.div_WORK.edt_rentHouseNm.set_value(this.objCntrct['RENT_HOUSE_NM']);
	this.div_WORK.edt_dong.set_value(this.objCntrct['DONG']);
	this.div_WORK.edt_ho.set_value(this.objCntrct['HO']);
	this.div_WORK.edt_nm.set_value(this.objCntrct['NM']);
	this.div_WORK.cal_rqestDe.set_value(this.gfn_today());
	this.div_WORK.cal_stTrDe.set_value(this.gfn_today());
	this.div_WORK.cal_edTrDe.set_value(this.gfn_today());
}


/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	this.ds_cond00.setColumn(0, "cntrctrNo", this.objCntrct['CNTRCTR_NO']);			// 계약자번호
	this.ds_cond00.setColumn(0, "cortCode", this.objCntrct['CORT_CODE']);			// 납부코드
	
	this.ds_cond03.clearData();
	this.ds_cond03.addRow();
	this.ds_cond03.setColumn(0, "houseCode", this.objCntrct['RENT_HOUSE_CODE']);	// 임대주택코드
	this.ds_cond03.setColumn(0, "dong", this.objCntrct['DONG']);					// 동
	this.ds_cond03.setColumn(0, "ho", this.objCntrct['HO']);						// 호
}


/*
 * 함수명  	: fn_load_combo
 * 설명 		: combo set
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.fn_load_combo = function ()
{				   
	// 가상계좌목록
	this.fn_fill_code(this.ds_vacctNoList,
					   "/hsco/pms/rnt/prm/puchasCyberAccount/vacctNoList.do", 
					   "",
					   {crtDiv    : this.objCntrct['CRT_DIV'], 
					    cntrctrNo : this.objCntrct['CNTRCTR_NO']},
					   "div_WORK_rdo_accntGubun_onitemchanged");

	// 문자발송 내용
	this.fn_fill_code(this.ds_smsCn,
					   "/hsco/cmm/ums/mssageManageList.do", 
					   "",
					   {CODE : "RNT003"},		// 문자코드 [RNT003:매입청구안내]
					   "");
					   
	this.div_search.edt_deptCode.set_value(this.gfn_getDeptId());		//부서코드				    			   
}


/*
 * 함수명  	: btn_search_onclick
 * 설명 		: 조회
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.btn_search_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_nonPaymentList.clearData();
	this.ds_receiptList.clearData();
	this.ds_cond00.setColumn(0, "cortCode", this.fn_get_checkValue(this.div_search));	// 상단에 선택된 납부코드
	this.fn_transaction("nonPaymentList"); 		// 미납내역 목록
	this.fn_transaction("receiptList"); 		// 입금내역 목록
}


/*
 * 함수명  	: btn_close_onclick
 * 설명 		: 종료
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}


/*
 * 함수명  	: div_WORK_rdo_accntGubun_onitemchanged
 * 설명 		: 계좌구분 선택
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_rdo_accntGubun_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	var cmbObj = null;
	if(this.div_WORK.rdo_accntGubun.enable)
		cmbObj = this.div_WORK.rdo_accntGubun;
	else if(this.div_WORK.rdo_accntGubun1.enable)
		cmbObj = this.div_WORK.rdo_accntGubun1;
	else
		return;
		
	var accntGubun = cmbObj.value;
	if(this.comUtils.isNull(accntGubun))
		return;

	this.ds_vacctNoList.filter("CORD_CODE == '" + accntGubun + "'");		// 계좌 dataset filter
	this.div_WORK.cmb_vacctNoList.set_index(0);
	this.fn_paySearch();
}


/*
 * 함수명  	: div_WORK_btn_search_onclick
 * 설명 		: 납부 진행내역 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_btn_search_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_paySearch();
}


/*
 * 함수명  	: div_WORK_grd_billList_onselectchanged
 * 설명 		: 납부 진행내역 로우 체인지
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */

this.div_WORK_grd_billList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	this.ds_billDetailList.clearData();		// 납부상세내역 dataset clear

	this.ds_cond02.clearData();
	this.ds_cond02.addRow();
	this.ds_cond02.setColumn(0, "bankCd",  this.ds_billList.getSelectColumn("BANK_CD"));	// 은행코드
	this.ds_cond02.setColumn(0, "vacctNo", this.ds_billList.getSelectColumn("VACCT_NO"));	// 가상계좌코드
	this.ds_cond02.setColumn(0, "cntrctrNo", this.objCntrct['CNTRCTR_NO']);					// 계약자번호
	this.ds_cond02.setColumn(0, "trDate",  this.ds_billList.getSelectColumn("TR_DATE"));	// 거래일자
		
	this.fn_transaction("billDetailList");	// 납부상세내역 조회
}


/*
 * 함수명  	: btn_Popup2_onclick
 * 설명 		: 문자발송
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.btn_Popup2_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rRow = this.ds_billList.rowposition;

	if(this.ds_billList.rowcount == 0){
		this.alert("납부진행내역이 존재하지 않습니다.");
		return;
	}
	if(rRow == -1){
		this.alert("선택된 납부진행내역이 없습니다.");
		return;
	}

    var nm      = this.objCntrct.NM || '';				// 고객명
	var telno   = this.objCntrct.TELNO || '';			// 연락처1
	var mbtlno  = this.objCntrct.MBTLNO || '';			// 연락처2	
	var bankNm  = this.ds_billList.getColumn(rRow, "BANK_NM") || '';	// 은행명
	var vcctNo  = this.ds_billList.getColumn(rRow, "VACCT_NO") || '';	// 계좌번호
	var billAmt = this.ds_billList.getColumn(rRow, "BILL_AMT") || '';	// 청구금액
	var accntSe = this.div_WORK.rdo_accntGubun.text || '';				// 청구종류
	var content = this.ds_smsCn.getColumn(0, "MSSAGE_CN") || '';		// 메시지 내용
	
	vcctNo  = "계좌번호(" + bankNm + ":" + vcctNo + ")";
	billAmt = this.comUtils.formatComma(billAmt);	// 청구금액 단위표시
	
	// 휴대폰번호 chk
	telno = this.fn_getTelNo(telno);							// 연락처1
	telno = telno == '' ? this.fn_getTelNo(mbtlno) : telno;		// 연락처2	
	
	accntSe = accntSe == '' ? this.div_WORK.rdo_accntGubun1.text : accntSe;
	
	this.ds_sms.clearData();
	var nRow = this.ds_sms.addRow();
	this.ds_sms.setColumn(0, "MBTLNO", telno);
	this.ds_sms.setColumn(0, "NM", nm);
	this.ds_sms.setColumn(0, "CHANGE_WORD0", nm);
	this.ds_sms.setColumn(0, "CHANGE_WORD1", vcctNo);
	this.ds_sms.setColumn(0, "CHANGE_WORD2", billAmt);
	this.ds_sms.setColumn(0, "CHANGE_WORD3", accntSe);
	this.ds_sms.setColumn(0, "CHANGE_WORD4", ' ');
	this.ds_sms.setColumn(0, "CHRCTR_PREPAR_VALUE_0", "2");		// 1 : 대기자, 2 : 계약자
	this.ds_sms.setColumn(0, "CHRCTR_PREPAR_VALUE_1", this.objCntrct['CNTRCTR_NO']);	//계약자번호

	this.gfn_popup("COM010700_P01.xfdl"
			   , 1000
			   , 600
			   , "SMS발송" 
			   , {
					CONTENT : content,
					MXTR_WRD0 : "고객명",
					MXTR_WRD1 : "계좌번호(은행:계좌)",
					MXTR_WRD2 : "청구금액",
					MXTR_WRD3 : "청구종류",
					MXTR_WRD4 : "",
					JOB_SE : "RNT003",
					ds : this.ds_sms
				 }
			   , "com::COM010700_P01.xfdl"
			   , "sms_callback", true);
}


/*
 * 함수명  	: div_WORK_btn_reqstRciv_onclick
 * 설명 		: 납부요청
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_btn_reqstRciv_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var idxArr = this.fn_checkIdx(this.div_WORK.Tab00.tabpage1.grd_nonPaymentList);
	var checkCnt = idxArr.length;

	if(checkCnt == 0){
		this.alert("선택된 미납 목록이 없습니다.");
		return;
	}
	else if(this.ds_vacctNoList.rowcount == 0){
		this.alert("가상계좌 목록이 없습니다.");
		return;
	}
	
	if(this.confirm("기존 요청자료가 존재할 경우 삭제됩니다. 작업을 진행하시겠습니까?")){
		this.ds_mt_rntchrgDelete.clearData();
		this.ds_mt_rntchrgDetail.clearData();
		this.ds_mt_rntchrgMaster.clearData();
		
		// 가상계좌 데이터셋을 객체로 변환
		var objVacctNo = this.fn_dsToObj(this.ds_vacctNoList, this.div_WORK.cmb_vacctNoList.index);
		var trDate  = this.div_WORK.cal_rqestDe.value;	// 청구일자
		var trSdate = this.div_WORK.cal_stTrDe.value;	// 시작 결제기간
		var trEdate = this.div_WORK.cal_edTrDe.value;	// 종료 결제기간
		
		// 기존에 저장되어 있던 청구내역을 삭제
		var nRow = this.ds_mt_rntchrgDelete.addRow();
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_FLAG",     "DELETE");								// 작업구분
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_BANK_CD",  objVacctNo["BANK_CD"]);					// 은행코드
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_ACCT_NO",  objVacctNo["VACCT_NO"]);				// 계좌번호
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_TR_DATE",  trDate);								// 거래일자
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_TR_SDATE", trSdate);								// 거래시작일
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_TR_EDATE", trEdate);								// 거래종료일
		this.ds_mt_rntchrgDelete.setColumn(nRow, "IN_CRT_DIV",  this.objCntrct['CRT_DIV']);				// 계약구분
