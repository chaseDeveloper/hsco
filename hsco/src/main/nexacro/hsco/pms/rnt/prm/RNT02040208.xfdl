	this.ds_cond01.addRow();
	this.ds_cond01.setColumn(0, "cntrctrNo", this.ds_specialManagement.getColumn(this.ds_specialManagement.rowposition, "CNTRCTR_NO"));
	this.fn_transaction("selectSpecialManagementDetailList");
}


