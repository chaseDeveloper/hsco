 ***********************************************************************/	
 		
this.fn_delete = function(){
	if(this.ds_specialManagement.rowcount == 0){
		return;
	}
	if(this.confirm("D")){
		this.ds_specialManagement.deleteRow(this.ds_specialManagement.rowposition);
	}
}


