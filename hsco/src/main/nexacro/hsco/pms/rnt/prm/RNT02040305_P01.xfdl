    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   	: RNT02040305_P01
 * 화면명   	: 관리비 안내문 팝업
 * 화면설명 	: 관리비 안내문 팝업
 * 작성일   	: 2015.11.10
 * 작성자   	: 변승우
 * 수정이력 	: 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "pms_lib::pmsUtilInclude.xjs";
include "pms_lib::rntUtilInclude.xjs";

this.payTmlmt = null;		

this.RNT02040305_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{

}

this.RNT02040305_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.ds_cond00.addRow();
	this.ds_cond00.setColumn(0, "GDCC_NO", "8");
	this.ds_cond00.setColumn(0, "GDCC_KND", "1");

// 	var today = this.gfn_today()
// 	//시행일자 = 현재일자
// 	this.div_WORK.cal_stDe.set_value(today);
// 	
// 	//납입일자 현재일자의 말일
// 	this.div_WORK.cal_payTmlmt.set_value(this.gfn_lastDateNum(today));
	
	this.fn_transaction("selectInfoDocList");
	
	var rowcount = this.parent.ds_managectGenerationNpym.rowcount;
	
	for(var i=0; i<rowcount; i++){
			var nRow = this.ds_managectGenerationNpym.addRow();
			this.ds_managectGenerationNpym.copyRow(nRow, this.parent.ds_managectGenerationNpym, i);
	}
	
	this.payTmlmt = this.parent.payTmlmt;

}


 /**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {		
		
		case "selectInfoDocList":	
			var sController   	= "/hsco/pms/rnt/prm/RNT02000000/selectInfoDocList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_infoDoc=output1";
			break;
			
		case "infoDocListU":	
			var sController   	= "/hsco/pms/rnt/prm/RNT02000000/infoDocListU.do"; 
			var sInDatasets   	= "input1=ds_infoDoc:U";
			var sOutDatasets  	= "";
			break;	
					
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch (strSvcId) {	
		
			case "selectInfoDocList" :
				var today = this.gfn_today();
				
				this.div_WORK.cal_stDe.set_value(today);
				this.div_WORK.cal_payTmlmt.set_value(this.fn_last_yyyymmdd(today));
			break;
			
			case "infoDocListU":
				this.fn_print();
			break;
			
		}
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}
 
 
 
/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}

//레포트 출력
this.btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_transaction("infoDocListU");
}


this.fn_print = function(){
	
	this.ds_damdangjaList.clearData();
	this.ds_damdangjaList.addRow();
	this.ds_damdangjaList.setColumn(0, "ZIP" ,this.div_WORK.edt_zip.value)
	this.ds_damdangjaList.setColumn(0, "ADRES" ,this.div_WORK.edt_adres.value)
	this.ds_damdangjaList.setColumn(0, "TELNO" ,this.div_WORK.edt_telno.value)
	this.ds_damdangjaList.setColumn(0, "FAX" ,this.div_WORK.edt_fax.value )
	this.ds_damdangjaList.setColumn(0, "CENTER_JANG" ,this.div_WORK.edt_centjangNm.value )
	this.ds_damdangjaList.setColumn(0, "DAMDANG" ,this.div_WORK.edt_waitrNm.value)
	
	var rowcount = this.parent.ds_managectGenerationNpym.rowcount;
	
	for(var i=0; i<rowcount; i++){
		this.ds_managectGenerationNpym.setColumn(i, "DOC_NO", this.div_WORK.edt_docNo.value);
		this.ds_managectGenerationNpym.setColumn(i, "ST_DE", this.div_WORK.cal_stDe.value);
		this.ds_managectGenerationNpym.setColumn(i, "SUBJECT", this.div_WORK.edt_subject.value);
		this.ds_managectGenerationNpym.setColumn(i, "RCIV_DE", this.payTmlmt);
	}
	
	this.ds_managectGenerationNpymRpt.clearData();
	this.ds_managectGenerationNpymRpt.copyData(this.ds_managectGenerationNpym);

	var rptOption = {};
	rptOption["path"] = "/pms/rnt/prm/RNT02040305_R01.jrf";
	rptOption["title"] = "매입임대료 관리비 체납안내장";
	rptOption["filename"] = "매입임대료 관리비 체납안내장";
	rptOption["dataset"] = [
		  ["ds_managectGenerationNpym", this.ds_managectGenerationNpymRpt]
		  ,["ds_damdangjaList", this.ds_damdangjaList]
	];
	
	this.pmsUtil.popupReportView(this, rptOption);
}

