this.div_WORK_btn_insertFamily_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var rRow = this.ds_mvnWaitrList.rowposition;
	if(rRow == -1){
		this.alert("선택된 대기자가 없습니다.");
		return;
	}
	else if(this.ds_mvnWaitrList.getRowType(rRow) == Dataset.ROWTYPE_INSERT){
		this.alert("대기자를 등록중입니다.");
		return;
	}
	
	var nRow = this.ds_rentCstmrFamilyList.addRow();
	this.ds_rentCstmrFamilyList.setColumn(nRow, "AREA_CODE", this.ds_mvnWaitrList.getSelectColumn("AREA_CODE"));
	this.ds_rentCstmrFamilyList.setColumn(nRow, "RCEPT_DE", this.ds_mvnWaitrList.getSelectColumn("RCEPT_DE"));
	this.ds_rentCstmrFamilyList.setColumn(nRow, "WAIT_RCEPT_SN", this.ds_mvnWaitrList.getSelectColumn("WAIT_RCEPT_SN"));	
}


