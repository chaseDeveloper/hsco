	this.fn_transaction("selectMvnWaitrList");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
	if(this.gfn_checkValidation(this.ds_mvnWaitrList, this.ds_validation) == false) {
		return;
	}

	if(this.ds_mvnWaitrList.isUpdated() == true){	
		if(this.confirm("저장 하시겠습니까?")){
			this.ds_mvnWaitrList.backupPosition = this.ds_mvnWaitrList.rowposition;
			this.fn_transaction("saveMvnWaitrCUD");
		}
	}
}

/*
 * 함수명  	: div_WORK_grd_mvnWaitrList_onselectchanged
 * 설명 		: 대기자 그리드 선택시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_WORK_grd_mvnWaitrList_onselectchanged();
 */
this.div_WORK_grd_mvnWaitrList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	this.ds_rentCstmrFamilyList.clearData();
	//this.ds_cond01.setColumn(0, "cntrctrCstmrNo", this.ds_mvnWaitrList.getColumn(this.ds_mvnWaitrList.rowposition,"PUCHAS_CSTMR_NO"));
	this.ds_cond01.setColumn(0, "WAITR_CNTRCTR_SE", '01');													// 대기자, 계약자 구분[01:대기자]
	this.ds_cond01.setColumn(0, "AREA_CODE", this.ds_mvnWaitrList.getSelectColumn("AREA_CODE"));			// 지역코드	
	this.ds_cond01.setColumn(0, "RCEPT_DE", this.ds_mvnWaitrList.getSelectColumn("RCEPT_DE"));				// 접수일자
	this.ds_cond01.setColumn(0, "WAIT_RCEPT_SN", this.ds_mvnWaitrList.getSelectColumn("WAIT_RCEPT_SN"));	// 대기접수일련번호
	
	this.fn_setHighlight();
	
	this.fn_transaction("rentCstmrFamilyList");
	
	// 대기자의 접수일자, 주택코드, 평형을 수정하지 못하도록 변경
	var rowType = this.ds_mvnWaitrList.getRowType(this.ds_mvnWaitrList.rowposition);
	var isEdit  = rowType != Dataset.ROWTYPE_INSERT;
	if(this.ds_mvnWaitrList.rowposition > -1){
		this.div_WORK.Calendar00.set_readonly(isEdit);
		this.div_WORK.cmb_areaCode.set_readonly(isEdit);
		
		this.div_WORK.Calendar00.set_cssclass(isEdit ? 'cal_WF_Readonly' : 'cal_WF_Essential');
		this.div_WORK.cmb_areaCode.set_cssclass(isEdit ? 'cmb_WF_Readonly' : 'cmb_WF_Essential');
	}
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {			
			
		// 입주대기자 목록 조회	
		case "selectMvnWaitrList" :
			var sController   	= "/hsco/pms/rnt/prm/RNT02020100/selectMvnWaitrList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_mvnWaitrList=output1";			
			break;
			
		// 입주대기자 CUD
		case "saveMvnWaitrCUD" :
			var sController   	= "/hsco/pms/rnt/prm/RNT02020100/saveMvnWaitrCUD.do";			
			var sInDatasets   	= "input1=ds_mvnWaitrList:U";
			var sOutDatasets  	= "";			
			break;		
			
		// 입주대기자 가족목록 조회
		case "rentCstmrFamilyList" :
			var sController   	= "/hsco/pms/rnt/prm/puchasRentCstmr/selectPuchasCstmrFamilyList.do";
			//var sController   	= "/hsco/pms/rnt/prm/RNT02020100/rentCstmrFamilyList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentCstmrFamilyList=output1";			
			break;	

		// 입주대기자 가족사항 저장
		case "rentCstmrFamilyCUD":
			var sController   	= "/hsco/pms/rnt/prm/puchasRentCstmr/puchasCstmrFamilyCUD.do";
			//var sController   	= "/hsco/pms/rnt/prm/RNT02020100/rentCstmrFamilyCUD.do"; 
			var sInDatasets   	= "input1=ds_rentCstmrFamilyList:U";
			var sOutDatasets  	= "";
			break;
		
		case "mvnWaitrGdccSndngDeList":
			var sController		= "/hsco/pms/rnt/prm/RNT02020200/mvnWaitrGdccSndngDeList.do"
			var sInDatasets		= "";
			var sOutDatasets	= "ds_mvnWaitrGdccSndngDeList=output1";
			break;
			
		default :
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	//this.debug(this.ds_mvnWaitrList,true)
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}		



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
 this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
 {  
 	var bSuccess = nErrorCode == 0?true:false;
 	if (bSuccess == true){
 		switch(strSvcId){
			case "selectMvnWaitrList":
				//this.ds_mvnWaitrList.set_rowposition(this.ds_mvnWaitrList.backupPosition) ;	//저장후 조회일경우 저장했던 Row로 위치 이동
				this.ds_mvnWaitrList.backupPosition = -1;
				this.fn_setHighlight();
				break;
			
			case "mvnWaitrGdccSndngDeList":
				this.ds_mvnWaitrGdccSndngDeList.insertRow(0);
				this.ds_mvnWaitrGdccSndngDeList.setColumn(0, "GDCC_SNDNG_DE", "전체");
				this.ds_mvnWaitrGdccSndngDeList.setColumn(0, "GDCC_VALUE", "");
				this.div_search.cmb_sndngDe.set_index(0);
				break;

			case "saveMvnWaitrCUD" :	// 입주대기자 CUD
				this.fn_search();	//조회시에 저장하고 있는 현재의 Row를 전달
				break;
 		}
 	}
 	
 	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
 }


/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
/*
 * 함수명  	: fn_insert
 * 설명 		: 추가
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_insert = function(){
	var nRow = this.ds_mvnWaitrList.addRow();
	
	this.ds_mvnWaitrList.setColumn(nRow, "RENT_HOUSE_CODE", this.div_search.cmb_houseCode.value);
	this.ds_mvnWaitrList.setColumn(nRow, "RENT_HOUSE_NM", this.div_WORK.cmb_houseCode.text);
	this.ds_mvnWaitrList.setColumn(nRow, "RCEPT_DE", this.gfn_today());	
}	


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제
 * params  		: None
