/*
 * 함수명  	: div_WORK_btn_rcivPop_onclick
 * 설명 		: 연체이자생성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_WORK_btn_rcivPop_onclick();
 */
this.div_WORK_btn_createPuchasMtRntchrg_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
		// 체크된 건이 없으면 return
		if(this.ds_puchasRntchrg.findRow("CHK" , 1) == -1){
			return;
		}
	

		if(application.confirm("납입일자를 기준으로 연체이자를 재생성하시겠습니까?")){
		
			this.ds_cond02.clearData();
			this.ds_cond02.addRow();
			for(var i = 0 ; i < this.ds_puchasRntchrg.rowcount ; i++){	    
				if(this.ds_puchasRntchrg.getColumn( i , "CHK") == 1){
					trace(" i = " + i);
					this.ds_cond02.setColumn(0, "cntrctrNo", this.ds_puchasRentCntrct.getSelectColumn("CNTRCTR_NO"));
					this.ds_cond02.setColumn(0, "payTmlmt", this.div_WORK.cal_payTmlmt.value);

				}	
			}
			this.fn_transaction("savePuchasRentMtRntchrgU");	
		}
}



this.ds_puchasRentCntrct_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{	
	//수정중인 데이터가 있는지 체크
	//행추가로 인한 로우 체인지가 발생한 경우 행추가한 행의 업데이트 여부를 스킵한다.
	if(this.fn_checkUpdate(this.ds_puchasRentCntrct, e.newrow, false) ||
	   this.fn_checkUpdate(this.ds_puchasRntchrg)){
		if(this.confirm("confirm.변경.취소.여부") == false){
			return false;
		}
	}
	return true;
}



