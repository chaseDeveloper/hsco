		this.ds_puchasRntchrg.setColumn(this.ds_puchasRntchrg.rowposition, "MDAT_YM" ,this.parent.objParam.rciv_de.substring(0,6));
		this.ds_puchasRntchrg.setColumn(this.ds_puchasRntchrg.rowposition, "MDAT_DE" ,this.parent.objParam.rciv_de);
	}
 }
 
 
