}


this.fn_totalAmountSum = function(){
	var nRow = this.ds_puchasRntchrg.rowposition;
	var total =   (parseInt(this.div_WORK.MaskEdit00.value)||0)  
				+ (parseInt(this.div_WORK.MaskEdit01.value)||0)  
				+ (parseInt(this.div_WORK.MaskEdit02.value)||0);  
				
	this.ds_puchasRntchrg.setColumn(nRow, "TOTAL_AMOUNT",total); 
}


