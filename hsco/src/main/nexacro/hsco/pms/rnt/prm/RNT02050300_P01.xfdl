 */
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}


 /*
 * 함수명  	: btn_print_onclick
 * 설명 		: 레포트 출력
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_damdangjaList.setColumn(0, "TITLE", this.edt_title.value);
	this.ds_damdangjaList.setColumn(0, "STDE", this.cal_stDe.text);
	this.ds_damdangjaList.setColumn(0, "STOKDE", this.cal_stOkDe.text);
	this.ds_damdangjaList.setColumn(0, "EDOKDE", this.cal_edOkDe.text);
	this.ds_damdangjaList.setColumn(0, "TIME_LMT", "");
	this.ds_damdangjaList.setColumn(0, "OK_RM", this.edt_okRm.value);
	this.ds_damdangjaList.setColumn(0, "PLACE1", this.edt_place1.value);
	this.ds_damdangjaList.setColumn(0, "PLACE2", "");
	this.ds_damdangjaList.setColumn(0, "PLACE3", "");
	this.ds_damdangjaList.setColumn(0, "DOC_NO", this.edt_docNo.value);
	this.ds_damdangjaList.setColumn(0, "MGR_NO", "");
	this.ds_damdangjaList.setColumn(0, "TODAY", this.today.substring(0,4))
		
	var vOpts = new this.cf_Opts();
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setReportPath("/pms/rnt/prm/RNT02050300_R01.jrf");
	vOpts.setSaveFilename("매입임대료 문제세대 안내장");
	vOpts.setLocalDs(["ds_damdangjaList", this.ds_damdangjaList]); 
	vOpts.setLocalDs(["ds_quesHouseHold", this.parent.ds_quesHouseHold]);
		
	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		opts : vOpts
	};
	
	this.gfn_popup("Popup_Report", 884, 735, "안내문", parm, "common::frmReportPopup.xfdl", "report_callback");
}



