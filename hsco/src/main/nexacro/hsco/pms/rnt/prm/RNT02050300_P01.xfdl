this.RNT02050300_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo){}


this.RNT02050300_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.today = this.gfn_today();
	this.cal_stDe.set_value(this.today);
	this.cal_stOkDe.set_value(this.today); 
	this.cal_edOkDe.set_value(this.today);
	
	// 담당자(RN2003)
    this.fn_fill_code(this.ds_damdangjaList, "/hsco/pms/com/PMS000000/damdangjaList.do", "", {damdangjaCode:'RN2003'});		// RN2003 : 재계약(매입임대)
}


/*
 * 함수명  	: btn_close_onclick
 * 설명 		: 종료
