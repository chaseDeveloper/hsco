/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		:
 * return Type 	: None
 * 사용법		:
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
	this.checkDs.push(this.ds_cntrctrInfo);
	this.checkDs.push(this.ds_familyRelate);
}


/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.fn_search = function()
{
	this.ds_cntrctrPosblHouse.clearData();					// 계약가능 주택정보 clear
	this.ds_waitrList.clearData();							// 대기자 리스트 clear

	this.fn_divToDS(this.ds_cond00, this.div_search);

	this.fn_transaction("selectWaitrList"); 				// 대기자 리스트
	this.fn_transaction("selectCntrctrPosblHouseList");		// 계약가능 주택정보
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.fn_save = function()
{
	if(this.gfn_checkValidation(this.ds_cntrctrInfo, this.ds_validation) == false) {
		this.alert("comm.데이터.변경.없음");
		return false;
	}

	var cntrctrNo = this.ds_cntrctrInfo.getColumn(0, "CNTRCTR_NO");		// 계약자번호
	var suplySe = this.ds_cntrctrInfo.getColumn(0, "SUPLY_SE");			// 주택 공급가능여부

	if(this.comUtils.isNotNull(cntrctrNo)){
		this.alert("해당 대기자는 이미 계약이되었습니다.");
		return;
	}
	if(suplySe == '0'){
		this.alert("해당 주택은 이미 계약이되었습니다.");
		return;
	}

	if(this.confirm("S")){
		this.fn_transaction("puchasCntrctC");
	}
}


/*
 * 함수명  	: fn_print
 * 설명 		: 인쇄 div 실행
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.fn_print = function()
{
	var cntrctrNo = this.ds_cntrctrInfo.getColumn(0, "CNTRCTR_NO");		// 계약자번호
	var suplySe = this.ds_cntrctrInfo.getColumn(0, "SUPLY_SE");			// 주택 공급가능여부
	var isBoolean = this.comUtils.isNotNull(cntrctrNo) && suplySe == '0';
	
	if(!this.div_print.visible){
		this.div_print.set_right(136);
		this.div_print.set_visible(true);
		var len = this.div_print.components.length;
		for(var i = 0; i < len; i++){
			var item = this.div_print.components.get_item(i);
			if(item instanceof CheckBox){
				item.set_value(0);
				item.set_enable(isBoolean);
			}
		}
	}
	else{
		this.div_print.set_visible(false);
	}
}


