	this.fn_get_cmmCode(this.ds_roomStructure,    "",   {grpCode:"RNT027"});		// 방구조
		   
	this.fn_fill_code(this.ds_precedentAccount, "/hsco/pms/rnt/prm/RNT02030200/selectPrecdntAccount.do");	// 선수금계좌
