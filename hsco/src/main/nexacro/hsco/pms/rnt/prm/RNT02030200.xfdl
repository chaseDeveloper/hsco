/*
 * 함수명  	: div_print_btn_print_onclick
 * 설명 		: 출력물 팝업
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.printChkArr = [];	// 출력물 배열
this.div_print_btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.printChkArr = [];
	var len = this.div_print.components.length;
	for(var i = 0; i < len; i++){
		var item = this.div_print.components.get_item(i);
		if(item instanceof CheckBox){
			if(item.value == "1"){
				this.printChkArr.push(item.id.substr(item.id.indexOf("_") + 1, item.id.length));
			}
		}
	}

	//this.printChkArr.reverse();
	this.fn_printExecut();
}


/*
 * 함수명  	: div_work_grd_waitrList_oncelldblclick
 * 설명 		: 대기자 리스트 oncelldblclick - 선택 row 대기자 정보를 계약정보로 복사
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.div_work_grd_waitrList_oncelldblclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var nPos = this.ds_waitrList.rowposition;										// 선택 row pos
	this.ds_cntrctrInfo.clearData();
	this.ds_cntrctrInfo.addRow();
	this.ds_cntrctrInfo.copyRow(0, this.ds_waitrList, nPos);						// 대기자 정보 copy

	var roomStructure = this.ds_waitrList.getColumn(nPos, "ROOM_STRUCTURE");		// 대기자의 주택형
	this.ds_cntrctrPosblHouse.filter("ROOM_STRUCTURE == '" + roomStructure + "'");	// 계약가능 주택정보를 주택형으로 filter
	
	this.ds_cond01.clearData();
	this.ds_cond01.addRow();
	this.ds_cond01.setColumn(0, "AREA_CODE", this.ds_waitrList.getColumn(nPos, "AREA_CODE"));
	this.ds_cond01.setColumn(0, "RCEPT_DE",  this.ds_waitrList.getColumn(nPos, "RCEPT_DE"));
	this.ds_cond01.setColumn(0, "WAIT_RCEPT_SN", this.ds_waitrList.getColumn(nPos, "WAIT_RCEPT_SN"));
	this.fn_transaction("selectPuchasRentCstmrFamilyList");		
	
	if(this.ds_cntrctrInfo.getColumn(0, "HOUSE_POSESN_AT") == 1)
		this.div_detail.chkEdit.set_visible(true);
	else
		this.div_detail.chkEdit.set_visible(false);
	
	// 버튼 비활성화
	this.btn_cyberAccount.set_enable(false);			// 가상계좌
	this.div_detail.btn_insert.set_enable(false);		// 행추가
	this.div_detail.btn_cancel.set_enable(false);		// 행삭제
	this.div_detail.btn_save.set_enable(false);			// 가족사항 저장
}


