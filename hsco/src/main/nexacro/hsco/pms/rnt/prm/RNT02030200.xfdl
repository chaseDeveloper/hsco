this.RNT02030200_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.div_search.div_sndngMt.setValue(this.gfn_today());
	this.fn_get_houseCode(this.div_detail.cmb01, "", "");		// 매입임대주택 코드 조회
	
	this.fn_get_qualfSe(this.ds_qualfSe,    "", {grpCode:"RNT001", se:"p"});		// 자격구분
