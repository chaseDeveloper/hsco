/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	switch (strSvcId) {
		case "selectWaitrList":							// 대기자 리스트 정보
			var sController   	= "hsco/pms/rnt/prm/RNT02030200/selectWaitrList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_waitrList=output1";
			break;

		case "selectCntrctrPosblHouseList":				// 계약가능 주택정보
			var sController   	= "hsco/pms/rnt/prm/RNT02030200/selectCntrctrPosblHouseList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_cntrctrPosblHouse=output1";
			break;

		case "puchasCntrctC":							// 계약등록
			var sController   	= "hsco/pms/rnt/prm/RNT02030200/puchasCntrctC.do";
			var sInDatasets   	= "input1=ds_cntrctrInfo:U";
			var sOutDatasets  	= "ds_cntrctrInfo=output1";
			break;

		case "savePuchasRentCstmrFamilyCUD":			// 대기자 가족 저장
			for(var i = 0; i < this.ds_waitrFamilyList.getRowCount(); i++){
				this.ds_waitrFamilyList.setColumn(i, "PUCHAS_CSTMR_NO", this.ds_cntrctrInfo.getColumn(0, "PUCHAS_CSTMR_NO"));
			}
			var sController   	= "hsco/pms/rnt/prm/RNT02030100/savePuchasRentCstmrFamilyCUD.do";
			var sInDatasets   	= "input1=ds_waitrFamilyList:U";
			var sOutDatasets  	= "";
			break;
			
		case "selectPuchasRentCstmrFamilyList":			// 대기자 가족정보 조회
			var sController   	= "/hsco/pms/rnt/prm/RNT02030200/selectPuchasRentCstmrFamilyList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_waitrFamilyList=output1";
			break;
		}
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync);
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	var bSuccess = nErrorCode == 0 ? true : false;

	if (bSuccess){
		switch(strSvcId){
			case "puchasCntrctC":
				// 대기자 목록 계약완료 표시
				this.ds_waitrList.setColumn(this.ds_waitrList.rowposition, "CNTRCTR_NO", this.ds_cntrctrInfo.getColumn(0, "CNTRCTR_NO"));	// 계약자번호
				
				// 계약가능 주택목록 계약완료 표시
				this.ds_cntrctrPosblHouse.setColumn(this.ds_cntrctrPosblHouse.rowposition, "SUPLY_SE", '0');								// 주택 공급가능여부
					
				// 버튼 활성화
				this.btn_cyberAccount.set_enable(true);				// 가상계좌
				this.div_detail.btn_insert.set_enable(true);		// 행추가
				this.div_detail.btn_cancel.set_enable(true);		// 행삭제
				this.div_detail.btn_save.set_enable(true);			// 가족사항 저장
				break;
		}
	}
}



/*
 * 함수명  	: fn_printExecut
 * 설명 		: 선택한 출력물 실행
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_printExecut();
 */
this.fn_printExecut = function()
{
	if(this.printChkArr.length == 0){
		this.div_print.set_visible(false);
		return;
	}

	var docSe = this.printChkArr.pop();
	var reportNm = "";	
	var vOpts = new this.cf_Opts();
	vOpts.setToolbar(false);
	vOpts.setScale(100);

	switch(docSe){
		case "1":		// 매입임대 계약서
			this.ds_contract.clearData();
			this.ds_contract.addRow();	
			this.ds_contract.copyRow(0, this.ds_cntrctrInfo, 0);
			
			var tempCntrctDe = this.ds_cntrctrInfo.getSelectColumn("CNTRCT_DE");
			var tempAdres	 = this.ds_cntrctrPosblHouse.getSelectColumn("RENT_HOUSE_ADRES");
			
			this.ds_contract.setColumn(0, "YYYY", tempCntrctDe.substring(0, 4));
			this.ds_contract.setColumn(0, "MM", tempCntrctDe.substring(4, 6));
			this.ds_contract.setColumn(0, "DD", tempCntrctDe.substring(6, 8));
			this.ds_contract.setColumn(0, "RENT_HOUSE_ADRES", tempAdres);
			
			this.ds_contract.addRow();				
			this.ds_contract.copyRow(1, this.ds_cntrctrInfo, 0);
			this.ds_contract.setColumn(1, "YYYY", tempCntrctDe.substring(0, 4));
			this.ds_contract.setColumn(1, "MM", tempCntrctDe.substring(4, 6));
			this.ds_contract.setColumn(1, "DD", tempCntrctDe.substring(6, 8));
			this.ds_contract.setColumn(1, "RENT_HOUSE_ADRES", tempAdres);
			
			
	
			reportNm = "매입임대 계약서";
	
			vOpts.setReportPath("/pms/rnt/prm/RNT02030200_R02.jrf");
			vOpts.setSaveFilename(reportNm);
			vOpts.setLocalDs(["ds_contract", this.ds_contract]);
			break;

		case "2":		// 선수금고지서
			this.ds_cntrctrInfo.setColumn(0, "ACCOUNT", this.ds_precedentAccount.getColumn(0, "ACCOUNT"));
			
			this.ds_cntrctrInfo1.clearData();
			this.ds_cntrctrInfo1.addRow();	
			this.ds_cntrctrInfo1.copyRow(0, this.ds_cntrctrInfo, 0);

			reportNm = "선수금고지서";
			
			vOpts.setReportPath("/pms/rnt/prm/RNT02030200_R03.jrf");
			vOpts.setSaveFilename(reportNm);
			vOpts.setLocalDs(["ds_cntrctrInfo1", this.ds_cntrctrInfo1]);
			break;
	}
	
	
	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		opts : vOpts
	};

	this.gfn_popup("Popup_Report", 884, 735, reportNm, parm, "common::frmReportPopup.xfdl", "fn_printExecut");
}

