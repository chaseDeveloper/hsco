		}
}


//레포트의 다음 idx 저장
this.nextReportIdx = -1;

//체크한 보고서만 출력
this.fn_Print = function(start, end){

	//재귀함수 종료 조건
	if(start > 3){
		return;
	}
	
	
	for(var i = start; i <= end; i++){
		
		var value = this.div_WORK['CheckBox0' + i].value;
		
		if(value == true){
			this.nextReportIdx = i+1;			
			var fileName = this.ds_reportList.getColumn(i, "CODE_NM");
			var r = 'R0' + (i+1);
			
			var vOpts = new this.cf_Opts();
			vOpts.setToolbar(false);
			vOpts.setScale(100);
			vOpts.setReportPath("/pms/rnt/prm/RNT02040206_" + r + ".jrf");
			vOpts.setSaveFilename(fileName);
			vOpts.setLocalDs(["ds_puchasOverRpay", this.ds_puchasOverRpayRpt]);
			
			this.debug(this.ds_puchasOverRpayRpt);
			
			var parm = {
				arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
				opts : vOpts
			};

			this.gfn_popup("Popup_Report" + i, 884, 735, fileName, parm, "common::frmReportPopup.xfdl", "report_callback");
			break;
		}
	}	

