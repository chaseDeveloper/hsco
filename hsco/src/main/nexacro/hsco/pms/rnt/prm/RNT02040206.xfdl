/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			

			//매입임대 과오내역 처리 정보
			case "selectPuchasOverRpayList":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040206/selectPuchasOverRpayList.do"; 
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_puchasOverRpay=output1";
			break;	

			//매입임대 과오액(지급완료,지급보류) 처리
			case "sprntPuchasRecDescMissIu":		
				var sController   	= "/hsco/pms/rnt/prm/RNT02040206/sprntPuchasRecDescMissIu.do"; 
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "";
			break;		
					
			}
			
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
				
				//매입임대 과오액(지급완료,지급보류) 처리 성공시,
				case "sprntPuchasRecDescMissIu":
					this.fn_search();
				break;
								
		}
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


/*
 * 함수명  	: div_WORK_grd_puchasOverRpayList_onselectchanged
 * 설명 		: 과오액 반환 처리된것에 관해, 지급보류 버튼(ENABLE,DISABLE)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_WORK_grd_puchasOverRpayList_onselectchanged ();
 */	
this.div_WORK_grd_puchasOverRpayList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	if(typeof(this.ds_puchasOverRpay.getSelectColumn("DPSTR")) == 'undefined'){
		this.div_WORK.edt_DPSTR.set_value(this.ds_puchasOverRpay.getSelectColumn("NM"))
	}
	
	var returnAt = this.ds_puchasOverRpay.getSelectColumn("RETURN_AT");
	if(returnAt == '1'){
		this.div_WORK.btn_dfnntPymntHold.set_enable(false);
	}else{
		this.div_WORK.btn_dfnntPymntHold.set_enable(true);
	}
}


/*
 * 함수명  	: div_WORK_btn_dfnntPymntComplete_onclick
 * 설명 		: 과오액 반환 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_WORK_btn_dfnntPymntComplete_onclick ();
 */		
this.div_WORK_btn_dfnntPymntComplete_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var returnAt = this.ds_puchasOverRpay.getSelectColumn("RETURN_AT");
	var msg = "과오반환 완료처리를 하시겠습니까?";
	if(returnAt == '1'){
		msg = "이미 지급완료된 처리자료입니다. 재처리 하시겠습니까?"
	}
	
	if(application.confirm(msg)){
		this.ds_cond01.clearData();
		this.ds_cond01.addRow();
		//처리내역 ValidationCheck
		if(this.gfn_checkValidation(this.div_WORK, this.ds_validation) == false) {
			return;
		}
		this.ds_cond01.copyRow(0, this.ds_puchasOverRpay, this.ds_puchasOverRpay.rowposition);
		trace("ds_cond01 =" + this.ds_cond01.saveXML());
		this.ds_cond01.setColumn(0,"RETURN_AT",'1');
		this.ds_cond01.setColumn(0,"RETURN_DE",this.div_WORK.cal_dfnntPymntDe.value);
		
		if(typeof(this.ds_cond01.getColumn(this.ds_puchasOverRpay.rowcount, 'NTIC_SE')) == 'undefined'){
			this.ds_cond01.setColumn(0,"NTIC_SE",'1');
			this.ds_cond01.setColumn(0,"OVERRPAY_NO",'1');
		}

		this.fn_transaction("sprntPuchasRecDescMissIu");
	}	
}

 this.div_WORK_CheckBox03_onclick = function(obj:CheckBox, e:nexacro.CheckBoxChangeEventInfo)
{
		if(this.div_WORK.CheckBox03.value == true){
			this.div_WORK.CheckBox01.set_value(false)
