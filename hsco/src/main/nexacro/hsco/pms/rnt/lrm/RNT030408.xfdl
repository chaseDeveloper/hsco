//출력 2016.03.15
this.fn_print = function()
{
	this.fn_transaction("printList1");	
	this.ds_date.clearData();
	var nRow = this.ds_date.addRow();
	this.ds_date.setColumn( nRow , "CHUL_DT" , this.cal_expndtrDe.value);
	this.ds_date.setColumn( nRow , "BAN_DT" , this.cal_returnDe.value);
	this.ds_date.setColumn( nRow , "GUP_DT" , this.cal_pymntDe.value);
	this.fn_report(0,3);
}

//레포트의 다음 idx 저장
this.nextReportIdx = -1;

//체크한 보고서만 출력
this.fn_report = function(start, end){
	
	//재귀함수 종료 조건
	if(start > 3){
		return;
	}
	
	for(var i = start; i <= end; i++){
		var value = this['CheckBox0' + i].value;
		
		if(value == true){
			this.nextReportIdx = i+1;			
			var fileName = this.ds_reportList.getColumn(i, "CODE_NM");
			
			var vOpts = new this.cf_Opts();
			vOpts.setToolbar(false);
			vOpts.setScale(100);
			vOpts.setReportPath("/pms/rnt/lrm/RNT030408_" + (i+1) + ".jrf");
    		vOpts.setArgs("gup_dt", this.cal_pymntDe.value);	
 	  	    vOpts.setArgs("bank_nm", this.ds_lfsts_rciv.getColumn(this.ds_lfsts_rciv.rowposition , "RETURN_BANK_NM"));	
 		    vOpts.setArgs("vacct_no", this.ds_lfsts_rciv.getColumn(this.ds_lfsts_rciv.rowposition , "RETURN_ACNUTNO"));				
			vOpts.setSaveFilename(fileName);
			vOpts.setLocalDs(["ds_prt1", this.ds_prt1]);
