/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {		
		case "selectSlaryList": // 신청자정보 불러 오기							
			var sController   	= "hsco/pms/rnt/rnt030408/getList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_lfsts_rciv=output1";
			bSync = false;
			break;									
			
		case "printList1": // 출력물1
			bSync = false;
			this.ds_prt1.clearData();
			this.ds_cond01.clearData();
			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn( nRow , "RCIV_DE1" , this.div_search.Calendar01.value);
			this.ds_cond01.setColumn( nRow , "RCIV_DE2" , this.div_search.Calendar02.value);
			this.ds_cond01.setColumn( nRow , "CNTRCTR_NO" , this.ds_lfsts_rciv.getColumn(this.ds_lfsts_rciv.rowposition , "CNTRCTR_NO"));
			
			var sController   	= "hsco/pms/rnt/rnt030408/print.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_prt1=output1";
			break;
							
		case "saveSlaryList":	// 정보 저장하기
			
			this.ds_lfsts_rciv.setColumn(this.ds_lfsts_rciv.rowposition , "FALT_RETURN_AT" , "1");
			
			var sController   	= "hsco/pms/rnt/rnt030408/listCUD.do";	
			var sInDatasets   	= "input1=ds_lfsts_rciv:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}
 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.

	if(strSvcId == "saveSlaryList")
	{
		if(this.gfn_message("success.처리.성공"));
	    this.fn_transaction("selectSlaryList");	   
	   	this.ds_lfsts_rciv.setLastRowposition(); //저장후에는 마지막 로우에 포커스가 가도록..
	}	

	if(strSvcId == "printList1")
	{
		this.ds_prt2.clearData();
        var nRow = this.ds_prt2.addRow();
		this.ds_prt2.copyRow( nRow , this.ds_lfsts_rciv , this.ds_lfsts_rciv.rowposition);
		var FALT_AMOUNT = this.ds_prt1.getColumn( 0 , "FALT_AMOUNT");
		this.ds_prt1.setColumn( 0 , "FALT_AMOUNT_KR" , "금" +  this.fn_numToHangul(FALT_AMOUNT) + "원정");
		this.ds_prt1.setColumn( 0 , "FALT_AMOUNT" ,  this.comUtils.formatComma(FALT_AMOUNT));
	}		
	
	
	
	if(nErrorCode == 0) return;	
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
}

