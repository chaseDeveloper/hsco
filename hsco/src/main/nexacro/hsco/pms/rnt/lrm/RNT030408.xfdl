	this.ds_cond00.setColumn( nRow , "RCIV_DE1" , this.div_search.Calendar01.value);
	this.ds_cond00.setColumn( nRow , "RCIV_DE2" , this.div_search.Calendar02.value);	
	this.fn_transaction("selectSlaryList");	
}
