    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : RNT030309_P03
 * 화면명   : 소명제출 안내문 팝업
 * 화면설명 : 소명제출 안내문 팝업
 * 작성일   : 2016.06.24
 * 작성자   : 이광원
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";
this.fv_objForm;


this.RNT030309_P03_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	
}

this.RNT030309_P03_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var today = this.dateUtils.today();
	this.div_WORK.edt_zip.set_value('702-073');
	this.div_WORK.edt_telno.set_value("350-0232");
	this.div_WORK.edt_fax.set_value("350-0399");
	this.div_WORK.cal_stDe.set_value(today);
	this.div_WORK.cal_presentnDe.set_value(today);
	this.div_WORK.edt_adres.set_value("화성광역시 북구 침산로 73(고성3가 1-1번지)주거복지처");
	this.div_WORK.edt_waitrNm.set_value("박선영");
	this.div_WORK.edt_centjangNm.set_value("김찬수");
	this.div_WORK.div_inqireDe.setValue(today.substring(0,6));
	
	this.fv_objForm = this.parent.arg_0;
	this.ds_prtM.copyData(this.parent.arg_1);		
	this.ds_prtD.copyData(this.parent.arg_2);		
}

this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}


this.btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var filePath = '';
	if(this.rdo_gubun.value == '1'){
		filePath = "/pms/rnt/lrm/RNT030307_P03R01.jrf";
	}else{
		filePath = "/pms/rnt/pra/RNT030307_P03R02.jrf";
	}
	
	this.ds_damdangjaList.addRow();
	this.ds_damdangjaList.setColumn(0, "TITLE", this.div_WORK.edt_title.value);
	this.ds_damdangjaList.setColumn(0, "DOC_NO", this.div_WORK.edt_docNo.value);
	this.ds_damdangjaList.setColumn(0, "STDE", this.div_WORK.cal_stDe.value);
	this.ds_damdangjaList.setColumn(0, "PAY_TMLMT", this.div_WORK.cal_presentnDe.value);
	this.ds_damdangjaList.setColumn(0, "MGR_NO", this.div_WORK.edt_mgrNo.value);
	this.ds_damdangjaList.setColumn(0, "DOC_NO1", this.div_WORK.edt_docNo1.value);
	this.ds_damdangjaList.setColumn(0, "PAY_TMLMT1", this.div_WORK.cal_presentnDe1.value);
	
	var vOpts = new this.cf_Opts();
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setReportPath(filePath);
	vOpts.setSaveFilename("소명자료 제출 안내문");
	vOpts.setLocalDs(["ds_damdangjaList", this.ds_damdangjaList]);
	vOpts.setLocalDs(["ds_prtM", this.ds_prtM]); 
	vOpts.setLocalDs(["ds_prtD", this.ds_prtD]); 
	
	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		opts : vOpts
	};

	this.gfn_popup("Popup_Report", 884, 735, "소명자료 제출 안내문", parm, "common::frmReportPopup.xfdl", "report_callback");
}

this.rdo_gubun_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	this.div_WORK.edt_docNo1.set_enable(e.postvalue == '1'?false:true);
	this.div_WORK.cal_presentnDe1.set_enable(e.postvalue == '1'?false:true);
}
