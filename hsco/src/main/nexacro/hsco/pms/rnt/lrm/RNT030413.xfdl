/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search ();
 */
this.fn_search = function()
{
	this.ds_cond00.clearData();
	var nRow = this.ds_cond00.addRow();
	this.ds_cond00.setColumn( nRow , "AREA_SE" , this.div_search.cmb_houseCode.value);
	this.ds_cond00.setColumn( nRow , "CANCEL_SE" , this.div_search.rdo_canclSe.value);
	this.ds_cond00.setColumn( nRow , "SPORT_BGNDE1" , this.div_search.Calendar00.value);
	this.ds_cond00.setColumn( nRow , "SPORT_BGNDE2" , this.div_search.Calendar01.value);

	if(this.div_search.rdo_canclSe.value == 1){
		this.fn_transaction("getList");
	} else {
		this.fn_transaction("getList1");
	}
	
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
				
			case "getList":	 //신청자	
				var sController   	= "/hsco/pms/rnt/rnt030413/getList.do"; 
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_lfsts_cntrct=output1";
			break;				
			
			case "getList1": //계약자		
				var sController   	= "/hsco/pms/rnt/rnt030413/getList1.do"; 
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_lfsts_cntrct=output1";
			break;					
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	trace("ds_familyRelate =" + this.ds_familyRelate.saveXML());
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

