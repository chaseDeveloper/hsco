/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	switch (strSvcId) {
		//영구임대 아파트 , 상가		
		case "houseCode3": 
			var sController   	= "hsco/pms/rnt/rnt040401/houseCode3.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_rentHouseCodeList=output1";
			break;
		//수선요청내역 마스터
		case "selectSlaryList": 
			var sController   	= "hsco/pms/rnt/rnt040302/getList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rpairs_requst=output1";	
			break;
		// 수선요청내역
		case "selectSlaryList2": 
			this.ds_cond01.clearData();
			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn(nRow , "REQUST_SN" , this.ds_rpairs_requst.getColumn(this.ds_rpairs_requst.rowposition , "REQUST_SN"));
			
			var sController   	= "hsco/pms/rnt/rnt040302/getListR1.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_gnrl_rpairs=output1";
			break;
		// 수선요청건 수선내역
		case "selectSlaryList3": 	
			this.ds_cond01.clearData();
			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn(nRow , "HOUSE_CODE" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "HOUSE_CODE"));
			this.ds_cond01.setColumn(nRow , "DONG" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "DONG"));
			this.ds_cond01.setColumn(nRow , "HO" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "HO"));
		
			var sController   	= "hsco/pms/rnt/rnt040302/getListR2.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_gnrl_rpairs_detail=output1";
			break;				
		// 수선요청 마스터 저장	
		case "saveSlaryList":	
			var sController   	= "hsco/pms/rnt/rnt040302/listCUD.do";	
			var s  				= "input1=ds_rpairs_requst:U"
			                    + " input2=ds_gnrl_rpairs:U";
			var sInDatasets 	= s;                    
			var sOutDatasets  	= "";
			break;		

		// 승인요청/취소
		case "saveConfmse":	
			var sController   	= "hsco/pms/rnt/rnt040302/saveConfmse.do";	
			var s  				= "input1=ds_message:U";
			var sInDatasets 	= s;                    
			var sOutDatasets  	= "";
			break;					
			
		//	첨부파일조회	
		case "selectSlaryList4": 	
			this.ds_cond02.clearData();
			var nRow = this.ds_cond02.addRow();
			this.ds_cond02.setColumn(nRow , "FILE_SN" , this.ds_rpairs_requst.getColumn(this.ds_rpairs_requst.rowposition , "FILE_SN"));
            var sController   	= "hsco/pms/rnt/rnt030301/ATCH.do";
			//var sController   	= "hsco/pms/rnt/rnt040302/getListR3.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_rpairs_photo=output1";			
			break;				
		// 선택 파일 삭제				
		case "delete" :	
			var sController 	= "hsco/pms/rnt/rnt040302/atchDelete.do";
			var sInDatasets 	= "input1=ds_cond05";
			var sOutDatasets 	= "";
			break;						
		//내부결제 아이디 저장	
		case "rwmnyPymntU": 	
			var sController   	= "hsco/pms/rnt/rnt040302/rwmnyPymntU.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "";
			break;
		//결제자 목록	
		case "sanctnerNmDetail":	
			var sController   	= "hsco/pms/rnt/rnt040302/sanctnerNmDetail.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_sanctnerNmDetail=output1";
			bSync = false;
			break;			
		//업무key 변경	
		case "sanctnUpdate": 
			var sController   	= "hsco/pms/rnt/rnt040302/sanctnUpdate.do";
			var sInDatasets   	= "input1=ds_sanction";
			var sOutDatasets  	= "";
			bSync = false;
			break;			
	}

	if(sController == null) return;
	
	var sInDataType = typeof(sInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, sInDatasets, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, null, null, null, bSync);
	}
}
/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}	
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(strSvcId == "saveSlaryList")
	{
		if(this.gfn_message("success.처리.성공"));
		this.fn_transaction("selectSlaryList");
	}	
	if(strSvcId == "delete")
	{
		this.ds_rpairs_photo.clearData();
		this.fn_transaction("selectSlaryList4");
	}		
	if(strSvcId == "houseCode3")
	{
		this.fn_add_comboHeader(this, this.div_search.Combo00,  "전체");
		this.div_search.Combo00.set_index(0);
	}		
	if(strSvcId == "sanctnUpdate"){
		this.fn_transaction("selectSlaryList2");
	}			
	if(strSvcId == "rwmnyPymntU"){
		this.fn_transaction("selectSlaryList");
	}		
	if(strSvcId == "saveConfmse"){
		this.alert("승인요청 완료");
		this.fn_search();
	}				

}

