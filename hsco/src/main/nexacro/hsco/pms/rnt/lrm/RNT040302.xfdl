	this.ds_sanction.setColumn(nRow, "JOB_KEY"         , key);
	
	this.fn_transaction("sanctnUpdate");
	var result = this.gfn_openInnerSanctionPopup(this.ds_innerSanctnParam, fn_cb, registDt.toString());
