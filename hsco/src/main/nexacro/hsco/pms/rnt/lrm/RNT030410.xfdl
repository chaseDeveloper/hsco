/*
 * 함수명  	: Button03_onclick
 * 설명 		: 연체이자 재계산
 * params  		: 
 * return Type 	: None
 * 사용법		: this.Button03_onclick();
 */
this.Button03_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 체크된 건이 없으면 return
	if(this.ds_mt_rntchrg.findRow("CHK" , 1) == -1){
		return;
	}
	
	// 미납내역이 아니면 return
	if(this.div_search.Radio00.value != 2) return;
	
	if(this.confirm("납입일자를 기준으로 연체이자를 재생성하시겠습니까?")){	
		this.ds_cond03.clearData();
		for(var i = 0 ; i < this.ds_mt_rntchrg.rowcount ; i++){	    
		    if(this.ds_mt_rntchrg.getColumn( i , "CHK") == 1){
				var nRow = this.ds_cond03.addRow();
				this.ds_cond03.setColumn(nRow ,  "IN_AREA_SE" , this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "AREA_SE"));
				this.ds_cond03.setColumn(nRow ,  "IN_CNTRCTR_NO" , this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "CNTRCTR_NO"));
				this.ds_cond03.setColumn(nRow ,  "IN_CREATEDATE" , this.cal_payTmlmt.value);
				this.ds_cond03.setColumn(nRow ,  "IN_PAYENDDATE" , this.ds_mt_rntchrg.getColumn( i , "DEDT_DE"));
			}	
		}
			this.fn_transaction("Delay");
	}
