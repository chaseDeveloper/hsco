	ds.setColumn( nRow , "THNG_CODE" , ds2.getColumn(ds2.rowposition , "THNG_CODE"));
	ds.setColumn( nRow , "THNG_STNDRD" , ds3.getColumn(ds3.rowposition , "STNDRD_CODE"));
			
	this.close();	
}

