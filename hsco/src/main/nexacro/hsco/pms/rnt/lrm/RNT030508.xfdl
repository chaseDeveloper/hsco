 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_set_rentCntrctCstmr = function(objCstmr){
    var nRow = 0;
    
    if(this.ds_anactList.getRowCount() > 0){
       nRow = this.ds_anactList.addRow();
    }else{
       nRow = this.ds_anactList.insertRow(0);  
    }
    
    this.fn_objToDs(objCstmr,this.ds_anactList,nRow);
    
	return true;
}

