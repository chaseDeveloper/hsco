	var DEFAULT_YM = this.dateUtils.format(this.dateUtils.today(),'yyyymm');
	this.div_search.div_monCal.setValue(DEFAULT_YM);
	this.div_search.div_monCal00.setValue(DEFAULT_YM);
	//this.div_search.div_monCal00.fn_setBindValue(this,'','', '', this.DEFAULT_YM);	// 신청월 기본년월 설정	
