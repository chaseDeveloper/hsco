/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectSlaryList": // 계약자 정보 불러 오기	
				var sController   	= "hsco/pms/rnt/RNT030304/getList.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_lfsts_cntrct=output1";
				break;

		case "selectSlaryListR1": // 계약자 정보 불러 오기	
		        this.ds_cond01.clearData();
		        var nRow = this.ds_cond01.addRow();
		        this.ds_cond01.setColumn( nRow , "CNTRCTR_NO" , this.ds_lfsts_cntrct.getColumn( this.ds_lfsts_cntrct.rowposition , "CNTRCTR_NO"));
		
				var sController   	= "hsco/pms/rnt/RNT030304/getListR1.do";
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_guide=output1";
				break;				
				
		case "saveSlaryList": //안내문 발송정보 저장
				var sController   	= "hsco/pms/rnt/RNT030304/listCUD.do";
				var sInDatasets   	= "input1=ds_guide_c";
				var sOutDatasets  	= "";
				break;	
							
		case "damDangJa": // 출력물 담당자 조회
				this.ds_cond99.clearData();
				var nRow = this.ds_cond99.addRow();
				this.ds_cond99.setColumn( nRow , "CODE" , "RN3005");
								
				var sController   	= "hsco/pms/rnt/rnt030201/damDangJa.do";
				var sInDatasets   	= "input1=ds_cond99";
				var sOutDatasets  	= "ds_damdangja=output1";
				break;								

		case "reCntrctr": // 묵시적재계약
				var sController   	= "hsco/pms/rnt/RNT030304/reCntrctr.do";
				var sInDatasets   	= "input1=ds_cond02";
				var sOutDatasets  	= "";		
				break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 	

}
 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(strSvcId == "saveSlaryList")
	{
		this.fn_transaction("selectSlaryList");	
	}	
	if(strSvcId == "reCntrctr")
	{
		this.fn_transaction("selectSlaryList");		
	}
	if(nErrorCode == 0) return;	
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);

}


