/***********************************************************************
 * Transaction 관련 이벤트 핸들러 
 **********************************************************************/
/*
 * 함수명  	: fn_search
 * 설명 		: 신청자 조회 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectSlaryList": // 임대료 			
				var sController   	= "hsco/pms/rnt/rnt030306/P1.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_mt_rntchrg=output1";
				break;
		case "selectSlaryList1": // 지원금
				var sController   	= "hsco/pms/rnt/rnt030306/P2.do";
				var sInDatasets   	= "input2=ds_cond00";
				var sOutDatasets  	= "ds_mt_rntchrg=output2";
				break;
		case "selectSlaryList2": // 임대료 + 지원금 
				var sController   	= "hsco/pms/rnt/rnt030306/P3.do";
				var sInDatasets   	= "input3=ds_cond00";
				var sOutDatasets  	= "ds_mt_rntchrg=output3";
				break;
		case "selectSlaryList3": // 가상계좌
				var sController   	= "hsco/pms/rnt/rnt030306/P4.do";
				var sInDatasets   	= "input4=ds_cond00";
				var sOutDatasets  	= "ds_receipt=output4";
				break;
		case "selectSlaryList4": // 가상계좌 (va_vill)
				var sController   	= "hsco/pms/rnt/rnt030306/P5.do";
				var sInDatasets   	= "input5=ds_cond01";
				var sOutDatasets  	= "ds_bill=output5";
				break;													
		case "selectSlaryList5": // 가상계좌 (va_vill_detail)
				this.ds_cond01.clearData();
				var nRow = this.ds_cond01.addRow();
				this.ds_cond01.setColumn(nRow , "CRT_ID" , this.ds_bill.getColumn(this.ds_bill.rowposition , "CRT_ID"));
				this.ds_cond01.setColumn(nRow , "VACCT_NO" , this.ds_bill.getColumn(this.ds_bill.rowposition , "VACCT_NO"));
				this.ds_cond01.setColumn(nRow , "BANK_CD" , this.ds_bill.getColumn(this.ds_bill.rowposition , "BANK_CD"));
				
				var sController   	= "hsco/pms/rnt/rnt030306/P6.do";
				var sInDatasets   	= "input6=ds_cond01";
				var sOutDatasets  	= "ds_bill_detail=output6";
				break;			
		case "selectSlaryList6": // 프로시저(임대료 , 지원금 납부요청 프로시저)
				var sController   	= "hsco/pms/rnt/rnt030306/P7.do";
				var s  = "input1=ds_mt_rntchrg02:U"
				       + " input2=ds_mt_rntchrg01:U"
				       + " input3=ds_mt_rntchrg03:U";
				var sInDatasets   	=  s;
				var sOutDatasets  	= "";
				break;		
		case "selectSlaryList7": // 은행콤보 
				this.ds_va.clearData();
				this.ds_cond05.clearData();
				var nRow = this.ds_cond05.addRow();
				this.ds_cond05.setColumn( nRow , "OPT1" , this.Radio01.value);
				this.ds_cond05.setColumn( nRow , "CNTRCTR_NO" , this.ds_cond00.getColumn( this.ds_cond00.rowposition , "CNTRCTR_NO"));
				
				var sController   	= "hsco/pms/rnt/rnt030306/P8.do";
				var sInDatasets   	= "input8=ds_cond05";
				var sOutDatasets  	= "ds_va=output8";
				break;					
																										
		case "selectSlaryList8": // 고지서 출력
		
				var sController   	= "hsco/pms/rnt/rnt030306/PRINT3.do";
				var sInDatasets   	= "input1=ds_cond06";
				var sOutDatasets  	= "ds_billPrintList=output1";
				break;																											
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 	

}
 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if(nErrorCode != 0){ 
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
		return;
	}

	// 오류 및 성공 메시지를 화면에 보여준다.
	if(strSvcId == "selectSlaryList" || strSvcId == "selectSlaryList1" || strSvcId == "selectSlaryList2")
	{			
		this.fn_transaction("selectSlaryList3");
		this.fn_transaction("selectSlaryList7");		
	}
	if(strSvcId == "selectSlaryList4")
	{			
		this.fn_transaction("selectSlaryList5");	
	}
	if(strSvcId == "selectSlaryList6")
	{			
		this.Button05_onclick();	
	}	
	if(strSvcId == "selectSlaryList7")
	{
		this.Combo00.set_index(0);
		this.Combo00_onitemchanged(this.Combo00);
	}	

	if(strSvcId == "selectSlaryList8")
	{
		this.fn_billPrint();
	}		
	

}


