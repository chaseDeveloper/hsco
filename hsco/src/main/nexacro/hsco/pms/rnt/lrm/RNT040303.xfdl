this.fn_deleteAtchFile = function(){
	this.ds_cond05.clearData();
	var confirmMsg = "선택한 이미지를 삭제하시겠습니까?";
	var checkListLeng = 0;
	if(confirm(confirmMsg)){
	
			// 선택자료 가져오기
			for(var i = 0 ; i < this.ds_rpairs_photo.rowcount ; i++){
				if(this.ds_rpairs_photo.getColumn( i , "CHK") == '1'){
				    var nRow = this.ds_cond05.addRow();
					this.ds_cond05.setColumn( nRow , "RCEPT_SN" , this.ds_gnrl_opertn.getColumn(this.ds_gnrl_opertn.rowposition , "RCEPT_SN"));
					this.ds_cond05.setColumn( nRow , "FILE_SN" , this.ds_rpairs_photo.getColumn( i , "FILE_SN"));					
					this.ds_cond05.setColumn( nRow , "FILE_ORDR" , this.ds_rpairs_photo.getColumn( i  , "FILE_ORDR"));
					checkListLeng += 1;
																				
				}
			}
			
			// 선택자료 여부 체크
			if ( checkListLeng == 0 ) {
				this.alert("선택된 자료가없습니다.");
				return;
			}

				this.fn_transaction("delete");		// 첨부파일 삭제;
	}
}

