 */
this.fn_insert = function()
{
	var nRow = this.ds_estate_code.addRow();
	
    if(this.Tab00.tabindex == 0)
    {
		this.ds_estate_code.setColumn(nRow , "MDIATN_JCFFR_SE" , "001");
