	if(e.cell == obj.getBindCellIndex("body", "FILE_NM"))
	{
		if(!this.ds_thng_purchs_d.getColumn(e.row, 'FILE_SN'))
			return;

		if(this.confirm("선택한 파일을 다운로드 하시겠습니까?")){
			this.fn_downloadAtchFile2();
			}
			return;
	}
