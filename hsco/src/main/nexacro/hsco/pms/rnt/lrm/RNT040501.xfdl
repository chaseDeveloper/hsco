/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	switch (strSvcId) {
		case "houseCode3": //영구임대 아파트 , 상가		
			var sController   	= "hsco/pms/rnt/rnt040401/houseCode3.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_rentHouseCodeList=output1";
			break;	
			
		case "thngCode": //물품코드
			var sController   	= "hsco/pms/rnt/rnt040102/getList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_thng_code=output1";
			break;	
						
		case "selectSlaryList": // 물품 청구,요청
			var sController   	= "hsco/pms/rnt/rnt040501/getList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_thng_purchs_m=output1";
			//bSync = false;
			break;
			
		case "selectSlaryList2": 	// 구입물건내역	
			this.ds_cond01.clearData();
			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn(nRow , "RCEPT_SN" , this.ds_thng_purchs_m.getColumn( this.ds_thng_purchs_m.rowposition , "RCEPT_SN"));
				
			var sController   	= "hsco/pms/rnt/rnt040501/getListR1.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_thng_purchs_d=output1";
			break;
			
		case "selectSlaryList3": 	// 첨부조회
			this.ds_cond02.clearData();
			var nRow = this.ds_cond02.addRow();
			this.ds_cond02.setColumn(nRow , "FILE_SN" , this.ds_thng_purchs_m.getColumn(this.ds_thng_purchs_m.rowposition , "FILE_SN"));
	
			var sController   	= "hsco/pms/rnt/rnt040102/getList2.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_rpairs_photo=output1";			
			break;	
			
		case "selectSlaryList4": // 전체 규격코드 조회
			this.ds_cond01.clearData();
			this.ds_thng_stndrd.clearData();
			var nRow = this.ds_cond01.addRow();
			
			var sController   	= "hsco/pms/rnt/rnt040102/stndrdlist.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_thng_stndrd=output1";
			break;				
					
		case "saveSlaryList":	// 청구,요청 마스터  CRUD
			var sController   	= "hsco/pms/rnt/rnt040501/listCUD.do";	
			var s  				= "input1=ds_thng_purchs_m:U"
			                    + " input2=ds_thng_purchs_d:U";			
			var sInDatasets 	= s;                    
			var sOutDatasets  	= "";
			break;		
			
		case "delete" :	// 선택 파일 삭제			
			var sController = "hsco/pms/rnt/rnt040501/atchDelete.do";
			var sInDatasets = "input1=ds_cond05";
			var sOutDatasets = "";
			break;			
						
		case "saveConfmse":	// 승인요청/취소
			var sController   	= "hsco/pms/rnt/rnt040501/saveConfmse.do";	
			var sInDatasets 	= "input1=ds_message:U";
			var sOutDatasets  	= "";
			break;							
		
	}

	if(sController == null) return;
	
	var sInDataType = typeof(sInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, sInDatasets, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, null, null, null, bSync);
	}
}
/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}	

	// 오류 및 성공 메시지를 화면에 보여준다.
	if(strSvcId == "saveSlaryList")
	{
		if(this.gfn_message("success.처리.성공"));
		this.fn_transaction("selectSlaryList2");
	}		
	if(strSvcId == "delete")
	{
		this.ds_rpairs_photo.clearData();
		this.fn_transaction("selectSlaryList2");
	}		
	if(strSvcId == "houseCode3")
	{
		this.div_search.Combo00.set_index(0);
	}				
	if(strSvcId == "rwmnyPymntU"){
		this.fn_transaction("selectSlaryList2");
	}	
	if(strSvcId == "saveConfmse"){
		this.alert("승인요청 완료");
		this.fn_search();
	}							
}

