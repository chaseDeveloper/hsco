	this.ds_approve.setColumn(0, "RCEPT_SN",   ds.getColumn( ds.rowposition , "RCEPT_SN"));
	
	if(len <= 2000){ // 2천바이트 이하
	
		if(ds.getColumn(ds.rowposition , "RCEPT_SE") == 1){		
			var sController   	= "hsco/pms/rnt/rnt040502/approveU.do";	
		} else {		
