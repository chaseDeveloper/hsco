	// 요청
	} else {
	
		this.fn_transaction("prt2");	
		
		var filds = new Array(112);
		var num = 0;
		var columnNm = new Array(7);
		columnNm[0] = "THNG_NM";
		columnNm[1] = "THNG_STNDRD";
		columnNm[2] = "THNG_UNIT";
		columnNm[3] = "THNG_QY";
		columnNm[4] = "UNTPC";
		columnNm[5] = "AMT";
		columnNm[6] = "THNG_PRPOS";
		
		for(var i = 0 ; i < 15 ; i++){
			for(var x = 0 ; x < 7 ; x++){
				filds[num] = ds2.getColumn( i , columnNm[x]);
