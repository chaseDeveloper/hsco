	var dataValue = filds.join("|");
	var dataTitle = ds.getColumn(ds.rowposition , "RCEPT_SJ");
	var title     = ds.getColumn(ds.rowposition , "RCEPT_SJ");
	
	var len = 0;
	for(var i = 0 ; i < dataValue.length ; i++){
		
		if(dataValue.charCodeAt(i) > 127){
			len += 2;
		} else {
			len += 1;
		}
	}
	
	// 2천바이트 초과시 물품내역 초기화
	if(len > 2000){
		var dataValue = null;	
	}
	
	
	this.ds_approve.clearData();	
