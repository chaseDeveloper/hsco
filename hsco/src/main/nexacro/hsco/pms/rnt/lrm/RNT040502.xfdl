				if(x == 4 || x == 5 ){
					if(this.comUtils.isNull(ds2.getColumn( i , columnNm[x])) == false){
						filds[num] = this.comUtils.formatComma(ds2.getColumn( i , columnNm[x]));
					}	
				}				
 			    num += 1;
 			    
 			    if(num == 105) continue;
			}	
