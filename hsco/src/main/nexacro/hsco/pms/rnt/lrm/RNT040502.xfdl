		this.ds_thng_purchs_m.setColumn(currow, "RPRSNTV", dsTemp.getColumn(0, "RPRSNTV"));       // 대표자명
	}
		
	if(strID == "btn_empPopup"){
		var currow = this.ds_thng_purchs_m.rowposition;
		var dsTemp = new Dataset();
		dsTemp.loadXML(variant);		
		this.ds_thng_purchs_m.setColumn(currow, "CHRG_EMPNO", dsTemp.getColumn(0, "EMPNO"));		// 사번		
		this.ds_thng_purchs_m.setColumn(currow, "CHRG_EMPNM", dsTemp.getColumn(0, "EMPNM"));		// 사원명
	}
	
	if(strID == "mnthngCptalList") {
		var dsRtn = new Dataset();
		nRowCnt = dsRtn.loadXML(variant);	
		var bsnsSe        = dsRtn.getColumn(0, "BSNS_SE");
		var balcSe        = dsRtn.getColumn(0, "BALC_SE");
		var detailBsnsSe  = dsRtn.getColumn(0, "DETAIL_BSNS_CODE");
		var deptCode  	  = dsRtn.getColumn(0, "DEPT_CODE");
		var deptNm  	  = dsRtn.getColumn(0, "DEPT_NM");
		var acntCode      = dsRtn.getColumn(0, "BUDGET_ACNT_CODE");
		var acntNm        = dsRtn.getColumn(0, "BUDGET_ACNT_NM");
		var expndtrUnitCd = dsRtn.getColumn(0, "EXPNDTR_UNIT_CODE");
		var expndtrUnitNm = dsRtn.getColumn(0, "EXPNDTR_UNIT_NM");		
		var year = dsRtn.getColumn(0, "YEAR");		
