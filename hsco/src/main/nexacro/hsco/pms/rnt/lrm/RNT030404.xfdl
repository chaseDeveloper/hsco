 */
this.fn_setSelFileInfoTitle = function(fileName, fileCnt){
	var baseText = "참고 : 은행->공사";
	var selFileName = "";
	if(fileName){
		selFileName = baseText + " | 선택파일";
		if(fileCnt){
			selFileName += " (" + fileCnt + "건)";
		}
