/*탭 변경 시 이벤트*/
this.Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{

	//최초 화면 불러올 때, 수선내역의 행 수가 하나라도 있으면 계획 및 실행 조회 이벤트 실행
	if(!this.ds_gnrl_rpairs.rowcount == 0) {
		var tabid = this.Tab00.tabpages[this.Tab00.tabindex].name;
		switch(tabid)
		{
			case 'tabpage1':		// 장기수선계획
				this.fn_transaction("selectPlan");
				break;
			case 'tabpage2':		// 장기수선실행
				var today = this.dateUtils.today();		//시작일
				this.Tab00.tabpage2.cal_rpairs_de.set_value(today);
		
				this.fn_transaction("selectExecut");
				break;
		}
	}
}


/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리 (장비관리)
 * params  		: sPopupId : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 작성자 		: 김선진
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	//행추가 팝업창
	if(sPopupId == "Popup"){
		
		if(this.Tab00.tabindex == 0){
			var ds = this.ds_plan;
		} else {
			var ds = this.ds_execut;
		}
		var dsTemp = new Dataset();		
		var loadCnt = dsTemp.loadXML(sReturn);
		
		for(var i = 0 ; i < loadCnt ; i++){
			var nRow = ds.addRow();		
			ds.setColumn( nRow , "HOUSE_CODE" , dsTemp.getColumn( i , "RENT_HOUSE_CODE"));
			ds.setColumn( nRow , "DONG" , dsTemp.getColumn( i , "DONG"));
			ds.setColumn( nRow , "HO" , dsTemp.getColumn( i , "HO"));
			ds.setColumn( nRow , "PLAN_SN" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "PLAN_SN"));
		}
	} 
	else if(sPopupId == "btn_bcncPopup"){ //시공업체 팝업창
		var dsTemp1 = new Dataset();
		dsTemp1.loadXML(sReturn);
		this.Tab00.tabpage2.ed_bcncNm.set_value(dsTemp1.getColumn(0, "BCNC_NM"));		// 거래처명(시공업체) 
		this.bcnc_code = dsTemp1.getColumn(0, "BCNC_CODE");		// 거래처명(시공업체) 		
		
	} else if(sPopupId == "btn_bcncPopup1"){ // 그리드 시공업체 팝업창
		var nRow = this.ds_execut.rowposition;
		var dsTemp1 = new Dataset();
		dsTemp1.loadXML(sReturn);
		this.ds_execut.setColumn(nRow, "BCNC_NM", dsTemp1.getColumn(0, "BCNC_NM"));		// 거래처명(시공업체)
		this.ds_execut.setColumn(nRow, "BCNC_CODE", dsTemp1.getColumn(0, "BCNC_CODE"));	// 거래처코드(시공업체)		
	}		
}

