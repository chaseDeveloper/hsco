/***********************************************************************
 * 설명: 필수입력사항 체크
 ************************************************************************/
this.fn_check= function()
{
	var nRow = this.ds_gnrl_rpairs.rowposition;
	
	if(nRow > -1)   // 데이터셋의 크기가 1보다 클 때
	{
		ck1 = this.ds_gnrl_rpairs.getColumn(nRow, "RPAIRS_SE"); 	// 수선구분
		ck2 = this.ds_gnrl_rpairs.getColumn(nRow, "PLAN_YEAR");		// 수선연도
		ck3 = this.ds_gnrl_rpairs.getColumn(nRow, "HOUSE_CODE");	// 주택코드
		ck4 = this.ds_gnrl_rpairs.getColumn(nRow, "CNTRWK_SE");		// 공종구분
		ck5 = this.ds_gnrl_rpairs.getColumn(nRow, "CNTRWK_KND");	// 공정구분
		
		
		if(!ck1) {
			alert("수선구분은 필수입력 사항입니다.");
			return false;
		}else if(!ck2) {
			alert("수선연도는 필수입력 사항입니다.");
			return false;
		}else if(!ck3) {
			alert("주택명은 필수입력 사항입니다.");
			return false;
		}else if(!ck4) {
			alert("공종구분은 필수입력 사항입니다.");
			return false;
		}else if(!ck5) {
			alert("공정구분 필수입력 사항입니다.");
			return false;
		}else {
			return true;
		}
		
		return true;
		
	} else {
		return true;
	}
}




/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/ 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	switch (strSvcId) {
		case "houseCode3": //영구임대 아파트 , 상가		
			var sController   	= "hsco/pms/rnt/rnt040401/houseCode3.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_house_codeList=output1";
			
			trace("ds_house_codeList = " + this.ds_cond.saveXML());
			break;										
			
		case "houseCode3_1": //영구임대 아파트 , 상가 - Insert 클릭 시 		
			this.ds_house_codeList.clearData();
			var sController   	= "hsco/pms/rnt/rnt040401/houseCode3.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_house_codeList=output1";
			break;	
			
		case "selectRequest": //일반수선결과
			this.ds_cond00.clearData();
			var nRow = this.ds_cond00.addRow();
			this.ds_cond00.setColumn(nRow, "CODE" , this.div_search.Combo01.value);
			this.ds_cond00.setColumn(nRow, "PLAN_YEAR", this.div_search.spn_year.value);
			this.ds_cond00.setColumn(nRow, "PLAN_YEAR2", this.div_search.spn_year1.value);
			this.ds_cond00.setColumn(nRow, "RPAIRS_SE", this.div_search.rdo_rpairs00.value);
			
			//trace("ds_cond00 = " + this.ds_cond00.saveXML());
			
			var sController   	= "hsco/pms/rnt/rnt040308/selectRequest.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_gnrl_rpairs=output1";
			break;		
			
		case "selectSlaryList": // 공종코드 마스터 정보 불러 오기
			var sController   	= "hsco/pms/rnt/rnt040101/getList.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_cmmn_m=output1";
			bSync = false;
			break;	
			
		case "selectSlaryList1": // 공종코드 디테일 정보 불러 오기
			this.ds_cond01.clearData();
			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn(nRow , "VAL0" , this.Combo01.value);
			
			var sController   	= "hsco/pms/rnt/rnt040101/getList1.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_cmmn_d=output1";
			break;
			
		case "saveRequest":	// 장기수선관리 요청내역 저장하기
				var sController   	= "hsco/pms/rnt/rnt040101/requestCUD.do";	
				var s  = "input1=ds_gnrl_rpairs:U"
					   + " input2=ds_plan:U"
					   + " input3=ds_execut:U";
				var sInDatasets   	= s;
				var sOutDatasets  	= "";
				break;
				
		case "selectPlan": // 장기수선계획 조회
			this.ds_cond02.clearData();
			var nRow = this.ds_cond02.addRow();
			this.ds_cond02.setColumn(nRow , "PLAN_SN" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "PLAN_SN"));
			
 			var sController   	= "hsco/pms/rnt/rnt040308/selectPlan.do";
 			var sInDatasets   	= "input1=ds_cond02";
 			var sOutDatasets  	= "ds_plan=output1";
 			
 			this.fn_transaction("selectExecut");	
			break;		
				
		case "selectExecut": // 장기수선실행 조회
			this.ds_cond02.clearData();
			var nRow = this.ds_cond02.addRow();
			this.ds_cond02.setColumn(nRow , "PLAN_SN" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "PLAN_SN"));
			
 			var sController   	= "hsco/pms/rnt/rnt040308/selectExecut.do";
 			var sInDatasets   	= "input1=ds_cond02";
 			var sOutDatasets  	= "ds_execut=output1";
 			
			break;
				
		case "copyPlan": // 장기수선계획 조회
			this.ds_cond02.clearData();
			var nRow = this.ds_cond02.addRow();
			this.ds_cond02.setColumn(nRow , "PLAN_SN" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "PLAN_SN"));
			
 			var sController   	= "hsco/pms/rnt/rnt040308/copyPlan.do";
 			var sInDatasets   	= "input1=ds_cond02";
 			var sOutDatasets  	= "";
			break;		
	}

	if(sController == null) return;
	
	var sInDataType = typeof(sInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, sInDatasets, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, null, null, null, bSync);
	}
}
/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(strSvcId == "saveRequest"){
		this.gfn_message("success.처리.성공");
		//this.fn_transaction("selectRequest");	
	}
	
	if(strSvcId == "houseCode3_1"){
		this.Combo00.set_index(this.div_search.Combo01.index);
	}

// 	if(strSvcId == "selectSlaryList"){
// 		this.fn_transaction("selectSlaryList1");	
// 	}
	
	if(strSvcId == "houseCode3"){	
		this.fn_add_comboHeader(this, this.div_search.Combo01, "전체");
	}

  	if(strSvcId == "copyPlan") {
  		this.fn_transaction("selectExecut");
  	}

	if(nErrorCode == 0) return;	
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
}

