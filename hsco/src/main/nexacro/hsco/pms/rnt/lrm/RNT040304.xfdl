/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	switch (strSvcId) {
	
		case "houseCode3": //영구임대 아파트 , 상가		
			var sController   	= "hsco/pms/rnt/rnt040401/houseCode3.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_rentHouseCodeList=output1";
			break;	
			
		case "selectSlaryList": 
			var sController   	= "hsco/pms/rnt/rnt040304/getList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rp_ctr_opertn=output1";
			//bSync = false;
			break;
			
		case "selectSlaryList2": 	// 수선내역			
			var sController   	= "hsco/pms/rnt/rnt040304/getList1.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_gnrl_rpairs=output1";
			break;
			
		case "selectSlaryList3": 	// 담당자조회
			var sController   	= "hsco/pms/rnt/rnt040304/getList2.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_opertn_chrg=output1";
			break;			
								
		case "saveSlaryList":	
			var sController   	= "hsco/pms/rnt/rnt040304/listCUD.do";	
			var s  				= "input1=ds_rp_ctr_opertn:U"
			                    + " input2=ds_gnrl_rpairs:U"
			                    + " input3=ds_opertn_chrg:U";
			var sInDatasets 	= s;                    
			var sOutDatasets  	= "";
			break;					
			
		case "rwmnyPymntU": 	//내부결제 아이디 저장
			var sController   	= "hsco/pms/rnt/rnt040304/rwmnyPymntU.do";
			var sInDatasets   	= "input1=ds_cond06";
			var sOutDatasets  	= "";
			break;
			
		case "sanctnerNmDetail":	//결제자 목록
			var sController   	= "hsco/pms/rnt/rnt040301/sanctnerNmDetail.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_sanctnerNmDetail=output1";
			bSync = false;
			break;			
			
		case "sanctnUpdate": //업무key 변경
			var sController   	= "hsco/pms/rnt/rnt040304/sanctnUpdate.do";
			var sInDatasets   	= "input1=ds_sanction";
			var sOutDatasets  	= "";
			bSync = false;
			break;			
			
		case "sancKnd": // 전자결재 진행상태
				bSync = false;
				this.ds_cond07.clearData();
				var nRow = this.ds_cond07.addRow();
				this.ds_cond07.setColumn( nRow , "SANCTN_NO" , this.ds_gnrl_rpairs.getColumn(this.ds_gnrl_rpairs.rowposition , "ELCTRN_SANCTN_NO"));
			
				var sController   	= "hsco/pms/rnt/rnt030301/sancKnd.do";
				var sInDatasets   	= "input1=ds_cond07";
				var sOutDatasets  	= "ds_sancKnd=output1";
				break;			
	}

	if(sController == null) return;
	
	var sInDataType = typeof(sInDatasets);
	if(sInDataType == "object"){
		this.pmsUtil.pmsComTran(strSvcId, sController, sInDatasets, sOutDatasets, null, null, false);
	}else{
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, null, null, null, bSync);
	}
}
/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	if(nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}	

	// 오류 및 성공 메시지를 화면에 보여준다.
	if(strSvcId == "saveSlaryList")
	{
		if(this.gfn_message("success.처리.성공"));
		this.fn_transaction("selectSlaryList2");
		this.fn_transaction("selectSlaryList3");
		//this.ds_rent_cntrct.setLastRowposition(); //저장후에는 마지막 로우에 포커스가 가도록..
	}	
	if(strSvcId == "delete")
	{
		this.fn_transaction("selectSlaryList2");
		this.fn_transaction("selectSlaryList3");		
	}		
	if(strSvcId == "houseCode3")
	{
		this.div_search.Combo00.set_index(0);
	}		
	if(strSvcId == "approve"){
		var brow = this.ds_approve.getColumn(0,"O_URL");
		system.execBrowser(brow); 
		this.fn_transaction("selectSlaryList");
	}		
	if(strSvcId == "sanctnUpdate"){
		this.fn_transaction("selectSlaryList");
	}			
	if(strSvcId == "rwmnyPymntU"){
		this.fn_transaction("selectSlaryList");
	}			
		
	
}

