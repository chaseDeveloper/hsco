/* 출력물 인쇄 */
this.PRT = function(){
		this.Div01.set_visible(false);
		var ds = this.ds_opertn_chrg;
		this.ds_prt3.clearData(); // 공사감독자(착공)
		this.ds_prt4.clearData(); // 공사감독자(기성,준공)
		this.ds_prt5.clearData(); // 검사자
		this.ds_prt7.clearData(); // 본문
		
		this.ds_prt7.addRow();
		this.ds_prt7.copyRow( 0 , this.ds_rp_ctr_opertn , ds.rowposition);

		
		for(var i = 0 ; i < ds.rowcount ; i++){
			if(ds.getColumn( i , "CHRG_SE") == "01"){
				var nRow1 = this.ds_prt3.addRow();
				this.ds_prt3.copyRow( nRow1 , ds , i );
				trace("ds_prt3 =" + this.ds_prt3.saveXML());
			} else if(ds.getColumn( i , "CHRG_SE") == "02") {
				var nRow2 = this.ds_prt4.addRow();
				this.ds_prt4.copyRow( nRow2 , ds , i );		
				trace("ds_prt4 =" + this.ds_prt4.saveXML());
			} else if(ds.getColumn( i , "CHRG_SE") == "03") {
				var nRow3 = this.ds_prt5.addRow();
				this.ds_prt5.copyRow( nRow3 , ds , i );				
				trace("ds_prt5 =" + this.ds_prt5.saveXML());
			}  else {
				var nRow4 = this.ds_prt6.addRow();
				this.ds_prt6.copyRow( nRow4 , ds , i );
				trace("ds_prt6 =" + this.ds_prt6.saveXML());
			}
		}
		
		var vOpts = new this.cf_Opts();
		vOpts.setToolbar(false);
		vOpts.setScale(100);
		if(this.Div01.Radio00.value == 1){
			vOpts.setReportPath("/pms/rnt/lrm/RNT040304_1.jrf");
		} else {
			vOpts.setReportPath("/pms/rnt/lrm/RNT040304_2.jrf");
		}
		vOpts.setSaveFilename("결재 붙임문서");
		
		vOpts.setLocalDs(["ds_prt3", this.ds_prt3]);
		vOpts.setLocalDs(["ds_prt4", this.ds_prt4]);
		vOpts.setLocalDs(["ds_prt5", this.ds_prt5]);
		vOpts.setLocalDs(["ds_prt7", this.ds_prt7]);		

		var parm = {
			arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
			opts : vOpts
		};
		this.gfn_popup("Popup_Report", 884, 735, "결재 붙임문서", parm, "common::frmReportPopup.xfdl" , "report_callback");

}

/*
 * 함수명  	: fn_sanction
 * 설명 		: 결제 버튼 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_sanction = function(){
	
	var ds = this.ds_rp_ctr_opertn;
	this.ds_cond04.clearData();
	this.ds_prt.clearData();
	this.ds_prt2.clearData();
	var sttus = ds.getColumn( ds.rowposition , "LAST_SANCTN_STTUS");
	var nRow = this.ds_cond04.addRow();
	this.ds_cond04.setColumn( nRow , "PROCESS_SN" , ds.getColumn(ds.rowposition , "RCEPT_SN"));	
	
	switch(sttus){
	
	   case "S00":  // 대기
		    alert("내부결재가 대기중 입니다.");
			var param = {
				arg_0 : this,  
				arg_1 : ds.getColumn(ds.rowposition, "INNER_SANCTN_NO")
			}
			this.gfn_popup("SYM101000_P20", 1100, 730, "결재정보", param, "com_sym_ism::SYM101000_P20.xfdl");		    
		    return;
			break;
			
	   case "S01": // 완료
			//this.fn_sanction2();
			this.Div00.set_visible(true);
			return;
			break;			
				   
	   case "S02": // 결재중
			alert("내부결재가 결재중 입니다.");
			var param = {
				arg_0 : this,  
				arg_1 : ds.getColumn(ds.rowposition, "INNER_SANCTN_NO")
			}
			this.gfn_popup("SYM101000_P20", 1100, 730, "결재정보", param, "com_sym_ism::SYM101000_P20.xfdl");		    			
			return;
			break;
			
	   case "S08": // 취소
			alert("내부결재가 취소된 건 입니다.");
			return;
			break;			

	   case "S09": // 반송
			alert("내부결재가 반송된 건 입니다.");
			return;
			break;						
			
		default :
			break;						
	   }

	var sanctnId = "";
	var strSvcId = "innerSanctnUbiDataset";
	var reportTitle  = "작 업 지 시 서";
	var reportUri    = "/pms/rnt/lrm/RNT040304.jrf";
	var saveFileName = "작 업 지 시 서";	
	var sController   	= "/hsco/pms/rnt/rnt040304/prt.do";
	var sInDatasets   	= "input1=ds_cond04"; 
	var sOutDatasets  	= "ds_prt=output1"
	                    + " ds_prt2=output2";
	var registDt = ds.getColumn( ds.rowposition , "REGIST_DT");
	
	this.gfn_makeReportTranInfo(sanctnId, strSvcId, sController, sInDatasets, sOutDatasets, reportTitle, reportUri, saveFileName);
	
	//내부결재요청
	//업무키 조합한다.
	var key = "9" + "^" 
			+ ds.getColumn(ds.rowposition, "RCEPT_DE") + "^" 
			+ ds.getColumn(ds.rowposition, "RCEPT_SN");

	this.ds_innerSanctnParam.clearData();
	this.ds_innerSanctnParam.addRow();
	this.ds_innerSanctnParam.setColumn(0, "INNER_SANCTN_MDL_ID", "RNT-100019");	/*내부결재모형ID*/
	this.ds_innerSanctnParam.setColumn(0, "SYS_SE_CODE", "PMS");				/*시스템구분 COM024*/
	this.ds_innerSanctnParam.setColumn(0, "JOB_SE", "RNT");						/*업무구분 COM025*/
	this.ds_innerSanctnParam.setColumn(0, "JOB_KEY", key);						/*시스템 업무별 UNIQUE KEY*/
	
	
	var fn_cb = function(sPopupId, sReturn){
		var sanctnResult = sReturn.split("^"); /*index 0: 응답코드, index 1 : 내부결재ID*/
		if("0000"==sanctnResult[0]){
			this.ds_sanction.clearData();
			var nRow = this.ds_sanction.addRow();
			this.ds_sanction.setColumn(nRow, "RCEPT_SN"  , ds.getColumn(ds.rowposition , "RCEPT_SN"));

			this.ds_sanction.setColumn(nRow, "INNER_SANCTN_ID" , sanctnResult[1]);
			this.ds_sanction.setColumn(nRow, "JOB_KEY"         , key);
			
			this.fn_transaction("sanctnUpdate");
			this.debug("INNER_SANCTN_ID : " + sanctnResult[1]);
		}
		else {
			this.ds_sanction.clearData();
			var nRow = this.ds_sanction.addRow();
			this.ds_sanction.setColumn(nRow, "RCEPT_SN"  , ds.getColumn(ds.rowposition , "RCEPT_SN"));
			
			this.ds_sanction.setColumn(nRow, "INNER_SANCTN_ID" , "");
			this.ds_sanction.setColumn(nRow, "JOB_KEY"         , "");
			
			this.fn_transaction("sanctnUpdate");
		}
		
		this.debug("sdd : " + sanctnResult);
	}
	
	this.ds_sanction.clearData();
	var nRow = this.ds_sanction.addRow();
	this.ds_sanction.setColumn(nRow, "RCEPT_SN"  , ds.getColumn(ds.rowposition , "RCEPT_SN"));
			
	this.ds_sanction.setColumn(nRow, "INNER_SANCTN_NO" , "");
	this.ds_sanction.setColumn(nRow, "JOB_KEY"         , key);
	
	this.fn_transaction("sanctnUpdate");
	var result = this.gfn_openInnerSanctionPopup(this.ds_innerSanctnParam, fn_cb, registDt.toString());
}

