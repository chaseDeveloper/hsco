	         + ds2.getColumn(ds2.findRow("CHRG_SE" , "03") , "CHRG_HR_CLSF_NM") + " "
	         + ds2.getColumn(ds2.findRow("CHRG_SE" , "03") , "CHRG_EMPNM");    //COMMENT7
	filds[7] = this.dateUtils.format(ds2.getColumn(ds2.findRow("CHRG_SE" , "03") , "CHRG_CNFIRM_DE"), "yyyy. mm. dd");    //COMMENT8
	filds[8] = ds2.getColumn(ds2.findRow("CHRG_SE" , "03") , "CHRG_EMPNM")+ ')';    //COMMENT9
	filds[9] = ds2.getColumn(ds2.findRow("CHRG_SE" , "04") , "CHRG_DEPT_NM") + " "
	         + ds2.getColumn(ds2.findRow("CHRG_SE" , "04") , "CHRG_JSSFC_NM") + " "
	         + ds2.getColumn(ds2.findRow("CHRG_SE" , "04") , "CHRG_HR_CLSF_NM") + " "
	         + ds2.getColumn(ds2.findRow("CHRG_SE" , "04") , "CHRG_EMPNM");    //COMMENT10	
	filds[10] = this.dateUtils.format(ds2.getColumn(ds2.findRow("CHRG_SE" , "04") , "CHRG_CNFIRM_DE"), "yyyy. mm. dd") + '    '  + ds2.getColumn(ds2.findRow("CHRG_SE" , "04") , "CHRG_EMPNM") + ')';    //COMMENT11
	filds[11] = result;   //COMMENT12
		
		
		
	var dataValue = filds.join("|");
	var dataTitle = ds.getColumn(ds.rowposition , "RCEPT_SJ");
	var title     = ds.getColumn(ds.rowposition , "RCEPT_SJ");
	
