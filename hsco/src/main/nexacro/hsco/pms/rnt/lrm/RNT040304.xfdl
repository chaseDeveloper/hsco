	this.ds_approve.setColumn(0, "RCEPT_SN", ds.getColumn( ds.rowposition , "RCEPT_SN"));
	
	
	var sSvcID        	= "approve";                    
	var sController   	= "hsco/pms/rnt/rnt040304/approveU2.do";	
	var sInDatasets   	= "input1=ds_approve";
	var sOutDatasets  	= "ds_approve=output1";
