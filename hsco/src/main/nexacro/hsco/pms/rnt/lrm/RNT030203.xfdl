		case "savePrt":	// 출력물 정보 저장하기
			var sController   	= "hsco/pms/rnt/rnt030201/listCUD2.do";	
			var sInDatasets   	= "input3=ds_infoRpt:U";
			var sOutDatasets  	= "";
			break;																													
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

