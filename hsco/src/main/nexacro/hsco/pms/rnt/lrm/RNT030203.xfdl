			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn( nRow , "APPLCNT_CNTRCTR_NO" , this.ds_applcnt.getColumn(this.ds_applcnt.rowposition , "APPLCNT_NO"));
			
