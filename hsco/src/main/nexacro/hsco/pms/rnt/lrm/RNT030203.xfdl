 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	if(nErrorCode == 0) return;	
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "damDangJa": // 출력물 담당자 조회
		    this.ds_cond99.clearData();
			var nRow = this.ds_cond99.addRow();
			this.ds_cond99.setColumn( nRow , "CODE" , "RN3005");
							
			var sController   	= "hsco/pms/rnt/rnt030201/damDangJa.do";
			var sInDatasets   	= "input1=ds_cond99";
			var sOutDatasets  	= "ds_damdangja=output1";
			break;	
	
		case "selectSlaryList": // 신청자정보 불러 오기
		    this.ds_cond00.clearData();
			var nRow = this.ds_cond00.addRow();
			this.ds_cond00.setColumn( nRow , "AREA_SE" , this.div_search.Combo00.value);
			this.ds_cond00.setColumn( nRow , "RCEPT_DE1" , this.div_search.div_monCal.getValue("YYYYMM"));
			this.ds_cond00.setColumn( nRow , "RCEPT_DE2" , this.div_search.div_monCal00.getValue("YYYYMM"));
			this.ds_cond00.setColumn( nRow , "NM" , this.div_search.MaskEdit00.value);
			this.ds_cond00.setColumn( nRow , "CNTRCT_STTUS" , this.div_search.Combo01.value);
				
			var sController   	= "hsco/pms/rnt/rnt030201/getList.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_applcnt=output1";
			break;
			
		case "selectSlaryList1": // 세대원정보 불러오기
			this.ds_cond01.clearData();	
			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn( nRow , "APPLCNT_CNTRCTR_NO" , this.ds_applcnt.getColumn(this.ds_applcnt.rowposition , "APPLCNT_NO"));
			this.ds_cond01.setColumn( nRow , "APPLCNT_CNTRCTR_SE" , "001");
								
			var sController   	= "hsco/pms/rnt/rnt030201/getListR1.do";
			var sInDatasets   	= "input2=ds_cond01";
			var sOutDatasets  	= "ds_family_matter=output2";
			break;
			
		case "selectSlaryList2": // 발송내역 불러오기
			this.ds_cond01.clearData();	
			var nRow = this.ds_cond01.addRow();
			this.ds_cond01.setColumn( nRow , "APPLCNT_NO" , this.ds_applcnt.getColumn(this.ds_applcnt.rowposition , "APPLCNT_NO"));
			
