this.Div01_Button01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_prt1.clearData();
	var ds = this.ds_mt_rntchrg_m;
	
	for(var i = 0 ; i < ds.rowcount ; i++){
		if(ds.getColumn( i , "CHK") == "1"){

			var Tot = parseInt(ds.getColumn( i , "RNTCHRG_NPYM")) + parseInt(ds.getColumn( i , "OVDINTR_NPYM")) + parseInt(ds.getColumn( i , "RSVMNEY_NPYM"));
			var nRow = this.ds_prt1.addRow(); 
			this.ds_prt1.setColumn( nRow , "TITLE" , this.Div01.rdo_paymthd.text);
			this.ds_prt1.setColumn( nRow , "EN_DE" , this.Div01.cal_sndng_de.value);
			this.ds_prt1.setColumn( nRow , "DATE" , this.Div01.cal_sndng_de00.value);
			//this.ds_prt1.setColumn( nRow , "BANK_NM" , this.ds_va.getColumn( this.ds_va.findRow("BANK_CD" , "031") , "BANK_NM"));
			this.ds_prt1.setColumn( nRow , "BANK_NM" , this.ds_mt_rntchrg_m.getColumn( i , "BANK_NM"));
			this.ds_prt1.setColumn( nRow , "NM" , this.ds_mt_rntchrg_m.getColumn( i , "NM"));
			this.ds_prt1.setColumn( nRow , "CNT" , this.ds_mt_rntchrg_m.getColumn( i , "CNT"));
			this.ds_prt1.setColumn( nRow , "ZIP" , this.ds_mt_rntchrg_m.getColumn( i , "ZIP"));
			this.ds_prt1.setColumn( nRow , "ADRES" , this.ds_mt_rntchrg_m.getColumn( i , "ADRES"));
			this.ds_prt1.setColumn( nRow , "ADRES_DETAIL" , this.ds_mt_rntchrg_m.getColumn( i, "ADRES_DETAIL"));
			this.ds_prt1.setColumn( nRow , "TOT" , this.comUtils.formatComma(Tot)+'원');
			
		}
	}

	var rptOption = {};
	if(this.Div01.rdo_paymthd.value == 1){
		rptOption["path"] = "/pms/rnt/lrm/RNT030407.jrf";
	} else {
		rptOption["path"] = "/pms/rnt/lrm/RNT030407_1.jrf";
	}
	rptOption["title"] = this.Div01.rdo_paymthd.text;
	rptOption["filename"] = this.Div01.rdo_paymthd.text;
	rptOption["dataset"] = [["ds_prt1", this.ds_prt1] , ["ds_damdangja", this.ds_damdangja]];
			
	this.pmsUtil.popupReportView(this, rptOption);			
	this.Div01.set_visible(false);		
}

this.Div01_Button12_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.Div01.set_visible(false);	
}
/* 문자메세지 발송 */
this.Button01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var ds = this.ds_mt_rntchrg_m;
	var MBTLNO = "";
	var TELNO = "";
	var AMT = this.comUtils.formatComma(ds.getColumn(ds.rowposition , "TOT_AMT")) + " 원";
	
	this.ds_lfsts_cntrctsmsList.clearData();
	
	for(var i = 0 ; i < this.ds_mt_rntchrg_m.rowcount ; i++){
		if(this.ds_mt_rntchrg_m.getColumn( i , "CHK") == 1){
			var nRow = this.ds_lfsts_cntrctsmsList.addRow();
				
				MBTLNO = this.ds_mt_rntchrg_m.getColumn( i , "MBTLNO");
				 TELNO = this.ds_mt_rntchrg_m.getColumn( i , "TELNO");
				
				MBTLNO =  this.comUtils.isNullEmpty(MBTLNO) == '1' ? this.fn_getTelNo(TELNO) : MBTLNO;		// 연락처2		
				AMT = this.comUtils.formatComma(this.ds_mt_rntchrg_m.getColumn( i , "TOT_AMT")) + " 원";
				 
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "NM" , this.ds_mt_rntchrg_m.getColumn( i , "NM"));				
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "MBTLNO" , MBTLNO );
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHANGE_WORD0" , this.ds_mt_rntchrg_m.getColumn( i , "NM"));
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHANGE_WORD1" , this.ds_mt_rntchrg_m.getColumn( i , "BANK_NM"));
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHANGE_WORD2" , AMT);				
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHANGE_WORD3" , this.dateUtils.format(this.dateUtils.today() , "yyyy-mm-dd"));				
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHANGE_WORD4" , AMT);					
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHRCTR_PREPAR_VALUE_0" , "2");  // 대기자 : 1 , 계약자 : 2
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHRCTR_PREPAR_VALUE_1" , this.ds_cond00.getColumn(this.ds_cond00.rowposition , "CNTRCTR_NO"));   // 대기자 , 고객 번호
				this.ds_lfsts_cntrctsmsList.setColumn( nRow , "CHRCTR_PREPAR_VALUE_2" , this.ds_cond00.getColumn(this.ds_cond00.rowposition , "AREA_SE"));     // 대기자 주택코드				   				
			}	
		}
		
		this.gfn_popup("COM010700_P01.xfdl"
				   , 800
				   , 500
				   , "SMS발송"
				   , {JOB_SE : "FRM002", ds : this.ds_lfsts_cntrctsmsList}
				   , "com::COM010700_P01.xfdl"
				   , "sms_callback" , true);		
				   
}
]]></Script>
