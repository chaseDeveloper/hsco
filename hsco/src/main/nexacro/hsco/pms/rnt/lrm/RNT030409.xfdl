// 				this.ds_cond00.clearData();
// 				var nRow = this.ds_cond00.addRow();
// 				this.ds_cond00.setColumn(nRow , "in_CRT_DIV" , 'RN');
// 				this.ds_cond00.setColumn(nRow , "in_CORT_DIV" , 'JR');
// 				this.ds_cond00.setColumn(nRow , "in_billym" , this.div_search.div_monCal.getValue("YYYYMM"));
// 				this.ds_cond00.setColumn(nRow , "in_tr_date" , this.div_search.Calendar02.value);
// 				this.ds_cond00.setColumn(nRow , "in_tr_sdate" , this.div_search.Calendar03.value);
