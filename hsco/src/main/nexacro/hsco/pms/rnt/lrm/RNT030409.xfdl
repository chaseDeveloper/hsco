				this.ds_cond00.setColumn(nRow , "AREA_SE" , this.div_search.Combo00.value);
				this.ds_cond00.setColumn(nRow , "NM" , this.div_search.Edit01.value);
				this.ds_cond00.setColumn(nRow , "DONG" , this.div_search.MaskEdit01.value);
				this.ds_cond00.setColumn(nRow , "HO" , this.div_search.Edit00.value);
				var sController   	= "hsco/pms/rnt/rnt030409/getList.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_mt_rntchrg_m=output1";
				break;
		case "selectSlaryList1": // 월별 체납액 조회
				this.ds_cond00.clearData();
				var nRow = this.ds_cond00.addRow();
				this.ds_cond00.setColumn(nRow , "CNTRCTR_NO" , this.ds_mt_rntchrg_m.getColumn(this.ds_mt_rntchrg_m.rowposition , "CNTRCTR_NO"));
				this.ds_cond00.setColumn(nRow , "DEDT_DE1" , this.div_search.Calendar01.value);
				this.ds_cond00.setColumn(nRow , "DEDT_DE2" , this.div_search.Calendar00.value);
				this.ds_cond00.setColumn(nRow , "DEDT_DE3" , this.div_search.Calendar02.value);
								
				var sController   	= "hsco/pms/rnt/rnt030409/getList1.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_mt_rntchrg_d=output1";
				break;					
		case "saveSlaryList":	// 정보 저장하기
				var sController   	= "hsco/pms/rnt/rnt030409/listCUD.do";	
				var sInDatasets   	= "input2=ds_cond01";
				var sOutDatasets  	= "";
				break;		
		case "selectSlaryList7": // 은행콤보 
				this.ds_cond03.clearData();
				var nRow = this.ds_cond03.addRow();
				this.ds_cond03.setColumn( nRow , "CNTRCTR_NO" , this.ds_mt_rntchrg_m.getColumn(this.ds_mt_rntchrg_m.rowposition , "CNTRCTR_NO"));
		
				var sController   	= "hsco/pms/rnt/rnt030306/P8.do";
				var sInDatasets   	= "input8=ds_cond03";
				var sOutDatasets  	= "ds_va=output8";
				break;		
							
		case "damDangJa": // 출력물 담당자 조회
		    this.ds_cond99.clearData();
			var nRow = this.ds_cond99.addRow();
			this.ds_cond99.setColumn( nRow , "CODE" , "RN3004");
							
