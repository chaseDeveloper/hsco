/*
 * 함수명  	: div_search_Combo00_onitemchanged
 * 설명 		: 주택구분선택 이벤트 처리
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_search_Combo00_onitemchanged();
 */
this.div_search_Combo00_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.ds_house_codeList.clearData();
	this.ds_cond00.clearData();
	var nRow = this.ds_cond00.addRow();
	this.ds_cond00.setColumn(nRow , "HOUSE_SE" , this.div_search.Combo00.value);				
	var index = this.div_search.Combo00.index;
	switch (index) {
		case 0: //영구임대 아파트
			this.ds_house_codeList.clearData();
			this.fn_transaction("houseCode3");
			break;
		case 1: //매입임대 주택
			this.ds_house_codeList.clearData();
			this.fn_transaction("houseCode2");
			break;
		case 2: //영구임대 아파트 , 상가
			this.ds_house_codeList.clearData();
			this.fn_transaction("houseCode3");
			break;
		case -1: //영구임대 아파트
			this.ds_house_codeList.clearData();
			this.ds_cond00.clearData();
			var nRow = this.ds_cond00.addRow();
			this.ds_cond00.setColumn(nRow , "HOUSE_SE" , 'R1');				
			this.fn_transaction("houseCode3");
			break;			
			}													
}
/**
 * 이미지 뷰어
 * @param fileFullName 선택 전체 파일명
 * @param type 실행구분(ExtFileUpload/ExtFileDownload)
 * @param e 발생이벤트
 * @return void
 */
this.ds_rpairs_photo_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ImageViewer04.set_image('');
	if(obj.rowcount < 1) return;
	
	var fileExtsn = this.ds_rpairs_photo.getColumn(this.ds_rpairs_photo.rowposition , "FILE_EXTSN");
	if(fileExtsn != "jpg") return;

	var fileSn = this.ds_rpairs_photo.getColumn(this.ds_rpairs_photo.rowposition , "FILE_SN");
	var fileOrder = this.ds_rpairs_photo.getColumn(this.ds_rpairs_photo.rowposition , "FILE_ORDR");
	var fileName = this.ds_rpairs_photo.getColumn(this.ds_rpairs_photo.rowposition , "FILE_NM");

	var param = {};
	param["FILE_SN"] = fileSn;
	param["FILE_ORDR"] = fileOrder;

	this.ImageViewer04.set_image("");
	var strUrl = application.gv_host + "hsco/cmm/download/viewImage.do";
	var params = "?FILE_SN=" + fileSn + "&FILE_ORDR=" + fileOrder;
	this.ImageViewer04.set_image(strUrl + params);
}


