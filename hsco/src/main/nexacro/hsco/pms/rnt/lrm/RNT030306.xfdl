this.Div01_Button01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_lfsts_cntrct.rowcount < 1) return;
	if(this.ds_sprmny_return.rowcount < 1) return;

	this.fn_transaction("printList1");
	this.fn_transaction("printList2");
	var vOpts = new this.cf_Opts();
	
	
