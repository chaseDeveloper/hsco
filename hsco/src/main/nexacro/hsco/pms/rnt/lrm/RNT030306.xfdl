/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	switch (strSvcId) {
		case "selectSlaryList": // 대상자 정보 불러 오기
				var sController   	= "hsco/pms/rnt/rnt030306/getList.do";
				var sInDatasets   	= "input1=ds_cond00";
				var sOutDatasets  	= "ds_lfsts_cntrct=output1";
				break;

		case "selectSlaryList1": // 지원금반환청구 정보 불러오기		
				var sController   	= "hsco/pms/rnt/rnt030306/getListR1.do";
				var sInDatasets   	= "input2=ds_cond01";
				var sOutDatasets  	= "ds_sprmny_return=output2";
				break;

		case "printList1": // 출력물1
		        this.ds_prt1.clearData();
				var sController   	= "hsco/pms/rnt/rnt030306/PRINT1.do";
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_prt1=output1";
				bSync = false;
				break;
				
		case "printList2": // 출력물2
		        this.ds_prt2.clearData();
				var sController   	= "hsco/pms/rnt/rnt030306/PRINT2.do";
				var sInDatasets   	= "input1=ds_cond01";
				var sOutDatasets  	= "ds_prt2=output1";
				bSync = false;
				break;				
													
		case "saveSlaryList":	// 정보 저장하기
				var sController   	= "hsco/pms/rnt/rnt030306/listCUD.do";	
				var sInDatasets   	= "input2=ds_sprmny_return:U";
				var sOutDatasets  	= "";
				break;
				
		case "selectSlaryList4": // 가상계좌 (va_vill)
				this.ds_bill.clearData();
				this.ds_cond03.clearData();
				var nRow = this.ds_cond03.addRow();
				this.ds_cond03.setColumn( nRow , "CRT_ID" , this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "CNTRCTR_NO"));
				this.ds_cond03.setColumn( nRow , "VAACT_NO" , this.ds_va.getColumn( this.ds_va.rowposition , "VAACT_NO"));
				bSync = false;
				
				var sController   	= "hsco/pms/rnt/rnt030306/P5.do";
				var sInDatasets   	= "input5=ds_cond03";
				var sOutDatasets  	= "ds_bill=output5";
				break;			
														
		case "selectSlaryList7": // 은행콤보 
				this.ds_va.clearData();
				this.ds_cond05.clearData();
				var nRow = this.ds_cond05.addRow();
				this.ds_cond05.setColumn( nRow , "OPT1" , "2");
				this.ds_cond05.setColumn( nRow , "BANK_CODE" , "031");
				this.ds_cond05.setColumn( nRow , "CNTRCTR_NO" , this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "CNTRCTR_NO"));
				bSync = false;
				
				var sController   	= "hsco/pms/rnt/rnt030306/P8.do";
				var sInDatasets   	= "input8=ds_cond05";
				var sOutDatasets  	= "ds_va=output8";
				break;															
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 

}
 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	
	if(strSvcId == "saveSlaryList")
	{
		if(this.gfn_message("success.처리.성공"));
		this.fn_transaction("selectSlaryList1");
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
	}	

// 	if(strSvcId == "selectSlaryList7")
// 	{
// 		this.fn_transaction("selectSlaryList4");
// 		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);	
// 	}		
	
	
	if(nErrorCode == 0) return;	
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
}


/***********************************************************************
* Transaction 영역 끝
************************************************************************/

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 이벤트 처리(행추가)
 * params  		: Non
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_lfsts_cntrct.rowcount < 1) return;
	var sport_amt = this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "SPORT_AMOUNT");
	if(this.chk_amt1.value = 0){  // 임대부담금
	    sport_amt += this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "RENT_ALOTM");	    
	}
	if(this.chk_amt2.value = 0){ // 추가부담금
		sport_amt += this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "ADIT_ALOTM");
	}
	this.Radio01.set_enable(true);
	this.Button06.set_enable(true);
	this.Radio01_onitemchanged();
	this.cal_dedt_de.set_value(this.dateUtils.today());
	this.edt_amount00.set_value(sport_amt); // 반환대상금
	this.edt_amount.set_value(sport_amt); // 이체한도액
	this.Radio01.setFocus();
}
/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 이벤트 처리(행삭제)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.gfn_message("confirm.삭제여부"))	{
		var nCurRow = this.ds_sprmny_return.rowposition;
		var nRowType = this.ds_sprmny_return.getRowType(nCurRow);
		this.ds_sprmny_return.deleteRow(nCurRow);
		this.fn_transaction("saveSlaryList");
	}
}
/*
 * 함수명  	: ds_lfsts_cntrct_onrowposchanged
 * 설명 		: 마스터 그리드의 dataset의 row change가 일어난뒤 Detail 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_lfsts_cntrct_onrowposchanged();
 */
this.ds_lfsts_cntrct_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{

	this.Button06.set_enable(false);	
	this.ds_cond01.clearData();	
	this.Radio01.set_enable(false);		
	this.cal_intr_bgnde.set_enable(false);
	this.edt_amount.set_value("");
	this.cal_intr_bgnde.set_value("");
	this.cal_dedt_de.set_value("");
	var nRow = this.ds_cond01.addRow();
	this.ds_cond01.setColumn( nRow , "CNTRCTR_NO" , this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "CNTRCTR_NO"));
	this.ds_cond01.setColumn( nRow , "CNTRCTR_CO" , this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "CNTRCTR_CO"));
	this.fn_transaction("selectSlaryList1");
}
/*
 * 함수명  	: ds_lfsts_cntrct_canrowposchange
 * 설명 		: 마스터 그리드의 dataset의 row change 전에 이벤트 호출
 * params  		: None
 * return Type 	: None
 * 사용법		: this.ds_lfsts_cntrct_canrowposchange();
 */
this.ds_lfsts_cntrct_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	for(var i = 0; i < this.checkDs.length; i++) 
	{
		if(this.comUtils.isDatasetUpdated(this.checkDs[i])) 
		{
			var bCancel = this.gfn_message("confirm.변경.취소.여부");
			if(bCancel)
			{
				this.checkDs[i].reset();
				this.ds_lfsts_cntrct_onrowposchanged();
			}	
			return bCancel;
		}
	}		
}

/*
 * 함수명  	: Button92_onclick
