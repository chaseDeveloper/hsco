/*
 * 함수명  	: Radio01_canitemchange
 * 설명 		: 옵션버튼 선택시
 * params  		: None
 * return Type 	: None
 * 사용법		: this.Radio01_canitemchange();
 */
this.Radio01_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	if(this.Radio01.value == '1') 
	{
		this.edt_amount.set_value(this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "SPORT_AMOUNT"));
		this.cal_intr_bgnde.set_value();
		this.cal_intr_bgnde.set_enable(false);
	}	
	if(this.Radio01.value == '2') this.edt_amount.set_value(this.ds_lfsts_cntrct.getColumn(this.ds_lfsts_cntrct.rowposition , "SPORT_AMOUNT"));
	if(this.Radio01.value == '3') this.edt_amount.set_value("");
	if(this.ds_lfsts_cntrct.rowcount < 1) return;
	if(this.Radio01.value != 1 && this.ds_lfsts_cntrct.getColumn( this.ds_lfsts_cntrct.rowposition , "RESCI_TRMNAT_DE") == null)
	{
		this.alert("연체이자 부과 대상이 아닙니다");
		this.Radio01.set_value(1);
		return;
	}
	if(this.Radio01.value == '2' || this.Radio01.value == '3')
	{	
		this.cal_intr_bgnde.set_enable(true);	
		var dsRow = this.ds_lfsts_cntrct.rowposition;
		var srRow = this.ds_sprmny_return.rowcount -1 ;
		var date = this.ds_sprmny_return.getColumn(srRow , "RETURN_DE");
		if(this.ds_sprmny_return.getColumn(srRow , "RETURN_DE") == null)
		{	
			this.cal_intr_bgnde.set_value(this.ds_lfsts_cntrct.getColumn(dsRow , "RESCI_TRMNAT_DE"));	
			this.cal_dedt_de.set_value(this.dateUtils.today());			
		}
		if(this.ds_sprmny_return.getColumn(srRow , "RETURN_DE") != null)
		{
			this.cal_intr_bgnde.set_value(date);			
			this.cal_dedt_de.set_value(this.dateUtils.addMonth(date.toString() , 1 , "s"));
		}				
	}

}
/*
 * 함수명  	: cal_amount
 * 설명 		: 이체한도입력
 * params  		: None
 * return Type 	: None
 * 사용법		: this.cal_amount();
 */
this.cal_amount = function(){

	var value = this.Radio01.value;    // 옵션버튼 값 1: 지원금 2: 지원금+이자 3: 이자
	var dsRow = this.ds_lfsts_cntrct.rowposition;         
	var srRow = this.ds_sprmny_return.rowposition;
	var cnt_no = this.ds_lfsts_cntrct.getColumn(dsRow , "CNTRCTR_NO"); // 계약번호
	var cnt_co = this.ds_lfsts_cntrct.getColumn(dsRow , "CNTRCT_CO");  // 계약횟수
    var sport_amt = this.ds_lfsts_cntrct.getColumn(dsRow , "SPORT_AMOUNT"); // 지원금
    var return_amt = this.edt_amount00.value; // 반환대상금
    
	if(parseInt(return_amt) < parseInt(this.edt_amount.value)){
		this.alert("이체한도액이 지원금을 초과하였습니다");
		return;
	}	    
    
	switch(value) {
		case "1" :  //지원금 계산
				this.ds_sprmny_return.setColumn("DEDT_DE", this.cal_dedt_de.value);				
				var  a = parseInt(this.edt_amount.value); //  이체한도액
				var  b = parseInt(return_amt);  // 반환대상금
				var  c = parseInt(sport_amt);  // 지원금
				var  d = b - c; // 고객부담금	
				while(a < b)  // 지원금을 이체한도액 만큼 차감시켜 이체한도액보다 작을때 까지 루프
				{
					trace("c =" + c);
					nRow = this.ds_sprmny_return.addRow();
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_NO" , cnt_no);
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_CO" , cnt_co);
					this.ds_sprmny_return.setColumn(nRow , "SUM_AMOUNT" , a);           // 지원금 + 이자
					this.ds_sprmny_return.setColumn(nRow , "DEDT_DE" , this.cal_dedt_de.value);  // 납기일자
					this.ds_sprmny_return.setColumn(nRow , "RQEST_DE" , this.cal_dedt_de.value); // 청구일자								
					if(c  < a){
						a = a - c; // 이체한도액 - 남은지원금 
						if(d > a){
						d = d - a; // 고객부담금 
						}
						this.ds_sprmny_return.setColumn(nRow , "SPORT_RETURN_AMOUNT" , c);
						this.ds_sprmny_return.setColumn(nRow , "RQEST_AMOUNT" , a);           // 입주자부담금
						c = 0;  // 지원금 초기화
						a = parseInt(this.edt_amount.value); //  이체한도액 처음값으로	
					}
					if( c > a){										
						this.ds_sprmny_return.setColumn(nRow , "SPORT_RETURN_AMOUNT" , a);  // 지원금
						c = c - a;
					}					
					b = b - a
				}  // 지원금이 이체한도액보다 작아지면 나머지값 로우 추가
				    if(d >= 0){
						nRow = this.ds_sprmny_return.addRow();
						this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_NO" , cnt_no);
						this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_CO" , cnt_co);	
						this.ds_sprmny_return.setColumn(nRow , "SPORT_RETURN_AMOUNT" , c);
						this.ds_sprmny_return.setColumn(nRow , "RQEST_AMOUNT" , d);
						this.ds_sprmny_return.setColumn(nRow , "SUM_AMOUNT" , d + c);					
						this.ds_sprmny_return.setColumn(nRow , "DEDT_DE" , this.cal_dedt_de.value);
						this.ds_sprmny_return.setColumn(nRow , "RQEST_DE" , this.cal_dedt_de.value);
					}	
			break;
			
		case "2" :  // 지원금 + 이자계산
				var  e =  this.dateUtils.between(this.cal_intr_bgnde.value , this.cal_dedt_de.value); // 이자일자계산
				var  a = parseInt(this.edt_amount.value); // 이체한도액
				var  b = parseInt(return_amt); // 반환대상금
				var  y = parseInt(sport_amt);  // 지원금
				var  z = b - y; // 고객부담금				
				var  c = (((parseInt(sport_amt) * 0.05) / 365 ) * (e -1));  // 이자 계산 0.05 고정
				var  d = b + nexacro.floor(c , -1);

				while(a < b)
				{
					nRow = this.ds_sprmny_return.addRow();
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_NO" , cnt_no);
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_CO" , cnt_co);							
					this.ds_sprmny_return.setColumn(nRow , "SUM_AMOUNT" , a);						
					this.ds_sprmny_return.setColumn(nRow , "DEDT_DE" , this.cal_dedt_de.value);
					this.ds_sprmny_return.setColumn(nRow , "RQEST_DE" , this.cal_dedt_de.value);						
					if( y > a){										
						this.ds_sprmny_return.setColumn(nRow , "SPORT_RETURN_AMOUNT" , a);  // 지원금
						y = y - a;
					}							
					if(y  < a){
						a = a - y; // 이체한도액 - 남은지원금 
						if(z > a){
						z = z - a; // 고객부담금 
						}
						this.ds_sprmny_return.setColumn(nRow , "SPORT_RETURN_AMOUNT" , y);
						this.ds_sprmny_return.setColumn(nRow , "RQEST_AMOUNT" , a);           // 입주자부담금
						y = 0;  // 지원금 초기화
						z = parseInt(this.edt_amount.value); //  이체한도액 처음값으로	
					}
					b = b - a					
				}
					nRow = this.ds_sprmny_return.addRow();
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_NO" , cnt_no);
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_CO" , cnt_co);		
					this.ds_sprmny_return.setColumn(nRow , "SPORT_RETURN_AMOUNT" , y);
					this.ds_sprmny_return.setColumn(nRow , "RQEST_AMOUNT" , z);
					this.ds_sprmny_return.setColumn(nRow , "SUM_AMOUNT" , y + z + nexacro.floor(c , -1));		
					this.ds_sprmny_return.setColumn(nRow , "DEDT_DE" , this.cal_dedt_de.value);
					this.ds_sprmny_return.setColumn(nRow , "RQEST_DE" , this.cal_dedt_de.value);
					this.ds_sprmny_return.setColumn(nRow , "OVDINTR" ,  nexacro.floor(c , -1));
			break;			
			
		case "3" : // 이자계산
				this.edt_amount.set_value("");
		        var  a =  this.dateUtils.between(  this.cal_intr_bgnde.value , this.cal_dedt_de.value); // 이자일자계산
				var  c = (((parseInt(sport_amt) * 0.05) / 365 ) * (a - 1));  // 이자계산
					nRow = this.ds_sprmny_return.addRow();
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_NO" , cnt_no);
					this.ds_sprmny_return.setColumn(nRow , "CNTRCTR_CO" , cnt_co);		
					this.ds_sprmny_return.setColumn(nRow , "OVDINTR" ,  nexacro.floor(c , -1));
					this.ds_sprmny_return.setColumn(nRow , "DEDT_DE" , this.cal_dedt_de.value);
					this.ds_sprmny_return.setColumn(nRow , "RQEST_DE" , this.cal_dedt_de.value);						
					this.ds_sprmny_return.setColumn(nRow , "INTR_CREAT_ENDDE" , this.cal_intr_bgnde.value);	// 이자생성종료일			
			break;				
			}	
}

