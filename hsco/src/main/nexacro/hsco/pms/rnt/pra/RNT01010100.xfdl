		this.ds_rentHouseList.deleteRow(this.ds_rentHouseList.rowposition);
			
		this.ds_cond01.setColumn(0, "RENT_HOUSE_CODE", this.ds_rentHouseList.getColumn(this.ds_rentHouseList.rowposition, "RENT_HOUSE_CODE"));
		this.debug(this.ds_cond01, true);
		this.fn_transaction("rentHouseDongList");
	}*/
}

 /*
 * 함수명  	: fn_cancel
 * 설명 		: 취소
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_cancel = function(){
	if(this.confirm("변경하신 모든 사항이 취소됩니다")){
		var ds = this.ds_rentHouseList;
		ds.reset();
		ds.applyChange();
	}
}

/*
 * 함수명  	: div_WORK_grd_rentHouseList_onselectchanged
 * 설명 		: 임대주택정보 로우 체인지
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_grd_rentHouseList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	this.ds_cond01.setColumn(0, "RENT_HOUSE_CODE", this.ds_rentHouseList.getColumn(this.ds_rentHouseList.rowposition, "RENT_HOUSE_CODE"));
	this.debug(this.ds_cond01, true);
	this.fn_transaction("rentHouseDongList");
}

this.ds_rentHouseList_canrowposchange = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	//수정중인 데이터가 있는지 체크
	//행추가로 인한 로우 체인지가 발생한 경우 행추가한 행의 업데이트 여부를 스킵한다.
	if(this.fn_checkUpdate(this.ds_rentHouseList, e.newrow, false) ||
	   this.fn_checkUpdate(this.ds_rentHouseDongList)){
		if(this.confirm("confirm.변경.취소.여부") == true){
			var ds = this.ds_rentHouseList;
			ds.reset();
			ds.applyChange();
			return true;
		}
		else{
			return false;
		}
	}
	
	return true;
}

/*
 * 함수명  	: div_WORK_btn_saveVrify_onclick
 * 설명 		: 건축물 동 정보 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_Tab00_tabpage2_btn_saveDong_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.gfn_checkValidation(this.ds_rentHouseDongList, this.ds_validation) == false) {
		return;
	}
	
	if(Ex.util.isUpdated(this.ds_rentHouseDongList) == true){	
		if(this.confirm("저장 하시겠습니까?")){
			this.fn_transaction("rentHouseDongCUD");
		}
	}
}
 
 /*
 * 함수명  	: div_WORK_Tab00_tabpage2_btn_insertDong_onclick
 * 설명 		: 건축물 동 정보 추가
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_Tab00_tabpage2_btn_insertDong_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_rentHouseList.rowposition > -1){
		//임대주택정보(마스타) 상태확인(초기행,수정,삭제)
		if(this.ds_rentHouseList.getRowType(this.ds_rentHouseList.rowposition) != 2){
			var nPos = this.ds_rentHouseList.rowposition;
			var nRow = this.ds_rentHouseDongList.addRow();
			this.ds_rentHouseDongList.setColumn(nRow, "RENT_HOUSE_CODE", this.ds_rentHouseList.getColumn(nPos, "RENT_HOUSE_CODE"));
			this.ds_rentHouseDongList.setColumn(nRow, "ZIP", this.ds_rentHouseList.getColumn(nPos, "ZIP"));
			this.ds_rentHouseDongList.setColumn(nRow, "ADRES", this.ds_rentHouseList.getColumn(nPos, "ADRES"));
			this.ds_rentHouseDongList.setColumn(nRow, "ADRES_DETAIL", this.ds_rentHouseList.getColumn(nPos, "ADRES_DETAIL"));
			
			this.pmsUtil.setFocusGridBindComp(this.div_WORK.Grid00, nRow, "DONG");
		}else{
			this.alert("임대주택정보 등록중입니다.");
		}
	}else{
		this.alert("선택된 임대주택정보가 없습니다.");
	}
}

 /*
