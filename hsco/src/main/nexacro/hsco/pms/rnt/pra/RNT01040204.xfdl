this.div_WORK_btn_rcivReceiptProcess_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_rentRcvAmtByDayList.clearData();
	this.fn_divToDS(this.ds_cond00, this.div_search);
	
