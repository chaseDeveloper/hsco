this.print_rentPaymentBillPrint = function()
{
	if(this.ds_rentPaymentBillPrint.getRowCount() > 0){
		
		var vOpts = new this.cf_Opts();
		vOpts.setToolbar(false);
		vOpts.setScale(100);
		vOpts.setArgs("mdatYm", 		this.div_search.div_mdatYm.value + "");
//		vOpts.setArgs("reivDt", 		this.div_search.cal_reivDt.value + "");
		vOpts.setArgs("payTmlmt", 		this.div_search.cal_payTmlmt.value + "");
		
		vOpts.setReportPath("/pms/rnt/pra/RNT01070300_R01.jrf");
		vOpts.setSaveFilename("납부서");
		vOpts.setLocalDs(["ds_rentPaymentBillPrint", this.ds_rentPaymentBillPrint]);
		
		var parm = {
			arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
			opts : vOpts
		};

		this.gfn_popup("Popup_Report", 884, 735, "납부서", parm, "common::frmReportPopup.xfdl", "report_callback");
	}
	else{
		this.alert("조회된 데이터가 없습니다.");
		this.gfn_message("info.조회.데이터.없음");
	}
	
}


