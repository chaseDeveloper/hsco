/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
		case "rentNhtOutptList":	//지원금 목록
			var sController   	= "/hsco/pms/rnt/pra/RNT01070300/rentNhtOutptList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rentNhtOutptList=output1";
			break; 
			
		case "rentNhtOutptCnt":		//지원금 목록 카운터
			var sController   	= "/hsco/pms/rnt/pra/RNT01070300/rentNhtOutptCnt.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rentNhtOutptCnt=output1";
			break; 
			
		case "rentNhtOutptCUD":		//지원금 목록 카운터
			var sController   	= "/hsco/pms/rnt/pra/RNT01070300/rentNhtOutptCUD.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "";
			bSync = false;
			break; 
			
		case "spVaBillUidAll":	//지원금 납부 요청
			var sController   	= "/hsco/pms/rnt/pra/RNT00000000/spVaBillUidAll.do"; 
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "";
			
			break;
			 
		case "rentPrintFileList":
			var sController   	= "/hsco/pms/rnt/pra/RNT01070300/rentPrintFileList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rentPrintFileList=output1";
			
			break;
			
		case "rentPaymentBillPrint":
			var sController   	= "/hsco/pms/rnt/pra/RNT01070300/rentPaymentBillPrint.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rentPaymentBillPrint=output1";
			
			break;
			
		case "selectSlaryList2": // 일괄납부 프로시저 호출
				this.ds_cond01.clearData();
				var nRow = this.ds_cond01.addRow();
				this.ds_cond01.setColumn(nRow , "in_CRT_DIV" , 'BS');
				this.ds_cond01.setColumn(nRow , "in_CORT_DIV" , 'BS');
				this.ds_cond01.setColumn(nRow , "in_billym" , this.div_search.div_mdatYm.getValue("YYYYMM"));
				this.ds_cond01.setColumn(nRow , "in_tr_date" , this.div_WORK.Div01.Calendar02.value);
				this.ds_cond01.setColumn(nRow , "in_tr_sdate" , this.div_WORK.Div01.Calendar03.value);
				this.ds_cond01.setColumn(nRow , "in_tr_edate" , this.div_WORK.Div01.Calendar04.value);				

				var sController   	= "hsco/pms/rnt/rnt030402/getList2.do";
				var sInDatasets   	= "input2=ds_cond01";
				var sOutDatasets  	= "";
				break;					
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function(strSvcId, nErrorCode, strErrorMsg)
{
	//this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	var bSuccess = nErrorCode == 0 ? true : false;
	if(bSuccess == true){
		switch(strSvcId){
			case "rentNhtOutptCnt":		//지원금 목록 카운터
				var confirmMsg = "";
				if(Number(this.ds_rentNhtOutptCnt.getColumn(0, "CNT")) > 0){
					confirmMsg = "청구자료가 존재합니다. ";
				}
				confirmMsg += "청구자료를 재생성 하시겠습니까?";
				if(this.confirm(confirmMsg)){
					this.fn_transaction("rentNhtOutptCUD");
					this.fn_search(); 
				}

				break;
				
			case "rentPrintFileList":
				if(this.ds_rentPrintFileList.getRowCount() > 0){
					this.gfn_exportExcel(this.div_WORK.grd_rentPrintFileList, "jiwongum_chunggu_print_file");
				}
				else{
					this.alert("출력할 자료가 없습니다.");
				}
				break;
				
				
			case "rentPaymentBillPrint":
				if(this.ds_rentPaymentBillPrint.getRowCount() > 0){
					this.print_rentPaymentBillPrint();
				}
				else{
					this.alert("출력할 자료가 없습니다.");
				}

				break;

				
			case "selectSlaryList2":
				this.gfn_message("success.처리.성공");	
				this.div_WORK.Div01.set_visible(false);
				this.fn_transaction("rentNhtOutptList");
				break;				
				
		}
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


