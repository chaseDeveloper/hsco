/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
		case "mvnWaitrList":		// 입주대기자 목록
			var sController   	= "/hsco/pms/rnt/pra/RNT01020100/mvnWaitrList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_mvnWaitrList=output1";
			break;
			  
		case "mvnWaitrCUD":			// 입주대기자 CUD
			var sController   	= "/hsco/pms/rnt/pra/RNT01020100/mvnWaitrCUD.do"; 
			var sInDatasets   	= "input1=ds_mvnWaitrList:U";
			var sOutDatasets  	= "";
			break;
		/*
		case "mvnWaitrNoU":			// 입주대기자 대기번호 일괄 수정
			var sController   	= "/hsco/pms/rnt/pra/RNT01020100/mvnWaitrNoU.do"; 
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "";
			break;
		*/
		case "rentCstmrFamilyList":	// 입주대기자 가족목록 조회
			var sController   	= "/hsco/pms/rnt/pra/RNT01020100/rentCstmrFamilyList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentCstmrFamilyList=output1";
			break;

		case "rentCstmrFamilyCUD":	// 입주대기자 가족사항 저장
			var sController   	= "/hsco/pms/rnt/pra/RNT01020100/rentCstmrFamilyCUD.do"; 
			var sInDatasets   	= "input1=ds_rentCstmrFamilyList:U";
			var sOutDatasets  	= "";
			break;
			
		case "mvnWaitrGdccSndngDeList":
			var sController		= "/hsco/pms/rnt/pra/RNT01020200/mvnWaitrGdccSndngDeList.do"
			var sInDatasets		= "";
			var sOutDatasets	= "ds_mvnWaitrGdccSndngDeList=output1";
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	
	if(bSuccess == true){
		switch(strSvcId){
			case "mvnWaitrList":
				//this.ds_mvnWaitrList.set_rowposition(this.ds_mvnWaitrList.backupPosition) ;	//저장후 조회일경우 저장했던 Row로 위치 이동
				this.ds_mvnWaitrList.backupPosition = -1;
				this.fn_setHighlight();
				break;
				
			case "rentCstmrFamilyList":
				//this.ds_mvnWaitrList.setColumn(this.ds_mvnWaitrList.rowposition, "FAMILY_CO", this.ds_rentCstmrFamilyList.rowcount);
				break;
			
			case "mvnWaitrCUD":
				//this.ds_cond02.setColumn(0, "RENT_HOUSE_CODE", this.div_search.cmb_rentHouseCode.value);
				//this.fn_transaction("mvnWaitrNoU");
				this.fn_search();	//조회시에 저장하고 있는 현재의 Row를 전달
				break;
			
			case "mvnWaitrNoU":
				this.fn_search();	//조회시에 저장하고 있는 현재의 Row를 전달
				break;
			
			case "mvnWaitrGdccSndngDeList":
				this.ds_mvnWaitrGdccSndngDeList.insertRow(0);
				this.ds_mvnWaitrGdccSndngDeList.setColumn(0, "GDCC_SNDNG_DE", "전체");
				this.ds_mvnWaitrGdccSndngDeList.setColumn(0, "GDCC_VALUE", "");
				this.div_search.cmb_sndngDe.set_index(0);
				break;
			
		}
	}else {
		this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}

