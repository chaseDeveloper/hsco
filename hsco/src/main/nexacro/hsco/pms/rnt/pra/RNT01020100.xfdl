	this.div_search.spn_stRceptYr.set_min(1900);
	this.div_search.spn_edRceptYr.set_max(yyyy);
	this.div_search.spn_edRceptYr.set_min(1900);
	this.div_search.spn_edRceptYr.set_value(yyyy);
	this.fn_transaction("mvnWaitrGdccSndngDeList");
	
	//var today = this.gfn_today();
	//this.div_search.cal_gdccSndngDe.set_value(today)
	
	this.fn_load_combo();
	this.gfn_applyAuthButton();
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	//조회, 종료시에 수정여부 체크할 데이터셋 지정
	this.checkDs.push(this.ds_mvnWaitrList);
	this.gfn_initCondDs(this, this.div_search);
	//저장후에 저장한 rowposition에 포커스를 주기 위해
	this.ds_mvnWaitrList.posMode = "savePos";
}

/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	this.fn_get_houseSe(this.ds_houseSeList, "전체");	// 주택 구분
	this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체", {HOUSE_SE:'R1'});	// 주택 목록
	this.fn_get_qualfSe(this.ds_qualfSeList, "", {grpCode:"RNT001", se:"r"});				// 자격구분
	this.fn_get_cmmCode(this.ds_familyRelate, "", {grpCode:"HRM013"});	// 가족관계
}

