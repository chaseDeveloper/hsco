/*
 * 함수명  	: div_WORK_grd_mvnWaitrList_onselectchanged
 * 설명 		: 대기자 그리드 체인지
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_grd_mvnWaitrList_onselectchanged = function(obj:Grid, e:nexacro.GridSelectEventInfo)
{
	//저장시 백업해놓은 row가 있는경우 조회시의 로우 체인지는 무시하고
	//백업해놓은 로우의 복원시에만 로우체인지 이벤트 진행..
	if(this.ds_mvnWaitrList.backupPosition != -1){
		if(this.ds_mvnWaitrList.backupPosition != e.row){
			return;
		}else{
			this.ds_mvnWaitrList.backupPosition = -1;
		}
	}
	
	this.ds_rentCstmrFamilyList.clearData();
	this.ds_cond01.setColumn(0, "RENT_HOUSE_CODE", this.ds_mvnWaitrList.getSelectColumn("RENT_HOUSE_CODE"));		// 임대주택코드
	this.ds_cond01.setColumn(0, "RCEPT_DE", this.ds_mvnWaitrList.getSelectColumn("RCEPT_DE"));						// 접수일자
	this.ds_cond01.setColumn(0, "RCEPT_SN", this.ds_mvnWaitrList.getSelectColumn("RCEPT_SN"));						// 접수일련번호
	
	this.fn_setHighlight();
	
	this.fn_transaction("rentCstmrFamilyList");
	
	// 대기자의 접수일자, 주택코드, 평형을 수정하지 못하도록 변경
	var rowType = this.ds_mvnWaitrList.getRowType(this.ds_mvnWaitrList.rowposition);
	var isEdit  = rowType != Dataset.ROWTYPE_INSERT;
	if(this.ds_mvnWaitrList.rowposition > -1){
		this.div_WORK.Calendar00.set_readonly(isEdit);
		this.div_WORK.cmb_rentHouseCode.set_readonly(isEdit);
		this.div_WORK.Edit02.set_readonly(isEdit);
		
		this.div_WORK.Calendar00.set_cssclass(isEdit ? 'cal_WF_Readonly' : 'cal_WF_Essential');
		this.div_WORK.cmb_rentHouseCode.set_cssclass(isEdit ? 'cmb_WF_Readonly' : 'cmb_WF_Essential');
		this.div_WORK.Edit02.set_cssclass(isEdit ? 'edt_WF_Readonly' : 'edt_WF_Essential');
	}
}

