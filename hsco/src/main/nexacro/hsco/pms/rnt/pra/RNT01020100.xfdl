			this.ds_rentCstmrFamilyList.setColumn(nRow, "RCEPT_DE", this.ds_mvnWaitrList.getSelectColumn("RCEPT_DE"));
			this.ds_rentCstmrFamilyList.setColumn(nRow, "RCEPT_SN", this.ds_mvnWaitrList.getSelectColumn("RCEPT_SN"));	
		}else{
			this.alert("대기자정보 등록중입니다.");
		}
	//임대주택정보 없을경우
	}else{
		this.alert("선택된 대기자가 없습니다.");
	}
}

