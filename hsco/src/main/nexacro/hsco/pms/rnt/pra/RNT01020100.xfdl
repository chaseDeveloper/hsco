		return false;
	}
}

/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(backupPosition){
	//저장후에 저장한 rowposition에 포커스를 주기 위해 저장중인 데이터셋에 Row를 저장
	if(backupPosition != null){
		this.ds_mvnWaitrList.backupPosition = backupPosition;
	}else{
		this.ds_mvnWaitrList.backupPosition = -1;
	}

	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.fn_transaction("mvnWaitrList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function(){
	if(this.gfn_checkValidation(this.ds_mvnWaitrList, this.ds_validation) == false) {
		return;
	}
	
	if(this.ds_mvnWaitrList.isUpdated() == true){	
		if(this.confirm("저장 하시겠습니까?")){
			//저장후에 저장한 rowposition에 포커스를 주기 위해
			this.ds_mvnWaitrList.backupPosition = this.ds_mvnWaitrList.rowposition;
			this.fn_transaction("mvnWaitrCUD");
		}
	}
}

/*
 * 함수명  	: fn_insert
 * 설명 		: 추가
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_insert = function(){
	var nRow = this.ds_mvnWaitrList.addRow();
	this.ds_mvnWaitrList.setColumn(nRow, "RENT_HOUSE_CODE", this.div_search.cmb_rentHouseCode.value);
	this.ds_mvnWaitrList.setColumn(nRow, "RENT_HOUSE_NM", this.div_WORK.cmb_rentHouseCode.text);
	this.ds_mvnWaitrList.setColumn(nRow, "RCEPT_DE", this.gfn_today());
	this.ds_mvnWaitrList.setColumn(nRow, "PRMANENT_CSTMR_NO", this.div_WORK.Edit03.value);
}

