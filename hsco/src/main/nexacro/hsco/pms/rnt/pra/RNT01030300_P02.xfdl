/*
 * 함수명  	: fn_changeEdit
 * 설명 		: 주택구분, 안내문종류 변경처리 함수
 * params  		: houseSe : 주택구분
                  noticeSe : 안내문종류
 * return Type 	: None
 * 사용법		: 
 */
this.fn_changeEdit = function(houseSe, noticeSe){
	var title = '';
	var okRm = '(토,일요일은 제외, 점심시간:12시-1시)';
	var place1 = '';
	var place2 = '';
	var place3 = '';
	
	if(houseSe == '04'){
		switch(noticeSe){
			case '0':
			case '2':
				title = "영구임대주택 임대차계약 갱신 안내";
				break;
			case '1':
				title = "영구임대주택 임대차계약 갱신 촉구";
				break;
		}
		
		switch(noticeSe){
			case '0':
			case '1':
				place1 = "1) 우     리     공     사   : 매주 월,수,금요일";
				place2 = "2) 범물동 입주민지원센터 2층 : 매주 화요일";
				place3 = "3) 상 인 비 둘 기 관리사무소 : 매주 목요일";
				break;
			case '2':
				var cur_year	= this.gfn_today("NOW_YEAR");
				var cur_monty	= this.gfn_today("NOW_MONTH");
				
				place1 = cur_year + "년 " + cur_monty	 + "월중 별도통보";
				break;
		}
	}
	else{
		switch(noticeSe){
			case '0':
				title = "임대상가 계약갱신 체결 안내 통지";
				break;
			case '1':
				title = "임대상가 계약갱신 체결 촉구 통지";
				break;
			case '2':
				title = "임대상가 임대차계약 갱신 안내";
				break;
		}
		
		place1 = "화성도시공사 2층 주거복지처 (전화:350-0238,237)";
		place2 = "※ 구 주거복지처 건물";
	}
	
	this.div_WORK.edt_title.set_value(title);
	this.div_WORK.edt_okRm.set_value(okRm);
	this.div_WORK.edt_place1.set_value(place1);
	this.div_WORK.edt_place2.set_value(place2);
	this.div_WORK.edt_place3.set_value(place3);
}



