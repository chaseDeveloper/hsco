/*
 * 함수명  	: fn_smsExecut
 * 설명 		: 문자발송 팝업 호출
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_smsExecut();
 */
this.fn_smsExecut = function()
{
	if(this.chk_print.value == '1'){
		//발송이력 insert
		if(this.ds_cond01.rowcount == 0) {
			this.ds_cond01.addRow();
		}
		//this.ds_cond01.copyRow(0,this.parent.ds_rentReCntrctList,0);
		
		this.ds_cond01.clearData();
		
		var tempGdccKnd = "";
		
		if(this.div_WORK.rdo_noticeSe.value == "0"){
			tempGdccKnd = "002";
		}else if(this.div_WORK.rdo_noticeSe.value == "1"){
			tempGdccKnd = "003";
		}else {
			tempGdccKnd = "004";
		}
		
		for(var i=0; i<this.parent.ds_rentReCntrctList.getRowCount(); i++){
			var rownum = this.ds_cond01.addRow();
			this.ds_cond01.setColumn(rownum, "CSTMR_NO", this.parent.ds_rentReCntrctList.getColumn(i, "PRMANENT_CSTMR_NO"));
			this.ds_cond01.setColumn(rownum, "JOB_SE_CODE", "PRA");
			this.ds_cond01.setColumn(rownum, "GDCC_KND", tempGdccKnd);
			this.ds_cond01.setColumn(rownum, "ADRES", this.parent.ds_rentReCntrctList.getColumn(i, "CONT_ADRES"));
			this.ds_cond01.setColumn(rownum, "ADRES_DETAIL", this.parent.ds_rentReCntrctList.getColumn(i, "HOUSE_ADRES"));
			this.ds_cond01.setColumn(rownum, "TELNO", this.parent.ds_rentReCntrctList.getColumn(i, "TELNO"));
			this.ds_cond01.setColumn(rownum, "MBTLNO", this.parent.ds_rentReCntrctList.getColumn(i, "MBTLNO"));
		}
		
		this.fn_transaction("gdccSndngHistC");
		
	}
	
	if(this.chk_sms.value == '0')
		return;
	
	var ds_reCntrctList = this.parent.ds_rentReCntrctList;
	var rowcount = ds_reCntrctList.rowcount;
	var content  = this.ds_smsCn.getColumn(0, "MSSAGE_CN") || '';				// 메시지 내용
	var noticeSe = this.div_WORK.rdo_noticeSe.value;							// 안내문 종류
	var objParam = {};															// 문자전송 파라미터 object
	var objDate  = new Date();													// 날짜 object
	var sendDe   = objDate.getFullYear() + '년 ' + (objDate.getMonth() + 1) + '월 ' + objDate.getDate() + '일';	// 문자발송일
	
	// 문자발송 목록 생성
	this.ds_sms.clearData();
	for(var i = 0; i < rowcount; i++){			
		var nm        = ds_reCntrctList.getColumn(i, "NM") || '';				// 계약자명
		var telno     = ds_reCntrctList.getColumn(i, "TELNO") || '';			// 연락처1
		var mbtlno    = ds_reCntrctList.getColumn(i, "MBTLNO") || '';			// 연락처2
		var rentEndde = ds_reCntrctList.getColumn(i, "RENT_ENDDE_BFCHG") || '';	// 임대죵료일 - 변경전
		var cntrctrNo = ds_reCntrctList.getColumn(i, "CNTRCTR_NO") || '';		// 계약자명

		// 휴대폰번호 chk
		telno = this.fn_getTelNo(telno);							// 연락처1
		telno = telno == '' ? this.fn_getTelNo(mbtlno) : telno;		// 연락처2		
		
		// 계약년월
		var yy = rentEndde.substr(0, 4);
		var mt = rentEndde.substr(4, 2);
		var qq = 0;
		if(!isNaN(mt)){
			mt = parseInt(mt);
		}
		rentEndde = yy + '년 ' + mt + '월';
		
		var nRow = this.ds_sms.addRow();
		this.ds_sms.setColumn(nRow, "MBTLNO", telno);				// 발송대상자연락처
		this.ds_sms.setColumn(nRow, "NM", nm);						// 발송대상자명
		this.ds_sms.setColumn(nRow, "CHANGE_WORD0", nm);			// 고객명
		this.ds_sms.setColumn(nRow, "CHANGE_WORD1", ' ');
		if(noticeSe == '0')
			this.ds_sms.setColumn(nRow, "CHANGE_WORD2", sendDe);	// 안내문발송일
		else
			this.ds_sms.setColumn(nRow, "CHANGE_WORD2", ' ');		// 안내문발송일
		this.ds_sms.setColumn(nRow, "CHANGE_WORD3", rentEndde);		// 재계약마감월
		this.ds_sms.setColumn(nRow, "CHANGE_WORD4", ' ')
		this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_0", "2");			// 1 : 대기자, 2 : 계약자
		this.ds_sms.setColumn(nRow, "CHRCTR_PREPAR_VALUE_1", cntrctrNo);	//계약자번호
	}
	
	if(noticeSe == '0'){
		objParam = {
			CONTENT : content,
			MXTR_WRD0 : "고객명",
			MXTR_WRD1 : "",
			MXTR_WRD2 : "안내문발송일",
			MXTR_WRD3 : "재계약마감월",
			MXTR_WRD4 : "",
			JOB_SE : "RNT008",
			ds : this.ds_sms
		 }
	}
	else if(noticeSe == '1'){
		objParam = {
			CONTENT : content,
			MXTR_WRD0 : "고객명",
			MXTR_WRD1 : "",
			MXTR_WRD2 : "",
			MXTR_WRD3 : "재계약마감월",
			MXTR_WRD4 : "",
			JOB_SE : "RNT009",
			ds : this.ds_sms
		 }
	}

	this.gfn_popup("COM010700_P01.xfdl"
			   , 1000
			   , 600
			   , "SMS발송" 
			   , objParam
			   , "com::COM010700_P01.xfdl"
			   , "sms_callback", true);
}]]></Script>
