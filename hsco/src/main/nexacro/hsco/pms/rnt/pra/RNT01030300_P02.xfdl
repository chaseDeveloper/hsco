    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : RNT01030300_P01
 * 화면명   : 재계약안내문 팝업
 * 화면설명 : 재계약안내문 팝업
 * 작성일   : 2016.01.07
 * 작성자   : 지효정
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";

/*
 * 함수명  	: RNT01030300_P02_onload
 * 설명 		: form onload
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.RNT01030300_P02_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.today = this.gfn_today();
	this.div_WORK.cal_stDe.set_value(this.today);
	this.div_WORK.cal_stOkDe.set_value(this.today); 
	this.div_WORK.cal_edOkDe.set_value(this.today);
	this.div_WORK.div_monCal.msk_mon.set_value(this.gfn_today("NOW_YEAR_MONTH"));

    // 담당자(RN1003)
    this.fn_fill_code(this.ds_damdangjaList, "/hsco/pms/com/PMS000000/damdangjaList.do", "", {damdangjaCode:'RN1003'});
	
    // 문자발송내용
	this.fn_fill_code(this.ds_smsCn, "/hsco/cmm/ums/mssageManageList.do", "",{CODE : "RNT008"});	// RNT008:[영구]재계약안내
   
	this.fn_changeEdit(this.div_WORK.rdo_houseSe.value, this.div_WORK.rdo_noticeSe.value);
}


/*
 * 함수명  	: btn_close_onclick
 * 설명 		: 종료 버튼 onclick
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}


/*
 * 함수명  	: fn_changeEdit
 * 설명 		: 주택구분 onitemchanged
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_rdo_houseSe_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	this.fn_changeEdit(e.postvalue, this.div_WORK.rdo_noticeSe.value);
	
	this.ds_damdangjaList.clearData();
	if(this.div_WORK.rdo_houseSe.value == '04'){
		// 담당자(RN1003)
		this.fn_fill_code(this.ds_damdangjaList, "/hsco/pms/com/PMS000000/damdangjaList.do", "", {damdangjaCode:'RN1003'});
	} else {
		// 담당자(RN1005)
		this.fn_fill_code(this.ds_damdangjaList, "/hsco/pms/com/PMS000000/damdangjaList.do", "", {damdangjaCode:'RN1005'});
	}
}


/*
 * 함수명  	: div_WORK_rdo_noticeSe_onitemchanged
 * 설명 		: 안내문종류 onitemchanged
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_rdo_noticeSe_onitemchanged = function(obj:Radio, e:nexacro.ItemChangeEventInfo)
{
	this.fn_changeEdit(this.div_WORK.rdo_houseSe.value, e.postvalue);

	// check box enable set
	switch(e.postvalue){
		case '0':
			this.chk_sms.set_enable(true);
			this.fn_fill_code(this.ds_smsCn, "/hsco/cmm/ums/mssageManageList.do", "",{CODE : "RNT008"});	// RNT008:[영구]재계약안내
			break;
		case '1':
			this.chk_sms.set_enable(true);
			this.fn_fill_code(this.ds_smsCn, "/hsco/cmm/ums/mssageManageList.do", "",{CODE : "RNT009"});	// RNT009:[영구]재계약안내 도촉
			break;
		case '2':
			this.chk_sms.set_enable(false);
			this.chk_sms.set_value('0');
			break;
	}
}


/*
 * 함수명  	: btn_confirm_onclick
 * 설명 		: 출력 버튼 onclick
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */
this.btn_confirm_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{	
	if(this.chk_print.value == '0' && this.chk_sms.value == '0'){
		this.alert("출력 유형이 선택되지 않았습니다.");
		return;
	}

	if(this.chk_print.value == '1')
		this.fn_printExecut();
	else
		this.fn_smsExecut();
}

