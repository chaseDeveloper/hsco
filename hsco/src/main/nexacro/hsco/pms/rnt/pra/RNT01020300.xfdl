 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";

this.RNT01020300_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}

this.RNT01020300_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var yyyy = this.gfn_today("NOW_YEAR");
	this.div_search.spn_stRceptYr.set_max(yyyy);
	this.div_search.spn_edRceptYr.set_max(yyyy);
	this.div_search.spn_stRceptYr.set_min(1900);
	this.div_search.spn_edRceptYr.set_min(1900);
	this.div_search.spn_stRceptYr.set_value(1900);
	this.div_search.spn_edRceptYr.set_value(yyyy);
	
	this.fn_load_combo();

	var fileConfig = {
		onChange: 	this.fn_onFileSelected,
		onSuccess: 	this.fn_onFileSuccess,
		useMultiSelect : true             // 다중선택 옵션
	};

	this.FILE_MANAGER = new this.fileManager(this, "PMS", "PRA");
	this.FILE_MANAGER.init(fileConfig);
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	//조회, 종료시에 수정여부 체크할 데이터셋 지정
	this.checkDs.push(this.ds_rentPosesnThingList);
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	this.fn_get_houseSe(this.ds_houseSeList, "전체");	// 주택 구분
	//this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체");	// 주택 목록
	this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체", {HOUSE_SE:'R1'});	//주택 목록
	this.fn_get_cmmCode(this.ds_qualfSeList, "", {grpCode:"RNT001"});		// 자격구분
	this.fn_get_cmmCode(this.ds_familyRelate, "", {grpCode:"HRM013"});		// 가족관계
	this.fn_get_cmmCode(this.ds_prposSeList, "", {grpCode:"RNT012"});		// 용도
	this.fn_get_cmmCode(this.ds_explnSeList, "", {grpCode:"RNT013"});		// 소명구분
}

this.div_search_spn_stRceptYr_onspin = function(obj:Spin, e:nexacro.SpinEventInfo)
{
	if(e.postvalue > this.div_search.spn_edRceptYr.value){
		return false;
	}
}

this.div_search_spn_edRceptYr_onspin = function(obj:Spin, e:nexacro.SpinEventInfo)
{
	if(e.postvalue < this.div_search.spn_stRceptYr.value){
		return false;
	}
}

