/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
		case "mvnWaitrList":		// 입주대기자 목록
			var sController   	= "/hsco/pms/rnt/pra/RNT01020300/mvnWaitrList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_mvnWaitrList=output1";
			break;
			
		case "rentCstmrFamilyList":	// 입주대기자 목록 조회
			var sController   	= "/hsco/pms/rnt/pra/RNT01020300/rentCstmrFamilyList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentCstmrFamilyList=output1";
			break; 

		case "rentCstmrFamilyCUD":	// 입주대기자 가족사항 저장
			var sController   	= "/hsco/pms/rnt/pra/RNT01020100/rentCstmrFamilyCUD.do"; 
			var sInDatasets   	= "input1=ds_rentCstmrFamilyList:U";
			var sOutDatasets  	= "";
			break;
			
		case "rentPosesnThingList":	// 소유물건정보 목록
			var sController   	= "/hsco/pms/rnt/pra/RNT01020300/rentPosesnThingList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentPosesnThingList=output1";
			break;
			
		case "posesnThingCUD":		// 소유물건정보 생성
			var sController   	= "/hsco/pms/rnt/pra/RNT01020300/rentPosesnThingCUD.do";
			var sInDatasets   	= "input1=ds_rentPosesnThingList:U";
			var sOutDatasets  	= "";
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;

	if(bSuccess == true){
		switch(strSvcId){
			case "posesnThingCUD":
				//this.fn_search();
				//this.ds_mvnWaitrList.rowposition setLastRowposition();	//저장후 조회일경우 저장했던 Row로 위치 이동
				this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
				break;
			case "rentPosesnThingList":
				/*
				var file_sn = this.ds_rentPosesnThingList.getColumn(0, "FILE_SN");
				if (file_sn != undefined) {
					this.div_WORK.btn_downFile.set_visible(true);
				} else {
					this.div_WORK.btn_downFile.set_visible(false);
				}
				*/
				break;
		}
	}
	
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}

