include "pms_lib::rntUtilInclude.xjs";
this.RNT01040210_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}

this.RNT01040210_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var today = this.gfn_today()	
	var month = this.gfn_today("NOW_YEAR_MONTH");
	this.div_WORK.cal_payTmlmt.set_value(today);
	this.div_search.edt_dong.clearEventHandler("onkeydown");	
	
	this.fn_load_combo();
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	this.fn_add_comboHeader(this, this.div_search.cmb_dong, "전체");
	this.fn_add_comboHeader(this, this.div_search.cmb_ho,   "전체");
	
	this.fn_get_houseSe(this.ds_houseSeList, "전체");//주택 구분
	this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체");//주택 목록
	this.fn_get_cmmCode(this.ds_seizrSeList, "", {grpCode:"RNT007"});//압류구분
	this.fn_get_cmmCode(this.ds_cditSeList, "", {grpCode:"RNT006"});//채권구분
}

/*
 * 함수명  	: div_search_cmb_houseSe_onitemchanged
 * 설명 		: 주택구분에 맞는 주택코드Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_search_cmb_houseSe_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체", {HOUSE_SE:e.postvalue});
}

/*
 * 함수명  	: div_search_cmb_rentHouseCode_onitemchanged
 * 설명 		: dong Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_search_cmb_rentHouseCode_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var rentHouseCode = this.div_search.cmb_rentHouseCode.value;
	this.fn_get_rentDong(this.div_search.cmb_dong, "전체", {rentHouseCode:rentHouseCode});//동 목록
	this.ds_hoList.clearData();
	this.fn_add_comboHeader(this, this.div_search.cmb_ho,  "전체");
}

/*
 * 함수명  	: div_search_cmb_dong_onitemchanged
 * 설명 		: ho Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_search_cmb_dong_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var rentHouseCode = this.div_search.cmb_rentHouseCode.value;
	var dong = this.div_search.cmb_dong.value;
	this.fn_get_rentHo(this.div_search.cmb_ho, "전체", {rentHouseCode:rentHouseCode, dong:dong});//호 목록
}

/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	this.ds_rentCntrctList.clearData();
	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.ds_cond00.setColumn( 0 , "canclSe" , this.div_WORK.rdo_canclSe.value);
	this.fn_get_rentCntrct(this.ds_rentCntrctList, this.ds_cond00);
}


/*
 * 함수명  	: div_WORK_btn_cyberAccountPop_onclick
 * 설명 		: 가상계좌팝업
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_btn_cyberAccountPop_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var objRentCntrctDetail = this.fn_dsToObj(this.ds_rentCntrctDetail, this.ds_rentCntrctDetail.rowposition);
	
	trace("this.ds_rentCntrctDetail = " + objRentCntrctDetail);
	// 임대료
	if(this.div_WORK.Tab00.tabindex == 0){
		objRentCntrctDetail['CRT_DIV'] = 'RN';
		objRentCntrctDetail['CORT_CODE'] = 'RN';
	}
	// 지원금
	if(this.div_WORK.Tab00.tabindex == 1){
		objRentCntrctDetail['CRT_DIV'] = 'BS';
		objRentCntrctDetail['CORT_CODE'] = 'BS';
	}	
	// 보증금
	if(this.div_WORK.Tab00.tabindex == 2){	
		objRentCntrctDetail['CRT_DIV'] = 'RN';
		objRentCntrctDetail['CORT_CODE'] = 'R0';
	}
				   
	this.gfn_popup("cyberAccountPop"
				   ,714
				   ,872
				   ,""
				   ,{objRentCntrctDetail:objRentCntrctDetail}   
				   ,"pms_rnt_pra::RNT00000000_P01.xfdl"
				   , "" , true);				   
				   		   
}

