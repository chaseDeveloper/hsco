/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
		case "rentMtRntchrgList":		//임대료 목록
			var sController   	= "/hsco/pms/rnt/pra/RNT01040203/rentMtRntchrgList.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentMtRntchrgList=output1";
			break;
			
		case "puchasMtRntchrgU":		//연체이자 생성
			var sController   	= "/hsco/pms/rnt/pra/RNT01040203/rentMtRntchrgU.do"; 
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "";
			break;
			
		case "rentGtnMtRntchrgU":		//보증금연체이자 생성
			var sController   	= "/hsco/pms/rnt/pra/RNT01040203/rentGtnMtRntchrgU.do"; 
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "";
			break;			

		case "gtnSportList":			//지원금 목록			
			var sController   	= "/hsco/pms/rnt/pra/RNT01030100/gtnSportList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_gtnSportList=output1";
			break;			
			
		case "rentGtnList":				//보증금 목록			
			var sController   	= "/hsco/pms/rnt/pra/RNT01030100/rentGtnList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentGtnList=output1";
			break			
			
		case "selectUserNoticeSheetReportList":		//고지서 출력
			var sController   	= "hsco/pms/rnt/pra/RNT01040203/selectUserNoticeSheetReportList.do"; 
			var sInDatasets   	= "input1=ds_userNht"
			var sOutDatasets  	= "ds_noticeSheetReport=output1"
			break;				
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
		case "rentCntrctDetail":
				this.ds_rentCntrctDetail.set_rowposition(0);
				//this.debug(this.ds_rentCntrctDetail);
				break;
				
		case "puchasMtRntchrgU":
				this.fn_transaction("rentMtRntchrgList");	//임대료 목록
				break;			
				
		case "rentGtnMtRntchrgU":					
				this.fn_transaction("rentGtnList");	//보증금 목록
				break;					

		case "selectUserNoticeSheetReportList":
				this.fn_userNoticeSheetReportPrint();
				break;								
		}
	}
	this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


