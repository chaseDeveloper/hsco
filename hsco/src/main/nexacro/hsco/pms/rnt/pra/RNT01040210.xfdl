 /*
 * 함수명  	: div_WORK_btn_createPuchasMtRntchrg_onclick
 * 설명 		: 연체이자 재생성
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_btn_createPuchasMtRntchrg_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 임대료 탭이 아니면 return;
	if(this.div_WORK.Tab00.tabindex == 1) return;
	
	if(this.div_WORK.Tab00.tabindex == 0){
		// 체크된 건이 없으면 return
		if(this.ds_rentMtRntchrgList.findRow("CHK" , 1) == -1){
			return;
		}
		
		if(this.confirm("납입일자를 기준으로 연체이자를 재생성하시겠습니까?")){	
			for(var i = 0 ; i < this.ds_rentMtRntchrgList.rowcount ; i++){	    
				if(this.ds_rentMtRntchrgList.getColumn( i , "CHK") == 1){
					this.ds_cond02.setColumn(0, "cntrctrNo", this.ds_rentCntrctList.getSelectColumn("CNTRCTR_NO"));
					this.ds_cond02.setColumn(0, "payTmlmt", this.div_WORK.cal_payTmlmt.value);
				}	
			}
				this.fn_transaction("puchasMtRntchrgU");
		}
	}
		
	if(this.div_WORK.Tab00.tabindex == 2){	
	
		if(this.ds_rentGtnList.getSelectColumn("RCIV_AMT") == 0){
			if(this.confirm("납부기한을 기준으로 연체이자를 재생성하시겠습니까?")){	
				this.ds_cond02.setColumn(0, "cntrctrNo", this.ds_rentCntrctList.getSelectColumn("CNTRCTR_NO"));
				this.ds_cond02.setColumn(0, "rentHouseCode", this.ds_rentCntrctList.getSelectColumn("RENT_HOUSE_CODE"));
				this.ds_cond02.setColumn(0, "payTmlmt", this.div_WORK.cal_payTmlmt.value);
				this.ds_cond02.setColumn(0, "gtnSn", this.ds_rentGtnList.getSelectColumn("GTN_SN"));
				this.fn_transaction("rentGtnMtRntchrgU");
			}	
		}else {
			this.alert("이미 납부한 자료입니다.");
			return false;
		}	
	}	
		
}

