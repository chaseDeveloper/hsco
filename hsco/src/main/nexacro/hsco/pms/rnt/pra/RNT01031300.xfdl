/*
 * 함수명  	: fn_printExecut
 * 설명 		: 계약서 출력
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_printExecut = function()
{
	if(this.printChkArr.length == 0){
		this.div_print.set_visible(false);
		return;
	}

	var docSe = this.printChkArr.pop();
	var reportNm = "";
	var reportPath = "";

	this.ds_rentCntrctDetailReport.clearData();
	this.ds_rentCntrctDetailReport.addRow();
	this.ds_rentCntrctDetailReport.copyRow(0, this.ds_rentCntrct, 0);	// 계약정보

	var vOpts = new this.cf_Opts();
	vOpts.setToolbar(false);
	vOpts.setScale(100);
	vOpts.setLocalDs(["ds_rentCntrctDetailReport", 	this.ds_rentCntrctDetailReport]);
	
	switch(docSe){
		case "1":
			reportNm 	= "신규 계약서";
			reportPath 	= "/pms/rnt/pra/RNT01030100_R01.jrf";
		break;	
	}
	
	vOpts.setReportPath(reportPath);
	vOpts.setSaveFilename(reportNm);
	
	var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		opts : vOpts
	};

	this.gfn_popup("Popup_Report", 884, 735, reportNm, parm, "common::frmReportPopup.xfdl", "fn_printExecut");	
}



