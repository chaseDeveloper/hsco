    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : RNT01031300
 * 화면명   : 계약등록(상가)
 * 화면설명 : 계약등록(상가)
 * 작성일   : 2016.07.13
 * 작성자   : 한상훈
 * 수정이력 :  
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";
include "pms_lib::pmsUtilInclude.xjs";


this.RNT01031300_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}


this.RNT01031300_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_load_combo();
}


/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}


/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체", {HOUSE_SE:'R3'});//주택 목록
	this.fn_fill_code(this.ds_cntrctDe,"/hsco/pms/rnt/pra/RNT01031300/cntrctDe.do");	// 계약가능 날짜
	this.fn_get_cmmCode(this.ds_familyRelate, "", {grpCode:"HRM013"});		//가족관계
}


/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	this.ds_rentHouseBildList.clearData();
	this.fn_divToDS(this.ds_cond00, this.div_search);
	this.fn_transaction("rentHouseBildList");
}

/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_save = function(){
	if(this.gfn_checkValidation(this.ds_rentCntrct, this.ds_validation) == false) {
		this.alert("comm.데이터.변경.없음");
		return false;
	}
		
	// 공급가능여부가 0이면 계약등록 중단 : 중복저장  방지
	var suplySe = this.ds_rentHouseBildList.getColumn(this.ds_rentHouseBildList.rowposition, "SUPLY_SE");		// 공급가능여부
	if(suplySe == '0'){
		this.alert("해당 상가는 이미 계약이되었습니다.");
		return;
	}
        	
	if(this.confirm("S") == true){
		this.fn_transaction("rentCntrctCUD");
	}
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_cancel = function(){
	if(this.confirm("c") == true){
		this.ds_rentCntrct.cancel();
	}
}


/*
 * 함수명  	: fn_print
 * 설명 		: 인쇄 div 실행
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.fn_print = function()
{
	var cntrctrNo = this.ds_rentCntrct.getColumn(0, "CNTRCTR_NO");		// 계약자 번호
	var isBoolean = !this.comUtils.isNull(cntrctrNo);
	
	if(!this.div_print.visible){
		this.div_print.set_right(136);
		this.div_print.set_visible(true);
		var len = this.div_print.components.length;
		for(var i = 0; i < len; i++){
			var item = this.div_print.components.get_item(i);
			if(item instanceof CheckBox){
				item.set_value(0);
				item.set_enable(isBoolean);
			}
		}
	}
	else{
		this.div_print.set_visible(false);
	}
}


/*
 * 함수명  	: div_print_btn_printCancel_onclick
 * 설명 		: 인쇄 div close
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.div_print_btn_printCancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.div_print.set_visible(false);
}


/*
 * 함수명  	: div_print_btn_print_onclick
 * 설명 		: 출력물 팝업
 * params  		: 
 * return Type 	: None
 * 사용법		:
 */
this.printChkArr = [];	// 출력물 배열
this.div_print_btn_print_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.printChkArr = [];
	var len = this.div_print.components.length;
	for(var i = 0; i < len; i++){
		var item = this.div_print.components.get_item(i);
		if(item instanceof CheckBox){
			if(item.value){
				this.printChkArr.push(item.id.substr(item.id.indexOf("_") + 1, item.id.length));
			}
		}
	}

	this.printChkArr.reverse();
	this.fn_printExecut();
}


/*
 * 함수명  	: div_WORK_btn_cyberAccountPop_onclick
 * 설명 		: 가상계좌팝업
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_btn_cyberAccountPop_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var objRentCntrctDetail = this.fn_dsToObj(this.ds_rentCntrct, 0);
	objRentCntrctDetail['TELNO'] = this.ds_mvnWaitrList.getColumn(this.ds_mvnWaitrList.rowposition, "TELNO");
	objRentCntrctDetail['MBTLNO'] = this.ds_mvnWaitrList.getColumn(this.ds_mvnWaitrList.rowposition, "MBTLNO");
	objRentCntrctDetail['CRT_DIV'] = 'RN';
	objRentCntrctDetail['CORT_CODE'] = 'R0';
	this.gfn_popup("cyberAccountPop"
				   ,714
				   ,740
				   ,""
				   ,{objRentCntrctDetail:objRentCntrctDetail}   
				   ,"pms_rnt_pra::RNT00000000_P01.xfdl"		// 가상계좌
				   ,"", true);
}


/*
 * 함수명  	: div_WORK_grd_rentHouseBildList_oncelldblclick
 * 설명 		: 계약가능 주택정보 더블 클릭
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.div_WORK_grd_rentHouseBildList_oncelldblclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	this.ds_rentCntrct.clearData();
	this.ds_rentCntrct.addRow();
	this.ds_rentCntrct.copyRow(0, this.ds_rentHouseBildList, this.ds_rentHouseBildList.rowposition);	// 임대주택 정보 ds copy
	this.ds_rentCntrct.copyRow(0, this.ds_cntrctDe, 0);				// 계약일자 ds copy
	this.ds_rentCntrct.setColumn(0, "HOUSE_POSESN_AT", 0);			// 주택소유여부

	var stdrYear = this.ds_rentCntrct.getColumn(0, "CNTRCT_DE").substring(0, 4);
	this.ds_cond01.clearData();
	this.ds_cond01.addRow();
	this.ds_cond01.setColumn(0, "stdrYear", 	 stdrYear);												// 기준년도
	this.ds_cond01.setColumn(0, "rentHouseCode", this.ds_rentCntrct.getColumn(0, "RENT_HOUSE_CODE"));	// 임대주택코드
	this.ds_cond01.setColumn(0, "dong",  		 this.ds_rentCntrct.getColumn(0, "DONG"));				// 동
	this.ds_cond01.setColumn(0, "ho",  	    	 this.ds_rentCntrct.getColumn(0, "HO"));				// 호
	this.fn_transaction("rntchrgStdrStore");						// 임대료기준(상가) 조회
	
	// 버튼 비활성화
	this.div_WORK.btn_cyberAccountPop.set_enable(false);			// 가상계좌
	this.div_WORK.Tab00.tabpage1.btn_bsnmInsert.set_enable(false);	// 사업자등록	
	this.div_WORK.Tab00.tabpage2.btn_insert.set_enable(false);		// 행추가
	this.div_WORK.Tab00.tabpage2.btn_cancel.set_enable(false);		// 행삭제
	this.div_WORK.Tab00.tabpage2.btn_save.set_enable(false);		// 가족사항 저장	
