/*
 * 함수명  	: btn_save_onclick
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.btn_save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
   if(this.ds_anactList.getColumn(0,"FLAG1") == "false" && this.ds_anactList.getColumn(0,"FLAG2") == "false")
   {
       alert("결의구분을 선택해주십시요.");
       return;
   }
   
	if(this.ds_anactList.isUpdated() == true){	
		if(this.confirm("저장 하시겠습니까?")){
			this.fn_transaction("saveEtcAnactInfo");
		}
	}
}

