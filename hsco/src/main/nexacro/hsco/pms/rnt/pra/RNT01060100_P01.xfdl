{
	var today = this.dateUtils.today();
	this.ds_anactList.addRow();
	this.ds_anactList.set_rowposition(0);
	this.ds_anactList.setColumn(0, "MDAT_DE", today);    // 결의일자 현날짜
	this.ds_anactList.setColumn(0, "PAY_TMLMT", today);  // 납부일자 현날짜
	
	this.ds_anactList.setColumn(0, "RENT_HOUSE_CODE", "699");  // 주택코드 - 기타
	this.ds_anactList.setColumn(0, "DONG", "0000");            // 동
