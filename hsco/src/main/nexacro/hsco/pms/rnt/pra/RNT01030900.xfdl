/***********************************************************************
 * Script Include
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";

this.RNT01030900_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}

this.RNT01030900_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_load_combo();

	var fileConfig = {
		onChange: 	this.fn_onFileSelected,
		onSuccess: 	this.fn_onFileSuccess,
		useMultiSelect : true             // 다중선택 옵션
	};

	this.FILE_MANAGER = new this.fileManager(this, "PMS", "PRA");
	this.FILE_MANAGER.init(fileConfig);
	
	// 동 enter key down 이벤트 제거
	this.div_search.edt_dong.clearEventHandler("onkeydown");
}


/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	//조회, 종료시에 수정여부 체크할 데이터셋 지정
	//this.checkDs.push(this.ds_rentPosesnThingList);
	this.gfn_initCondDs(this, this.div_search);
}


/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	this.fn_add_comboHeader(this, this.div_search.cmb_dong, "전체");
	this.fn_add_comboHeader(this, this.div_search.cmb_ho,   "전체");

	this.fn_get_houseSe(this.ds_houseSeList, "전체");						// 주택 구분
	this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체");	// 주택 목록
	this.fn_get_cmmCode(this.ds_familyRelate, "", {grpCode:"HRM013"});		// 가족관계
	this.fn_get_cmmCode(this.ds_prposSeList, "", {grpCode:"RNT012"});		// 용도
	this.fn_get_cmmCode(this.ds_explnSeList, "", {grpCode:"RNT013"});		// 소명구분
}


/*
 * 함수명  	: div_search_cmb_houseSe_onitemchanged
 * 설명 		: 주택구분에 맞는 주택코드Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		:
 */
this.div_search_cmb_houseSe_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.fn_get_rentHouseCode(this.div_search.cmb_rentHouseCode, "전체", {HOUSE_SE:e.postvalue});
}

this.div_WORK_btn_thingRegPop_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.gfn_popup("thingRegPop"
				   , 1059
				   , 713
				   , ""
				   , null
				   , "pms_rnt_pra::RNT01030900_P02.xfdl"		// 소유물건 일괄등록
				   , "" , true);
}


/*
 * 함수명  	: fn_search
 * 설명 		: 조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
	this.ds_rentCntrctList.clearData();
	this.ds_rentPosesnThingList.clearData();
	this.ds_rentCstmrFamilyList.clearData();

	this.fn_divToDS(this.ds_cond00, this.div_search);

	// 물건소유자 경우 사용
	this.ds_cond00.setColumn(0, "anact1", this.div_WORK.chk_anact1.value);

	// 계약자 목록조회
	this.fn_transaction("rentCntrctList");
}


