		this.close();
	}
}


this.div_DETAIL_btn_ZIP_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var opt = {};
	opt.zipField = "ZIP";
	opt.jibunField = "ADRES";
	this.pmsUtil.popupZipCode(this.ds_rentBsnmList, opt);
}

