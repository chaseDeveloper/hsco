/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";

/***********************************************************************
 * 부모 Form에서 파라미터로 넘겨야 할 값
 * callback : function(obj)
 ***********************************************************************/
this.houseCode = "";			// 주택코드
this.nm = "";					// 계약자명
this.dong = "";					//동
this.ho = "";					//호
this.cntrctr_no = "";           //고객번호
this.ntic_se = "";              //임대료,지원금 구분


this.RNT01040203_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{	
	//공통 폼 초기화
	this.fn_init(false);	
	//폼 초기화
	this.fn_init_form();	
	//Dataset 초기화
	this.fn_init_dataset();
}


this.RNT01040203_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	
	this.ds_cond00.clearData();
	this.ds_cond00.addRow();
	this.ds_cond00.setColumn(0,"HOUSE_SE",houseSe);
	this.fn_transaction("selectHouseCodeList");	
	
}


/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	houseCode = this.parent.houseCode;
	houseSe = this.parent.houseSe;
	nm = this.parent.nm;
	dong = this.parent.dong;
	ho = this.parent.ho;	
	ntic_se = this.parent.ntic_se;
	cntrctr_no = this.parent.cntrctr_no;
}
	
	
/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{	

}	


/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{		
	this.fn_get_cmmCode(this.ds_paySe, "", {grpCode:"RNT009"}); //납부구분
	this.fn_transaction("selectUserNoticeSheetReportList");
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
	
		case "selectHouseCodeList":		//고지서 출력
			var sController   	= "hsco/pms/rnt/pra/RNT00000000/rentHouseCodeList.do"; 
			var sInDatasets   	= "input1=ds_cond00"
			var sOutDatasets  	= "ds_rentHouseCodeList=output1"
			break;	
			
			
		case "selectUserNoticeSheetReportList":		//고지서 출력
			var sController   	= "hsco/pms/rnt/pra/RNT01040203/selectUserNoticeSheetReportList.do"; 
			var sInDatasets   	= "input1=ds_userNht"
			var sOutDatasets  	= "ds_noticeSheetReport=output1"
			break;		
			
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
 		switch(strSvcId){   
 		
				case "selectHouseCodeList" :
					this.ds_userNht.clearData();
					this.ds_userNht.addRow();
					this.ds_userNht.setColumn(0, "RENT_HOUSE_CODE", houseCode);
					this.ds_userNht.setColumn(0, "NM", nm);
					this.ds_userNht.setColumn(0, "DONG", dong);
					this.ds_userNht.setColumn(0, "HO", ho);
					this.ds_userNht.setColumn(0, "NTIC_SE" , ntic_se);
					this.ds_userNht.setColumn(0, "CNTRCTR_NO" , cntrctr_no);
					
					
					this.div_monCal.setValue(this.gfn_today());
					this.cal_payTmlmt.set_value(this.gfn_today());
										
					//공통 코드 조회
					this.fn_load_combo();
				break;
 		
 				case "selectUserNoticeSheetReportList":		//고지서 출력
				//	this.fn_userNoticeSheetReportPrint();
 				break;
 		}
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}


/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/

