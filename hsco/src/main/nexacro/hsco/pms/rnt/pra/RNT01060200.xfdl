/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;

	var chkCnt = 0;
	var tabindex = this.div_WORK.tab.tabindex;

	if (tabindex == "0" || tabindex == "1") {

		if (this.div_search.chk_anact1.isChecked()) chkCnt++;
		if (this.div_search.chk_anact2.isChecked()) chkCnt++;
		if (this.div_search.chk_anact3.isChecked()) chkCnt++;
		if (this.div_search.chk_anact4.isChecked()) chkCnt++;
		
		if(chkCnt < 1) {
			this.gfn_message("comm.항목.필수", "결의구분");
			return;
		}
	}
	
	// 결의구분코드 필수 체크
	var anact_chk = new Array();

	if (this.div_search.chk_anact1.isChecked())	anact_chk.push("1");
	if (this.div_search.chk_anact2.isChecked())	anact_chk.push("2");
	if (this.div_search.chk_anact3.isChecked())	anact_chk.push("3");
	if (this.div_search.chk_anact4.isChecked())	anact_chk.push("4");
		
	this.ds_cond00.setColumn(0, "anact_chk", anact_chk);
	//trace(this.ds_cond00.saveXML());
	
	switch (strSvcId) {		
		
		case "selectDecsnListPrt":		//결의내역
			var sController   	= "/hsco/pms/rnt/pra/RNT01060200/selectDecsnListPrt.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_decsnListSelect=output1";
			break; 
			
	    case "selecAnactListPrt":		//결의서
			var sController   	= "/hsco/pms/rnt/pra/RNT01060200/selecAanactListPrt.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_anactListSelect=output1";
			break; 
			
	    case "selectRdcamtListPrt":		//감액내역서
			var sController   	= "/hsco/pms/rnt/pra/RNT01060200/selectRdcamtListPrt.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rdcamtListSelect=output1";
			break; 
			
		case "selectLgxpssListPrt":		//소송비 수납내역
			var sController   	= "/hsco/pms/rnt/pra/RNT01060200/selectLgxpssListPrt.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_lgxpssListSelect=output1";
			break; 
			
		// 수입결의서 조회
		case "selectIncmeDecsnList" :
			var sController   	= "hsco/pms/rnt/pra/RNT01060200/selectIncmeDecsnList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_houseIncmeDecsnList=output1";
	        bSync = false; //동기방식으로 처리			
			break;			
			
		// 수입내역서 조회
		case "selectIncmeDtstmnList" :
			var sController   	= "hsco/pms/rnt/pra/RNT01060200/selectIncmeDtstmnList.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_houseIncmeDtstmnList=output1";
	        bSync = false; //동기방식으로 처리			
			break;
			
			
		// 결의서/내역서 조회(전체)
		case "selectIncmeDecsnListAll" :
			var sController   	= "hsco/pms/rnt/pra/RNT01060200/selectIncmeDecsnListAll.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_houseIncmeDecsnList=output1 ds_houseIncmeDtstmnList=output2";
	        bSync = false; //동기방식으로 처리			
			break;				
			
		//내부결재 번호 조회
		case "searchDecsn": 
			var sController   	= "hsco/pms/com/COM000000/selectDecsn.do";		
			var sInDatasets   	= "input1=ds_cond99";
			var sOutDatasets  	= "ds_sanction=output1";
			bSync = false; //동기방식으로 처리
			break;			
			
		//내부결재 상태조회
		case "sanknKnd": 
 		    var sController   	= "hsco/pms/sls/hou/ctr/SLS020201/SANCTNKND.do";
	        var sInDatasets   	= "input1=ds_cond05";
	        var sOutDatasets  	= "ds_sanctnknd=output1";
	        bSync = false; //동기방식으로 처리
		    break;      

		//내부결재 번호 등록
		case "insertDecsn": 
			var sController   	= "hsco/pms/com/COM000000/insertDecsn.do";		
			var sInDatasets   	= "input1=ds_cond99";
			var sOutDatasets  	= "";
			break;				
	}
	  
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
	
		switch(strSvcId){
			
			// 주택 수입결의서 조회
			case "selectIncmeDecsnList" :
				if(this.ds_houseIncmeDecsnList.getRowCount() > 0){					
					this.fn_print1();
				}
				else{					
					this.alert("조회된 데이터가 없습니다.");
					this.gfn_message("info.조회.데이터.없음");
				}
				break;
		
			// 주택 수입내역서 조회
			case "selectIncmeDtstmnList" :
				if(this.ds_houseIncmeDtstmnList.getRowCount() > 0){
					this.fn_print2();
				}
				else{					
					//this.alert("조회된 데이터가 없습니다.");
					this.gfn_message("info.조회.데이터.없음");
				}
				break;
				
			// 주택 결의서/내역서 조회All
			case "selectIncmeDecsnListAll" :
				//1.결의서 조회
				if(this.ds_houseIncmeDecsnList.getRowCount() > 0){
					this.fn_printAll();
				}
				else{					
					//this.alert("조회된 데이터가 없습니다.");
					this.gfn_message("info.조회.데이터.없음");
				}
				break;	
				
			case "searchDecsn" :
					this.ds_cond05.clearData();
					var nRow = this.ds_cond05.addRow();
					this.ds_cond05.setColumn( nRow , "JOB_SE" , "RNT");
					this.ds_cond05.setColumn( nRow , "INNER_SANCTN_ID" , this.ds_sanction.getColumn(this.ds_sanction.rowposition, "INNER_SANCTN_NO"));
					this.fn_transaction("sanknKnd");
				break;					
		}
		
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}

