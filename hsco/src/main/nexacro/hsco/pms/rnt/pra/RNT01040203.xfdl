	var ETC2 = "기타 문의사항이 있으시면 350-0301(0303)으로 연락해주세요.";
	var ETC3 = "화성도시공사 주거복지처";
	FILE_NM = RENT_HOUSE_NM +DONG + "동" + HO + "호" + NM + yyyymm.substr(0,6) + ".txt";
	
	this.ds_file.clearData();
	this.ds_file.addRow();	
	this.ds_file.setColumn(this.ds_file.rowposition, "TITLE", TITLE);
