 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
		case "rentMtRntchrgList":		//임대료 목록
			var sController   	= "/hsco/pms/rnt/pra/RNT01040203/rentMtRntchrgList.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentMtRntchrgList=output1";
			break;
			
		case "rentMtRntchrgU":		//연체이자 생성
			var sController   	= "/hsco/pms/rnt/pra/RNT01040203/rentMtRntchrgU.do"; 
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "";
			break;
			
		case "rentMtRntNhtSheetInfo":	//고지서내역
			var sController   	= "/hsco/pms/rnt/pra/RNT01040203/rentMtRntNhtSheetInfo.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_rentMtRntNhtSinfo=output1";
			break;			
			
			//영구임대 월임대료(저장,수정,삭제)
		case "saveRentRntchrgCUD":		
			var sController   	= "hsco/pms/rnt/pra/RNT01040203/saveRentRntchrgCUD.do"; 
			var sInDatasets   	= "input1=ds_rentMtRntchrgList:U";
			var sOutDatasets  	= "";
			break;				
			
		case "downloadFile":
			var sController   	= "/hsco/pms/rnt/pra/RNT01040203/downloadFile.do";
			var sInDatasets   	= "input1=ds_file";
			var sOutDatasets  	= "";
			break;		
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	if(strSvcId == "downloadFile"){
		this.FILE_MANAGER.dataFileDownload(sController, sInDatasets, FILE_NM);
	}else{
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
	}
	
	//Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}


