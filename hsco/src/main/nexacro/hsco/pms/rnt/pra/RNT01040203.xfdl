 /*
 * 함수명  	: fn_insert
 * 설명 		: 신규
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
 this.fn_insert = function(){
	if(this.ds_rentCntrctList.rowcount == 0){
		this.alert("선택된 계약자가 없습니다.")
	}else{
		this.ds_rentMtRntchrgList.addRow();
		var nRow = this.ds_rentCntrctDetail.rowposition;
		this.ds_rentMtRntchrgList.setColumn(this.ds_rentMtRntchrgList.rowposition, "CNTRCTR_NO" ,this.ds_rentCntrctDetail.getColumn(nRow,"CNTRCTR_NO"));
		
		this.ds_rentMtRntchrgList.setColumn(this.ds_rentMtRntchrgList.rowposition, "MDAT_YM" ,this.dateUtils.today());
		this.ds_rentMtRntchrgList.setColumn(this.ds_rentMtRntchrgList.rowposition, "MDAT_DE" ,this.dateUtils.today());
		this.ds_rentMtRntchrgList.setColumn(this.ds_rentMtRntchrgList.rowposition, "PAY_TMLMT" ,this.gfn_lastDateNum(this.dateUtils.today()));
	}
 }

