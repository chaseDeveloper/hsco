 * params  		: None
 * return Type 	: None
 */
this.fn_set_searchValue = function(){
	
	this.inSchJobSe = this.div_search.cmb_jobSe.value;
	this.inSchPaySe = this.div_search.cmb_paySe.value;
	this.inSchDefraytYm = this.div_search.div_monCal.getValue("YYYYMM");
