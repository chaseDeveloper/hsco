	var nRow =  this.DS_EMP_COPY.addRow();	         
	var bSucc;
    bSucc = this.DS_EMP_COPY.copyRow(nRow,this.DS_EMP,this.DS_EMP.rowposition);
    	
	this.close(this.DS_EMP_COPY.saveXML());
}


