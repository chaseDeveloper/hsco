    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : PMS000004
 * 화면명   : 자동이체정보관리
 * 화면설명 : 자동이체정보관리
 * 작성일   : 2015.12.16
 * 작성자   : 김지훈
 * 수정이력 :
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
include "pms_lib::rntUtilInclude.xjs";
include "lib::comInclude.xjs";	// 시스템 공통 라이브러리 Include
var today = this.dateUtils.today();
this.JOB_SE = "";


this.PMS000004_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init();
	this.fn_init_dataset();//Dataset 초기화
}

this.PMS000004_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.fn_load_combo();
	
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

/*
 * 함수명  	: fn_load_combo
 * 설명 		: 화면의 Combo 에 데이터를 불러온다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_loadCombo();
 */
this.fn_load_combo = function ()
{
	var comboParams = [
			["ds_jobSeCode", "PCOM06", "1", ""],
			["ds_paySeCode", "PCOM05", "1", ""]
		];
	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);	
}

/*
 * 함수명  	: fn_setPaySeCodeNm // fn_setJobSeCodeNm
 * 설명 		: 콤보박스 모양 처리 ALL_CODE_NM = [CODE]CODE_NM  
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_setPaySeCodeNm = function(){
	var rowSize = this.ds_paySeCode.getRowCount();
	for(var i=0; i<rowSize; i++){
		var getCode = this.ds_paySeCode.getColumn(i, "CODE");
		var getCodeNm = this.ds_paySeCode.getColumn(i, "CODE_NM");
		var setLocalCodeNm = "[" + getCode + "]" + getCodeNm;
		this.ds_paySeCode.setColumn(i, "ALL_CODE_NM", setLocalCodeNm);
	}
}
this.fn_setJobSeCodeNm = function(){
	var rowSize = this.ds_jobSeCode.getRowCount();
	
	for(var i=0; i<rowSize; i++){
		var getCode = this.ds_jobSeCode.getColumn(i, "CODE");
		var getCodeNm = this.ds_jobSeCode.getColumn(i, "CODE_NM");
		var setLocalCodeNm = "[" + getCode + "]" + getCodeNm;
		this.ds_jobSeCode.setColumn(i, "ALL_CODE_NM", setLocalCodeNm);
	}
}

/*
 * 함수명  	: fn_callbackAfter
 * 설명 		: callback 처리 후에 더 필요한 작업이 있을 경우, 작성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
 */
 
this.fn_callbackAfter = function(){
	this.ds_tmpPaySeCode.copyData(this.ds_paySeCode);
	this.ds_jobSeCode.deleteRow(0);							// 첫번째행인 토지row 삭제(자동이체 없음)
	this.fn_setPaySeCodeNm();
	this.fn_setJobSeCodeNm();
	
	
	// 호출한 메뉴(화면)명에 따라 조회조건을 세팅한다.
	var strMenuId = application.gv_curFormId.split("_")[1];
	this.ds_cond02.clearData();
	this.ds_cond02.addRow();
	switch (strMenuId) {
		case "302060204" :	//주택
			this.ds_jobSeCode.filter("CODE == 'HOU'");
			this.ds_paySeCode.filter("VAL0 == 'HOU'");			
			this.div_search.cmb_jobSe.set_index(0);
			this.JOB_SE = "HOU";
			break;
		case "403050404" :	//영구
			this.ds_jobSeCode.filter("CODE == 'RN1'");		
			this.ds_paySeCode.filter("VAL0 == 'RN1'");
			this.div_search.cmb_jobSe.set_index(0);			
			this.JOB_SE = "RN1";
			break;
		case "2101040404" :	//매입
			this.ds_jobSeCode.filter("CODE == 'RN2'");		
			this.ds_paySeCode.filter("VAL0 == 'RN2'");
			this.div_search.cmb_jobSe.set_index(0);
			this.JOB_SE = "RN2";
			break;
		case "1902040300" :	//전세
			this.ds_jobSeCode.filter("CODE == 'RN3'");
			this.ds_paySeCode.filter("VAL0 == 'RN3'");
			this.div_search.cmb_jobSe.set_index(0);
			this.JOB_SE = "RN3";
			break;			
	}
	
	this.ds_cond02.setColumn(0, "JOB_SE", this.JOB_SE);
	this.fn_transaction("selectHouseCodeList");
}

/*
 * 함수명  	: fn_search / fn_save / fn_insert / fn_delete / fn_cancel
 * 설명 		: [조회]   / [저장]  / [신규]   / [삭제]    / [취소] 버튼 클릭 EVENT 메소드
 * params  		: None
 * return Type 	: None
 * 사용법		: 
 */
this.fn_search = function(){

	this.ds_rentCntrctList.clearData();
	var inSchJobSe = this.div_search.cmb_jobSe.value;
	var inSchHouseCode = this.div_search.cmb_houseCode.value;
	var inSchDpstr = this.div_search.edt_dpstr.value;
	var inSchNm = this.div_search.edt_nm.value;
	var inSchCntrctSe = this.div_search.rdo_paymthd.value;
	var dong = this.div_search.edt_dong.value;
	var ho = this.div_search.edt_ho.value;
	
	this.ds_cond00.clearData();
	this.ds_cond00.addRow();
	this.ds_cond00.setColumn(0, "JOB_SE", inSchJobSe);
	this.ds_cond00.setColumn(0, "HOUSE_CODE", inSchHouseCode);
	this.ds_cond00.setColumn(0, "DPSTR", inSchDpstr);
	this.ds_cond00.setColumn(0, "NM", inSchNm);
	this.ds_cond00.setColumn(0, "CNTRCT_SE", inSchCntrctSe);
	this.ds_cond00.setColumn(0, "DONG", dong);
	this.ds_cond00.setColumn(0, "HO", ho);
	
	this.fn_transaction("rentCntrctList");
	this.fn_insertInfoSetReadonly(true);
}

this.fn_save = function(){
	if(this.div_WORK.cmb_status.value == 2){
	
		if(!this.div_WORK.edt_contNm10.value || !this.div_WORK.edt_contNm11.value || !this.div_WORK.cmb_paySe00.value || !this.div_WORK.cal_decsnDe.value ){
			this.alert("필수값을 확인하세요");
			return;
		} 
// 해지일자 null 체크로 신규/해지를 구분하기 때문에 해지일자 확인
	} else if(this.div_WORK.cmb_status.value == 9) {	
	    if(!this.div_WORK.edt_contNm10.value || !this.div_WORK.edt_contNm11.value || !this.div_WORK.cmb_paySe00.value || !this.div_WORK.cal_decsnDe.value || !this.div_WORK.cal_decsnDe00.value  ){
			this.alert("필수값을 확인하세요");
			return;
		}
	} else {
		this.alert("필수값을 확인하세요");
			return;
	}
	
//	if(this.ds_atcngInfoList.isUpdated() == true){	
		if(this.confirm("저장 하시겠습니까?")){
			this.fn_transaction("saveAtcngInfoList");
		}
//	} else {
//		alert("변경내역이 없습니다.");
//	}
	this.fn_insertInfoSetReadonly(true);
}

this.fn_delete = function(){
     if(application.confirm("삭제하시겠습니까?")){
		
		this.ds_atcngInfoList.deleteRow(this.ds_atcngInfoList.rowposition);
	 } else {
		this.fn_transaction("atcngInfoList");
		this.fn_transaction("atcngRqestRcivInfoList");
     }
	  this.fn_insertInfoSetReadonly(true);
}
this.fn_insert = function(){
    var nRow = 0;
    nRow = this.ds_atcngInfoList.addRow();
    this.ds_atcngInfoList.setColumn(nRow ,"ATCNG_REQST_DE",today);
    this.ds_atcngInfoList.setColumn(nRow ,"DPSTR",this.ds_rentCntrctList.getColumn(this.ds_rentCntrctList.rowposition,"NM"));
    this.ds_atcngInfoList.setColumn(nRow ,"JOB_SE",this.JOB_SE); 
    this.ds_atcngInfoList.setColumn(nRow ,"CNTRCTR_NO",this.ds_rentCntrctList.getColumn(this.ds_rentCntrctList.rowposition,"CNTRCTR_NO"));
	this.fn_insertInfoSetReadonly(false);
}

this.fn_cancel = function() {
	this.fn_transaction("atcngInfoList");
	this.fn_transaction("atcngRqestRcivInfoList");
	this.fn_insertInfoSetReadonly(true);
}

/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {			
		case "rentCntrctList":		//자동이체 주택정보 조회
			var sController   	= "/hsco/pms/com/PMS000004/selectRentCntrctList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_rentCntrctList=output1";
			break;
			
		case "atcngInfoList":		//자동이체 정보 조회
			var sController   	= "/hsco/pms/com/PMS000004/selectAtcngInfoList.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_atcngInfoList=output1";
			break;
		case "atcngRqestRcivInfoList" : // 자동이체 출금결과 조회
			var sController   	= "/hsco/pms/com/PMS000004/selectAtcngRqestRcivInfoList.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_atcngRqestRcivInfoList=output1";
			break;
		case "saveAtcngInfoList":		//자동이체 정보 저장
			var sController   	= "/hsco/pms/com/PMS000004/saveAtcngInfoList.do"; 
			var sInDatasets   	= "input1=ds_atcngInfoList:U";
			var sOutDatasets  	= "";
			break;
			
		case "selectHouseCodeList" :
			var sController		= "/hsco/pms/com/PMS000004/selectHouseCodeList.do";
			var sInDatasets		= "input1=ds_cond02";
			var sOutDatasets	= "ds_houseNmCode=output1";
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch (strSvcId) {			
		case "rentCntrctList":	
		
		break;
		case "selectHouseCodeList":
			this.fn_add_comboHeader(this, this.div_search.cmb_houseCode, "전체");
		break;
		case "saveAtcngInfoList":
			this.fn_transaction("atcngInfoList");
			this.fn_transaction("atcngRqestRcivInfoList");
			
		break;
		case "atcngInfoList":
			this.fn_insertInfoSetCssClass();
		break;
	
		}
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}

/*
 * 함수명  	: this.ds_rentCntrctList_onrowposchanged
 * 설명 		: [주택정보내역] 사용자선택에 따른 [자동이체정보][자동이체출금내역] 정보 조회
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */


this.ds_rentCntrctList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	this.ds_atcngInfoList.clearData();
	this.ds_cond01.setColumn(0,"CNTRCTR_NO",obj.getColumn(e.newrow,"CNTRCTR_NO"));
	this.ds_cond01.setColumn(0,"JOB_SE",this.div_search.cmb_jobSe.value);
	this.ds_cond01.setColumn(0,"DEFRAY_DE",today);
	this.fn_transaction("atcngInfoList");
    this.fn_transaction("atcngRqestRcivInfoList");
    this.fn_insertInfoSetReadonly(true);
}

/*
 * 함수명  	: this.fn_insertInfoSetCssClass 
 * 설명 		: 상태구분(cmb_status) 값 변화에 따른 이벤트 메소드 / fn_insertInfoSetCssClass 메소드 호출 
 * params  		: 
 * return Type 	: None
 * 사용법		: 
 */

this.div_WORK_cmb_status_canitemchange = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.fn_insertInfoSetCssClass();
}

/*
 * 함수명  	: this.fn_insertInfoSetCssClass 
 * 설명 		: 상태구분(cmb_status) 값에 따른 해지일자 필수표시CSS 적용
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_insertInfoSetCssClass(); 
 */
this.fn_insertInfoSetCssClass = function() {
	
	if(this.div_WORK.cmb_status.value == 9){
		this.ds_atcngInfoList.setColumn(0, "ATCNG_CANCL_DE", today);
		this.div_WORK.cal_decsnDe00.set_cssclass("cal_WF_Essential");
		this.div_WORK.cal_decsnDe00.set_enable(true);
	} else if(this.div_WORK.cmb_status.value == 2) {
		this.ds_atcngInfoList.setColumn(0, "ATCNG_CANCL_DE", "");
		this.div_WORK.cal_decsnDe.set_cssclass("cal_WF_Essential");
		this.div_WORK.cal_decsnDe00.set_cssclass("");
		this.div_WORK.cal_decsnDe00.set_enable(false);	// 신규입력일때 해지일자 입력을 막아줘야함
	}
}

/*
 * 함수명  	: this.fn_insertInfoSetReadonly
 * 설명 		: 신규 외에 자동이체정보 사항 수정 불가 설정
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_insertInfoSetReadonly(boolean); 
 */

this.fn_insertInfoSetReadonly = function(bReadOnly){
	this.div_WORK.cal_decsnDe.set_readonly(bReadOnly);
	this.div_WORK.edt_contNm10.set_readonly(bReadOnly);
	this.div_WORK.edt_contNm11.set_readonly(bReadOnly);
	this.div_WORK.cmb_paySe00.set_readonly(bReadOnly);
}]]></Script>
