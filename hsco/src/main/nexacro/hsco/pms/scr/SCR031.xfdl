	this.div_신혼부부.set_left(this.div_고령자.left);
	this.div_주거급여수급자.set_left(this.div_고령자.left);
}


this.div_search_Combo01_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
