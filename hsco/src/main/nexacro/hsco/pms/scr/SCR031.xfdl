	this.div_사회초년생.set_visible(false);
	this.div_신혼부부.set_visible(false);
	this.div_주거급여수급자.set_visible(false);
	
	this.div_사회초년생.set_top(this.div_고령자.top);
	this.div_신혼부부.set_top(this.div_고령자.top);
