			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_psncpa7=output5";
			break;
		case "selectPsncpa5List": 	
			 var sController   	= "hsco/eis/eis/EIS030103/selectPsncpa5List.do";
			 var sInDatasets   	= "input1=ds_cond00";
			 var sOutDatasets  	= "ds_psncpa9=output6";
			break;
		case "selectPsncpa6List": 
			var sController   	= "hsco/eis/eis/EIS030103/selectPsncpa6List.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_psncpa10=output7";
			break;	
