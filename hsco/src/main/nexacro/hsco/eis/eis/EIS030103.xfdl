			  this.ds_psncpa8.setColumn(nRow,"A4",this.ds_psncpa9.getColumn(0,"A4"));  
			  this.ds_psncpa8.setColumn(nRow,"A5",this.ds_psncpa9.getColumn(0,"A5"));
			  this.ds_psncpa8.setColumn(nRow,"A6",this.ds_psncpa9.getColumn(0,"A6"));  
