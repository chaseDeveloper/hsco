			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_psncpa3=output1";
			break;
	    case "selectPsncpa3List": 
			var sController   	= "hsco/eis/eis/EIS030103/selectPsncpa3List.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_psncpa5=output3 ds_psncpa6=output4";
			break;
		case "selectPsncpa4List": 
