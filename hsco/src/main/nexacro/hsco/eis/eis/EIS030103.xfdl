this.fn_dynamicCol2 = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.tab_hrm.tabpage2.grd_psncpa3.set_enableredraw(false);

	// 직종부서별
	var jssfcDept_cellCnt = this.tab_hrm.tabpage2.grd_psncpa3.getCellCount("Head");
	
	// 삭제
	for(var i=jssfcDept_cellCnt; i>0; i--) {
		this.tab_hrm.tabpage2.grd_psncpa3.deleteContentsCol(i-1);
	}
	this.tab_hrm.tabpage2.grd_psncpa3.deleteContentsRow("Head", 0);
	this.tab_hrm.tabpage2.grd_psncpa3.deleteContentsRow("Head", 0);
	this.tab_hrm.tabpage2.grd_psncpa3.deleteContentsRow("Body", 0);
	
	// 생성
	this.tab_hrm.tabpage2.grd_psncpa3.appendContentsRow("Head");
	this.tab_hrm.tabpage2.grd_psncpa3.appendContentsRow("Head");
	this.tab_hrm.tabpage2.grd_psncpa3.appendContentsRow("Body");
	this.tab_hrm.tabpage2.grd_psncpa3.appendContentsCol("Head");
	this.tab_hrm.tabpage2.grd_psncpa3.appendContentsCol("Head");
	
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 0, "text", "bind:DEPT_LV1_NM");
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 1, "text", "bind:DEPT_LV2_NM");
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 2, "text", "bind:DEPT_LV3_NM");
	
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 2, "line",       "1 solid #ddddddff,1 solid #808080ff");
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 2, "selectline", "1 solid #efececff,1 solid #808080ff");
	
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 0 ,"align", "left");
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 1 ,"align", "left");
	this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", 2 ,"align", "left");
	
	this.tab_hrm.tabpage2.grd_psncpa3.setFormatColProperty("0", "size", 100);
	this.tab_hrm.tabpage2.grd_psncpa3.setFormatColProperty("1", "size", 100);
	this.tab_hrm.tabpage2.grd_psncpa3.setFormatColProperty("2", "size", 100);
	
	for(var i=0; i<(this.ds_jssfc.getRowCount()); i++) {

		this.tab_hrm.tabpage2.grd_psncpa3.appendContentsCol();
		this.tab_hrm.tabpage2.grd_psncpa3.appendContentsCol();
		this.tab_hrm.tabpage2.grd_psncpa3.appendContentsCol();
		
		// Head - 부서
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Head", 0, "text", "부서");
		this.tab_hrm.tabpage2.grd_psncpa3.mergeContentsCell("Head", 0, 0, 1, 2, 0, false);

		// Head - 직종
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Head", 3+i, "text", this.ds_jssfc.getColumn(i, "CODE_NM"));
		this.tab_hrm.tabpage2.grd_psncpa3.mergeContentsCell("Head", 0, (i*3)+3, 0, (i*3)+5, 3+i, false);
		
		// Head - 정, 현, 결
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Head", (i*4)+2, "text", "정");
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Head", (i*4)+3, "text", "현");
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Head", (i*4)+4, "text", "결");

		// Body 
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", (i*3)+3, "text", "bind:PSNCPA_"  +this.ds_jssfc.getColumn(i, "CODE"));
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", (i*3)+4, "text", "bind:NOW_NMPR_"+this.ds_jssfc.getColumn(i, "CODE"));
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", (i*3)+5, "text", "expr:nexacro.toNumber(PSNCPA_"+this.ds_jssfc.getColumn(i, "CODE")+")-nexacro.toNumber(NOW_NMPR_"+this.ds_jssfc.getColumn(i, "CODE")+")");
		
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", (i*3)+5, "line",       "1 solid #ddddddff,1 solid #808080ff");
		this.tab_hrm.tabpage2.grd_psncpa3.setCellProperty("Body", (i*3)+5, "selectline", "1 solid #efececff,1 solid #808080ff");
	} 
	
	this.tab_hrm.tabpage2.grd_psncpa3.set_enableredraw(true);
}



this.fn_dynamicCol1 = function (strSvcId, nErrorCode, strErrorMsg)
{
	this.tab_hrm.tabpage1.grd_psncpa.set_enableredraw(false);
	
	// 직종직급별
	var hrClsf_cellCnt = this.tab_hrm.tabpage1.grd_psncpa.getCellCount("Head");
	
	// 삭제
	for(var i=hrClsf_cellCnt; i>0; i--) {
		this.tab_hrm.tabpage1.grd_psncpa.deleteContentsCol(i-1);
	}
	this.tab_hrm.tabpage1.grd_psncpa.deleteContentsRow("Head", 0);
	this.tab_hrm.tabpage1.grd_psncpa.deleteContentsRow("Head", 0);
	this.tab_hrm.tabpage1.grd_psncpa.deleteContentsRow("Body", 0);
	
	// 생성
	this.tab_hrm.tabpage1.grd_psncpa.appendContentsRow("Head");
	this.tab_hrm.tabpage1.grd_psncpa.appendContentsRow("Head");
	this.tab_hrm.tabpage1.grd_psncpa.appendContentsRow("Body");
	
	this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", 0, "text", "bind:JSSFC_NM");
	this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", 0 ,"align", "left");
	this.tab_hrm.tabpage1.grd_psncpa.setFormatColProperty("0", "size", 100);

	for(var i=0; i<(this.ds_hrClsf.getRowCount()); i++) {

		this.tab_hrm.tabpage1.grd_psncpa.appendContentsCol();
		this.tab_hrm.tabpage1.grd_psncpa.appendContentsCol();
		this.tab_hrm.tabpage1.grd_psncpa.appendContentsCol();
		
		// Head - 직종
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Head", 0, "text", "직종");
		this.tab_hrm.tabpage1.grd_psncpa.mergeContentsCell("Head", 0, 0, 1, 0, 0, false);

		// Head - 직급
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Head", 1+i, "text", this.ds_hrClsf.getColumn(i, "CODE_NM"));
		this.tab_hrm.tabpage1.grd_psncpa.mergeContentsCell("Head", 0, (i*3)+1, 0, (i*3)+3, 1+i, false);
		
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", 0, "line",       "1 solid #ddddddff,1 solid #808080ff");
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", 0, "selectline", "1 solid #efececff,1 solid #808080ff");
		
		// Head - 정, 현, 결
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Head", (i*4)+2, "text", "정");
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Head", (i*4)+3, "text", "현");
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Head", (i*4)+4, "text", "결");
	
		// Body 
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", (i*3)+1, "text", "bind:PSNCPA_"  +this.ds_hrClsf.getColumn(i, "CODE"));
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", (i*3)+2, "text", "bind:NOW_NMPR_"+this.ds_hrClsf.getColumn(i, "CODE"));
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", (i*3)+3, "text", "expr:nexacro.toNumber(PSNCPA_"+this.ds_hrClsf.getColumn(i, "CODE")+")-nexacro.toNumber(NOW_NMPR_"+this.ds_hrClsf.getColumn(i, "CODE")+")");
		
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", (i*3)+3, "line",       "1 solid #ddddddff,1 solid #808080ff");
		this.tab_hrm.tabpage1.grd_psncpa.setCellProperty("Body", (i*3)+3, "selectline", "1 solid #efececff,1 solid #808080ff");
	} 
	
	this.tab_hrm.tabpage1.grd_psncpa.set_enableredraw(true);
}
