this.EIS030103_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	trace("onload");
	this.gfn_initForm(this);	    
	this.gfn_initCondDs(this, this.div_search);	

	
	var comboParams = [
		["ds_jssfc", "HRM001", 1, ""],   // 직종
		["ds_clsf_se", "HRM002", 1, ""],   // 직급
 	    ["ds_grpSe", "HRM268", "1", ""], 	     // (그룹)구분 = 정원구분
	    ["ds_gnfd", "HRM012", "1", ""],   // 발령 
	    ["ds_hrClsf" , "HRM002", 1, ""]  	  	// 직급
	];
	
	var callback = function (svcId, errorCode, errorMsg) {
		var nRow = this.ds_jssfc.addRow();
		this.fn_year_cmb();
		this.fn_deptcode();
		this.fn_search();
	}
	 
	this.gfn_comboLoad(comboParams, callback);	
}

