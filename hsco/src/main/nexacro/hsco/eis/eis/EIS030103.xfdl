/***********************************************************************************
* 설명	: 탭변경 
***********************************************************************************/
this.tab_hrm_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	if(this.tab_hrm.tabindex == 0){
		if(this.ds_psncpa.rowcount < 1){
			this.fn_psncpa();  // 직종별 정현원
		}
	} else if(this.tab_hrm.tabindex == 1){
		if(this.ds_psncpa3.rowcount < 1){
			this.fn_psncpa_2();                    // 부서별 정현원
		}
	} else if(this.tab_hrm.tabindex == 2){
		if(this.ds_psncpa5.rowcount < 1){
			this.ds_psncpa5.clearData();
			this.ds_psncpa6.clearData();
			this.ds_psncpa7.clearData();
			this.fn_psncpa_3();                     // 직급별인력
		}
	} else if(this.tab_hrm.tabindex == 3){
		if(this.ds_psncpa8.rowcount < 1){
			this.ds_psncpa8.clearData();
			this.ds_psncpa10.clearData();
			this.fn_psncpa_4();                     // 퇴직자
		}
	} else if(this.tab_hrm.tabindex == 4){
		if(this.ds_psncpa11.rowcount < 1){
			this.ds_psncpa11.clearData();
			this.ds_psncpa12.clearData();
			this.ds_psncpa13.clearData();
			this.fn_psncpa_5();                     // 임금피크대상자
		}
	} else if(this.tab_hrm.tabindex == 5){
		if(this.ds_psncpa14.rowcount < 1){
			this.ds_psncpa14.clearData();	   
			this.fn_psncpa_6();                     // 역대임원현황
		}
	}
	
}

/***********************************************************************************
* 설명	: 그리드 합계( 직급별인력)
***********************************************************************************/
this.fn_sum = function(row)
{
	var sum = parseInt(this.ds_psncpa6.getColumn(row,"A0")+this.ds_psncpa6.getColumn(row,"A1")+this.ds_psncpa6.getColumn(row,"A2")+this.ds_psncpa6.getColumn(row,"A3")+
	          this.ds_psncpa6.getColumn(row,"A4")+this.ds_psncpa6.getColumn(row,"A5")+this.ds_psncpa6.getColumn(row,"A6"));
	var s = 0;
	for(var i=0; i<= 6; i++){	
	     if(this.ds_psncpa6.getColumn(row,"A"+i) > 0){
	           s = s +1;
	     }
	}
	sum = Math.round(sum/s);
	
	return sum;
}

