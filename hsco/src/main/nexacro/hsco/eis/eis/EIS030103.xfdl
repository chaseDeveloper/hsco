			  this.ds_psncpa8.setColumn(nRow,"A2",0);  this.ds_psncpa8.setColumn(nRow,"A3",0);
			  this.ds_psncpa8.setColumn(nRow,"A4",0);  this.ds_psncpa8.setColumn(nRow,"A5",0);
			  this.ds_psncpa8.setColumn(nRow,"A6",0);  
		  }	 
		  if(this.ds_psncpa9.getColumn(1,"YEAR")) {
			  this.ds_psncpa8.setColumn(nRow,"GUBUN2","충원");
			  this.ds_psncpa8.setColumn(nRow,"A20",this.ds_psncpa9.getColumn(1,"A0"));  this.ds_psncpa8.setColumn(nRow,"A21",this.ds_psncpa9.getColumn(1,"A1"));
			  this.ds_psncpa8.setColumn(nRow,"A22",this.ds_psncpa9.getColumn(1,"A2"));  this.ds_psncpa8.setColumn(nRow,"A23",this.ds_psncpa9.getColumn(1,"A3"));
			  this.ds_psncpa8.setColumn(nRow,"A24",this.ds_psncpa9.getColumn(1,"A4"));  this.ds_psncpa8.setColumn(nRow,"A25",this.ds_psncpa9.getColumn(1,"A5"));
