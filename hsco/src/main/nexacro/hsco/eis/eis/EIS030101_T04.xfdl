this.fn_tab_cond = function(sArgs)
{
	var paramsArr = new Array();

	for(var i in sArgs){
		paramsArr[i] = sArgs[i];
	}

	this.ds_cond.clearData();
	this.ds_cond.addRow();
	this.ds_cond.setColumn(0, "STDR_YEAR", paramsArr[0]);
}

