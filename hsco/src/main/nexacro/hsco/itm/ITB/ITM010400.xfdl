}

// 사용자명 에디트 onKeyDown EVENT
this.div_SEARCH_edt_ENTRPS_EMP_NM_onkeydown = function(obj:Edit, e:nexacro.KeyEventInfo)
{
	if (e.keycode == 13) {
		// 엔터키 입력시 자동 조회
		this.fn_search();
	}
}

