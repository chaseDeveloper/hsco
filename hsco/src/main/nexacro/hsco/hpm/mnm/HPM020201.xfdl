 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function(){
	this.ds_menuList.clearData();
	this.fn_transaction("selectMenuListEng");
}

/*
 * 함수명  	: fn_menuComboList
 * 설명 		: 메뉴그룹조회
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_menuComboList = function(){
	this.ds_menuListCombo.clearData();
	this.fn_transaction("selectMenuComboListEng");
}



/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
 //   if(this.utlf_necessaryParam(this.div_work.grd_userListMst,"USER_ID,USER_NM")) return; // 유효성체크
 
	 if(this.gfn_message("comm.행위여부","저장"))
	 { 
	     this.fn_transaction("saveMenuListEng"); // 메뉴정보저장
	        
	 }else{
	 
	    return;
	 }           
}

/*
 * 함수명  	: fn_init_dataset
 * 설명 		: Dataset을 초기화한다.
 *                주로, 조건 Dataset들(ds_cond, ds_cond_00, ds_cond_01, ...) 을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initDataset();
 */
this.fn_init_dataset = function()
{
	this.gfn_initCondDs(this, this.div_search);
}

// 취소
this.fn_Cancel = function()
{
	this.ds_menuList.reset();
}

// 추가
this.fn_insert = function ()
{
	var nCurRowMenu = this.ds_menuList.rowposition;
	
	var nRowMenu = this.ds_menuList.insertRow(nCurRowMenu + 1);
			
	if (nCurRowMenu == -1) 
	{
		return;
	}
	
	this.ds_menuList.setColumn(nRowMenu,"VIEW_YN","0");
	this.ds_menuList.setColumn(nRowMenu,"LOGIN_YN","0");
	this.ds_menuList.setColumn(nRowMenu,"USE_YN","0");
	
	var menu_lvl = Number(this.ds_menuList.getColumn(nCurRowMenu, "MENU_DEPTH"));
	
	if( menu_lvl == null || menu_lvl == "undefined") {
		menu_lvl = 0;
	}
	
	switch (this.Div00.cmb_Level.value) 
	{
		case "1":	// 하위레벨
			this.ds_menuList.setColumn(nRowMenu, "MENU_DEPTH", menu_lvl + 1);			
			break;
		case "2":	// 등위레벨
			this.ds_menuList.setColumn(nRowMenu, "MENU_DEPTH", menu_lvl);
			break;
		default:
			break;
	}

	var nUpperRow = this.Div00.grd_menuList.getTreeParentRow(nRowMenu, true);
	
	var nTopRow = this.fn_getTopRow(nRowMenu);
	
	if (nTopRow > -1) 
	{
		if(this.ds_menuList.getColumn(nRowMenu, "MENU_DEPTH") == "1")
		{
			this.ds_menuList.setColumn(nRowMenu, "SORT_NUM", 1);
			this.ds_menuList.setColumn(nRowMenu, "PARENT_IDX", 0);
			this.ds_menuList.setColumn(nRowMenu, "MENU_IDX", "자동채번");
			this.ds_menuList.setColumn(nRowMenu, "PARENT_MENU_NAME", "ROOT");
		}
		else {
			this.ds_menuList.setColumn(nRowMenu, "MENU_IDX", "자동채번");
			this.ds_menuList.setColumn(nRowMenu, "SORT_NUM", Number(this.ds_menuList.getColumn(nCurRowMenu,"SORT_NUM")) + 1);
		    this.ds_menuList.setColumn(nRowMenu, "PARENT_IDX", this.ds_menuList.getColumn(nUpperRow, "MENU_IDX"));
		    this.ds_menuList.setColumn(nRowMenu, "PARENT_MENU_NAME", this.ds_menuList.getColumn(nUpperRow, "MENU_NAME"));		
		}
	}
}

this.fn_getTopRow = function(rowIndex)
{

	var menu_lvl = this.ds_menuList.getColumn(rowIndex, "MENU_DEPTH");
	
	if( menu_lvl == null || menu_lvl == "undefined" || menu_lvl <= 1 )
		return rowIndex;
		
	var nRow = this.Div00.grd_menuList.getTreeParentRow(rowIndex, true);
	
	if(nRow < 0 ) {
		return nRow;
	}
		
	return this.fn_getTopRow(nRow);
}


// 메뉴 삭제
this.fn_delete = function ()
{
	var nRowMenu = this.ds_menuList.rowposition;
	this.ds_menuList.deleteRow(nRowMenu);

}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	switch (strSvcId) {		
		
	    case "selectMenuListEng": //메뉴목록조회
			var sController   	= "/hsco/hpm/mnm/HPM020101/selectMenuListEng.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_menuList=output1";
			break; 
		
		case "saveMenuListEng":		//메뉴정보저장
			var sController   	= "/hsco/hpm/mnm/HPM020101/mainEngCUD.do"; 
			var sInDatasets   	= "input1=ds_menuList:U";
			var sOutDatasets  	= "";
			break; 
       
        case "selectMenuComboListEng": //메뉴목록조회
			var sController   	= "/hsco/hpm/mnm/HPM020101/selectMenuComboListEng.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_menuListCombo=output1";
			break; 	
			
			
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}



/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode < 0)	{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
		return;
	}
	
	switch(strSvcId){
		case "saveMenuListEng" :
			  this.gfn_message("success.처리.성공"); 
			  //this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg);		  
			  this.fn_search();
			  break;
		 case "selectMenuComboListEng" :
			  this.div_search.cmb_menuCombo.set_index(0);
			  break;
	}
	
}


this.btn_Insert_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_insert();
}

this.btn_Cancel_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.fn_Cancel();
}

