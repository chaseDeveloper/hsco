***********************************************************************/

this.grd_lguplusStoreManageList_onexpanddown = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	this.div_deptSearch_onclick();
}

/**********************************************************************
* 공통함수 처리
***********************************************************************/

/**********************************************************************
* Tab 관련 이벤트 처리
***********************************************************************/

/**********************************************************************
