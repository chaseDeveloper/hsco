/*
 * 함수명  	: fn_search
 * 설명 		: 조회 버튼 클릭시 이벤트 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_search();
 */
this.fn_search = function()
{
     if(Ex.util.isUpdated(this.ds_rentFcltsManageList)) {
         if(!this.gfn_message("confirm.변경.취소.여부")) {
             return false;    // 아니오
         }
     }

	// dataset 초기화
	this.ds_rentFcltsManageList.clearData();
	
	//validation 체크
    if(!this.gfn_checkValidation_NoUpdChk(this.ds_cond, this.ds_validation)) {
        return;
    }

	this.fn_transaction("selectRentFcltsManageList");
}


/*
 * 함수명  	: fn_save
 * 설명 		: 저장 버튼 클릭시 이벤트 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function()
{
    // 데이터셋 미변경 체크
    if (!this.comUtils.isDatasetUpdated(this.ds_rentFcltsManageList)) {
        this.gfn_message("comm.데이터.변경.없음")
        return false;
    }
    
        //validation 체크
    if(!this.gfn_checkValidation_NoUpdChk(this.ds_rentFcltsManageList, this.ds_validation)) {
        return;
    }

    if(this.gfn_message("confirm.저장.여부")) {
        this.saveRow = this.ds_rentFcltsManageList.rowposition;
		this.fn_transaction("saveRentFcltsManageList");  
    }
}


/*
 * 함수명  	: fn_insert
 * 설명 		: 신규 버튼 클릭시 이벤트 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_insert();
 */
this.fn_insert = function() {
	var nRow = this.ds_rentFcltsManageList.addRow();
	
	var deptCode = this.ds_cond.getColumn(0, "DEPT_CODE");
	var deptNm   = this.ds_cond.getColumn(0, "DEPT_NM");
	var sortOrdr = !this.comUtils.isNull(this.ds_rentFcltsManageList.getMax("SORT_ORDR")) ? this.ds_rentFcltsManageList.getMax("SORT_ORDR") : 0;
	var hmpgExpsrSortOrdr = !this.comUtils.isNull(this.ds_rentFcltsManageList.getMax("HMPG_EXPSR_SORT_ORDR")) ? this.ds_rentFcltsManageList.getMax("HMPG_EXPSR_SORT_ORDR") : 0;

	this.ds_rentFcltsManageList.setColumn(nRow, "DEPT_CODE", deptCode);
	this.ds_rentFcltsManageList.setColumn(nRow, "DEPT_NM"  , deptNm);
	this.ds_rentFcltsManageList.setColumn(nRow, "INNER_AT", 0); 	// 내부(입주사)여부
	
	// NULL 값 셋팅 
	this.ds_rentFcltsManageList.setColumn(nRow, "WKDAY_RESVE_POSBL_BEGIN_TIME", null);	// 사용시작시각
	this.ds_rentFcltsManageList.setColumn(nRow, "WKDAY_RESVE_POSBL_END_TIME", null);	// 사용종료시각
	this.ds_rentFcltsManageList.setColumn(nRow, "BIZPLC_CODE", "9999");	// 사업소
	
	this.ds_rentFcltsManageList.setColumn(nRow, "SORT_ORDR", nexacro.toNumber(sortOrdr)+1);	// 정렬순서
	this.ds_rentFcltsManageList.setColumn(nRow, "HMPG_EXPSR_SORT_ORDR", nexacro.toNumber(hmpgExpsrSortOrdr)+1);	// 정렬순서
	
	var codeVal = this.ds_beginTimeSe.getColumn(0, "CODE");
	var dataVal = this.ds_rentFcltsManageList.getColumn(nRow, "WKDAY_RESVE_POSBL_END_TIME");

}


/*
 * 함수명  	: fn_delete
 * 설명 		: 삭제 버튼 클릭시 이벤트 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_delete();
 */
this.fn_delete = function() {
	
   if (this.gfn_message("confirm.삭제여부")) {
        if(this.ds_rentFcltsManageList.getRowCount() > 0) {
			var cRow = this.ds_rentFcltsManageList.rowposition;
			this.ds_rentFcltsManageList.deleteRow(cRow);
        }
    }
}


/*
 * 함수명  	: fn_cancel
 * 설명 		: 취소 버튼 클릭시 이벤트 발생
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_cancel();
 */
this.fn_cancel = function() {

     // 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.ds_rentFcltsManageList)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }

    if (this.gfn_message("confirm.취소여부")) {
        this.ds_rentFcltsManageList.reset();
        this.ds_rentFcltsManageList.applyChange();
    }

}
/**********************************************************************
* call Transaction 영역
***********************************************************************/

/*
 * 함수명      : fn_transaction
 * 설명         : Transaction 요청 처리 함수
 * params          : strSvcId : 서비스요청 ID
 * return Type     : None
 * 사용법        : this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
    switch (strSvcId) {
        // 대관시설 목록 조회
        case "selectRentFcltsManageList":
            var sController       = "hsco/hpm/ctm/HPM060101/selectRentFcltsManageList.do";
            var sInDatasets       = "input1=ds_cond";
            var sOutDatasets      = "ds_rentFcltsManageList=output1";
            break;
        //  대관시설 목록 저장
        case "saveRentFcltsManageList":
            var sController       = "hsco/hpm/ctm/HPM060101/saveRentFcltsManageList.do";
            var sInDatasets       = "input1=ds_rentFcltsManageList:U";
            var sOutDatasets      = "";
            break;
        //  대관시설 이관
        case "trnsferFcltsInfoList":
            var sController       = "hsco/hpm/ctm/HPM060101/trnsferFcltsInfoList.do";
            var sInDatasets       = "input1=ds_cond";
            var sOutDatasets      = "";
            break;
        //  사업소 조회
        case "selectBizplcCode":
			var sController   	= "/hsco/hpm/usm/HPM010203/selectBizplcList.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_bizplcList_s=output1";
			break;
        // ======== 첨부파일 관련 transaction ========
        // 첨부파일 조회
        case "selectAtchFile":
            var sController  = "/hsco/cmm/file/selectAtchmnFileList.do";
            var sInDatasets  = "input1=ds_fileCond";
            var sOutDatasets = "ds_fileList=output1";
            break;
        // 첨부파일 삭제
        case "deleteAtchFile":
            var sController     = "hsco/cmm/file/deleteFile.do";
            var sInDatasets     = "input1=ds_fileList:U";
            var sOutDatasets    = "ds_fileList=output1";
            break;
    }

    Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명      : fn_callBack
 * 설명         : Transaction Callback 처리
 * params          : strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type     : None
 * 사용법        :
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
    if(nErrorCode != '0') {
        this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
    }

    if (nErrorCode == '0'){
    switch (strSvcId) {
        // 대관시설 목록 조회
        case "selectRentFcltsManageList":
             if(this.saveRow != -1) {
                 this.ds_rentFcltsManageList.set_rowposition(this.saveRow);
                 this.saveRow = -1;
             }
              
             if(this.ds_rentFcltsManageList.getRowCount() == 0) {
				this.div_fcltsInfo.btn_fileUpload.set_enable(false);
				this.div_fcltsInfo.btn_fileDelete.set_enable(false);
				this.div_hscFcltsInfo.btn_fileUpload.set_enable(false);
				this.div_hscFcltsInfo.btn_fileDelete.set_enable(false);
             }else {
  				this.div_fcltsInfo.btn_fileUpload.set_enable(true);
				this.div_fcltsInfo.btn_fileDelete.set_enable(true); 
				this.div_hscFcltsInfo.btn_fileUpload.set_enable(true);
				this.div_hscFcltsInfo.btn_fileDelete.set_enable(true);
             }
             
			var deptCode = this.ds_cond.getColumn(0, "DEPT_CODE");   
			// 경기타운인지 아닌지에 따라 우측 시설상세정보 항목이 달라진다. 
			if(deptCode == "040100") {
				// 상세정보 위치에 div 두개를 위치 시킨다.
				this.div_hscFcltsInfo.set_top(this.div_fcltsInfo.top);
				this.div_hscFcltsInfo.set_right(this.div_fcltsInfo.right);
				this.div_fcltsInfo.set_visible(false);
				this.div_hscFcltsInfo.set_visible(true);
	
			}else {
				this.div_hscFcltsInfo.set_visible(false);		
				this.div_fcltsInfo.set_visible(true);
					
			}
			
            break;
        //  대관시설 목록 저장
         case "saveRentFcltsManageList":
             this.fn_search();
             break;
        //  대관시설 이관
         case "trnsferFcltsInfoList":
			 this.gfn_message("success.처리.성공"); 
			 this.fn_search();
             break;
        //  사업소 조회
        case "selectBizplcCode":
			trace("xx");
			if(this.ds_bizplcList_s.rowcount < 1) {
				this.ds_bizplcList_s.insertRow(0);
				this.ds_bizplcList_s.setColumn(0, "BIZPLC_CODE", "9999");
				this.ds_bizplcList_s.setColumn(0, "BIZPLC_NM"  , "해당없음");
			}
			this.ds_bizplcList.copyData(this.ds_bizplcList_s);
			this.div_search.cmb_bizplcList.set_index(0);
			
			break;
        // ======== 첨부파일 관련 transaction ========
        // 첨부파일 조회
        case "selectAtchFile":
			// 첨부파일(사진) 표시 실행
			if(this.ds_fileList.rowcount > 0){
				this.fn_photo_view();
			}else {
				this.div_fcltsInfo.img_photo.set_image("");
				this.div_hscFcltsInfo.img_photo.set_image("");
			}
            break;
        // 첨부파일 삭제
        case "deleteAtchFile":
				this.div_fcltsInfo.img_photo.set_image("");
				this.div_hscFcltsInfo.img_photo.set_image("");
            break;
        }
    }
}

