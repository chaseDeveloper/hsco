/*
 * 함수명  	: fn_popBoardInfoCallBack
 * 설명 		: 게시글조회 팝업 콜백 처리
 * params  		: sPopupId, sReturn
 * return Type 	: None
 * 사용법		: this.fn_popBoardInfoCallBack(sPopupId, sReturn)
 */
this.fn_popBoardInfoCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "popBoard_mainInfo")
	{
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
        if(loadCnt) {
			var cRow = this.ds_rentFcltsManageList.rowposition;
			this.ds_rentFcltsManageList.setColumn(cRow, "MAIN_INFO"  , dsTemp.getColumn(0, "BOARD_IDX"));
			this.ds_rentFcltsManageList.setColumn(cRow, "MAIN_INFO_TITLE", dsTemp.getColumn(0, "BOARD_TITLE"));
        }
	}
	
	if(sPopupId == "popBoard_atpn")
	{
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
        if(loadCnt) {
			var cRow = this.ds_rentFcltsManageList.rowposition;
			this.ds_rentFcltsManageList.setColumn(cRow, "ATPN"  , dsTemp.getColumn(0, "BOARD_IDX"));
			this.ds_rentFcltsManageList.setColumn(cRow, "ATPN_TITLE", dsTemp.getColumn(0, "BOARD_TITLE"));
        }
	}
}
