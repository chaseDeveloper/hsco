				, this.ds_rentFcltsManageList	    	// fileSn을 불러오거나 반영해야 하는 컬럼
				, "TBHPM_RENT_FCLTS_MANAGE"				// fileSn을 저장할 테이블
				, "FILE_SN"								// fileSn 컬럼명이 다를 경우를 대비
				, ["DEPT_CODE", "FCLTS_SN"]				// fileSn을 저장할 테이블의 PK (FileSn 확인 및 자동반영목적)
			);
		}
		
		this.gfn_message("info.저장.초과", "이미지 파일만", null, null, mCallback);
		//this.mCallback();
	} else {
		this.gfn_message("info.저장.초과", "최대 1개 까지만");
	}
}


