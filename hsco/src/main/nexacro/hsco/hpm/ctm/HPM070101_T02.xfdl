this.fn_popMberInfoCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "popMberInfo")
	{
		var dsTemp = new Dataset();
		var loadCnt = dsTemp.loadXML(sReturn);
        if(loadCnt) {
			var cRow = this.parent.ds_rentReqstManageList.rowposition;
			this.parent.ds_rentReqstManageList.setColumn(cRow, "MBER_NM", dsTemp.getColumn(0, "MBER_NM"));
			this.parent.ds_rentReqstManageList.setColumn(cRow, "GRP_NM" , dsTemp.getColumn(0, "GRP_NM"));
			this.parent.ds_rentReqstManageList.setColumn(cRow, "ADRES"  , dsTemp.getColumn(0, "ADRES"));
			this.parent.ds_rentReqstManageList.setColumn(cRow, "TELNO"  , dsTemp.getColumn(0, "TELNO"));
			this.parent.ds_rentReqstManageList.setColumn(cRow, "MBTLNUM", dsTemp.getColumn(0, "MBTLNUM"));
        }
	}
}


this.fn_changeObjectProperty = function(cRow) {
	var cRow = this.parent.ds_rentReqstManageList.rowposition;
	var rowType      = this.parent.ds_rentReqstManageList.getRowType(cRow);
	var setleSttus   = this.parent.ds_rentReqstManageList.getOrgColumn(cRow, "SETLE_STTUS");
	var setleComptDe = this.parent.ds_rentReqstManageList.getOrgColumn(cRow, "SETLE_COMPT_DE");
	var useTime      = this.parent.ds_rentReqstManageList.getColumn(cRow, "USE_TIME");
	var setleAt      = this.parent.ds_rentReqstManageList.getColumn(cRow, "SETLE_AT");
	var rentCanclAt = this.parent.ds_rentReqstManageList.getColumn(cRow, "RENT_CANCL_AT");
	var deleteAt = this.parent.ds_rentReqstManageList.getColumn(cRow, "DELETE_AT");
	var bankNm = !this.comUtils.isNull(this.parent.ds_rentReqstManageList.getColumn(cRow, "REFND_BANK_NM")) ? "1" : "0";
	var bankCode = !this.comUtils.isNull(this.parent.ds_rentReqstManageList.getColumn(cRow, "REFND_BANK_CODE")) ? "1" : "0";
	
	switch(setleSttus) {
	case "006" :
		this.tab_detailInfo.tpg_refndInfo.cmb_refndBackCode.set_readonly(false);		// 환불은행코드
		this.tab_detailInfo.tpg_refndInfo.cal_refndReqstDt.set_readonly(false);			// 환불요청일자
		this.tab_detailInfo.tpg_refndInfo.edt_refndAcuntno.set_readonly(false);			// 환불계좌번호
		this.tab_detailInfo.tpg_refndInfo.rdo_indvdlCprSe.set_readonly(false);			// 개인/법인 구분
		this.tab_detailInfo.tpg_refndInfo.edt_refndDpstr.set_readonly(false);			// 예금주명
		this.tab_detailInfo.tpg_refndInfo.edt_refndGrpCprNm.set_readonly(false);		// 단체/법인명
		this.tab_detailInfo.tpg_refndInfo.msk_refndAm.set_readonly(false);				// 환불금액
	
		this.tab_detailInfo.tpg_refndInfo.edt_refndBackNm.set_cssclass("");		// 환불은행명(기존)
		this.tab_detailInfo.tpg_refndInfo.cmb_refndBackCode.set_cssclass("");	// 환불은행코드
		this.tab_detailInfo.tpg_refndInfo.cal_refndReqstDt.set_cssclass("");	// 환불요청일자
		this.tab_detailInfo.tpg_refndInfo.edt_refndAcuntno.set_cssclass("");	// 환불계좌번호
		this.tab_detailInfo.tpg_refndInfo.rdo_indvdlCprSe.set_cssclass("");		// 개인/법인 구분
		this.tab_detailInfo.tpg_refndInfo.edt_refndDpstr.set_cssclass("");		// 예금주명
		this.tab_detailInfo.tpg_refndInfo.edt_refndGrpCprNm.set_cssclass("");	// 단체/법인명
		this.tab_detailInfo.tpg_refndInfo.msk_refndAm.set_cssclass("");			// 환불금액
		break;
	case "007" :
		this.tab_detailInfo.tpg_refndInfo.edt_refndBackNm.set_readonly(true);		// 환불은행명(기존)
		this.tab_detailInfo.tpg_refndInfo.cmb_refndBackCode.set_readonly(true);		// 환불은행코드
			
		this.tab_detailInfo.tpg_refndInfo.cal_refndReqstDt.set_readonly(true);		// 환불요청일자
		this.tab_detailInfo.tpg_refndInfo.edt_refndAcuntno.set_readonly(true);		// 환불계좌번호
		this.tab_detailInfo.tpg_refndInfo.rdo_indvdlCprSe.set_readonly(true);		// 개인/법인 구분
		this.tab_detailInfo.tpg_refndInfo.edt_refndDpstr.set_readonly(true);		// 예금주명
		this.tab_detailInfo.tpg_refndInfo.edt_refndGrpCprNm.set_readonly(true);		// 단체/법인명
		this.tab_detailInfo.tpg_refndInfo.msk_refndAm.set_readonly(true);			// 환불금액
		
		this.tab_detailInfo.tpg_refndInfo.edt_refndBackNm.set_cssclass("edt_WF_Readonly");		// 환불은행명(기존)
		this.tab_detailInfo.tpg_refndInfo.cmb_refndBackCode.set_cssclass("cmb_WF_Readonly");	// 환불은행코드
		this.tab_detailInfo.tpg_refndInfo.cal_refndReqstDt.set_cssclass("cal_WF_Readonly");		// 환불요청일자
		this.tab_detailInfo.tpg_refndInfo.edt_refndAcuntno.set_cssclass("edt_WF_Readonly");		// 환불계좌번호
		this.tab_detailInfo.tpg_refndInfo.rdo_indvdlCprSe.set_cssclass("rdo_WF_Readonly");		// 개인/법인 구분
		this.tab_detailInfo.tpg_refndInfo.edt_refndDpstr.set_cssclass("edt_WF_Readonly");		// 예금주명
		this.tab_detailInfo.tpg_refndInfo.edt_refndGrpCprNm.set_cssclass("edt_WF_Readonly");	// 단체/법인명
		this.tab_detailInfo.tpg_refndInfo.msk_refndAm.set_cssclass("msk_WF_Readonly");			// 환불금액
		break;
	}
	
	// 환불은행 명/코드 (이관데이터 때문에)
	if(bankCode == "0" && bankNm == "1") {	// 텍스트
		this.tab_detailInfo.tpg_refndInfo.edt_refndBackNm.set_visible(true);		// 환불은행명(기존)
		this.tab_detailInfo.tpg_refndInfo.cmb_refndBackCode.set_visible(false);		// 환불은행코드
	}else {	 // 코드
		this.tab_detailInfo.tpg_refndInfo.edt_refndBackNm.set_visible(false);		// 환불은행명(기존)
		this.tab_detailInfo.tpg_refndInfo.cmb_refndBackCode.set_visible(true);		// 환불은행코드
	}
	

	// 대관취소, 삭제 일 때는 대관취소 버튼이 보이지 않음!!
	if(rentCanclAt == '1' || deleteAt == '1') {
		this.tab_detailInfo.tpg_reqstInfo.btn_reqstCancl.set_visible(false);
	}else {
		this.tab_detailInfo.tpg_reqstInfo.btn_reqstCancl.set_visible(true);
	}
}


/*
* 함수명 		: btn_setleInfoCancel_onclick
* 설명  		: 결제정보취소 이벤트 처리
* params   		: None
* return Type   : None
* 사용법   	: this.btn_setleInfoCancel_onclick();
*/
this.btn_setleInfoCancel_onclick= function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(application.confirm("결제정보를 취소 처리하시겠습니까?")) {
	
		var cRow = this.parent.ds_rentReqstManageList.rowposition;
		var mngrRegistAt = this.parent.ds_rentReqstManageList.getColumn(cRow, "MNGR_REGIST_AT");
		
		// 신청정보
		this.ds_rentReqstManage.clearData();
		this.ds_rentReqstManage.addRow();
		this.ds_rentReqstManage.copyRow(0, this.parent.ds_rentReqstManageList, cRow);

		// 진행상태 변경
		if(mngrRegistAt == '0') {
			//결제완료 -> 입금대기
			this.ds_rentReqstManage.setColumn(0, "SETLE_STTUS", "004");
		}else {
			//결제완료 -> 심사완료
			this.ds_rentReqstManage.setColumn(0, "SETLE_STTUS", "003");
		}
		
		this.fn_transaction("cancelRcpmnyDtlsMapping");
	}
}

/*
* 함수명 		: tab_detailInfo_tpg_reqstInfo_btn_reqstCancl_onclick
* 설명  		: 대관취소
* params   		: obj, e
* return Type   : None
* 사용법   	: this.tab_detailInfo_tpg_reqstInfo_btn_reqstCancl_onclick(obj:Button,  e:nexacro.ClickEventInfo)
*/
this.tab_detailInfo_tpg_reqstInfo_btn_reqstCancl_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 데이터셋 변경 체크
    if(Ex.util.isUpdated(this.parent.ds_rentReqstManageList)) {
        if(!this.gfn_message("confirm.변경.취소.여부")) {
            return false;
        }
    }
    
	if(application.confirm("대관을 취소하시겠습니까?")) {
		var cRow = this.parent.ds_rentReqstManageList.rowposition;
		this.parent.ds_rentReqstManageList.setColumn(cRow, "RENT_CANCL_AT", 1);
		this.parent.fn_transaction("saveRentReqstManageList");
	}
}


