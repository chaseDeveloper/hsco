this.btn_link_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var ym = this.ds_cond.getColumn(0, "DE");
	
	var param = "srchYm="+ym;
	var url = "http://yeyak.hsuco.or.kr/hstown/sisulDeInfo.do";
	url += ("?"+param);
	
	system.execBrowser(url);
}
