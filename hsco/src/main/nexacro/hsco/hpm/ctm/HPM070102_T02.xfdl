/*
 * 함수명		: fn_transaction
 * 설명			: Transaction 요청 처리 함수
 * params		: strSvcId : 서비스요청 ID
 * return Type	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
    switch (strSvcId) {
        //  결제정보취소
        case "cancelRcpmnyDtlsMapping":
            var sController       = "hsco/hpm/ctm/HPM070102/cancelRcpmnyDtlsMapping.do";
            var sInDatasets       = "input1=ds_rentProgrmReqstManage:U";
            var sOutDatasets      = "";
            break;
    }

    Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets);
}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명		: fn_callBack
 * 설명			: Transaction Callback 처리
 * params 		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
    if(nErrorCode != '0') {
        this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
    }

    if (nErrorCode == '0'){
    switch (strSvcId) {
		  //  결제정보취소
			case "cancelRcpmnyDtlsMapping":
				this.parent.fn_search();
				this.gfn_message("success.처리.성공"); 
			 break;
		}
    }
}


this.fn_changeObjectProperty = function(cRow) {
	
	var rowType      = this.parent.ds_rentProgrmReqstManageList.getRowType(cRow);
	var setleSttus   = this.parent.ds_rentProgrmReqstManageList.getOrgColumn(cRow, "SETLE_STTUS");
	var setleComptDe = this.parent.ds_rentProgrmReqstManageList.getOrgColumn(cRow, "SETLE_COMPT_DE");
	var useTime      = this.parent.ds_rentProgrmReqstManageList.getColumn(cRow, "USE_TIME");
	var setleAt      = this.parent.ds_rentProgrmReqstManageList.getColumn(cRow, "SETLE_AT");
	var reqstCanclAt = this.parent.ds_rentProgrmReqstManageList.getColumn(cRow, "REQST_CANCL_AT");
	var deleteAt = this.parent.ds_rentProgrmReqstManageList.getColumn(cRow, "DELETE_AT");
		
	// 환불정보 수정조건
	// 조건 -	01. 결제상태 : 심사완료, 결제완료일자 NOT NULL
	//   		02. 결제상태 : 환불요청
	
	var check  = (((setleSttus == "003" && !this.comUtils.isNull(setleComptDe)) || setleSttus == "006") ? true : false);
	var css   = ((check == true) ? "" : "WF_Readonly");
	

	this.tab_detailInfo.tpg_refndInfo.edt_refndBackNm.set_visible(!check);		// 환불은행명(기존)
	this.tab_detailInfo.tpg_refndInfo.cmb_refndBackCode.set_visible(check);		// 환불은행코드
	this.tab_detailInfo.tpg_refndInfo.cal_refndReqstDt.set_readonly(!check);	// 환불요청일자
	this.tab_detailInfo.tpg_refndInfo.edt_refndAcuntno.set_readonly(!check);	// 환불계좌번호
	this.tab_detailInfo.tpg_refndInfo.edt_refndDpstr.set_readonly(!check);		// 예금주명
	
		// 신청취소, 삭제 일 때는 대관취소 버튼이 보이지 않음!!
	if(reqstCanclAt == '1' || deleteAt == '1') {
		this.tab_detailInfo.tpg_reqstInfo.btn_reqstCancl.set_visible(false);
	}else {
		this.tab_detailInfo.tpg_reqstInfo.btn_reqstCancl.set_visible(true);
	}
	
	this.tab_detailInfo.tpg_refndInfo.cal_refndReqstDt.set_cssclass("cal_"+css);
	this.tab_detailInfo.tpg_refndInfo.edt_refndAcuntno.set_cssclass("edt_"+css);
	this.tab_detailInfo.tpg_refndInfo.edt_refndDpstr.set_cssclass("edt_"+css);

}


