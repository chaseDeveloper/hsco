			 * this.viewer.toolbarComboClickTime+50);
			 */
			for (var i = 1; i <= this.viewer.totalPage; i++) {
				this.viewer.drawPage(this.viewer, i);
			}

			this.page = currpage;

			if (this.viewer.params.flicking == "true") {
				/*
				 * for (var i=1; i<=this.viewer.totalPage; i++) {
				 * this.viewer.drawPage(this.viewer, i); }
				 */
				this.viewer.flicker.OnChangeScale();
