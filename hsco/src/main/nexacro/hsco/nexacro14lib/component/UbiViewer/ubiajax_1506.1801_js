	setScaleState : function() {
		var scaleindex = -1;
		for (var i=0; i<this.scaleValues.length; i++) {
			if (this.scaleValues[i] == this.currScale) {
				scaleindex = i;
				break;
			}
		}

		if (scaleindex == 0) {

			this.toolbarobj.disableItem("zoomout");
		} else {

			this.toolbarobj.enableItem("zoomout");
		}

		if (scaleindex == (this.scaleCount - 1)) {

