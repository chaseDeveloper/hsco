                div.id = item.id;
                divid.appendChild(div);
                div.style.position = "absolute";
                div.style.top = y + "px";
                div.style.left = x + "px";
                div.style.width = ((width == 0) ? getLineThickness(doc, item) : (width + 1)) + "px";
