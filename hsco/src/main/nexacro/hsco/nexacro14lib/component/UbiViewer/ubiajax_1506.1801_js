	this.removeOption = function(id) {
		if (id == "pages") {
			var mylistbox = document.getElementById(this.vid + 'UbiToolbarSelect_Page');
			while (mylistbox.length > 0) {
				mylistbox.remove(0);
			}
		}

		if (id == "zoom") {
			var mylistbox = document.getElementById(this.vid + 'UbiToolbarSelect_Zoom');
			while (mylistbox.length > 0) {
				mylistbox.remove(0);
			}
		}
	};

