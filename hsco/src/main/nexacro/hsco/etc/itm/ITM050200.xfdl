 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode < 0) 
	{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
		return ;
	}
	
	switch (strSvcId) 
	{
	    case "selectListCombo":
	        this.fn_add_comboHeader(this, this.div_search.cmb_objTy, "전체");	 
	      //  this.div_search.cmb_objTy.set_index(0);
		case "saveList":
			this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);

			break;	
		case "saveDetailList":
			this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);

			break;
		
		case "deleteDetailList":
			this.fn_transaction('selectJob');

			break;
		default:
			break;
	}
}

/*
 * 함수명  	: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		: 
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	if(sPopupId == "Popup1") {
		var ds_Code = new Dataset();
		var loadCnt = ds_Code.loadXML(sReturn);
       
		if(loadCnt) {
			var Svc_Rcept_No   = ds_Code.getColumn(0, "SVC_RCEPT_NO");
