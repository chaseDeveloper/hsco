			var sController		= "/hsco/etc/itm/ITM050200/ITM050200DetailList.do";
			var sInDatasets		= "input1=ds_condDetail";
			var sOutDatasets	= "ds_detail=output1";			
			//trace(this.ds_condDetail.saveXML());
			break;			
		case "saveDetailList": // 
			var sController   	= "/hsco/etc/itm/ITM050200/ITM050200DetailCU.do";	
			var s  = "input1=ds_detail:U";
			var sInDatasets   	= s;
			var sOutDatasets  	= "";
						
			break;	
					
		case "deleteDetailList" :		// 선택 토지 상세정보 삭제
		
			this.ds_cond00.clearData();
			var nRow = this.ds_cond00.addRow();
					
			this.ds_cond00.setColumn(0,"CHANGE_ATCHMN_FLTPH",this.ds_detail.getColumn(this.ds_detail.rowposition,"CHANGE_ATCHMN_FLTPH"));
			this.ds_cond00.setColumn(0,"SN",this.ds_detail.getColumn(this.ds_detail.rowposition,"SN"));
			this.ds_cond00.setColumn(0,"OBJ_ID",this.ds_detail.getColumn(this.ds_detail.rowposition,"OBJ_ID"));
			this.ds_cond00.setColumn(0,"FILE_SN",this.ds_detail.getColumn(this.ds_detail.rowposition,"CHANGE_ATCHMN_FLTPH"));
				
			trace(this.ds_cond00.saveXML());
			
			var sController   	= "/hsco/etc/itm/ITM050200/ITM050200DetailD.do";	
			var s  = "input1=ds_cond00";
				  //+ " input2=ds_rnet_ph:U"  //다중저장할때
				  //+ " input3=ds_cocnr:U";	  //다중저장할때
				  
			var sInDatasets   	= s;
			var sOutDatasets  	= "";
			break;
			
		case "deleteFile" :	// 파일정보 삭제
			var selRowItem = this.pmsUtil.getGridSelectedItem(this.grd_detail00);
			this.SELECTED_LAD_DETAIL_INFO = selRowItem;	
			
			reqParam = this.SELECTED_LAD_DETAIL_INFO;
