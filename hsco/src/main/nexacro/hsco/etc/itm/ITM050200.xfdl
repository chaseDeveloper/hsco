			var sController		= "/hsco/etc/itm/ITM050200/ITM050200ListCombo.do";
			var sInDatasets		= "input1=ds_condCombo";
			var sOutDatasets	= "ds_objty_1=output1";
			this.ds_condCombo.setColumn(0,"GRP_CODE","ITMA05");
			this.ds_condCombo.setColumn(0,"USE_AT","1");
			this.ds_condCombo.setColumn(0,"CHRCTR_PREPAR_VALUE_0","OBJECT");	
			//this.ds_condCombo.setColumn(0,"ALL_FLAG","");
						
			break;					
		case "selectList": // 
		    var sController		= "/hsco/etc/itm/ITM050200/ITM050200List.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_grid=output1";
			break;
		case "selectJob": // 
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListJob.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_job=output1";
			break;			
		case "saveList": // 
			var sController   	= "/hsco/etc/itm/ITM050200/ITM050200CUD.do";	
			var s  = "input1=ds_grid:U";
			var sInDatasets   	= s;
			var sOutDatasets  	= "";
			break;	
		case "selectDetailList": // 
