this.btn_Save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	// 파일 첨부여부 확인
	var hasAttFile = this.FILE_MANAGER.hasFile();
	if(hasAttFile){
		var attFileCount = this.FILE_MANAGER.getfileCount();
		trace("첨부파일 있음!" + attFileCount);
		var sController = "/hsco/etc/itm/ITM050200/ITM050200DetailCU.do";
		var oInDatasets = "input1=ds_detail:U";
		var sOutDatasets = "ds_failList=failList";
		
		this.FILE_MANAGER.upload(sController, oInDatasets, sOutDatasets);
	}else{
	
		if (!this.comUtils.isDatasetUpdated(this.ds_detail)) {
		alert("저장할 내역이 없습니다.");
		return false;
		}
		if ( application.confirm( "변경된 내용을 저장하시겠습니까?" ) )
		{	
			for (var i = 0; i < this.ds_detail.getRowCount(); i++) {
				var temp = this.ds_detail.getColumn(i, 'OBJ_ID');
				
				var col_Check;
				if(temp=="undefined"||temp==undefined){
					col_Check = 'Y';
				}
				
				if(col_Check == 'Y'){
					alert(i+"열에 처리구분 값이 없습니다. \n 값입력 후 저장하세요.");
					return false;
				}
			}
		
			this.fn_transaction("saveDetailList");
		}
	
	}
}

