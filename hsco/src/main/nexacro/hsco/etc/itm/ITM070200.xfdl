this.Tab00_onchanged = function(obj:Tab, e:nexacro.TabIndexChangeEventInfo)
{
	this.ds_cond.setColumn(0,"S_SLA_RESULT_YM",this.div_search.cal_stdrYm.getValue());
	
	var tabid = this.Tab00.tabpages[this.Tab00.tabindex].name;
	switch(tabid)
	{
		case 'tabpage1':		// 평가결과표
          this.fn_search();			 
			break;
		case 'tabpage2':		// 서비스시간
		  this.fn_search_T01();
			break;
		case 'tabpage3':		// 장애현황
		  this.fn_search_T02();
			break;
		case 'tabpage4':		// SR현황
		  this.fn_search_T03();
			break;
		case 'tabpage5':		// Call처리율
		  this.fn_search_T04();
			break;
		case 'tabpage6':		// 백업준수
		  this.fn_search_T05();
			break;
		case 'tabpage7':		// 보안준수
		  this.fn_search_T06();
			break;
	}
}


