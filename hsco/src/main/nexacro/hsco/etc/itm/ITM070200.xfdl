this.Tab00_tabpage1_btn_conc_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_grid.getRowCount() < 1){
		alert('조회된 내역이 없습니다.');
		return false;
	}
	
	this.ds_cond.addRow();
	
	var sSvcID			= "ITM070200.ITM070200CUD";
	var sController		= "/hsco/etc/itm/ITM070200/ITM070200CUD.do";
	var s  = "input1=ds_cond:U";		  
	var sInDatasets   	= s;
	var sOutDatasets  	= "";
	
	this.ds_cond.setColumn(0,"S_SLA_RESULT_YM",this.div_search.cal_stdrYm.getValue());
	this.ds_cond.setColumn(0,"SLA_DCSN_ENNC","1");
	
	Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, "");//, sCallbackFunc, sShowProgress, bAsync, bBinary);
	
	this.fn_search();	
