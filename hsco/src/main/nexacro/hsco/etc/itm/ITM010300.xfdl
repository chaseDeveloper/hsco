			this.ds_grid.setColumn(bmRow, "DETAIL_ADRES", roadAddrPart2);
			trace("ds_grid = " + this.ds_grid.saveXML());
		}
	}
	this.gfn_popup('zipPopup', 500, 500, '', null, "common::frmZipAddressPopup.xfdl", fn_callback);		
