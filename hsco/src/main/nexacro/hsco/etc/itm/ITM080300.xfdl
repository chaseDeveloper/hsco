this.fn_save = function()
{
	// 데이터셋 수정 여부체크
	if (!this.comUtils.isDatasetUpdated(this.ds_grid)) {
		alert("저장할 내역이 없습니다.");
		return false;
	}
	if ( application.confirm( "변경된 내용을 저장하시겠습니까?" ) )
