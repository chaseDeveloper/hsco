		case "1": //미처리
			this.ds_cond.setColumn(0,"PROCESS_ST","0");
			//this.ds_cond.setColumn(0,"PROCESS_ST_F","1");		
			break;
		case "2": //처리
			this.ds_cond.setColumn(0,"PROCESS_ST","1");
			//this.ds_cond.setColumn(0,"PROCESS_ST_F","");
			break;
	}
	
	this.ds_cond.setColumn(0, "SVC_RCEPT_DT_F", this.div_search.cal_rceptDeBegin.value);	// 
 	this.ds_cond.setColumn(0, "SVC_RCEPT_DT_T", this.div_search.cal_rceptDeEnd.value);	// 
 	 	
 	this.ds_cond.setColumn(0, "IT_CL_1", this.div_search.cmb_itCl.value);	// 
 	this.ds_cond.setColumn(0, "SVC_RCEPTER", this.div_search.edt_empNm.value);	// 
 	
 	this.ds_cond.setColumn(0, "SVC_RCEPT_CN", this.div_search.edt_rceptDtls.value);	// 
 	this.ds_cond.setColumn(0, "SVC_PROCESS_CN", this.div_search.edt_processDtls.value);	// 
 	
	this.fn_transaction("selectList");	
	
}



