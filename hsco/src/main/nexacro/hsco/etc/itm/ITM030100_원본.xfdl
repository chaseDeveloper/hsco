/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectList": // 
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100List.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_grid=output1";
			
			trace("ds_cond =" + this.ds_cond.saveXML());
			
			break;
			
		case "selectListCode": // 
			//var sController		= "/hsco/etc/itm/ITM020300/ITM020300ListCode.do";
			var sController		= "/hsco/etc/itm/ITM030200/ITM030200ListCode.do";			
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_itcl=output1";
			break;
		case "selectJob": // 
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListJob.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_job=output1";
			break;
		case "selectObjId": // 
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListObjId.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_objid=output1";
			break;			
		case "selectItCl_1": // 
			//var sController		= "/hsco/etc/itm/ITM020300/ITM020300ListCode.do";
			var sController		= "/hsco/etc/itm/ITM030200/ITM030200ListCode.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_itcl_1=output1";
			break;
		case "selectItCl_2": // 
			//var sController		= "/hsco/etc/itm/ITM020300/ITM020300ListCodeDetail.do";
			var sController		= "/hsco/etc/itm/ITM030200/ITM030200ListCodeDetail.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_itcl_2=output1";
			break;
		case "selectDept": // 
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListDept.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_dept=output1";
			break;			
		case "selectEmp": // 
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListEmp.do";
			var sInDatasets		= "input1=ds_condDept";
			var sOutDatasets	= "ds_emp=output1";
			break;			
		case "selectEmpConf": // 
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListEmp.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_emp_conf=output1";
			break;	
		case "selectRceptSe": // 접수구분
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListCode.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_rceptse=output1";
			break;
		case "selectChangeIpcr": // 변경중요도
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListCode.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_change=output1";
			break;	
		case "selectSvcTroblGrad": // 장애등급
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100ListCode.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_trobl=output1";
			break;							
		case "selectListCodeDetail": // 
			var sController		= "/hsco/etc/itm/ITM020300/ITM020300ListCodeDetail.do";
			var sInDatasets		= "input1=ds_cond";
			var sOutDatasets	= "ds_comb02=output1";
			
			break;				
		case "saveList": // 
			var sController   	= "/hsco/etc/itm/ITM030100/ITM030100CUD.do";	
			var s  = "input1=ds_grid:U";
			var sInDatasets   	= s;
			var sOutDatasets  	= "";
			
			//trace("ds_grid =" + this.ds_grid.saveXML());
			break;	
		case "selectDetailList": // 
			this.ds_conddetail.clearData();
	        this.ds_conddetail.addRow();
			var sController		= "/hsco/etc/itm/ITM030100/ITM030100DetailList.do";
			var sInDatasets		= "input1=ds_conddetail";
