		if(!ck1) {
			alert("항목은 필수입력 사항입니다.");
			return false;
		}else if(!ck2) {
			alert("날짜는 필수입력 사항입니다.");
			return false;
		}else if(!ck3) {
			alert("지원내용은 필수입력 사항입니다.");
			return false;
		}else {
			return true;
		} 

		return true;
	}else {
		return true;
	}
}


