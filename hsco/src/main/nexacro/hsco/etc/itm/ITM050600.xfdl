/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "selectList": // 메뉴목록
			this.ds_cond.addRow();
			
			var sController   	= "/hsco/etc/itm/ITM050600/ITM050600List.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_grid=output1";
			
			//this.ds_cond.setColumn(0 , "DTA_EXTRC_STDDE_S" , this.div_search00.cal_rceptDeBegin.getValue("YYYYMMDD"));
			//this.ds_cond.setColumn(0 , "DTA_EXTRC_STDDE_E" , this.div_search00.cal_rceptDeBegin.getValue("YYYYMMDD"));
			this.ds_cond.setColumn(0 , "DTA_EXTRC_STDDE_S" , this.div_search00.cal_rceptDeBegin.value);
			this.ds_cond.setColumn(0 , "DTA_EXTRC_STDDE_E" , this.div_search00.cal_rceptDeBegin.value);
			break;
		
		case "saveList": // 개인정보파일대장저장
			trace("ds_grid = " + this.ds_grid.saveXML());
		
			var sController   	= "/hsco/etc/itm/ITM050600/ITM050600CUD.do";	
			var s  = "input1=ds_grid:U";
			var sInDatasets   	= s;
			var sOutDatasets  	= "";
			break;
							
	    case "saveAll": // 개인정보파일대장 자료생성
			var sController   	= "/hsco/etc/itm/ITM050600/ITM050600SaveAll.do";	
			var sInDatasets   	= "input2=ds_condall";
			var sOutDatasets  	= "";
			break;		
	}
			
	Ex.core.tran(this,strSvcId, sController, sInDatasets, sOutDatasets);//, sCallbackFunc, sShowProgress, bAsync, bBinary);
}

 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode < 0) 
	{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
		return ;
	}
	
	switch (strSvcId) 
	{
		case "saveList":
			//this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
			alert('저장이 완료 되었습니다.');
			this.fn_transaction("selectList");			
			break;		
		case "saveAll":
			this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
			this.fn_transaction("selectList");
			break;		
		default:
			break;
	}
}
 
