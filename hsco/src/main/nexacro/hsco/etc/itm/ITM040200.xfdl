			  this.ds_cond_month.setColumn(0,"EQPMN_NO",this.ds_model.getColumn(this.ds_model.rowposition,"EQPMN_NO"));
			  this.ds_cond_month.setColumn(0,"SERVC_ENTRPS_CODE",this.div_search.edt_serv_entrps_code.value);
            }
            
			this.ds_cond_month.setColumn(0,"CHCK_YM",this.div_search.cal_chckMt.getValue());			
			
