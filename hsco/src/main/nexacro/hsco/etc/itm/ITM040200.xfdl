			var sOutDatasets	= "ds_workdetail=output1";
			
			this.ds_cond_work_detail.setColumn(0,"CHCK_YM",this.div_search.cal_chckMt.getValue());
			this.ds_cond_work_detail.setColumn(0,"SERVC_ENTRPS_CODE",this.div_search.edt_serv_entrps_code.value);
			this.ds_cond_work_detail.setColumn(0,"IT_CODE",this.div_search.cmb_itCl.value);
			
			break;
		case "selectMODELCodeList": // 장비목록
		    this.ds_cond_model.clearData();
			var nRow = this.ds_cond_model.addRow();
			
			this.ds_cond_model.setColumn(0,"CHCK_YM",this.div_search.cal_chckMt.getValue());
			this.ds_cond_model.setColumn(0,"SERVC_ENTRPS_CODE",this.div_search.edt_serv_entrps_code.value);
			this.ds_cond_model.setColumn(0,"IT_CODE",this.div_search.cmb_itCl.value);
			this.ds_cond_model.setColumn(0,"IT_DETAIL_CODE",this.div_search.cmb_itDetailCode.value);
	        
