			var sOutDatasets	= "ds_work=output1";
			
			this.div_search.cal_chckMt.Value
			this.ds_cond_work.setColumn(0,"CHCK_YM",this.div_search.cal_chckMt.getValue());
			this.ds_cond_work.setColumn(0,"SERVC_ENTRPS_CODE",this.div_search.edt_serv_entrps_code.value);
			trace('장비구분=='+this.ds_cond_work.saveXML());
			
			break;
			
		case "selectITCodeDetail": // 장비구분상세
		    this.ds_cond_work_detail.clearData();
