			this.ds_cond_copy.clearData();
			this.ds_cond_copy.addRow();
			
			this.ds_cond_copy.setColumn(0,"CHCK_YM",this.div_search.cal_chckMt.getValue());
			this.ds_cond_copy.setColumn(0,"MODEL_NO", this.ds_model.getColumn(this.ds_model.findRow("CODE_NM" , this.div_search.cmb_itDetailDetailCode.text) ,"CODE"));
			this.ds_cond_copy.setColumn(0,"EQPMN_NO",this.ds_model.getColumn(this.ds_model.findRow("CODE_NM" , this.div_search.cmb_itDetailDetailCode.text) ,"EQPMN_NO"));
			
  			var sController   	= "/hsco/etc/itm/ITM040200/ITM040200COPY.do";	
