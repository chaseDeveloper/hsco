 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	this.ds_cond.addRow();
	var bSync = true;
		
	switch (strSvcId) {
	

		case "selectITCodeList": // 장비구분
	 	    this.ds_cond_work.clearData();
