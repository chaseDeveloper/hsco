	else if(e.cell == obj.getBindCellIndex("body", "BTN"))
	{
		if(this.ds_grid.getRowType(this.ds_grid.rowposition) != Dataset.ROWTYPE_NORMAL) return;
		this.fn_addAtchFile();	
	}
}
