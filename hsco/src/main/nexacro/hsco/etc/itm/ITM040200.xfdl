 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode < 0) 
	{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
		return ;
	}
	
	switch (strSvcId) 
	{
	    case "selectList1":
			
            this.fn_Grd_HeadSet();
            
			break;	
		case "selectReport":

			var rptOption = {};
			rptOption["path"] = "/etc/itm/ITM040200_1.jrf";
			rptOption["title"] = "월점검 보고";
			rptOption["filename"] = "월점검 보고";
			rptOption["dataset"] = [["ds_report", this.ds_report],["ds_report_RM", this.ds_report_RM],["ds_report_month", this.ds_report_month]];
			rptOption["arguments"] = 
					
			this.pmsUtil.popupReportView(this, rptOption);	

			break;		
		case "selectITCodeList":
			trace("ds_work =" + this.ds_work.saveXML());
			
			//장비구분
		    this.ds_work_detail.clearData();
			this.fn_add_comboHeader(this, this.div_search.cmb_itCl, "전체");
			this.div_search.cmb_itCl.set_index(0);
			
			//this.fn_transaction("selectITCodeDetail");
			break;
		case "selectITCodeDetail":
			trace("ds_workdetail = " + this.ds_cond_work_detail.saveXML());
		    this.ds_model.clearData();
		    //this.fn_transaction("selectMODELCode");
		    //this.fn_transaction("selectMODELCodeList");
		    
			//this.fn_add_comboHeader(this, this.div_search.cmb_itDetailCode, "전체");
			break;
		case "selectMODELCodeList":
			//this.fn_add_comboHeader(this, this.div_search.cmb_itDetailDetailCode, "전체");	
			trace("ds_model = " + this.ds_cond_model.saveXML());
			break;
		case "selectListCodeEntrPs":
			this.fn_add_comboHeader(this, this.div_search.cmb_entrpsSe, "전체");
			break;
		case "saveList":
			this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
			this.fn_transaction("selectList");

			break;	
		case "saveDetailList":
			this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);

			break;	
		case "copy":
			this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
			this.fn_transaction("selectList");	
			break;
		
		default:
			break;
	}
}

