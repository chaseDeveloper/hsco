			this.ds_cond.setColumn(0,"MODEL_NO", this.ds_model.getColumn(this.ds_model.findRow("CODE_NM" , this.div_search.cmb_itDetailDetailCode.text) ,"CODE"));
			this.ds_cond.setColumn(0,"EQPMN_NO",this.ds_model.getColumn(this.ds_model.findRow("CODE_NM" , this.div_search.cmb_itDetailDetailCode.text) ,"EQPMN_NO"));
			
			break;
		
		case "saveList": // IT분류코드 저장
			var sController   	= "/hsco/etc/itm/ITM040200/ITM040200CUD.do";	
			var s  = "input1=ds_grid:U";				  
			var sInDatasets   	= s;
			var sOutDatasets  	= "";
			
			break;
			
		case "selectList1": // 운영현황표 Master

		    this.ds_cond_month.clearData();
			this.ds_cond_month.addRow();
			var sController		= "/hsco/etc/itm/ITM040200/ITM040200Tab2List.do";
