			var sController		= "/hsco/etc/itm/ITM040200/ITM040200Report.do";
			var sInDatasets		= "input1=ds_cond_month";
			var sOutDatasets	= "ds_report=output1"
			                    + " ds_report_RM=output2"
			                    + " ds_report_month=output3";
			
			var ckAll = this.div_search.CkbAll.value;
            // 전체현황이 보기가 아닌경우 선택된 업체 또는 장비만 조회함
            if (!ckAll) {
              this.ds_cond_month.setColumn(0,"MODEL_NO",this.ds_model.getColumn(this.ds_model.rowposition,"CODE"));
			  this.ds_cond_month.setColumn(0,"EQPMN_NO",this.ds_model.getColumn(this.ds_model.rowposition,"EQPMN_NO"));
			  this.ds_cond_month.setColumn(0,"SERVC_ENTRPS_CODE",this.div_search.edt_serv_entrps_code.value);
            }
            
			this.ds_cond_month.setColumn(0,"CHCK_YM",this.div_search.cal_chckMt.getValue());			
			 
			 
			break;		
		
		case "selectListCodeEntrPs": // 업체목록
			var sController		= "/hsco/etc/itm/ITM020100/ITM020100ListCodeEntrPs.do";
			var sInDatasets		= "";
			var sOutDatasets	= "ds_servc_entrps=output1";
			break;	
			
