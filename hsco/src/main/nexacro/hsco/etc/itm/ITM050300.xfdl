             // this.alert("항목 필수값 누락");
             return;
        }
        
        if(jobSeCode == "" || jobSeCode == null || jobSeCode == undefined){
            
            boolChk = false;
            // this.alert("항목 필수값 누락");
            return;
        }
    }
    return boolChk;
}

