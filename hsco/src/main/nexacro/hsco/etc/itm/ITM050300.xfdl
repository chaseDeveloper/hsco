    } else {
        var sysSeCode = this.ds_fileSn.getColumn(0, "SYS_SE_CODE");
        var jobSeCode = this.ds_fileSn.getColumn(0, "JOB_SE");
        if(sysSeCode == "" || sysSeCode == null || sysSeCode == undefined){
             
