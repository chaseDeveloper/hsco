 */
this.fn_delete = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var tabid = this.Tab00.tabpages[this.Tab00.tabindex].name;
	if(tabid == "tabpage2"){
		alert('운영현황표에선 삭제를 사용할 수 없습니다.');
