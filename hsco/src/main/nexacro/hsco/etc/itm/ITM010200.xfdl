 */
this.fn_loadCombo = function ()
{	
	//분류
	//this.gfn_loadCodeComboWithDs(this, this.div_search.cmb_cl, this.ds_group, "ITM001", "", "N", "전체", "N", "");
	
	this.gfn_initCondDs(this, this.div_search);

