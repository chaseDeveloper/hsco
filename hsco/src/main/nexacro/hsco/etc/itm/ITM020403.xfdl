{
	this.ds_workdetail.filter("IT_CODE == '" + e.postvalue + "'");
	this.fn_add_comboHeader(this, this.div_search.cmb_eqpmnSe, "전체");
	this.div_search.cmb_eqpmnSe.set_index(0);
}


