    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 화면ID   : COM010400_P01
 * 화면명   : 전자결재진행 현황
 * 화면설명 : 전자결재진행 현황을 조회한다.
 * 작성일   : 2015.09.07
 * 작성자   : 정윤원
 * 수정이력 : 
 ***********************************************************************
 *     수정일     수정자   내용
 ***********************************************************************
 *
 ***********************************************************************
 */
 
/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ***********************************************************************/
 
 /***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.COM010400_P01_oninit = function(obj:Form, e:nexacro.InitEventInfo){
	// Form 초기화
	this.gfn_initForm(this);
	// DataSet 초기화
	this.gfn_initCondDs(this, this.div_search);
}

this.COM010400_P01_onload = function(obj:Form, e:nexacro.LoadEventInfo){

	// 차세대 오픈전 데이터는 연계 불가능함
	var registDt = this.parent.arg_2
	if(registDt == undefined || registDt==""){
		this.gfn_message("info.처리불가","업무데이터 등록일시가 입력되지 않았습니다.", "", "", function(){this.close();});
	}
	
	var sysOpenDt = application.gds_userInfo.getColumn(0,"SYS_OPEN_DT");
	if(sysOpenDt == undefined || sysOpenDt ==""){
		this.gfn_message("info.처리불가","시스템 오픈일자가 없습니다.", "", "", function(){this.close();});
	}
	
	switch(registDt.length){
		case 14 :
			if(Number(registDt)<Number(sysOpenDt+"000000")){
				this.gfn_message("info.처리불가","차세대 시스템 오픈전 등록 데이터는 상세보기가 불가능합니다.", "", "", function(){this.close();});
			}
			break;
		case 8 :
			if(Number(registDt)<Number(sysOpenDt)){
				this.gfn_message("info.처리불가","차세대 시스템 오픈전 등록 데이터는 상세보기가 불가능합니다.", "", "", function(){this.close();});
			}
			break;
		default :
			this.gfn_message("info.처리불가","업무데이터 등록일시 자리수를 확인하여 주시기 바랍니다.", "", "", function(){this.close();});
			break;
	}
	
	var v_popupMsg = this.parent.arg_1;
	if(!v_popupMsg){
		this.gfn_message("comm.항목.필수","결재ID", "", "", function(){this.close();});
	}
	
	var comboParams = [
		    ["ds_FORMTYPE", "COM055", "1", " "]   // 서식유형
		  , ["ds_APPROVALSTATUS", "COM023", "1", " "]   // 결재진행상태
		];
	this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
}

this.fn_callbackAfter = function(){
	this.fv_objForm = this.parent.arg_0;
	var v_popupMsg = this.parent.arg_1;
	this.ds_cond.setColumn(0, "SANCTN_NO", v_popupMsg);
	this.fn_search();
}

this.fn_search = function(){
	this.ds_elctrnSanctnMastr.clearData();
	this.fn_transaction("SanctnInfo");
}

/**********************************************************************
 * TRANSACTION
 **********************************************************************/
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId){
	switch (strSvcId) {
		case "SanctnInfo":
			var sController   	= "hsco/com/sym/esm/SYM080100/SYM080100Info.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_elctrnSanctnMastr=output1 ds_confinfo=outputConf";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}

this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg){
	// 오류 및 성공 메시지를 화면에 보여준다.
	if (nErrorCode != 0){
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}

	if(this.ds_elctrnSanctnMastr.rowcount > 0 ){
		this.ds_elctrnSanctnMastr.applyChange();
		this.ds_elctrnSanctnMastr.set_rowposition(0);
	}      
}

/***********************************************************************
 * non-Transaction 관련 컴포넌트 이벤트 처리 함수
 ***********************************************************************/
this.btn_close_onclick = function(obj:Button,  e:nexacro.ClickEventInfo){
	this.close("");
}

this.btn_destroy_onclick = function(obj:Button,  e:nexacro.ClickEventInfo){
	this.close("");
}


this.btn_ink_drft_onclick = function(obj:Button,  e:nexacro.ClickEventInfo){
	var drft_url = this.ds_confinfo.getColumn(0, 'DRFT_URL');
	var empid = this.ds_elctrnSanctnMastr.getColumn(0, 'EMPID');
	var apprlist = this.ds_elctrnSanctnMastr.getColumn(0, 'APPRID');
	var apprdeptid = this.ds_elctrnSanctnMastr.getColumn(0, 'DRAFTERDEPTID');
	drft_url = drft_url+"?"
			 + "EMPID="+empid
			 + "&APPRIDLIST="+apprlist
			 + "&APPRDEPTID="+apprdeptid;
	system.execBrowser(drft_url);
}]]></Script>
