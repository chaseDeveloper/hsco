 */
this.btn_Reset_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	this.Div00.txarea_con.set_value("");
	this.Div00.txarea_sub.set_value("");
	this.Div00.txarea_con_onkeyup();
}
