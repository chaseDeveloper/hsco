	var sOutDatasets  	= "DS_PAGE=output1";
	
	var sArgs = "";	
	Ex.core.tran(this,sSvcID, sController, sInDatasets, sOutDatasets, sArgs); 

	this.div_work.edt_buttonID.set_readonly("true");
	this.div_work.edt_buttonNM.set_readonly("true");
	

