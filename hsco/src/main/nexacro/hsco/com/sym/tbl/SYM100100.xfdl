/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {
		case "asisTableList":			//AS-IS 테이블 목록
			var sController   	= "/hsco/com/sym/tbl/SYM100100/asisTableList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_asisTableList=output1";
			break;
			
		case "tobeTableList":			//TO-BE 테이블 목록
			var sController   	= "/hsco/com/sym/tbl/SYM100100/tobeTableList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_tobeTableList=output1";
			break;
			
		case "cnvrsDeList":				//전환일자 목록
			var sController   	= "/hsco/com/sym/tbl/SYM100100/cnvrsDeList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_cnvrsDeList=output1";
			break;
			
		case "dataConversionVrifyList": //데이터 전환 목록
			var sController   	= "/hsco/com/sym/tbl/SYM100100/dataConversionVrifyList.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_dataConversionVrifyList=output1";
			break;
			
		case "dataConversionVrifyCUD":	//데이터 전환 CUD
			var sController   	= "/hsco/com/sym/tbl/SYM100100/dataConversionVrifyCUD.do"; 
			var sInDatasets   	= "input1=ds_dataConversionVrifyList:U";
			var sOutDatasets  	= "";
			break; 
			
		case "dataConversionAllNewVrifyList": //데이터 전환 목록 일괄생성 
			var sController   	= "/hsco/com/sym/tbl/SYM100100/dataConversionAllNewVrifyList.do"; 
			var sInDatasets   	= "input1=ds_cond01";
			//var sOutDatasets  	= "ds_dataConversionAllNewVrifyList=output1";
			var sOutDatasets  	= "";
			break;
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}
	
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 
}

/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
		switch(strSvcId){
			case "cnvrsDeList":
				this.fn_add_comboHeader(this, this.div_WORK.cmb_cnvrsDe, "전체", "CODE", "CODE_NM");
				break;
			
			case "dataConversionVrifyCUD":
				this.ds_cnvrsDeList.clearData();
				this.fn_transaction("cnvrsDeList");
				break;
			
			case "dataConversionAllNewVrifyList":
				this.fn_transaction("cnvrsDeList");
				//this.fn_insertRow(this.ds_dataConversionAllNewVrifyList, this.ds_dataConversionVrifyList);
				this.ds_dataConversionVrifyList.clearData();
				this.ds_cond01.setColumn(0, "cnvrsDe", ""); 
				this.ds_cond01.setColumn(0, "jobSysNm", ""); 
				this.ds_cond01.setColumn(0, "lastDataAt", "1");
				this.fn_transaction("dataConversionVrifyList");
				break;
		}
	}else{
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);
	}
}

