/*
 * 함수명  	: fn_userPopup
 * 설명 		: 사용자찾기 팝업 처리
 * params  		: strCallback : 팝업 Callback
 * return Type 	: None
 * 사용법		: this.fn_userPopup(strCallback);
 */ 
this.fn_userPopup = function(strCallback){
    var parm = {
		arg_0 : this,  //==> 필수, 팝업을 호출하는 form을 참조할 수 있도록 팝업에 Argument로 넘겨준다. (반드시 기술해야 함.)
		arg_1 : this.ds_user  // 데이터셋
	};
	
	this.gfn_popup("UserPopup", 550, 500, "사용자찾기", parm, "com::COM010100_P01.xfdl", strCallback);
}
/*
 * 함수명  	: btn_userPopup_callback
 * 설명 		: 기안자 사용자찾기 팝업 콜백
 * params  		: 
 * return Type 	: None
 * 사용법		: this.btn_userPopup_callback(id, rtn);
 */ 
this.btn_userPopup_callback = function(id, rtn){
    this.ds_user.loadXML(rtn);
    this.ds_cond00.setColumn(0, "EMPNO", this.ds_user.getColumn(0, "EMPNO"));
    this.ds_cond00.setColumn(0, "USER_NM", this.ds_user.getColumn(0, "USER_NM"));
    this.ds_cond00.setColumn(0, "DEPT_CODE", this.ds_user.getColumn(0, "DEPT_CODE"));
}
/*
 * 함수명  	: btn_userPopup2_callback
 * 설명 		: 결재자 사용자찾기 팝업 콜백
 * params  		: 
 * return Type 	: None
 * 사용법		: this.btn_userPopup2_callback(id, rtn);
 */ 
this.btn_userPopup2_callback = function(id, rtn){
    this.ds_user.loadXML(rtn);
    this.ds_cond00.setColumn(0, "SANCTNER_EMPNO", this.ds_user.getColumn(0, "EMPNO"));
    this.ds_cond00.setColumn(0, "SANCTNER_EMPNM", this.ds_user.getColumn(0, "USER_NM"));
    this.ds_cond00.setColumn(0, "SANCTNER_DEPT_CODE", this.ds_user.getColumn(0, "DEPT_CODE"));
}
/*
 * 함수명  	: fn_initCalendar
 * 설명 		: Calendar 날짜를 서버날짜로 초기화
 * params  		: 
 * return Type 	: None
 * 사용법		: this.fn_initCalendar();
 */ 
this.fn_initCalendar = function(){
    var sdate = this.dateUtils.now();
    
    var strDate = sdate.substr(0,8);
    //trace("sdate====" + sdate + ", strDate====" + strDate);
    this.ds_cond00.setColumn(0, "BGNDT", strDate);
    this.ds_cond00.setColumn(0, "ENDDT", strDate);
}


