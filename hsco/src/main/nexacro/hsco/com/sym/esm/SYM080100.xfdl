 * return Type 	: None
 * 사용법		: this.div_search_btn_popup_onclick();
 */
this.div_search_btn_popup_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.ds_user.clearData();
	this.ds_user.addRow();
	this.ds_user.copyRow(0, this.ds_cond00, 0, "EMPNO=EMPNO,USER_NM=USER_NM");
	this.fn_userPopup("btn_userPopup_callback");
}

