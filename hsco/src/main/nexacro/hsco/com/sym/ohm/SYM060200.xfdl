 * 설명 		: 도움말 버전 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_saveHpcmVer = function()
{
	if(!this.comUtils.isDatasetUpdated(this.ds_hpcmVer)) {
        this.gfn_message("comm.데이터.변경.없음");
        return;
    } else {
		if(this.comUtils.isDatasetUpdated(this.ds_hpcmVer)){
			if(!this.gfn_checkValidation(this.ds_hpcmVer, this.ds_validation)) {
				return;
			}
		}
	} 
	
	if(this.gfn_message("comm.행위여부", "저장")){
		this.fn_getHpcmVerFileSn();
		this.fn_transaction("SYM060200CUD");
	}
}

/*
 * 함수명  	: fn_getHpcmVerFileSn
 * 설명 		: 도움말 버전 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_getHpcmVerFileSn = function()
{
	var nDsLen	= this.ds_hpcmVer.getRowCount();
	for (var i = 0; i < nDsLen; i++) {
		var nCurFileSn = this.ds_hpcmVer.getColumn(i, "FILE_SN");
		//if (this.comUtils.isNullEmpty(nCurFileSn) && Dataset.ROWTYPE_INSERT == this.ds_hpcmVer.getRowType(i)) {
		if (this.comUtils.isNullEmpty(nCurFileSn)) {
			this.fn_transaction("GetFileSn");
			this.ds_hpcmVer.setColumn(i, "FILE_SN", this.ds_fileSn.getColumn(0, "FILE_SN"));
		}
	}
}

