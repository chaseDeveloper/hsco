/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/


/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var argument     = "";
	var callbackFunc = "fn_callBack";
	var showProgress = true;
	var bAsync       = true;
	
	switch (strSvcId) {
		case "SYM060200List": 	// 온라인 도움말 버전 목록 조회
			var sController   	= "hsco/com/sym/ohm/SYM060200/SYM060200List.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_hpcmVer=output1";
			break;
		case "SYM060201List":	//온라인 도움말  목록 조회
			var sController   	= "hsco/com/sym/ohm/SYM060200/SYM060201List.do";
			var sInDatasets   	= "input1=ds_cond01";
			var sOutDatasets  	= "ds_hpcm=output1";
			break;
		case "SYM060202List":	//온라인 도움말 상세정보 목록 조회
			var sController   	= "hsco/com/sym/ohm/SYM060200/SYM060202List.do";
			var sInDatasets   	= "input1=ds_cond02";
			var sOutDatasets  	= "ds_hpcmDtls=output1";
			break;
		case "SYM060200CUD":	// 온라인 도움말 버전 정보 목록 저장
			var sController   	= "hsco/com/sym/ohm/SYM060200/SYM060200CUD.do";
			var sInDatasets   	= "input1=ds_hpcmVer:U";
			var sOutDatasets  	= "";
			break;
		case "SYM060201CUDWithFile":	// 온라인 도움말 정보 목록 저장
			var sController   	= "hsco/com/sym/ohm/SYM060200/SYM060201CUDWithFile.do";
			var sInDatasets   	= "input1=ds_hpcm:U";
			var sOutDatasets  	= "";
			break;
		case "SYM060202CUD":	// 온라인 도움말 정보 목록 저장
			var sController   	= "hsco/com/sym/ohm/SYM060200/SYM060202CUD.do";
			var sInDatasets   	= "input1=ds_hpcmDtls:U";
			var sOutDatasets  	= "";
			break;
		case "GetFileSn": 		// 파일일련번호 조회
			//var sController   	= "hsco/cmm/file/getFileSn.do";
			var sController   	= "hsco/com/sym/ohm/SYM060200/getFileSn.do";
			var sInDatasets   	= "";
			var sOutDatasets  	= "ds_fileSn=output_fsn";
			bAsync = false;
			break;
	}

	if (strSvcId == "SYM060201CUDWithFile") {
		this.File_UpDownManager.upload(sController, sInDatasets, sOutDatasets);
	} else {
		Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, argument, callbackFunc, showProgress, bAsync);
	}
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		:
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);

	if (nErrorCode != 0){
		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch (strSvcId) {
			case "SYM060200List": 	// 온라인 도움말 Ver  목록 조회
			    break;
			case "SYM060201List":	//온라인 도움말 목록 조회
				break;
			case "SYM060202List":	//온라인 도움말 상세정보 목록 조회
				break;
		    case "SYM060200CUD":	// 온라인 도움말 버전 정보 목록 저장
			    break;
			case "SYM060201CUD":	// 온라인 도움말 저장
			    break;
			case "SYM060202CUD":	// 온라인 도움말 저장
			    break;
			case "GetFileSn"	: 	// 첨부파일 일련번호
				break;
		}
	} else {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.

		switch (strSvcId) {
			case "SYM060200List": 	// 온라인 도움말 목록 조회
				break;
			case "SYM060200CUD":	// 온라인 도움말 버전 저장
				if (this.ds_cond.rowcount < 1) {
					this.ds_cond.addRow();
				}
				this.ds_cond.setColumn(0, "PAGE_ID", this.ds_hpcmVer.getColumn(0, "PAGE_ID"));			//화면ID
				this.ds_cond.setColumn(0, "MENU_ID", this.ds_hpcmVer.getColumn(0, "MENU_ID"));			//화면ID
				this.ds_cond.setColumn(0, "PAGE_NM", this.ds_hpcmVer.getColumn(0, "PAGE_NM"));			//화면명
				this.ds_cond.setColumn(0, "PAGE_URL", this.ds_hpcmVer.getColumn(0, "PAGE_URL"));		//화면 경로
			    this.fn_search();
			    break;
			case "SYM060201CUD":	// 온라인 도움말 저장
			    this.fn_hpcmSearch();
				break;
			case "SYM060202CUD":	// 온라인 도움말 저장
			    this.fn_hpcmDtlsSearch(); 
				break;
			case "GetFileSn"	:	// 첨부파일 일련번호
				break;
		}
	}
}


/**********************************************************************
* Popup Callback 영역
***********************************************************************/

 /*
 * 함수명  		: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		:
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	switch(sPopupId) {
		case "pagePopup":	// 화면목록 팝업 콜백
			if(sReturn != null && sReturn.length > 0) {
				var dsTemp = new Dataset();
				dsTemp.loadXML(sReturn);
				if (this.ds_cond.rowcount == 0) this.ds_cond.addRow();
				this.ds_cond.setColumn(0, "PAGE_ID", dsTemp.getColumn(0, "PAGE_ID"));		//화면ID
				this.ds_cond.setColumn(0, "MENU_ID", dsTemp.getColumn(0, "MENU_ID"));		//화면ID
				this.ds_cond.setColumn(0, "PAGE_NM", dsTemp.getColumn(0, "PAGE_NM"));		//화면명
				this.ds_cond.setColumn(0, "PAGE_URL", dsTemp.getColumn(0, "PAGE_URL"));		//화면 경로
				this.fn_search();
			}
			break;
		case "pagePopupRegist":	// 등록을 위한 화면목록 팝업 콜백
			if(sReturn != null && sReturn.length > 0) {
				var dsTemp = new Dataset();
				var nCurRow = this.ds_hpcmVer.rowposition;
				dsTemp.loadXML(sReturn);
				this.ds_hpcmVer.setColumn(nCurRow, "PAGE_ID", dsTemp.getColumn(0, "PAGE_ID"));		//화면ID
				this.ds_hpcmVer.setColumn(nCurRow, "MENU_ID", dsTemp.getColumn(0, "MENU_ID"));		//화면ID
				this.ds_hpcmVer.setColumn(nCurRow, "PAGE_NM", dsTemp.getColumn(0, "PAGE_NM"));		//화면명
				this.ds_hpcmVer.setColumn(nCurRow, "PAGE_URL", dsTemp.getColumn(0, "PAGE_URL"));	//화면 경로
			}
			break;
	}
}


