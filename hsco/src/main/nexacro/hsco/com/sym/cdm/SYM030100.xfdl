/***********************************************************************
 * 설명: 그룹코드 등록전 기 등록 체크
 ************************************************************************/
this.fn_dupCheck = function()
{
	if(this.utlf_necessaryParam(this.div_work.grd_GRP_CODE,"GRP_CODE,GRP_CODE_NM,SYS_SE_CODE,JOB_SE_CODE")) return; // 필수컬럼체크

  	var iRow = this.DS_GRP_CODE.rowposition;

    var grpcode = this.DS_GRP_CODE.getColumn(iRow, "GRP_CODE");
    var grpcodesn = this.DS_GRP_CODE.getColumn(iRow, "GRP_CODE_SN");
      
 	this.ds_cond00.clearData();
 	this.ds_dupcheck.clearData();
    this.ds_cond00.addRow();
 
    this.ds_cond00.setColumn(0, "GRP_CODE", grpcode);
    this.ds_cond00.setColumn(0, "GRP_CODE_SN", grpcodesn);

	this.ins_flag="N";

	if (this.fn_dupCheckSttus()) this.fn_callbackdupcheck();  // 기 그룹코드 중복 조회
}

this.fn_dupCheckSttus = function(){

 	var sSvcID        	= "SYM030100dupCheckSttus";                    
    var sController   	= "hsco/com/sym/cdm/SYM030100/SYM030100dupCheckSttus.do"; 
	var sInDatasets   	= "input1=ds_cond00";
	var sOutDatasets  	= "ds_dupcheck=output1";
	var sArgs = "";	
	
	var callbackFunc = "fn_callBack";
	var showProgress = true;
	var bAsync       = false;
	 	
	Ex.core.tran(this, sSvcID, sController, sInDatasets, sOutDatasets, sArgs, callbackFunc, null, bAsync);
	
	return true;

}

this.fn_callbackdupcheck = function(){

	if(this.ds_dupcheck.getRowCount() > 0){

		var cnt = this.ds_dupcheck.getColumn(0,"CNT");
		var grpcode = this.ds_dupcheck.getColumn(0,"GRP_CODE");
	
		var msg = ""+grpcode+"("+cnt+"건)";
	
		this.gfn_message("comm.데이터.기등록.존재", msg);	


				
	}
	else this.ins_flag="Y"
	
}										
