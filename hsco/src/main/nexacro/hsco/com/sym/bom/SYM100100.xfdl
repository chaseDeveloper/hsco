/*
 * 함수명  	: fn_checkUpdated
 * 설명 		: 결재모형 현황 그리드 변경 여부  체크
 * params  		: None
 * return Type 	: true(변경 내역 없음)/ false(변경내역 존재)
 * 사용법		: this.fn_checkUpdated();
 */
this.fn_checkUpdated = function(){
	if(this.comUtils.isDatasetUpdated(this.ds_innerSanctnMdl) || this.comUtils.isDatasetUpdated(this.ds_innerSanctionerInfo)){
	    if(this.gfn_message("confirm.변경.저장.여부")){
	    
		    this.fn_transaction("saveMdlList");
		    return false;
	    } else {
	        return true;
	    }
	} else {
	    return true;
	}
}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/

 
/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	switch (strSvcId) {
		case "MdlList": // 배치작업 목록 조회
		    trace("MdlList");
			var sController   	= "hsco/com/sym/ism/SYM100100/SYM100100List.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_innerSanctnMdl=output1";
			trace("ds_cond00.saveXML====" + this.ds_cond00.saveXML());
			break;
		case "SanctionerList": // 내부결재자 목록 조회
		    trace("SanctionerList");
		    this.ds_innerSanctionerInfo.clearData();
			var sController   	= "hsco/com/sym/ism/SYM100100/SYM100100SanctionerList.do";
			var sInDatasets   	= "input2=ds_cond01";
			var sOutDatasets  	= "ds_innerSanctionerInfo=output2";
			break;
		case "saveMdlList":	// 배치작업 정보 목록 저장
			var sController   	= "hsco/com/sym/ism/SYM100100/SYM100100CUD.do";	
			var sInDatasets   	= "input3=ds_innerSanctnMdl:U, input4=ds_innerSanctionerInfo:U";
			var sOutDatasets  	= "";
			trace("fn_transaction:::saveMdlList");
			break;
		case "saveSanctionerList":	// 내부결재자 정보 목록 저장
			var sController   	= "hsco/com/sym/ism/SYM100100/SYM100100SanctionerCUD.do";	
			var sInDatasets   	= "input4=ds_innerSanctionerInfo:U";
			var sOutDatasets  	= "";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets); 
}


 /**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
 /*
 * 함수명  	: fn_callbackAfter
 * 설명 		: callback 처리 후에 더 필요한 작업이 있을 경우, 작성
 * params  		: None
 * return Type 	: None
 * 사용법		: this.gfn_comboLoad(comboParams, this.fn_callbackAfter);
 */
this.fn_callbackAfter = function ()
{
    if(this.ds_jobSeCode.rowcount>0) this.ds_jobSeCode.setColumn(0, "CODE", "ALL");
}
 /*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	// 오류 및 성공 메시지를 화면에 보여준다.
	this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg);

	if (nErrorCode != 0){
		// 오류 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		switch (strSvcId) {
			case "MdlList": // 배치작업 목록 조회
				break;
			case "SanctionerList": // 내부결재자 목록 조회
				break;
			case "saveMdlList":	// 배치작업 정보 목록 저장
				break;
			case "saveSanctionerList":	// 내부결재자 정보 목록 저장
				break;
		}
	} else {
		// 성공 메시지를 보여주는 동작 외에 처리할 내용이 있으면 아래에 기술.
		// 처리할 내용이 없다면, switch 문은 생략합니다.
		
		switch (strSvcId) {
			case "MdlList": // 배치작업 목록 조회
			    trace("callback---MdlList");
				if(this.ds_innerSanctnMdl.rowcount > 0 ){
					this.ds_innerSanctnMdl.set_rowposition(0);
					this.fn_search2();
					this.ds_innerSanctnMdl.applyChange();
				}      
				break;
			case "SanctionerList": // 내부결재자 목록 조회
			    if(this.ds_innerSanctionerInfo.rowcount > 0 ){
					this.ds_innerSanctionerInfo.set_rowposition(0);
					this.ds_innerSanctionerInfo.applyChange();
				} 
				break;
			case "saveMdlList":	// 배치작업 정보 목록 저장
			    this.fn_transaction("MdlList");
				break;
			case "saveSanctionerList":	// 내부결재자 정보 목록 저장
				break;
		}
	}
}

