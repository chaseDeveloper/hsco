* 사용자 정의 함수
************************************************************************/
/*
 * 함수명  	: fn_setCss
 * 설명 		: 입력항목 enable/disable 및 선택적 필수 css  처리
 * params  		: obj : input 객체
                  boolEnable : Enable 항목 true/false
                  strCss     : input css Class  필수는 edt_WF_Essential  옵션은 ""
 * return Type 	: None
 * 사용법		: this.fn_setCss();
 */
this.fn_setCss = function(obj, boolEnable, strCss){
    var boolReadonly = false;
    if(boolEnable){
        boolReadonly= false;
    } else {
        boolReadonly = true;
    }
    obj.set_enable(boolEnable); 
    obj.set_readonly(boolReadonly); 
    obj.set_cssclass(strCss);
}

/*
 * 함수명  	: fn_setCssBySanctnAppnSe
 * 설명 		: 입력항목 enable/disable 및 선택적 필수 css  처리
 * params  		: boolEnable : Enable 항목 true/false
                  strCss     : input css Class  필수는 edt_WF_Essential  옵션은 ""
                  strCmbCss  : combo css Class  필수는 cmb_WF_Essential 옵션은 ""
 * return Type 	: None
 * 사용법		: this.fn_setCssBySanctnAppnSe(true, "edt_WF_Essential", "cmb_WF_Essential");
 */
this.fn_setCssBySanctnAppnSe = function(boolEnable, strCss, strCmbCss){
    this.fn_setCss(this.div_work.edt_sanctnerId01, boolEnable, strCss);
    this.fn_setCss(this.div_work.edt_sanctnerNm01, boolEnable, strCss);
    this.fn_setCss(this.div_work.cmb_sanctnDeptCd, boolEnable, strCmbCss);
    
}

