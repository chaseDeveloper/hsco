this.fn_schdulText = function()
{
  var curRow = this.ds_batchSchdul.rowposition;
  
  var executCycle = this.ds_batchSchdul.getColumn(curRow,"EXECUT_CYCLE");
  
  var year  = this.ds_batchSchdul.getColumn(curRow,"EXECUT_SCHDUL_YEAR");
  var month = this.ds_batchSchdul.getColumn(curRow,"EXECUT_SCHDUL_MONTH");
  var day   = this.ds_batchSchdul.getColumn(curRow,"EXECUT_SCHDUL_DAY");
  var hour  = this.ds_batchSchdul.getColumn(curRow,"EXECUT_SCHDUL_HOUR");
  var mnt   = this.ds_batchSchdul.getColumn(curRow,"EXECUT_SCHDUL_MNT");
  var secnd = this.ds_batchSchdul.getColumn(curRow,"EXECUT_SCHDUL_SECND");
  
  
  var text = "";
  
  switch(executCycle){
			case "01":  
			    text = "[ 매 일 ] " + hour + " 시 " + mnt + " 분 " + secnd + " 초";
			break;
			case "02":
			     text = this.fn_calText(curRow,hour,mnt,secnd);
			break;
			case "03":
			    text = "[ 매 월 ] " + day + " 일 " + hour + " 시 " + mnt + " 분 " + secnd + " 초";
			break;
			case "04":
			    text = "[ 매 년 ] " +  month + " 월 " + day + " 일 " + hour + " 시 " + mnt + " 분 " + secnd + " 초";
			break;
			case "05":
			    text = "[ 수 시 ] " + year + " 년 " +  month + " 월 " + day + " 일 " + hour + " 시 " + mnt + " 분 " + secnd + " 초";
			break;
	      }
  
  this.ds_batchSchdul.setColumn(curRow,"EXECUT_SCHDUL_TEXT",text);
}

this.fn_calText = function(curRow,hour,mnt,secnd)
{
 
  /*  1 : 일 , 2: 월 , 3: 화 , 4, 수 , 5 : 목 , 6 : 금 , 7 : 토 */
  
  var sun   = this.ds_batchSchdul.getColumn(curRow,"CHK_SUN");
  var mon   = this.ds_batchSchdul.getColumn(curRow,"CHK_MON");
  var tue   = this.ds_batchSchdul.getColumn(curRow,"CHK_TUE");
  var wed   = this.ds_batchSchdul.getColumn(curRow,"CHK_WED");
  var thu   = this.ds_batchSchdul.getColumn(curRow,"CHK_THU");
  var fri   = this.ds_batchSchdul.getColumn(curRow,"CHK_FRI");
  var sat   = this.ds_batchSchdul.getColumn(curRow,"CHK_SAT");
  
  var arry  = [sun,mon,tue,wed,thu,fri,sat];
  var temp  = ["일","월","화","수","목","금","토"];
  var calVal = "";
  var result = "";
 
      for(var i = 0 ; i < 7 ; i++)
	  {
		 if(arry[i] == "1")
		 {
			if(calVal == ""){
			  
			   calVal =  calVal + temp[i];
			}else{
	
			   calVal =  calVal + " / " + temp[i];  
			}
		 }
	   }
  result = "[ 매 주 ] " + calVal + " " + hour + " 시 " + mnt + " 분 " + secnd + " 초";
  
  return result;
  
}

this.fn_globalendisYn = function(executCycle)
{
  switch(executCycle){
		case "01":  
			 endisbleYn = [ false , false , false ];
			 this.fn_endisableProc(endisbleYn);  
			 this.fn_dayEndisableProc(false); // 요일 제어
		break;
		case "02":
			 endisbleYn = [ false , false , false ];
			 this.fn_endisableProc(endisbleYn);
			 this.fn_dayEndisableProc(true); // 요일 제어
		break;
		case "03":
			 endisbleYn  = [ false , false , true ];
			 this.fn_endisableProc(endisbleYn);
			 this.fn_dayEndisableProc(false); // 요일 제어
		break;
		case "04":
			 endisbleYn = [ false , true , true ];
			 this.fn_endisableProc(endisbleYn);
			 this.fn_dayEndisableProc(false); // 요일 제어
		break;
		case "05":
			 endisbleYn = [ true , true , true ];
			 this.fn_endisableProc(endisbleYn);
			 this.fn_dayEndisableProc(false);// 요일 제어
		break;
   }
   
	this.div_work.cmb_schdulHour.set_enable(true);
	this.div_work.cmb_schdulMnt.set_enable(true);
	this.div_work.cmb_schdulSecnd.set_enable(true);

}


/**********************************************************************
 * CALL TRANSACTION 영역
 **********************************************************************/
 /*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var bSync = true;
	
	switch (strSvcId) {		
		
	   case "selectBatchProc":		//배치작업조회
			var sController   	= "/hsco/com/sym/bom/SYM120100/selectBatchList.do"; 
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_batchOpert=output1";
			break; 
	  case "selectBatchSchdulList":		//배치스케줄 조회
			var sController   	= "/hsco/com/sym/bom/SYM120100/selectBatchSchdulList.do"; 
			var sInDatasets   	= "input1=ds_schVal";
			var sOutDatasets  	= "ds_batchSchdul=output1";
			break; 
	   case "saveBatchProc":		   //배치작업저장
			var sController   	= "/hsco/com/sym/bom/SYM120100/saveBatchProc.do"; 
			var sInDatasets   	= "input1=ds_batchOpert:U input2=ds_batchSchdul:U";
			var sOutDatasets  	= "ds_retunData=output1";
			break; 
	   case "GetInnerSanctnId": // 내부결재id 조회   
			var sController   	= "hsco/com/sym/ism/SYM090200/InnerSanctionId.do";
			var sInDatasets   	= "input1=ds_recomInfo";
			var sOutDatasets  	= "ds_recomInfo=output1";
			break;
	   case "saveInnerSanctionId":  // 내부결재 번호 저장   
			var sController   	= "/hsco/com/sym/bom/SYM120100/saveInnerSanctionId.do";
			var sInDatasets   	= "input1=ds_batchOpert:U";
			var sOutDatasets  	= "";
		    break;
		
	}
	
	if(this.fn_validate_tranasaction(strSvcId, sController, sInDatasets, sOutDatasets) == false){
		return;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, "", null, null, bSync); 

}


/**********************************************************************
 * Transaction Callback 영역
 ***********************************************************************/
/*
 * 함수명  	: fn_callBack
 * 설명 		: Transaction Callback 처리
 * params  		: strSvcId    : 서비스요청 ID
 *                nErrorCode  : 오류 메시지 코드 ([예] 0 : Success, 그밖의 경우, error)
 *                strErrorMsg : 오류 메시지([예]  success.insert|정상적으로 등록되었습니다.)
 * return Type 	: None
 * 사용법		: 
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	
	
	
	var bSuccess = nErrorCode == 0 ? true : false;
	if (bSuccess){
	    
		switch(strSvcId){
		    case "saveBatchProc":
		         this.fn_callback_message(strSvcId, nErrorCode, strErrorMsg); 
		         this.fn_search();
	        break;
	        case "selectBatchSchdulList":
		           var executCycle = this.ds_batchSchdul.getColumn(this.ds_batchSchdul.rowposition,"EXECUT_CYCLE"); 
		           this.fn_globalendisYn(executCycle);
	        break;
	        case "GetInnerSanctnId":  // 내부결재
				this.fn_popup_InnerSanctionDrft();
				break;
			case "saveInnerSanctionId":  // 내부결재번호 저장
			      this.fn_search();
			break;
			
				
		}
	}
	this.debug( (bSuccess==true?"Success : ":"Fail : ") + strSvcId);
}

