this.div_work_btn_addRow_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var crow = this.ds_batchOpert.getRowCount();
	
	if(crow < 1) return ;
	
	var santStt = this.ds_batchOpert.getColumn(this.ds_batchOpert.rowposition,"SANCTN_STTUS");
	
	if(santStt != "T12")
	{
	   this.gfn_message("info.처리불가","결재완료후 진행하십시요.");
	   return;
	}
	
	var batchOpertId = this.ds_batchOpert.getColumn(this.ds_batchOpert.rowposition,"BATCH_OPERT_ID");
	var batchOpertNm = this.ds_batchOpert.getColumn(this.ds_batchOpert.rowposition,"BATCH_OPERT_NM");
	
	var row  = this.ds_batchSchdul.addRow();
	
	this.ds_batchSchdul.setColumn(row,"BATCH_OPERT_ID",batchOpertId);
	this.ds_batchSchdul.setColumn(row,"BATCH_OPERT_NM",batchOpertNm);
	this.ds_batchSchdul.setColumn(row,"EXECUT_CYCLE","1");	
	
}


// 실행주기별 스케줄 조정
this.div_work_cmb_executCycle_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	var executCycle = e.postvalue;
	
	var year  = this.gfn_today("NOW_YEAR");
	var month = this.gfn_today("NOW_MONTH");
	var day   = this.gfn_today("NOW_DAY");
	
	var endisbleYn  = [ true , true , true];
	var endisValue  = [ "0000","00","00" ] ;
	
        switch(executCycle){
			case "01":  
			     endisbleYn = [ false , false , false ];
			     this.fn_endisableProc(endisbleYn);  
			     this.fn_endisValueProc(endisValue);
			     this.fn_dayProc("0",false); // 요일 제어
			break;
			case "02":
			     endisbleYn = [ false , false , false ];
			     this.fn_endisableProc(endisbleYn);
			     this.fn_endisValueProc(endisValue);
			     this.fn_dayProc("1",true); // 요일 제어
			break;
			case "03":
			     endisbleYn  = [ false , false , true ];
			     endisValue  = [ "0000","00","01" ] ;
			     this.fn_endisableProc(endisbleYn);
			     this.fn_endisValueProc(endisValue);
			     this.fn_dayProc("0",false); // 요일 제어
			break;
			case "04":
			     endisbleYn = [ false , true , true ];
			     endisValue  = [ "0000",month,"01" ] ;
			     this.fn_endisableProc(endisbleYn);
			     this.fn_endisValueProc(endisValue);
			     this.fn_dayProc("0",false); // 요일 제어
			break;
			case "05":
			     endisValue  = [year,month,day] ;
			     this.fn_endisableProc(endisbleYn);
			     this.fn_endisValueProc(endisValue);
			     this.fn_dayProc("0",false); // 요일 제어
			break;
	      }
	
	this.div_work.cmb_schdulHour.set_value("00");
	this.div_work.cmb_schdulMnt.set_value("00");
	this.div_work.cmb_schdulSecnd.set_value("00");
	
	this.div_work.cmb_schdulHour.set_enable(true);
	this.div_work.cmb_schdulMnt.set_enable(true);
	this.div_work.cmb_schdulSecnd.set_enable(true);
}


