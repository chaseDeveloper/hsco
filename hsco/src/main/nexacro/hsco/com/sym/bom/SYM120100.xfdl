/*
 * 함수명  	: fn_save
 * 설명 		: 저장
 * params  		: None
 * return Type 	: None
 * 사용법		: this.fn_save();
 */
this.fn_save = function(){
   
	// 배치작업
	if(this.ds_batchOpert.isUpdated() == true){
	
	   if(this.gfn_checkValidation(this.ds_batchOpert, this.ds_validation) == false){  
		  return;
	   }
	}
	
	// 배치스케줄
	if(this.ds_batchSchdul.isUpdated() == true){
	
	   if(this.gfn_checkValidation(this.ds_batchSchdul, this.ds_validation) == false){  
		  return;
	   }
	}
	
	 
	if( this.ds_batchOpert.isUpdated() == false && this.ds_batchSchdul.isUpdated() == false )
	{
	    this.gfn_message("comm.내역.변경.없음");
	    
	    return;
	}
	
	 if(this.gfn_message("comm.행위여부","저장"))
	 { 
	      this.ds_retunData.clearData();
	      this.fn_transaction("saveBatchProc");
	   
	    
	 }else{
	    return;
	 }           
}

