	};
	
	this.gfn_popup("UserPopup", 550, 500, "사용자찾기", parm, "com::COM010100_P01.xfdl", strCallback);
}
/*
 * 함수명  	: btn_userPopup2_callback
 * 설명 		: 결재자 사용자찾기 팝업 콜백
 * params  		: 
 * return Type 	: None
 * 사용법		: this.btn_userPopup2_callback(id, rtn);
 */ 
this.btn_userPopup2_callback = function(id, rtn){
    this.ds_user.loadXML(rtn);
    this.ds_user.getColumn(0, "ROW_NO");
    this.ds_innerSanctionerInfo.setColumn(this.userRow, "SANCTNER_ID", this.ds_user.getColumn(0, "EMPNO"));
    this.ds_innerSanctionerInfo.setColumn(this.userRow, "SANCTNER_NM", this.ds_user.getColumn(0, "USER_NM"));
    this.ds_innerSanctionerInfo.setColumn(this.userRow, "SANCTN_DEPT_CD", this.ds_user.getColumn(0, "DEPT_CODE"));
}


/*
 * 함수명  	: ds_innerSanctionerInfo_oncolumnchanged
 * 설명 		: 결재단계에 대한 순번을 설정한다.
 * params  		: 
 * return Type 	: None
 * 사용법		: this.ds_innerSanctionerInfo_oncolumnchanged();
 */ 
this.ds_innerSanctionerInfo_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "SANCTN_STEP"){
	    var val = obj.getColumn(e.row, e.columnid);

	    if(val == "S1"){
	        obj.setColumn(e.row, "SANCTN_SN", 1);
	        obj.setColumn(e.row, "SANCTN_APPN_SE", "T0");
	        this.fn_setCssBySanctnAppnSe(false, "", "");
	    } else if(val = "S2"){
	        obj.setColumn(e.row, "SANCTN_SN", 2);
	        obj.setColumn(e.row, "SANCTN_APPN_SE", "T0");
	        this.fn_setCssBySanctnAppnSe(false, "", "");
	    }
	}
}
