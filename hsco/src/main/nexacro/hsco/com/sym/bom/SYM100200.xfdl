{
	var sanctnApplSe = this.div_work.cmb_sanctnAppnSe.value;
	if(sanctnApplSe == "T1"){
	    this.fn_setCssBySanctnAppnSe(true, "edt_WF_Essential", "cmb_WF_Essential");
	} else if(sanctnApplSe == "T2" || sanctnApplSe == "T3" || sanctnApplSe == "T4"){ // P/L 부서장 소속부서
	    this.fn_setCssBySanctnAppnSe(false, "", "");
