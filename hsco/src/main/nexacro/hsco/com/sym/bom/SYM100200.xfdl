      			["ds_deptCode", "ds_searchDept", "", "선택", "hsco/com/sym/cdm/deptCode/selectDeptCode.do", 1],
      		];
      
      	this.gfn_comboLoadNonCodeTbl(comboParams, this.fn_callbackAfter, strDs, obj);
      }

