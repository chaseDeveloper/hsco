/*
 * 함수명  	: fn_checkPosProcInfo
 * 설명 		: 후처리여부 체크 시 validation 체크
 * params  		: None
 * return Type 	: true/ false(위배 시)
 * 사용법		: this.fn_checkPosProcInfo();
 */
this.fn_checkPosProcInfo = function(){
    
    if(this.ds_innerSanctnMdl.rowcount> 0){
        for(var i=0; i<this.ds_innerSanctnMdl.rowcount; i++){
            if(this.comUtils.isUpdateRow(this.ds_innerSanctnMdl, i)){
                if(this.ds_innerSanctnMdl.getColumn(i, "POSPROC_AT") == "1"){
                    var posprocClass = this.ds_innerSanctnMdl.getColumn(i, "POSPROC_CLASS")
                    if(posprocClass == "" || posprocClass == undefined){
                        this.gfn_message("comm.입력.요청.를", "후처리 클래스");
                        return;
                    }
                    var posprocPortinNm = this.ds_innerSanctnMdl.getColumn(i, "POSPROC_OPRTIN_NM")
                    if(posprocPortinNm == "" || posprocPortinNm == undefined){
                        this.gfn_message("comm.입력.요청.를", "후처리 메소드");
                    }
                    return;
                }
            }
        }
    }
}
