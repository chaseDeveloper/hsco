 ************************************************************************/
this.fn_cancel = function()
{
	if (this.comUtils.isDatasetUpdated(this.ds_critMenu)
	  || this.comUtils.isDatasetUpdated(this.ds_critSvc)
	  || this.comUtils.isDatasetUpdated(this.ds_critBtn)
	  || this.comUtils.isDatasetUpdated(this.ds_giveMenu)
	  || this.comUtils.isDatasetUpdated(this.ds_giveSvc)
	  || this.comUtils.isDatasetUpdated(this.ds_giveBtn)
	  || this.comUtils.isDatasetUpdated(this.ds_critUserGrp)
	  || this.comUtils.isDatasetUpdated(this.ds_giveUserGrp)) {
		if (this.gfn_message("confirm.변경.취소.여부")) {
			this.ds_critMenu.reset();
			this.ds_critSvc.reset();
			this.ds_critBtn.reset();
			this.ds_giveMenu.reset();
			this.ds_giveSvc.reset();
			this.ds_giveBtn.reset();
			this.ds_critUserGrp.reset();
			this.ds_giveUserGrp.reset();
		}
	}
}

/***********************************************************************
 * 설명: 신규처리(초기화)
 ************************************************************************/
this.fn_insert = function()
{
	if (this.gfn_message("comm.행위여부", "초기화")) {
		this.ds_selGiveUser.clearData();
		this.ds_selCritUser.clearData();
		this.ds_critMenu.clearData();
		this.ds_critSvc.clearData();
		this.ds_critBtn.clearData();
		this.ds_giveMenu.clearData();
		this.ds_giveSvc.clearData();
		this.ds_giveBtn.clearData();
		this.ds_critUserGrp.clearData();
		this.ds_giveUserGrp.clearData();
	}
}
