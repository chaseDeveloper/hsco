/***********************************************************************************
* CallBack Event (strSvcId - Sevice ID, nErrorCode - ErrorCode, strErrorMsg - Error Message)
***********************************************************************************/
/* callBack함수 */
/*
 * 함수명  	: fn_callback
 * 설명 		: combo에 대한 callback 함수 이후에 처리되어야 할 코딩 기술
 * params  		: svcId - Transaction Service ID.
 *				  ※ 반드시, 데이터를 담아 올 dataset명(예 : "ds_comCode03")을 사용해야 한다.
 * return Type 	: None
 * 사용법		: this.fn_init_dataset();
 * ※ 함수의 특성상, comboParams의 개수만큼 호출되므로, svcId로 분기하여 처리해야 한다.
 */
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg)
{
	if (nErrorCode < 0) 
	{
		this.gfn_callBackMsg(strSvcId, nErrorCode, strErrorMsg);
	} else {
		switch (strSvcId)
		{
			case "getCritListGrpByUser":

				//this.fn_searchCritUserAuth();
				break;
			case "getGiveListGrpByUser":
				//this.fn_searchGiveUserAuth(); 
				break;	
			case "getCritUserAuthAll":
				//alert("ds_critMenu = "+this.ds_critMenu.rowcount);
				//alert("ds_critSvc = "+this.ds_critSvc.rowcount);
				//alert("ds_critBtn = "+this.ds_critBtn.rowcount);
				break;	
			case "getGiveUserAuthAll":
				
				break;
			case "authCopy" :
				this.fn_searchGiveUser();
				//this.fn_searchCritUser();
				break;
			case "authExchange" :
				this.fn_searchGiveUser();
				this.fn_searchCritUser();
				break;
			case "critUserMenuSave" :
				this.fn_searchCritUserAuthMenu();
				break;
			case "critUserSvcSave" :
				this.fn_searchCritUserAuthSvc();
				break;
			case "critUserBtnSave" :
				this.fn_searchCritUserAuthBtn();
				break;
			case "giveUserMenuSave" :
				this.fn_searchGiveUserAuthMenu();
				break;		
			case "giveUserSvcSave" :
				this.fn_searchGiveUserAuthSvc();
				break;				
			case "giveUserBtnSave" :
				this.fn_searchGiveUserAuthBtn();
				break;		
			case "getGiveUserAuthSvc" :
			    break;
			case "getGiveUserAuthMenu" :
			    break;
			case "getGiveUserAuthBtn" :
				break;
			case "getCritUserAuthSvc" :
			    break;
			case "getCritUserAuthMenu" :
			    break;
			case "getCritUserAuthBtn" :
				break;
			default:
				break;
		}
	}
}


/**********************************************************************
* Popup Callback 영역
***********************************************************************/

 /*
 * 함수명  		: fn_popupCallBack
 * 설명 		: Popup Callback 처리
 * params  		: sPopupId    : 팝업 ID
 *                sReturn  : 팝업 처리 결과(dataset.saveXML())
 * return Type 	: None
 * 사용법		:
 */
this.fn_popupCallBack = function(sPopupId, sReturn)
{
	switch(sPopupId) {
		case "critPopup":	//기준사용자 팝업 콜백
			if(sReturn != null && sReturn.length > 0) {
				var dsTemp = new Dataset();
				dsTemp.copyData(this.ds_selCritUser);
				dsTemp.clearData();
				dsTemp.loadXML(sReturn);
				if (dsTemp.rowcount > 0) {
					if (this.ds_selCritUser.rowcount == 0) this.ds_selCritUser.addRow();
					this.ds_selCritUser.copyRow(0, dsTemp, 0);
				}
				this.fn_searchCritUser();
			}
			break;
		case "givePopup":	//권한부여 사용자 팝업 콜백
			if(sReturn != null && sReturn.length > 0) {
				var dsTemp = new Dataset();
				dsTemp.copyData(this.ds_selGiveUser);
				dsTemp.clearData();
				dsTemp.loadXML(sReturn);
				if (dsTemp.rowcount > 0) {
					if (this.ds_selGiveUser.rowcount == 0) this.ds_selGiveUser.addRow();
					this.ds_selGiveUser.copyRow(0, dsTemp, 0);
				}
				this.fn_searchGiveUser();
			}
			break;
	}
}

