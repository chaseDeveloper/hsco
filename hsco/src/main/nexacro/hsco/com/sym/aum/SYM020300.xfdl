    <Script type="xscript5.0"><![CDATA[/***********************************************************************
 * 01. 업무구분 : 권한 복사 및 교체
 * 02. 화면명   : SYM020300.xfdl
 * 03. 화면설명 : 권한그룹을  관리한다.
 * 04. 작성일   : 2016-01-13
 * 05. 작성자   : 고진호
 * 06. 수정이력 : 
 ***********************************************************************
 *     수정일     작성자   내용
 ***********************************************************************
 *    2016-01-13   고진호   최초작성 
 ***********************************************************************
 */

/***********************************************************************
 * Script Include
 ************************************************************************/
include "lib::comInclude.xjs";

/***********************************************************************
 * 화면 변수 선언부
 ************************************************************************/
 
/***********************************************************************
 * Form Function
 ************************************************************************/
/***********************************************************************
 * onInit / onLoad 영역
 ***********************************************************************/
this.form_oninit = function(obj:Form, e:nexacro.InitEventInfo)
{
	this.fn_init_form();		// Form 초기화
}

// Form Load 시 공통 기능 처리
this.form_onload = function (obj:Form, e:LoadEventInfo)
{

}

/*
 * 함수명  	: fn_init_form
 * 설명 		: Form을 초기화한다.
 * params  		: None
 * return Type 	: None
 * 사용법		: this.initForm();
 */
this.fn_init_form = function()
{
	this.gfn_initForm(this);	// 시스템 공통으로 Form을 Initialize 한다.
}


/***********************************************************************************
* Transaction Function
***********************************************************************************/

/*
 * 함수명  	: fn_transaction
 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var argument     = "";
	var callbackFunc = "fn_callBack";
	var showProgress = true;
	var bAsync       = true;
	
	switch (strSvcId) {
		case "getCritListGrpByUser": 	// 기준사용자 그룹 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListGrpByUser.do";
			var sInDatasets   	= "input1=ds_selCritUser";
			var sOutDatasets  	= "ds_critUserGrp=output1";
			bAsunc = false;
			break;
		case "getGiveListGrpByUser":	// 권한부여대상자 그룹 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListGrpByUser.do";
			var sInDatasets   	= "input1=ds_selGiveUser";
			var sOutDatasets  	= "ds_giveUserGrp=output1";
			bAsunc = false;
			break;
		case "getCritUserAuthAll":		// 권한부여대상자 메뉴, 서비스, 화면버튼 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListAllByUser.do";
			var sInDatasets   	= "input1=ds_selCritUser";
			var sOutDatasets  	= "ds_critMenu=output1 ds_critSvc=output2 ds_critBtn=output3";
			break;
		case "getGiveUserAuthAll":		// 권한부여대상자 메뉴,서비스, 화면버튼 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListAllByUser.do";
			var sInDatasets   	= "input1=ds_selGiveUser";
			var sOutDatasets  	= "ds_giveMenu=output1 ds_giveSvc=output2 ds_giveBtn=output3";
			break;
		case "authCopy" :				//권한복사
			var sController   	= "hsco/com/sym/aum/SYM020300/copyAuth.do";
			var sInDatasets   	= "input1=ds_authManage:U input2=ds_critMenu:U input3=ds_critSvc:U input4=ds_critBtn:U ";
			var sOutDatasets  	= "";
			break;
		case "authExchange" :			//권한교환
			var sController   	= "hsco/com/sym/aum/SYM020300/exchangeAuth.do";
			var sInDatasets   	= "input1=ds_authManage:U";
			var sOutDatasets  	= "";
			break;
		case "getCritUserAuthMenu" :	//기준사용자 메뉴 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListMenuByUser.do";
			var sInDatasets   	= "input1=ds_selCritUser";
			var sOutDatasets  	= "ds_critMenu=output1";
			break;
		case "getCritUserAuthSvc" :		//기준사용자 서비스 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListSvcByUser.do";
			var sInDatasets   	= "input1=ds_selCritUser";
			var sOutDatasets  	= "ds_critSvc=output1";
			break;
		case "getCritUserAuthBtn" :		//기준사용자 버튼 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListBtnByUser.do";
			var sInDatasets   	= "input1=ds_selCritUser";
			var sOutDatasets  	= "ds_critBtn=output1";
			break;
		case "getGiveUserAuthMenu" :	//권한부여대상자 메뉴 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListMenuByUser.do";
			var sInDatasets   	= "input1=ds_selGiveUser";
			var sOutDatasets  	= "ds_giveMenu=output1";
			break;
		case "getGiveUserAuthSvc" :		//권한부여대상자 서비스 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListSvcByUser.do";
			var sInDatasets   	= "input1=ds_selGiveUser";
			var sOutDatasets  	= "ds_giveSvc=output1";
			break;
		case "getGiveUserAuthBtn" :		//권한부여대상자 버튼 조회
			var sController   	= "hsco/com/sym/aum/SYM020300/getListBtnByUser.do";
			var sInDatasets   	= "input1=ds_selGiveUser";
			var sOutDatasets  	= "ds_giveBtn=output1";
			break;
		case "critUserMenuSave" :		//기준사용자 메뉴 개별저장
			var sController   	= "hsco/com/sym/aum/SYM020300/saveUserMenu.do";
			var sInDatasets   	= "input1=ds_critMenu:U";
			var sOutDatasets  	= "";
			break;
		case "critUserSvcSave" :		//기준사용자 서비스 개별저장
			var sController   	= "hsco/com/sym/aum/SYM020300/saveUserSvc.do";
			var sInDatasets   	= "input1=ds_critSvc:U";
			var sOutDatasets  	= "";
			break;
		case "critUserBtnSave" :		//기준사용자 버튼 개별저장
			var sController   	= "hsco/com/sym/aum/SYM020300/saveUserBtn.do";
			var sInDatasets   	= "input1=ds_critBtn:U";
			var sOutDatasets  	= "";
			break;
		case "giveUserMenuSave" :		//부여대상자 메뉴 개별저장
			var sController   	= "hsco/com/sym/aum/SYM020300/saveUserMenu.do";
			var sInDatasets   	= "input1=ds_giveMenu:U";
			var sOutDatasets  	= "";
			break;
		case "giveUserSvcSave" :		//부여대상자 서비스 개별저장
			var sController   	= "hsco/com/sym/aum/SYM020300/saveUserSvc.do";
			var sInDatasets   	= "input1=ds_giveSvc:U";
			var sOutDatasets  	= "";
			break;
		case "giveUserBtnSave" :		//부여대상자 버튼 개별저장
			var sController   	= "hsco/com/sym/aum/SYM020300/saveUserBtn.do";
			var sInDatasets   	= "input1=ds_giveBtn:U";
			var sOutDatasets  	= "";
			break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, argument, callbackFunc, showProgress, bAsync);
}

//사용자 및 권한 부여 대상자 그룹 권한 조회
this.fn_search = function() 
{
	this.fn_searchCritUser();
	this.fn_searchGiveUser();
}

//기준사용자 그룹조회
this.fn_searchCritUser = function()
{
	if (this.ds_selCritUser.rowcount == 0) return;
	this.ds_critUserGrp.clearData();
	this.fn_transaction("getCritListGrpByUser");
}
//기준사용자 그룹조회
this.fn_searchGiveUser = function() 
{
