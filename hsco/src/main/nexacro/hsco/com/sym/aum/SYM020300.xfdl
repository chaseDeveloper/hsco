this.fn_setSelectExchange = function()
{
	this.ds_authManage.clearData();
	this.ds_authManage.addRow();
	
	var strCritUserId = this.ds_critUserGrp.getColumn(0, "USER_ID");
	var strGiveUserId = this.ds_giveUserGrp.getColumn(0, "USER_ID");
	
	this.ds_authManage.setColumn(0, "CRIT_USER_ID", strCritUserId);
	this.ds_authManage.setColumn(0, "GIVE_USER_ID", strGiveUserId);
	
	return true;
}


//데이터 설정
this.fn_setSelectAuth = function(strValue)
{
	if (this.comUtils.isNullEmpty(strValue)) {
		strValue = "COPY";
	}

	this.ds_authManage.clearData();
	this.ds_authManage.addRow();
	
	var nCritRow = this.ds_critUserGrp.rowposition;
	var nGiveRow = this.ds_giveUserGrp.rowposition;
	var strCritUserId = this.ds_critUserGrp.getColumn(nCritRow, "USER_ID");
	var strGiveUserId = this.ds_giveUserGrp.getColumn(nGiveRow, "USER_ID");
	var strCritAuthId = this.ds_critUserGrp.getColumn(nCritRow, "AUTH_GRP_ID");
	var strGiveAuthId = this.ds_giveUserGrp.getColumn(nGiveRow, "AUTH_GRP_ID");
	
	//데이터 설정
	if (nCritRow == -1) return false;

	this.ds_authManage.setColumn(0, "CRIT_USER_ID", strCritUserId);
	this.ds_authManage.setColumn(0, "CRIT_AUTH_GRP_ID", strCritAuthId);
	
	//복사인 경우(부여대상자 선택이 없는 경우)
	if (strValue == "COPY") {
		if (nGiveRow == -1) {
			var strSelUserId = this.ds_selGiveUser.getColumn(0, "USER_ID");
			if (this.comUtils.isNullEmpty(strSelUserId)) {
				return false;
			} else {
				this.ds_authManage.setColumn(0, "GIVE_USER_ID", strSelUserId);
			}
			this.ds_authManage.setColumn(0, "GIVE_AUTH_GRP_ID","");
		} else {
			this.ds_authManage.setColumn(0, "GIVE_USER_ID", strGiveUserId);
			this.ds_authManage.setColumn(0, "GIVE_AUTH_GRP_ID", strGiveAuthId);
		}
	} else if (strValue == "EXCHANGE") {
		return false;
	}
	return true;
}



