//권한복사 체크
this.fn_checkAuthCopy = function()
{
	var nCnt = this.ds_critUserGrp.rowcount;
	
	if (nCnt <= 0) {
		this.gfn_message("comm.항목.선택.요청", "기준사용자 권한");
		return false;
	}
	/*
	var nCnt = this.ds_critUserGrp.getCaseCount("CHK == '1'");
	if (nCnt <= 0) {
		this.gfn_message("comm.항목.선택.요청", "기준사용자 권한");
		return false;
	} else if (nCnt >= 2) {
		this.gfn_message("info.파일선택.알림", "2건");
		return false;
	}
	*/
	
	//받는 대상자가 있는지 확인
	if (this.ds_selGiveUser.rowcount == 0) {
		//this.gfn_message("info.선택데이터.없음.선택.요청");
		this.gfn_message("fail.정보검색.실패", "권한부여대상자를");
		return false;
	}
	//받는 대상자  UserId 확인
	if(this.comUtils.isNullEmpty(this.ds_selGiveUser.getColumn(0, "USER_ID"))) {
		//this.gfn_message("info.선택데이터.없음.선택.요청");
		this.gfn_message("fail.정보검색.실패", "권한부여대상자를");
		return false;
	}
	
	//받는 대상자에게 동일권한이 있는지 확인
	var critAuthRow = this.ds_critUserGrp.rowposition;
	var critAuth = this.ds_critUserGrp.getColumn(critAuthRow, "AUTH_GRP_ID");
	var sameAuthRow  = this.ds_giveUserGrp.findRow("AUTH_GRP_ID", critAuth);
	
	if (sameAuthRow != -1) {
		this.gfn_message("comm.데이터.기등록.존재",  "권한부여 대상자에게 " + critAuth + " 권한");
		return false;
	}
	
	//기준사용자의 하단 변경정보 저장
	
	if (critAuth == this.ds_selCritUser.getColumn(0, "AUTH_GRP_ID")) {
		if(this.comUtils.isDatasetUpdated(this.ds_critMenu) 
		|| this.comUtils.isDatasetUpdated(this.ds_critSvc)
		|| this.comUtils.isDatasetUpdated(this.ds_critBtn)) {
			//메세지 출력
			if (!this.gfn_message("comm.행위여부", "기준사용자 변경정보를 적용하여 복사합니다. \\n 진행"))
				return false;
		}
	}
	return true;
}

