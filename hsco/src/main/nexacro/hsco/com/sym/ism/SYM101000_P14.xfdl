/***********************************************************************
 * Script Include 
 ***********************************************************************/
// 시스템 공통 라이브러리 Include
include "lib::comInclude.xjs";
include "lib::comInnerSanct.xjs";
include "lib::comFile.xjs";
include "lib::comUpDownUtils.xjs";	// 메인화면 파일 업로드/다운로드 라이브러리

//내부결재 저장여부 확인
/***********************************************************************
* onInit / onLoad 영역
***********************************************************************/
this.SYM101000_P14_oninit = function(obj:Form, e:nexacro.InitEventInfo){
	//form init
	this.gfn_initForm(this);
	//dataset init
	this.ds_innerSanctnCond.clearData();
	if(this.ds_innerSanctnCond.rowcount==0) this.ds_innerSanctnCond.addRow();

	this.ds_SanctnSttusCode.clearData();
	this.ds_innerSanctnMastr.clearData();
	this.ds_apprline.clearData();
	this.ds_innserSanctnReprtInfo.clearData();
	this.ds_innerSanctnReprtData.clearData();	
}

this.SYM101000_P14_onload = function(obj:Form, e:nexacro.LoadEventInfo){
	this.fv_objForm = this.parent.arg_0;
	this.ds_innerSanctnCond.setColumn(0,"INNER_SANCTN_ID",this.parent.innerSanctnId);
	this.ds_innerSanctnCond.setColumn(0,"MASTR_U","Y"); //결재UPDATE 정보조회
	//this.ds_SanctnSttusCode = this.parent.ds_SanctnSttusCode;
	this.fn_search();
}

/***********************************************************************
* Transaction
***********************************************************************/
this.fn_search = function(){
	this.fn_transaction("innerSanctnInfo");
}

this.fn_transaction = function(strSvcId, innerSanctnId){
	var argument     = "";
	var callbackFunc = "fn_callBack";
	var showProgress = true;
	var bAsync       = false;
	var sController   	= "";
	var sInDatasets   	= "";
	var sOutDatasets  	= "";
	
	switch (strSvcId) {
		case "innerSanctnInfo":
			this.isViewMode=false;
	        sController   	= "hsco/com/sym/ism/SYM090200/innerSanctnInfo.do";	
			sInDatasets   	= "input1=ds_innerSanctnCond";
			sOutDatasets  	= "ds_innerSanctnMastr=innerSanctnMastr "
							+"ds_apprline=innerSanctnReprtApprline "
							+"ds_innserSanctnReprtInfo=innerSanctnReprtInfo "
							+"ds_innerSanctnReprtData=innerSanctnReprtData "
							+"ds_SanctnSttusCode=sanctnSttusCode ";
	        break;
	}
	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, argument, callbackFunc, showProgress, bAsync);
}

// fn_transaction callback
this.fn_callBack = function (strSvcId, nErrorCode, strErrorMsg){
	
    if (nErrorCode != 0){
		// 비정상
		switch (strSvcId) {
		    case "innerSanctnInfo":
		        break;
		}
		this.gfn_callback_message(strSvcId, nErrorCode, strErrorMsg); //process message
	} else {	
		switch (strSvcId) {
			case "innerSanctnInfo" :
				var innerSanctnId = this.ds_innerSanctnMastr.getColumn(0, "INNER_SANCTN_ID");
				if(this.ds_innserSanctnReprtInfo.getRowCount() > 0 && this.ds_innerSanctnReprtData.getRowCount() > 0) {
					// report용 메타 데이터셋 초기화
					this.gfn_makeRptDatasets(this.ds_innerSanctnReprtData, this.ds_apprline);
					// report용 데이터 조회
					this.gfn_selectReportDs(this.ds_innserSanctnReprtInfo, this.ds_innerSanctnReprtData);
				}
				
 				this.gfn_exportRptToPdf(this.ds_innerSanctnMastr.getColumn(0, "INNER_SANCTN_ID")
 						,this.ds_innserSanctnReprtInfo.getColumn(0, "REPRT_URL")
 						,this.ds_innerSanctnMastr.getColumn(0,"JOB_SE")
 						,this.parent.sanctn_sttus
 						,this.ds_innerSanctnMastr.getColumn(0,"LAST_SANCTNER_ID")==this.ds_innerSanctnMastr.getColumn(0,"CURR_SANCTNER_ID")
 						,this.vOpts.getLocalDs()
 						,this.ds_SanctnSttusCode);
				this.close("0000");
				break;
	    }
	}
}
this.btn_exit_onclick = function(obj:Button,  e:nexacro.ClickEventInfo){
	this.close("");
}
