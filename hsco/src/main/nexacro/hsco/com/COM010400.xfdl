	};
	
	this.gfn_popup("Popup_0", 710, 636, "전자결재진행 현황 검색", parm, "com::COM010400_P02.xfdl", "div_work_btn_popupSearchSanctnStts_callback");
}

this.div_work_btn_popupSearchSanctnStts_callback = function (id, rtn)
{
	
}




]]></Script>
