//		this.div_search.div_srchDept.fn_setBind("ds_cond","DEPT_CODE","DEPT_NM");		
// 		this.ds_cond.setColumn(0, "DEPT_CODE", "000000"        );	// 부서코드
// 		this.ds_cond.setColumn(0, "DEPT_NM",   "화성도시공사");	// 부서명
	}	
