		if(!this.comUtils.isNull(dsParentSrch.getColumn(0, "BALC_SE"))) {
			balcSe   = dsParentSrch.getColumn(0, "BALC_SE");
		}
		if(!this.comUtils.isNull(dsParentSrch.getColumn(0, "DEPT_CODE"))) {
