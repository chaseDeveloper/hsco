		}
		if(!this.comUtils.isNull(dsParentSrch.getColumn(0, "DEPT_NM"))) {
			deptNm   = dsParentSrch.getColumn(0, "DEPT_NM");
		}
		if(!this.comUtils.isNull(dsParentSrch.getColumn(0, "MONTH"))) {
