/*
 * 함수명  	: div_taxitmCode_grd_expndtrUnitCode_oncelldblclick
 * 설명 		: Grid 셀 더블클릭 후 이벤트 처리(세목/지출단위 코드 Grid)
 * params  		: None
 * return Type 	: None
 * 사용법		: this.div_taxitmCode_grd_expndtrUnitCode_oncelldblclick(obj,e);
 */
this.div_taxitmCode_grd_expndtrUnitCode_oncelldblclick = function(obj:Grid, e:nexacro.GridClickEventInfo)
{
	var dsTemp = new Dataset();
	dsTemp.copyData(this.ds_expndtrUnitCode);
	dsTemp.clearData();
	var nRow = dsTemp.addRow();
	dsTemp.copyRow(nRow, this.ds_expndtrUnitCode, this.ds_expndtrUnitCode.rowposition);
	dsTemp.addColumn("GWAN"  , "string", 256);
	dsTemp.addColumn("HANG"  , "string", 256);
	dsTemp.addColumn("SEHANG", "string", 256);
	dsTemp.addColumn("MOK"   , "string", 256);
	dsTemp.addColumn("SEMOK" , "string", 256);
	
	var acRow  = this.ds_acntCode.rowposition;
	var gwan   = this.ds_acntCode.getColumn(acRow, "GWAN");
	var hang   = this.ds_acntCode.getColumn(acRow, "HANG");
	var sehang = this.ds_acntCode.getColumn(acRow, "SEHANG");
	var mok    = this.ds_acntCode.getColumn(acRow, "MOK");
	var semok  = this.ds_acntCode.getColumn(acRow, "SEMOK");
	
	dsTemp.setColumn(nRow, "GWAN"  , gwan);
	dsTemp.setColumn(nRow, "HANG"  , hang);
	dsTemp.setColumn(nRow, "SEHANG", sehang);
	dsTemp.setColumn(nRow, "MOK"   , mok);
	dsTemp.setColumn(nRow, "SEMOK" , semok);
	
	this.close(dsTemp.saveXML());
}



