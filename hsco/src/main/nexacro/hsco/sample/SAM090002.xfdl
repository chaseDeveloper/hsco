	if(ds.rowcount < 1) return;

	var r = ds.rowposition;

	var tbl_name = ds.getColumn(r, "TABLE_NAME");

	var s = "";
	var keys = new Array();
	s += '      <Dataset id="'+dsId+'">\r\n';
	s += '        <ColumnInfo>\r\n';
	for(var i=0;i < ds.rowcount;i++)
	{
		var colid = ds.getColumn(i, "COLUMN_NAME");
		var colAlias = ds.getColumn(i, "COL_ALIAS");
		colAlias = Eco.isEmpty(colAlias) ? colid : colAlias;
		
		var cType = ds.getColumn(i, "DATA_TYPE");
		var sType = 'STRING';
		var sSize = '255';
		switch(cType)
		{
			case 'NUMBER':
				sType = 'BIGDECIMAL';
				sSize = ds.getColumn(i, "LEN2");
				break;
			case 'CHAR':
			case 'VARCHAR2':
				sSize = ds.getColumn(i, "LEN");
				break;
			case 'DATE':
				sSize = '22';
				break;
		    case 'TIMESTAMP':
				sSize = '22';
				break;
			default:
				break;
		}
		s += '          <Column id="' + colAlias + '" type="' + sType + '" size="' + sSize + '"/>\r\n';
	}

	s += '        </ColumnInfo>\r\n';
	s += '      </Dataset>\r\n';
	s += '\r\n';

	this.setTxt(s);
}

