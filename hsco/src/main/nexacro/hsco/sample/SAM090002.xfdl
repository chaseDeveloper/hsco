this.fn_selecttable = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo){
    var tblNm = obj.getColumn(e.newrow, "TABLE_NAME");
    if(tblNm == "TBCOM_CMMN_ATCHMNFL_MASTR"){
        this.ds_2.copyData(this.ds_21, false);
        this.ds_2.applyChange();
    } else {
        this.ds_2.copyData(this.ds_22, false);
        this.ds_2.applyChange();
    }
}

this.fn_checkColDupl = function(){
    var boolChk = true;
    this.ds_chkCol.copyData(this.ds_02, false);
    this.ds_chkCol.applyChange();
    for(var i=0; i<this.ds_chkCol.rowcount; i++){
    
        var colNm = this.ds_chkCol.getColumn(i, "COLUMN_NAME");
        var alias = this.ds_chkCol.getColumn(i, "COL_ALIAS");
        if(alias=="" || alias== undefined ){
            this.ds_chkCol.setColumn(i, "COL_ALIAS", colNm);

        } 
    }
    this.ds_chkCol.applyChange();
    this.ds_filtering.copyData(this.ds_chkCol, false);
    var rCnt = this.ds_chkCol.rowcount;
    for(var i=0; i < rCnt; i++){
        this.ds_filtering.filter("COL_ALIAS=='"+this.ds_chkCol.getColumn(i, "COL_ALIAS")+"'");
        
        if(boolChk && this.ds_filtering.rowcount > 1){
            alert(this.ds_chkCol.getColumn(i, "COL_ALIAS")+" 컬럼항목이 중복되어습니다.\n 컬럼 또는 컬럼 ALIAS를 확인하세요.");
            boolChk = false;
            i = rCnt;
        }
    }
    return boolChk;
    
}


