	var r = ds.rowposition;

	var tbl_name = ds.getColumn(r, "TABLE_NAME");

	var s = "";
    s += 'Camel Notation ::: \r\n';
	for(var i=0;i < ds.rowcount;i++)
	{
		var colid = ds.getColumn(i, "COLUMN_NAME");
		var colAlias = ds.getColumn(i, "COL_ALIAS");
