this.div_w_btn_genSql_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
    this.ds_01.set_keystring("S:ALIAS+TABLE_NAME");
    this.ds_02.set_keystring("S:COLUMN_ID+COLUMN_NAME");
    
    var sqlGb = this.div_w.cmb_jobSe.value;
    var sqlId = this.div_w.edt_sqlId.value;
	var sqlComment = this.div_w.edt_sqlComment.value;
	if(sqlGb == "" || sqlGb == undefined) {
	    this.alert("업무구분을 선택하세요.");
	    return;
	}
	if(sqlId == "" || sqlId == undefined) {
	    this.alert("SELECT ID를 입력하세요.");
	    return;
	}
	if(sqlComment == "" || sqlComment == undefined) {
	    this.alert("SQL 주석을 입력하세요.");
	    return;
	}

	if(!this.fn_checkColDupl()) return;
	
	var ds = this.ds_02;
	if(ds.rowcount < 1) return;
 	if(this.ds_01.rowcount < 1) return;

	var r = ds.rowposition;

	var s_fix = this.div_w.edt_sFix.value;

	var s = "";


	// SELECT
	s += "	<!-- " + sqlComment + " 조회 -->\r\n";
	s += '	<select id="'+ sqlGb + sqlId + '.' + s_fix + 'List" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">\r\n';
	s += "SELECT\r\n";
	for(var i=0;i < ds.rowcount;i++)
	{
		var colid = ds.getColumn(i, "COLUMN_NAME");
		var alias = ds.getColumn(i, "ALIAS")
		if(colid == 'REGISTER_ID') continue;
		if(colid == 'REGIST_DT') continue;
		if(colid == 'UPDUSR_ID') continue;
		if(colid == 'UPDT_DT') continue;
		var colAlias = ds.getColumn(i, "COL_ALIAS");
		colAlias = Eco.isEmpty(colAlias) ? "" : " AS " + colAlias + " ";

		var remark = ds.getColumn(i, "C_NM");
		remark = Eco.isEmpty(remark) ? "\r\n" : "\t\t\t\/** " + remark + " *\/\r\n";
		s += (i==0 ? '\t '+ alias +'.' : '\t,'+ alias +'.') + colid + colAlias + remark;

	}
	
	s += "FROM ";
	for(var i=0;i < this.ds_01.rowcount;i++) {
	    if(i!=0) s += " ,  ";
	    var tbl_name = this.ds_01.getColumn(i, "TABLE_NAME") + " " 
	    var tbl_alias = this.ds_01.getColumn(i, "ALIAS") + " ";
	    var tbl_remark = this.ds_01.getColumn(i, "T_NM");
	    tbl_remark = Eco.isEmpty(tbl_remark) ? "\r\n" : "\t\t\t\/** " + tbl_remark + " *\/\r\n";
	    s += tbl_name + tbl_alias + tbl_remark;
	}
	
	
	s += 'WHERE 1=1\r\n';

	for(var i=0; i<ds.rowcount; i++){
	    if(ds.getColumn(i, "WHERE_AT") == 1){
	         var remark3 = ds.getColumn(i, "C_NM");
		     remark3 = Eco.isEmpty(remark3) ? "\r\n" : "\t\t\t\/** " + remark3 + " *\/\r\n";
	         s += '  AND ' + ds.getColumn(i, "ALIAS") + '.' + ds.getColumn(i, "COLUMN_NAME") + " = #" + ds.getColumn(i, "COLUMN_NAME") + "#" + remark3;
	    } 
	}

	
//	s += '<isNotEmpty property="CD1" >\r\n';
//	s += "</isNotEmpty>\r\n";
	s += "ORDER BY 1,2,3\r\n";
	s += "	</select>\r\n\r\n";
	


	this.setTxt(s);
	
	
}

