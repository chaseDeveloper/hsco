this.getCamels = function(sList)
{
	var sArr = sList.toLowerCase().split('_');
	var sOut = '';
	for(var i in  sArr)
		sOut += (i == 0) ? sArr[i] : this.getCamelWord(sArr[i]);

	return sOut;
}

this.getCamelWord = function(sName)
{
	if(Eco.isEmpty(sName))
		return '';
	return sName[0].toUpperCase() + sName.substr(1);
}




