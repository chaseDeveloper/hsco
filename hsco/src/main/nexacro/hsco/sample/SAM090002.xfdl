 * 설명 		: Transaction 요청 처리 함수
 * params  		: strSvcId : 서비스요청 ID
 * return Type 	: None
 * 사용법		: this.fn_transaction("SELECT");
 */
this.fn_transaction = function(strSvcId)
{
	var sArgs = "";	
//	sArgs += Ex.util.setParam("OWNER",			this.div_search.edt_owner.value);
	// like 조건 땜에 적용 불가
// 	var gb = this.div_search.cmb_jobSeCode.value;
// 	if(gb=="" || gb == undefined){
// 	    var tblName = this.div_search.edt_tblNm.value;
// 	    if(tblName != "" && tblName != undefined){
// 	        gb = tblName.substr(2,3);
// 	    }
// 	} 

// 	sArgs += Ex.util.setParam("GB", this.div_search.cmb_jobSeCode.value);
// 
// 	
// 	
// 	sArgs += Ex.util.setParam("TABLE_NAME",		this.div_search.edt_tblNm.value);
// 	sArgs += Ex.util.setParam("T_NM",			this.div_search.edt_tNm.value);
// 	sArgs += Ex.util.setParam("COLUMN_NAME",	this.div_search.edt_colNm.value);
// 	sArgs += Ex.util.setParam("C_NM",			this.div_search.edt_cNm.value);
// 	trace("Param2:"+sArgs);

	switch (strSvcId) {
		case "list_Tbls.mList": // 테이블 목록 조회
			var sController   	= "hsco/dev/codegen/listTbl.do";
			var sInDatasets   	= "input1=ds_cond";
			var sOutDatasets  	= "ds_1=output1";
			break;
		case "list_Cols.rList": // 컬럼 목록 조회
			var sController   	= "hsco/dev/codegen/listCols.do";
			var sInDatasets   	= "input1=ds_cond00";
			var sOutDatasets  	= "ds_2=output1";
			break;
	}

	Ex.core.tran(this, strSvcId, sController, sInDatasets, sOutDatasets, sArgs); 
}

